        nftMetaData[tokenId].forSale = false;
        _transfer(creator, payable(buyer), tokenId);

        (bool success, ) = creator.call{value: (msg.value * 80) / 100}("");
        require(success, 'transfer to creator failed');
        (success, ) = jelly.call{value: (msg.value * 20) / 100}("");
        require(success, 'transfer to jelly failed');
    
    }

    /**
     * @notice takes payment from buyer, calls the transfer function, and distributes payment
     * sends 90% of ether to former owner, 3% to originalowner (us), 7% to client
     * @param tokenId token that is being bought
     */
    function purchaseTokenMarketPlace(uint256 tokenId) external payable {
        require(creator != ownerOf(tokenId), "PORTRAITS: Creators can't use marketplace function");
        require(msg.value == nftMetaData[tokenId].price, "PORTRAITS: Payment-Amount is wrong");
        require(nftMetaData[tokenId].forSale == true, "PORTRAITS: This Token is not for sale");
