                sender := and(
                    mload(add(array, index)),
                    0xffffffffffffffffffffffffffffffffffffffff
                )
            }
        } else {
            sender = payable(msg.sender);
        }
        return sender;
    }
}

// File: contracts/child/ChildToken/Portraits.sol
contract Portraits is
    ERC721,
    ERC721Pausable,
    ERC721Enumerable,
    ERC721URIStorage,
    IChildToken,
    AccessControlMixin,
    EIP712Base,
    ContextMixin
{
    bytes32 public constant DEPOSITOR_ROLE = keccak256("DEPOSITOR_ROLE");
    mapping (uint256 => bool) public withdrawnTokens;

    // limit batching of tokens due to gas limit restrictions
    uint256 public constant BATCH_LIMIT = 20;
    uint256 public constant MULTIPL_COORDINATES = 10000000; // coordinates stored with 7 decimal places, e.g. 11.2435483

    struct MetaDataToken {
        uint256 price;
        bool forSale;
        string edition;
    }
    
    address payable public creator;
    address payable public jelly;

    // marks whether this collection is currently on display
    bool public saleRunning;
    mapping(uint256 => MetaDataToken) public nftMetaData;

    // keeping track of token listed on the marketplace
    mapping(uint256 => uint256) public resaleTokenIndex;
    mapping(uint256 => uint256) public resaleTokenByIndex;
    uint256[] public resaleToken;

    event WithdrawnBatch(address indexed user, uint256[] tokenIds);
    event TransferWithMetadata(address indexed from, address indexed to, uint256 indexed tokenId, bytes metaData);

    event TokenMinted(
        address indexed creator,
        string edition,
        uint256 indexed tokenId,
        string uri,
        uint256 price
    );

    event TokenOnsale(uint256 indexed tokenId, uint256 price);
    event TokenOnResale(uint256 indexed tokenId, uint256 price);
    event TokenRevoked(uint256 indexed tokenId);
    event collectionStatusChanged(bool active);

    constructor() ERC721("Portraits", "PRTRS") {
        _setupContractId("Portraits");
        _setupRole(DEPOSITOR_ROLE, address(0xb5505a6d998549090530911180f38aC5130101c6)); // todo: change before deploying to mainnet!!! 
        _initializeEIP712("Portraits");

        creator = payable(address(0xee861213DB6Fd2c6cbA783c3A041C99e0038f667));
        jelly = payable(address(0x77a06Af353bA3d792c1f704601c3440160e741d1));
    }

    // This is to support Native meta transactions
    // never use msg.sender directly, use _msgSender() instead
    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }

    // Restricts access to jelly-accounts
    modifier onlyJelly {
        require(_msgSender() == jelly,"PORTRAITS: unauthorized access");
        _;
    }

    /**
     * @dev See {ERC721-_beforeTokenTransfer}.
     **/
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Enumerable, ERC721Pausable) {
        super._beforeTokenTransfer(from, to, tokenId);

        require(!paused(), "ERC721Pausable: token transfer while paused");
    }

    /** 
     * @dev See {ERC721-_burn}
    **/
    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     **/
    function tokenURI(uint256 tokenId) 
        public 
        view 
        override(ERC721, ERC721URIStorage) 
        returns (string memory) 
    {
        return super.tokenURI(tokenId);
    }

    /**
     * @notice called when token is deposited on child chain
     * @dev Should be callable only by ChildChainManager
     * Should handle deposit by minting the required tokenId(s) for user
     * Should set `withdrawnTokens` mapping to `false` for the tokenId being deposited
     * Minting can also be done by other functions
     * @param user user address for whom deposit is being done
     * @param depositData abi encoded tokenIds. Batch deposit also supported.
     */
    function deposit(address user, bytes calldata depositData)
        external
        override
        only(DEPOSITOR_ROLE)
    {

        // deposit single
        if (depositData.length == 32) {
            uint256 tokenId = abi.decode(depositData, (uint256));
            withdrawnTokens[tokenId] = false;
            _mint(user, tokenId);

        // deposit batch
        } else {
            uint256[] memory tokenIds = abi.decode(depositData, (uint256[]));
            uint256 length = tokenIds.length;
            for (uint256 i; i < length; i++) {
                withdrawnTokens[tokenIds[i]] = false;
                _mint(user, tokenIds[i]);
            }
        }
    }

    /**
     * @notice called when user wants to withdraw token back to root chain
     * @dev Should handle withdraw by burning user's token.
     * Should set `withdrawnTokens` mapping to `true` for the tokenId being withdrawn
     * This transaction will be verified when exiting on root chain
     * todo change withdraw functions to include metadata withdrawal?
     * @param tokenId tokenId to withdraw
     */
    function withdraw(uint256 tokenId) external {
        require(_msgSender() == ownerOf(tokenId), "TREES: INVALID_TOKEN_OWNER");
        require(nftMetaData[tokenId].forSale == false, "TREES: TOKEN_STILL_ON_SALE");
        withdrawnTokens[tokenId] = true;
        _burn(tokenId);

    }

    /**
     * @notice called when user wants to withdraw multiple tokens back to root chain
     * @dev Should burn user's tokens. This transaction will be verified when exiting on root chain
     * @param tokenIds tokenId list to withdraw
     */
    function withdrawBatch(uint256[] calldata tokenIds) external {

        uint256 length = tokenIds.length;
        require(length <= BATCH_LIMIT, "TREES: EXCEEDS_BATCH_LIMIT");

        // Iteratively burn ERC721 tokens, for performing
        // batch withdraw
        for (uint256 i; i < length; i++) {

            uint256 tokenId = tokenIds[i];

            require(_msgSender() == ownerOf(tokenId), string(abi.encodePacked("TREES: INVALID_TOKEN_OWNER ", tokenId)));
            require(nftMetaData[tokenId].forSale == false, "TREES: TOKEN_STILL_ON_SALE");
            withdrawnTokens[tokenId] = true;
            _burn(tokenId);

        }

        // At last emit this event, which will be used
        // in MintableERC721 predicate contract on L1
        // while verifying burn proof
        emit WithdrawnBatch(_msgSender(), tokenIds);

    }

    /**
     * @notice called when user wants to withdraw token back to root chain with token URI
     * @dev Should handle withdraw by burning user's token.
     * Should set `withdrawnTokens` mapping to `true` for the tokenId being withdrawn
     * This transaction will be verified when exiting on root chain
     *
     * @param tokenId tokenId to withdraw
     */
    function withdrawWithMetadata(uint256 tokenId) external {

        require(_msgSender() == ownerOf(tokenId), "TREES: INVALID_TOKEN_OWNER");
        require(nftMetaData[tokenId].forSale == false, "TREES: TOKEN_STILL_ON_SALE");
        withdrawnTokens[tokenId] = true;

        // Encoding metadata associated with tokenId & emitting event
        emit TransferWithMetadata(ownerOf(tokenId), address(0), tokenId, this.encodeTokenMetadata(tokenId));

        _burn(tokenId);

    }

    /**
     * @notice This method is supposed to be called by client when withdrawing token with metadata
     * and pass return value of this function as second paramter of `withdrawWithMetadata` method
     *
     * It can be overridden by clients to encode data in a different form, which needs to
     * be decoded back by them correctly during exiting
     *
     * @param tokenId Token for which URI to be fetched
     */
    function encodeTokenMetadata(uint256 tokenId) external view virtual returns (bytes memory) {

        // You're always free to change this default implementation
        // and pack more data in byte array which can be decoded back
        // in L1
        return abi.encode(tokenURI(tokenId));

    }

    /**
     * @dev pauses the smart-contract in case of emergencies
     */
    function pausePortraits() external onlyJelly {
      require(!paused(), "PORTRAITS: contract_already_paused");
      _pause();

    }

    /**
     * @dev unpauses the smart-contract
     */
    function unPausePortraits() external onlyJelly {
      require(paused(), "PORTRAITS: contract_not_paused");
      _unpause();

    }

    /**
     * @notice Function that handles minting on the matic chain
     * @dev Minting can be done as per requirement,
     * This implementation allows only jelly to mint tokens, and sets token on sale
     * verifies if token is withdrawn by checking `withdrawnTokens` mapping
     * @param tokenId token which should be minted
     * @param edition edition which this token is part of
     * @param uri resource identifier of the token in the form of an ipfs-cid
     */
    function jellyMint(
        uint256 tokenId,
        string calldata edition,
        string calldata uri,
        uint256 price
    ) public onlyJelly 
    {
        require(!withdrawnTokens[tokenId], "PORTRAITS: TOKEN_EXISTS_ON_ROOT_CHAIN");

        _mint(creator, tokenId);
        _setTokenURI(tokenId, uri);
        nftMetaData[tokenId] = MetaDataToken(0, false, edition);
        nftMetaData[tokenId].price = price;
        nftMetaData[tokenId].forSale = true;

        emit TokenMinted(creator, edition, tokenId, uri, price);

    }

    /**
     * @notice Function that sets a token on sale again in case it was revoked
     * @param tokenId token which should be set for sale
     * @param price price for which the token should be sold
     */
    function setTokenForSale(uint256 tokenId, uint256 price) external onlyJelly {
        require(creator == ownerOf(tokenId), "PORTRAITS: Unauthorized Account");
        require(nftMetaData[tokenId].forSale == false, "PORTRAITS: token already set");
        require(price > 0, "PORTRAITS: price can't be 0");

        nftMetaData[tokenId].price = price;
        nftMetaData[tokenId].forSale = true;

        emit TokenOnsale(tokenId, price);

    }

    /**
     * @notice Function that allows users to re-sell their token
     * @dev should add the token to the array of token on resale, and add it
     * the indices necessary to retrieve/remove it later on
     * @param tokenId token to be resold
     * @param price price for which the token should be resold
     */
    function setTokenForResale(uint256 tokenId, uint256 price) external {
        require(creator != ownerOf(tokenId), "PORTRAITS: Creators can't use marketplace function");
        require(_msgSender() == ownerOf(tokenId), "PORTRAITS: Unauthorized Account");
        require(nftMetaData[tokenId].forSale == false, "PORTRAITS: token already set");
        require(price > 0, "PORTRAITS: price can't be 0");

        nftMetaData[tokenId].price = price;
        nftMetaData[tokenId].forSale = true;

        resaleTokenIndex[tokenId] = resaleToken.length;
        resaleTokenByIndex[resaleToken.length] = tokenId;
        resaleToken.push(tokenId);

        emit TokenOnResale(tokenId, price);

    }

    /**
     * @notice function that unlists a token from the store, reversing 'setTokenForSale'
     * @param tokenId token to be unlisted
     */
    function revokeToken(uint256 tokenId) external onlyJelly {
        require(creator == ownerOf(tokenId), "PORTRAITS: Non-Owner trying to set price");
        require(nftMetaData[tokenId].forSale == true, "PORTRAITS: This token is currently not set for sale");
        require(saleRunning == false, "PORTRAITS: can't revoke a token while a sale is active");

        nftMetaData[tokenId].price = 0;
        nftMetaData[tokenId].forSale = false;

        emit TokenRevoked(tokenId);

    }

    /**
     * @notice function that unlists a token from the marketplace
     * reverses 'setTokenForResale'
     * @param tokenId token to be unlisted
     */
    function revokeTokenMarketPlace(uint256 tokenId) external {
        require(_msgSender() == ownerOf(tokenId), "PORTRAITS: Non-Owner trying to set price");
        require(nftMetaData[tokenId].forSale == true, "PORTRAITS: This token is currently not set for sale");

        nftMetaData[tokenId].price = 0;
        nftMetaData[tokenId].forSale = false;
        _removeFromEnumeration(tokenId);

        emit TokenRevoked(tokenId);

    }

    /**
     * @notice function that controls whether the collection is on sale or not
     * will be set to true when a collection is dropping
     * will be set to false after all token have been sold
     * @dev can be optimized in the future
     * @param newStatus the new status of the collection
     */
    function startOrStopSale(bool newStatus) onlyJelly external {
        saleRunning = newStatus;
        emit collectionStatusChanged(newStatus);

    }

    /*
     * takes payment from buyer, distributes it, and calls the transfer function
     * sends 80% of ether to originalowner (client), 20% to jelly (us)
     */
    
    /**
     * @notice function that accepts payment, distributes it, and sends the token
     * @dev has been designed after the checks-effects-interactions pattern
     * call has ben chosen for eth-transfer to stay agnostic to gas value of OPCODES
     * re-entrancy is avoided with the design pattern of the function
     * @param tokenId token that user wants to buy
     */
    function purchaseToken(uint256 tokenId) external payable {
        require(saleRunning == true, "PORTRAITS: There is currently no active sale");
        require(creator == ownerOf(tokenId), "PORTRAITS: This function may only be called for initial transfers");
        require(nftMetaData[tokenId].forSale == true, "PORTRAITS: This token is currently not on sale");
        require(msg.value == nftMetaData[tokenId].price, "PORTRAITS: Payment-Amount is wrong");
        require(balanceOf(_msgSender()) <= 100, "PORTRAITS: More than 100 Token per account disallowed");

        nftMetaData[tokenId].forSale = false;
        _transfer(creator, payable(_msgSender()), tokenId);

        (bool success, ) = creator.call{value: (msg.value * 80) / 100}("");
        require(success, 'transfer to creator failed');
        (success, ) = jelly.call{value: (msg.value * 20) / 100}("");
        require(success, 'transfer to jelly failed');
    
    }

    /**
