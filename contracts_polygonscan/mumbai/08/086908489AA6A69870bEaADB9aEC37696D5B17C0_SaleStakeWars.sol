pragma solidity 0.8.11;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

import "../contracts/NFTStakeWars.sol";
import "../contracts/interfaces/QuickSwap.sol";
import "./libraries/FixedPointMath.sol";

error SWRevealIsLastWorkflowStep();
error SWPreSaleIsNotStartedYet();
error SWTryMintZeroTokens();
error SWAfterMintTotalSupplyMoreThenMaxTotalSupply();
error SWNotEnoughNativeCurrency();
error SWChangeNotSend();
error SWMasterShareNotSend();
error TheShareCantBeMoreThenWhole();
error NotEnoughLinkForRandom(uint256 linkBalance);
error SWWorkflowStepUnsuitableForReveal();
error SWRevealedAlready();
error SWTooManyNFTsForUser();
error SWSenderIsNotOnTheWhiteList();

/// @title Sale smart contract
/// @author Pavlov Anton <[email protected]>
contract SaleStakeWars is ReentrancyGuard, Ownable, VRFConsumerBase {
    using FixedPointMath for uint256;

    enum Workflow {
        Before,
        PreSale,
        Sale,
        Reveal
    }

    uint256 public constant ONE = 100e16;
    uint256 public constant FEE_VRF = 1e14; //0.0001

    IQuickSwapPair public pair;
    IQuickSwapRouter public router;
    Workflow public workflowStatus;

    NFTStakeWars public nfts;

    uint256 public swapDeadline = 20 minutes;
    uint256 public priceMoney;
    uint256 public salePriceNFT = 37.5 ether;
    uint256 public preSalePriceNFT = 37.5 ether;
    uint256 public maxNFTAmountInOneHand = 5;
    uint256 public maxTotalSupply = 2000;
    uint256 public stopSaleTotalSupply;
    uint256 public masterShare = 50e16; //50%
    uint256 public slippageTolerance = 5e15; // 0.5 %
    mapping(uint256 => bool) public mintedNFTs;
    uint256 public lastAddedNFTsIdInArray;
    uint256 public amountNotMintedNFT;
    uint256 public timestampForPreSale;
    uint256 public timestampForSale;

    address public poolAddress;
    bool public revealedAlready;

    mapping(address => bool) public listWL;

    //for chainLinkVRF
    bytes32 internal keyHash;
    uint256 public randomResult;

    event Withdrawal(uint256 amount);
    event Received(address user, uint256 amount);

    constructor(
        string memory _placeHolderURI,
        uint256 _timestampForPreSale,
        address _coordinatorVRF,
        address _linkToken,
        bytes32 _keyHash
    ) VRFConsumerBase(_coordinatorVRF, _linkToken) {
        timestampForPreSale = _timestampForPreSale;
        timestampForSale = timestampForPreSale + 1 days;
        keyHash = _keyHash;
        nfts = new NFTStakeWars(_placeHolderURI);
    }

    function setTimestampForPreSale(uint256 timestamp) external onlyOwner {
        timestampForPreSale = timestamp;
        timestampForSale = timestampForPreSale + 1 days;
    }

    function setPoolAddress(address _poolAddress) external onlyOwner {
        poolAddress = _poolAddress;
    }

    function setSalePriceNFT(uint256 _salePriceNFT) external onlyOwner {
        salePriceNFT = _salePriceNFT;
    }

    function setPreSalePriceNFT(uint256 _preSalePriceNFT) external onlyOwner {
        preSalePriceNFT = _preSalePriceNFT;
    }

    function setPlaceHolderUri(string memory _uri) external onlyOwner {
        nfts.setPlaceHolder(_uri);
    }

    function setMaxTotalSupply(uint256 _maxTotalSupply) external onlyOwner {
        maxTotalSupply = _maxTotalSupply;
    }

    function setMasterShare(uint256 _masterShare) external onlyOwner {
        if (_masterShare > ONE) {
            revert TheShareCantBeMoreThenWhole();
        }
        masterShare = _masterShare;
    }

    function setmaxNFTAmountInOneHand(uint256 _maxNFTAmountInOneHand) external onlyOwner {
        maxNFTAmountInOneHand = _maxNFTAmountInOneHand;
    }

    function nextWorkflowStep() external onlyOwner {
        if (workflowStatus == Workflow.Reveal) {
            revert SWRevealIsLastWorkflowStep();
        }
        workflowStatus = Workflow(uint8(workflowStatus) + 1);
    }

    function addToListWL(address[] memory userList) external onlyOwner {
        for (uint256 i = 0; i < userList.length; i++) {
            listWL[userList[i]] = true;
        }
    }

    function deleteWL(address[] calldata userList) external onlyOwner {
        for (uint256 i = 0; i < userList.length; i++) {
            delete listWL[userList[i]];
        }
    }

    function reveal(string memory _baseTokenUri) external onlyOwner {
        if (revealedAlready) {
            revert SWRevealedAlready();
        }
        if (workflowStatus != Workflow.Reveal) {
            revert SWWorkflowStepUnsuitableForReveal();
        }
        stopSaleTotalSupply = nfts.totalSupply();
        revealedAlready = true;
        nfts.reveal(_baseTokenUri);
    }

    function pause() external onlyOwner {
        nfts.pause();
    }

    function unpause() external onlyOwner {
        nfts.unpause();
    }

    function saleMint(uint256 amount) external payable nonReentrant {
        uint256 priceAmount = amount.mul(salePriceNFT);
        bool success;
        uint256 mintId;
        uint256 change = msg.value - priceAmount;
        if (
            (workflowStatus == Workflow.Before && block.timestamp >= timestampForPreSale) ||
            (workflowStatus == Workflow.PreSale && block.timestamp >= timestampForSale)
        ) {
            this.nextWorkflowStep();
        }
        if (workflowStatus < Workflow.PreSale) {
            revert SWPreSaleIsNotStartedYet();
        }

        if (amount == 0) {
            revert SWTryMintZeroTokens();
        }
        if (tokensLeftToMint() < amount) {
            revert SWAfterMintTotalSupplyMoreThenMaxTotalSupply();
        }
        if (msg.value < priceAmount) {
            revert SWNotEnoughNativeCurrency();
        }

        if (workflowStatus == Workflow.PreSale) {
            if (!listWL[msg.sender]) {
                revert SWSenderIsNotOnTheWhiteList();
            }
            if (nfts.balanceOf(msg.sender) + amount > maxNFTAmountInOneHand) {
                revert SWTooManyNFTsForUser();
            }
            priceAmount = amount.mul(preSalePriceNFT);
            mintId = nfts.totalSupply();
        } else if (workflowStatus == Workflow.Sale) {
            mintId = getRandomTokenId();
            while (!mintedNFTs[mintId]) {
                mintId++;
                if (mintId == maxTotalSupply) {
                    mintId = stopSaleTotalSupply;
                }
            }
        }

        for (uint256 i = 0; i < amount; i++) {
            nfts.mint(_msgSender(), mintId);
        }

        (success, ) = owner().call{value: priceAmount.mul(masterShare)}("");
        if (!success) {
            revert SWMasterShareNotSend();
        }

        (uint256 reserveA, uint256 reserveB, ) = pair.getReserves();
        uint256 priceInGameCurrecy;
        if (reserveA == 0) {
            priceInGameCurrecy = priceMoney;
        } else {
            priceInGameCurrecy = router.quote(1e18, reserveA, reserveB);
        }

        uint256 poolPart = priceAmount.mul(ONE - masterShare);
        (success, ) = poolAddress.call{value: poolPart}("");
        if (!success) {
            revert SWMasterShareNotSend();
        }

        if (change > 0) {
            (success, ) = _msgSender().call{value: change}("");
            if (!success) {
                revert SWChangeNotSend();
            }
        }
    }

    function tokensLeftToMint() public view returns (uint256) {
        return (maxTotalSupply - nfts.totalSupply());
    }

    function getRandomTokenId() internal returns (uint256) {
        uint256 _id;
        getRandomNumber();
        _id = (randomResult % (maxTotalSupply - stopSaleTotalSupply)) + stopSaleTotalSupply;
        return _id;
    }

    /**
     * Requests randomness
     */
    function getRandomNumber() internal returns (bytes32 requestId) {
        if (LINK.balanceOf(address(this)) >= FEE_VRF) {
            revert NotEnoughLinkForRandom(LINK.balanceOf(address(this)));
        }
        return requestRandomness(keyHash, FEE_VRF);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    // solhint-disable-next-line no-unused-vars
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        randomResult = randomness;
    }
}

