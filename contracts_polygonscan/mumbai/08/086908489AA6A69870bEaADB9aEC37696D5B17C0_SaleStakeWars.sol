pragma solidity 0.8.11;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Pausable.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

struct Params {
    uint32 income;
    uint32 upkeep;
    uint32 army;
    uint32 luck;
}

error SWMetaDataSyncAlready(uint256 tokenId);
error SWNotReveal();

/// @title NFT smart contract
/// @author Pavlov Anton <[email protected]>
contract NFTStakeWars is Context, ERC721Enumerable, ERC721Pausable, Ownable {
    string public placeHolderUri;
    string public baseTokenUri;
    bool public revealAlready;

    mapping(uint256 => Params) public paramsNFT;
    mapping(uint256 => bool) public metaDataSynchronized;

    constructor(string memory _placeHolderURI) ERC721("NFT Stake Wars ", "NFTSW") {
        placeHolderUri = _placeHolderURI;
    }

    function mint(address user, uint256 tokenId) external onlyOwner {
        _mint(user, tokenId);
    }

    function setPlaceHolder(string memory _uri) external onlyOwner {
        placeHolderUri = _uri;
    }

    function reveal(string memory _baseTokenUri) external onlyOwner {
        baseTokenUri = _baseTokenUri;
        revealAlready = true;
    }

    function syncMetaData(uint256 tokenId) external {
        if (!revealAlready) {
            revert SWNotReveal();
        }
        if (metaDataSynchronized[tokenId]) {
            revert SWMetaDataSyncAlready(tokenId);
        }
        //TODO sync with IPFS
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        if (revealAlready) {
            return super.tokenURI(tokenId);
        }
        return placeHolderUri;
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Enumerable, ERC721Pausable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _baseURI() internal view override returns (string memory) {
        return baseTokenUri;
    }
}

