pragma solidity ^0.8.2;

import "hardhat/console.sol";
import "./interfaces/IReputation.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Reputation is IReputation, AccessControl {
    bytes32 public constant DAO_ADMIN_ROLE = keccak256("DAO_ADMIN_ROLE");
    using SafeMath for uint256;
    string internal constant _name = "DAO_REP";
    string internal constant _symbol = "DAO_REPUTATION";
    uint8 internal constant _decimals = 18;
    mapping(address => uint256) internal lockedReputation;
    // key : owner , value : balance
    mapping(address => uint256) internal reputationBalances;
    uint256 internal membersCount;
    uint256 internal _totalLimit;
    uint256 internal _currentSupply;
    //this array keep members with reputation
    address[] internal membersWithReputation;
    mapping(address => bool) internal checkMemberWithReputation;

    constructor() {
        // no tokens minted on deploy
        _currentSupply = 0;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    event MemberAdded(address newMember);

    modifier isAllowed() {
        require(( hasRole(DAO_ADMIN_ROLE, msg.sender)), "address not allowed to mint");
        _;
    }

    function name() public pure override returns (string memory) {
        return _name;
    }

    function symbol() public pure override returns (string memory) {
        return _symbol;
    }

    function decimals() public pure override returns (uint8) {
        return _decimals;
    }

    function totalLimit() public view override returns (uint256) {
        return _totalLimit; /// max value for uint256
    }

    function currentSupply() public view override returns (uint256) {
        return _currentSupply; /// max value for uint256
    }

    //return the reputation minted to this address
    function balanceOf(address _owner) public view override returns (uint256) {
        return reputationBalances[_owner]; /// return requested owner balance
    }

    function mint(address account, uint256 amount) external override isAllowed {
        require(account != address(0), "No zero address allowed");
        require(!(hasRole(DAO_ADMIN_ROLE, account)),
            "Authorized addresses can't mint reputation for himself");

        _currentSupply = _currentSupply.add(amount);
        reputationBalances[account] = reputationBalances[account].add(amount);
        updateStatusMembersWithReputation(account);
        emit Issued(account, amount);
    }

    function lockReputationOf(address account, uint256 amount) external override isAllowed {
        require(
            reputationBalances[account] >= lockedReputation[account].add(amount),
            "you don't have enough reputation to lock"
        );
        lockedReputation[account] = lockedReputation[account].add(amount); /// return requested owner balance
        emit LockedReputation(account, amount);
    }

    function unLockReputationOf(address _owner, uint256 amount) external override isAllowed {
        lockedReputation[_owner] = lockedReputation[_owner].sub(amount); /// return requested owner balance
        emit UnlockReputation(_owner, amount);
    }

    //return the real reputation that the address can stake now
    function availableReputationOf(address account) external view override returns (uint256) {
        return reputationBalances[account].sub(lockedReputation[account]); /// return requested owner balance
    }

    function addAddressToWhitelist(address account) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        _grantRole(DAO_ADMIN_ROLE, account);
    }

    function getMembers() external view override returns (uint256) {
        return membersCount;
    }

    function removeReputation(address account, uint256 amount) external override isAllowed {
        require(reputationBalances[account] >= amount, "Theres not enough balance reputation to complete operation");
        uint256 balance = reputationBalances[account].sub(amount);
        reputationBalances[account] = balance;
        updateStatusMembersWithReputation(account);
    }

    function updateStatusMembersWithReputation(address _address) internal {
        if (reputationBalances[_address] == 0) {
            checkMemberWithReputation[_address] = false;
            uint256 positionToDelete;
            for (uint256 i = 0; i < membersWithReputation.length; i++) {
                if (membersWithReputation[i] == _address) {
                    positionToDelete = i;
                    break;
                }
            }
            membersWithReputation[positionToDelete] = membersWithReputation[membersWithReputation.length - 1];
            membersWithReputation.pop();
        } else {
            if (checkMemberWithReputation[_address] != true) {
                checkMemberWithReputation[_address] = true;
                membersWithReputation.push(_address);
            }
        }
    }

    function getMembersWithReputation() external view override returns (address[] memory) {
        return membersWithReputation;
    }

    function isMemberWithReputation(address account) external view override returns (bool) {
        return checkMemberWithReputation[account];
    }

    function removeWhitelistAddress(address account) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _revokeRole(DAO_ADMIN_ROLE, account);
    }

}

