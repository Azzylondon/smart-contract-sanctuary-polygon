        formula = _formula;
        token = MetaSoccerToken(_token);
        beneficiary = _beneficiary;
        curveSupply = _curveSupply;
        minCurveSupply = _minCurveSupply;
        batchBlocks = _batchBlocks;
        buyFeePct = _buyFeePct;
        sellFeePct = _sellFeePct;
    }

