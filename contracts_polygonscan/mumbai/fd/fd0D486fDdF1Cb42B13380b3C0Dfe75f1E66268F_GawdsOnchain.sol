  // all gawds, indexed by gawd ID
  mapping(uint16 => Gawd) public gawds;
  uint16[] public gawdIDs;

  // all parts, indexed with their distinct part IDs
  mapping(uint16 => GawdPart) public parts;
  uint16[] public partIDs;

  // map a given gawd to an array of its part ids
  mapping(uint16 => uint16[]) public gawdParts;

  // lookup tables for the 7 trait types
  mapping(uint16 => string[]) public gawdOculuses;
  mapping(uint16 => string[]) public gawdIrises;
  mapping(uint16 => string[]) public gawdAuras;
  mapping(uint16 => string[]) public gawdRealms;
  mapping(uint16 => string[]) public gawdCompositions;
  mapping(uint16 => string[]) public gawdConveyences;
  mapping(uint16 => string[]) public gawdAccents;

  // lookup tables for each gawd's powers, plus global list of all powers
  mapping(uint16 => string[]) public gawdPowers;
  mapping(string => bool) public allPowers;

  // same for sets
  mapping(uint16 => string[]) public gawdSets;
  mapping(string => bool) public allSets;

  struct Gawd {
      uint16 id;
      string name;
      string hash;
  }

  struct GawdPart {
      uint16 partID;
      string kind;
      string value;
      string power;
      string set;
  }

  constructor() {
      // addGawd(4569, "Exquisite Quiet Creator", "dbbce9a8a5308640de1d8f37dfaaae114a5e9aa612fb019a95d6ff81936eb6ed");
      // addPart(4569, 1, "Realm", "Space", "Dark", "OG");
      // addPart(4569, 23, "Aura", "Red Nova", "Dark");
      // addPart(4569, 22, "Aura", "Psych", "Mundane");
      // addPart(4569, 85, "Composition", "Wooden", "Mundane", "Wood");
      // addPart(4569, 142, "Accent", "Cracked", "Chaos");
      // addPart(4569, 102, "Wood", "Oculus", "Mundane", "Wood");

      // addGawd(2, "Gentle Abnormal Foreigner", "f6cf7a71c85b23feca97441793a1d76aa204996cea8238f246f62143cadbd338");
      // addPart(2, 15, "Realm", "Vibe World", "Cosmos");
      // addPart(2, 16, "Aura", "Dot Swarm", "Automaton", "Techno");
      // addPart(2, 61, "Accent", "Scraped", "Corporeal");
      // addPart(2, 5, "Oculus", "Ruby", "Geological");
      // addPart(2, 112, "Composition", "Swirled", "Spirit");

      // TODO setDominantPower
      // TODO animation info
      // TODO asset links?
  }

  function addGawd(uint16 id, string memory name, string memory hash) public onlyOwner {
      gawds[id] = Gawd(id, name, hash);
      gawdIDs.push(id);
  }

  function isGawd(uint16 id) public view returns (bool) {
      return gawds[id].id != 0;
  }

  function gawdsCount() public view returns (uint256) {
      return gawdIDs.length;
  }

  function getGawdName(uint16 id) public view returns (string memory) {
    return gawds[id].name;
  }

  function getGawdHash(uint16 id) public view returns (string memory) {
    return gawds[id].hash;
  }

  function getGawdPowers(uint16 id) public view returns (string[] memory) {
      return gawdPowers[id];
  }

  function getGawdSets(uint16 id) public view returns (string[] memory) {
      return gawdSets[id];
  }

  // TODO measure which is more efficient
  // these per-part indexes, or the smart-but-complicated getGawdParts()?
  function getGawdOculus(uint16 id) public view returns (string[] memory) {
    return gawdOculuses[id];
  }

  function getGawdRealm(uint16 id) public view returns (string[] memory) {
    return gawdRealms[id];
  }

  function getGawdComposition(uint16 id) public view returns (string[] memory) {
    return gawdCompositions[id];
  }

   function getGawdAccents(uint16 id) public view returns (string[] memory) {
    return gawdAccents[id];
  }

  function getGawdConveyence(uint16 id) public view returns (string[] memory) {
    return gawdConveyences[id];
  }

  function getGawdAuras(uint16 id) public view returns (string[] memory) {
    return gawdAuras[id];
  }

  function getGawdIris(uint16 id) public view returns (string[] memory) {
    return gawdIrises[id];
  }

  // don't use
  // inefficient
  function getGawdParts(uint16 id, string memory kind) public view returns (string[] memory) {
    uint numParts = gawdParts[id].length;

    string[] memory output = new string[](numParts);
    for(uint16 i = 0; i < numParts; i++){
        uint16 partID = gawdParts[id][i];
        GawdPart storage part = parts[partID];

        if(!compareStrings(part.kind, kind)) {
          // console.log("skipping part with kind=", part.kind);
          continue;
        }

        // console.log("part:", part.kind, part.value, part.power);
        string[] memory line = new string[](3);
        line[0] = part.kind;
        line[1] = ": ";
        line[2] = part.value;
        output[i] = string(abi.encodePacked(line[0], line[1], line[2]));
        // console.log("output[i]", output[i]);
    }

    return output;
  }

  // `set` can be blank
  // all others always required
  // FIXME ethers.js can't handle solidity functions with same name but diff arities?
  // function addPart(uint16 gawdID, uint16 partID, string memory kind, string memory value, string memory power)
  // public onlyOwner
  // {
  //     addPart(gawdID, partID, kind, value, power, "");
  // }

  // main magic
  function addPart(uint16 gawdID, uint16 partID, string memory kind, string memory value, string memory power, string memory set)
  public onlyOwner
  {
    	console.log("addPart", gawdID, kind, value);
      parts[partID] = GawdPart(partID, kind, value, power, set);
      partIDs.push(partID);
      gawdParts[gawdID].push(partID);

      // build indexes of powers
      gawdPowers[gawdID].push(power);
      if(!allPowers[power]) {
        console.log("adding to allPowers", power);
        allPowers[power] = true;
      }

      // same for sets, but don't add blank sets to the list
      if(!compareStrings(set, '')) {
        gawdSets[gawdID].push(set);
        if(!allSets[set]) {
          console.log("adding to allSets", set);
          allSets[set] = true;
        }
      }

      // doing per-trait indexes like this was more efficient than the smart getGawdParts()
      // does it use too much memory? it uses `for` loops and storage variables too
      if(compareStrings(kind, "Oculus")) {
        gawdOculuses[gawdID].push(value);
      }
      else if(compareStrings(kind, "Accent")) {
        gawdAccents[gawdID].push(value);
      }
      else if(compareStrings(kind, "Iris")) {
        gawdIrises[gawdID].push(value);
      }
      else if(compareStrings(kind, "Composition")) {
        gawdCompositions[gawdID].push(value);
      }
      else if(compareStrings(kind, "Aura")) {
        gawdAuras[gawdID].push(value);
      }
      else if(compareStrings(kind, "Realm")) {
        gawdRealms[gawdID].push(value);
      }
      else if(compareStrings(kind, "Conveyence")) {
        gawdConveyences[gawdID].push(value);
      }
      else {
        // TODO can i use a require() here?
        console.log("Don't know how to index part with kind=", kind);
      }
  }

  function partsCount() public view returns (uint256) {
      return partIDs.length;
  }

  function imageSVG(uint16 tokenId) public view returns (string memory) {
    string[21] memory pieces;
    pieces[0] = '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><style>.base { fill: white; font-family: serif; font-size: 14px; }</style><rect width="100%" height="100%" fill="black" /><text x="10" y="20" class="base" font-weight="bold">';
    pieces[1] = string(abi.encodePacked(getGawdName(tokenId), " #", toString(tokenId)));
    pieces[2] = '</text><text x="10" y="40" class="base">Realm: ';
    pieces[3] = concatenateStrings(getGawdRealm(tokenId));
    pieces[4] = '</text><text x="10" y="60" class="base">Composition: ';
    pieces[5] = concatenateStrings(getGawdComposition(tokenId));
    pieces[6] = '</text><text x="10" y="80" class="base">Oculus: ';
    pieces[7] = concatenateStrings(getGawdOculus(tokenId));
    pieces[8] = '</text><text x="10" y="100" class="base">Iris: ';
    pieces[9] = concatenateStrings(getGawdIris(tokenId));
    pieces[10] = '</text><text x="10" y="120" class="base">Aura(s): ';
    pieces[11] = concatenateStrings(getGawdAuras(tokenId));
    pieces[12] = '</text><text x="10" y="140" class="base">Accent(s): ';
    pieces[13] = concatenateStrings(getGawdAccents(tokenId));
    pieces[14] = '</text><text x="10" y="160" class="base">Conveyence: ';
    pieces[15] = concatenateStrings(getGawdConveyence(tokenId));
    pieces[16] = '</text><text x="10" y="180" class="base">Powers:';
    pieces[17] = concatenateStrings(getGawdPowers(tokenId));
    pieces[18] = '</text><text x="10" y="200" class="base">Sets:';
    pieces[19] = concatenateStrings(getGawdSets(tokenId));
    pieces[20] = '</text></svg>';

    string memory output = string(abi.encodePacked(pieces[0], pieces[1], pieces[2], pieces[3], pieces[4], pieces[5], pieces[6], pieces[7], pieces[8], pieces[9]));
    output = string(abi.encodePacked(output, pieces[10], pieces[11], pieces[12], pieces[13], pieces[14], pieces[15], pieces[16], pieces[17], pieces[18], pieces[19], pieces[20]));

    return output;
  }

  function imageDataURI(uint16 tokenId) public view returns (string memory) {
    return string(abi.encodePacked("data:image/svg+xml;base64,", Base64.encode(bytes(imageSVG(tokenId)))));
  }

  function tokenHTML(uint16 tokenId) public view returns (string memory) {
    string[] memory pieces = new string[](3);
    pieces[0] = "<html><meta charset='utf-8'>";
    pieces[1] = imageSVG(tokenId);
    pieces[2] = "</html>";
    return string(abi.encodePacked(pieces[0], pieces[1], pieces[2]));
  }

  function tokenURI(uint16 tokenId) public view returns (string memory) {
    string memory json = Base64.encode(bytes(string(abi.encodePacked(
      '{"name": "Gawd #', toString(tokenId), '", "description": "GawdsOnchain", "image": "', imageDataURI(tokenId), '"}'
    ))));
    string memory output = string(abi.encodePacked("data:application/json;base64,", json));

    return output;
  }

  function compareStrings(string memory a, string memory b) internal pure returns (bool) {
    return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
  }

  function concatenateStrings(string[] memory arr) internal view returns(string memory) {
    if(arr.length == 0) {
      return "";
    }
    string memory output = arr[0];
    if(arr.length > 1) {
      for(uint i = 1; i < arr.length; i++) {
        output = string(abi.encodePacked(output, ", " , arr[i]));
      }
    }
    return output;
}

  function toString(uint256 value) internal pure returns (string memory) {
    // Inspired by OraclizeAPI's implementation - MIT license
    // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

    if (value == 0) {
        return "0";
    }
    uint256 temp = value;
    uint256 digits;
    while (temp != 0) {
        digits++;
        temp /= 10;
    }
    bytes memory buffer = new bytes(digits);
    while (value != 0) {
        digits -= 1;
        buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
        value /= 10;
    }
    return string(buffer);
  }
}

/// [MIT License]
/// @title Base64
/// @notice Provides a function for encoding some bytes in base64
/// @author Brecht Devos <[email protected]>
library Base64 {
  bytes internal constant TABLE = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";

  /// @notice Encodes some bytes to the base64 representation
  function encode(bytes memory data) internal pure returns (string memory) {
      uint256 len = data.length;
      if (len == 0) return "";

      // multiply by 4/3 rounded up
      uint256 encodedLen = 4 * ((len + 2) / 3);

      // Add some extra buffer at the end
      bytes memory result = new bytes(encodedLen + 32);

      bytes memory table = TABLE;

      assembly {
          let tablePtr := add(table, 1)
          let resultPtr := add(result, 32)

          for {
              let i := 0
          } lt(i, len) {

          } {
              i := add(i, 3)
              let input := and(mload(add(data, i)), 0xffffff)

              let out := mload(add(tablePtr, and(shr(18, input), 0x3F)))
              out := shl(8, out)
              out := add(out, and(mload(add(tablePtr, and(shr(12, input), 0x3F))), 0xFF))
              out := shl(8, out)
              out := add(out, and(mload(add(tablePtr, and(shr(6, input), 0x3F))), 0xFF))
              out := shl(8, out)
              out := add(out, and(mload(add(tablePtr, and(input, 0x3F))), 0xFF))
              out := shl(224, out)

              mstore(resultPtr, out)

              resultPtr := add(resultPtr, 4)
          }

          switch mod(len, 3)
          case 1 {
              mstore(sub(resultPtr, 2), shl(240, 0x3d3d))
          }
          case 2 {
              mstore(sub(resultPtr, 1), shl(248, 0x3d))
          }

          mstore(result, encodedLen)
      }

      return string(result);
  }
}

