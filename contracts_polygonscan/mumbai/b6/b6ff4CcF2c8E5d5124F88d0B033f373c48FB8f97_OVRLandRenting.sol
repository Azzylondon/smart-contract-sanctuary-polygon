//price calculator works with uniswap fork liquidity pool TODO CHANGE ADDRESSES ON PRICECALCULATORETH.SOL FOR POLYGON
contract OVRLandRenting is UUPSUpgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable, PausableUpgradeable{
    using AddressUpgradeable for address;
    using SafeMath for uint256;
 
    IERC20 public token;
    IOVRLand public OVRLand;
    uint256 public  feeUSD;
    
 

    function initialize(address _tokenAddress, address _OVRLandAddress) public initializer {
        token = IERC20(_tokenAddress);
        OVRLand = IOVRLand(_OVRLandAddress);
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        feeUSD = 10; 
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    /*
     * List of offers for specific NFT id
     * It should keep the last 5 best offers
     */

    mapping(uint256 => Offer) public bestOffers;
    mapping(uint256 => UriSent) public sentURI;
    mapping(uint256 => string) public originalUri;


    /**
     * @param from - msg.sender
     * @param landId - NFT id
     * @param value - offer value in OVR token
     * @param timestamp - when offer was placed
     * @param firstTime - only true if the land was never rented before
     * @param requestSent - only true if the experience has been submitted but not yet accepted
     * @param fee - fee in OVR to pay to us for renting
     */
    struct Offer {
        address from;
        uint256 landId;
        uint256 value;
        uint256 fee;
        uint256 timestamp;
        bool requestSent;
        bool firstTime;
    }

     /**
     * @param URIsent - the uri that need to be approved
     * @param approved - was it approved? 
     */

    struct UriSent {
        string URIsent;
        bool approved;
    }

    modifier isLandOwner(uint256 _nftId){
        require(OVRLand.ownerOf(_nftId) == msg.sender, "you're not the owner of this land");
        _;
    }

    modifier isOfferer(uint256 _nftId){
        require(msg.sender == bestOffers[_nftId].from, "you're not a offerer");
        _;
    }



    function requestSent(uint256 _nftId) public view returns(bool){
        bool _requestSent = bestOffers[_nftId].requestSent;
        return _requestSent;
    }

    function accepted(uint256 _nftId) public view returns(bool){
        return  sentURI[_nftId].approved;
    }

    function lastOffer(uint256 _nftId) public view returns(address, uint256, uint256, uint256, uint256, bool, bool){
        return (bestOffers[_nftId].from, 
        bestOffers[_nftId].landId,
         bestOffers[_nftId].value,
         bestOffers[_nftId].fee,
         bestOffers[_nftId].timestamp,
         bestOffers[_nftId].requestSent,
         bestOffers[_nftId].firstTime);
    }

     function priceOfOVR() public view returns (uint) {
        /* (, uint OVRPriceInUSD) = valueOfAsset(0x21BfBDa47A0B4B5b1248c767Ee49F7caA9B23697, 1e18);
        return OVRPriceInUSD;  */
    }

    /**
    * @notice function to send the URI to the land Owner for approval
    * @dev if block.timestamp is greater than 1 day and less
    * than 8 days from the last offer, will mean that we are in the "approvation period". 
    * The offerer Land can send the URI to the land owner for approval if he/her meets
    * requirements of the modifier isOfferer()
    */
    function sendUriForApproval(string memory _uri, uint256 _nftId) public isOfferer(_nftId){
       require(block.timestamp > bestOffers[_nftId].timestamp.add(1 days),  "offers are still open");
       require(block.timestamp < bestOffers[_nftId].timestamp.add(8 days),  "time to send uri is ended");
       sentURI[_nftId].URIsent = _uri;
       sentURI[_nftId].approved = false;
       bestOffers[_nftId].requestSent = true; 
    }

    
    function uriSentView(uint256 _nftId) public view returns(string memory, bool){
        return (sentURI[_nftId].URIsent, sentURI[_nftId].approved);
    }
    
     
    /**
    * @notice function to place an offer for a specified land 
    * @dev if block.timestamp is less than 1 day from the last offer it means that 
    * this offer is in competition with others, so the value must 
    * be greater than the last value entered.
    * otherwise it means that the offer is the first for this land, 
    * or it is the first after the expiry of another rent, so the value 
    * can be any (greater than $ 10, which are our fees)
    */
    function placeOffer(uint256 _nftId, uint256 _value) public {
        require(block.timestamp < bestOffers[_nftId].timestamp.add(1 days) || block.timestamp > bestOffers[_nftId].timestamp.add(38 days),  "closed offers");
        uint256 priceOVR = priceOfOVR();
        uint256 fee = feeUSD.div(priceOVR);
        require(_value > fee && _nftId > 0, "the offer must be > 10$ in OVR and the ID must be > 0" );
        if(block.timestamp < bestOffers[_nftId].timestamp.add(1 days)){
             require(_value > bestOffers[_nftId].value, "the offer is too low!");
        }

        bestOffers[_nftId].fee = fee;
        bestOffers[_nftId].landId = _nftId;
        bestOffers[_nftId].from = msg.sender;
        bestOffers[_nftId].value = _value;
        bestOffers[_nftId].timestamp = block.timestamp;
        bestOffers[_nftId].requestSent = false;
       
    }

    /**
    * @notice function to cancel an offer by the OwnerLand
    * @dev if block.timestamp is greater than 1 day and less
    * than 8 days from the last offer, will mean that we are in the "approvation period". 
    * The owner Land can refuse the renting if it does'nt accept the
    * URI sent, or doesn't accept the amount offered
    */

    function cancelOffer(uint256 _nftId) public isLandOwner(_nftId){
        require(block.timestamp > bestOffers[_nftId].timestamp.add(1 days),  "offers are still open");
        require(block.timestamp < bestOffers[_nftId].timestamp.add(8 days),  "time to cancel is ended");
        bestOffers[_nftId].from = 0x0000000000000000000000000000000000000000;
        bestOffers[_nftId].value = 0;
        bestOffers[_nftId].timestamp = 0;
        bestOffers[_nftId].requestSent = false;
    }

     /**
    * @notice function to accept the URI sent by the offerer to the Land Owner
    * @dev if block.timestamp is greater than 1 day and less than 8 days from the last offer, we are
    * in the "approvation period", so the land owner can accept the URI sent
    */

    function acceptUri(uint256 _nftId) public isLandOwner(_nftId){
        require(block.timestamp > bestOffers[_nftId].timestamp.add(1 days),  "offers are still open");
        require(block.timestamp < bestOffers[_nftId].timestamp.add(8 days),  "time to accept is ended");
        sentURI[_nftId].approved = true;
    }

  /**
    * @notice function to cancel an offer by the owner contract if something went wrong
    * @dev if someone send an offer and then loose the private key, and also the owner
    *  of the land lost the private key, the land will be in a "limbo" where is accepted but not payed
    *  and not used for the next 37 days, so we can cancel that offer and make the land
    * rentable again
    */
    function emergencyCancelOffer(uint256 _nftId) public onlyOwner{
        bestOffers[_nftId].from = 0x0000000000000000000000000000000000000000;
        bestOffers[_nftId].value = 0;
        bestOffers[_nftId].timestamp = 0;
        bestOffers[_nftId].requestSent = false;
    }

   /**
    * @notice function to change the URI by the offerer
    * @dev if the URI was approved OR if the "approvation time" is ended 
    * but the offer is not deleted (it means the owner land is inactive on the platform)
    * the offerer can change the URI of the token paying the amounts due.
    * we also save the original URI to restore it at the end of the rent.
    */

    function changeURI(uint256 _nftId) public isOfferer(_nftId){
        require(sentURI[_nftId].approved == true || block.timestamp > bestOffers[_nftId].timestamp.add(8 days), "the URI wasn't approved yet");
        require(token.balanceOf(msg.sender) >= bestOffers[_nftId].value, "you don't have enough OVR");
        require(token.transferFrom(msg.sender, owner(), bestOffers[_nftId].fee), "Insufficient Token Allowance");
        uint256 tokenForLandOwner = bestOffers[_nftId].value.sub(bestOffers[_nftId].fee);
        require(token.transferFrom(msg.sender, OVRLand.ownerOf(_nftId), tokenForLandOwner), "Insufficient Token Allowance");

        if(bestOffers[_nftId].firstTime != false){
            originalUri[_nftId] = OVRLand.tokenURI(_nftId);
            bestOffers[_nftId].firstTime == false;
        }
        OVRLand.setOVRLandURI(_nftId, sentURI[_nftId].URIsent);

    }

    /**
    * @notice function to restore the original URI of the land
    * @dev if block.timestamp is greater than 38 days from the last offer
    * will mean that the land is free, so everyone can restore the old URI from every land
    */
    function restoreOriginalUri(uint256 _nftId) public {
        require(block.timestamp > bestOffers[_nftId].timestamp.add(38 days),  "you can't restore the URI now");
        bestOffers[_nftId].from = 0x0000000000000000000000000000000000000000;
        bestOffers[_nftId].value = 0;
        bestOffers[_nftId].timestamp = 0;
        bestOffers[_nftId].requestSent = false;
         OVRLand.setOVRLandURI(_nftId, originalUri[_nftId]);
    }

    function pause() public onlyOwner whenNotPaused {
        _pause();
    }

    function unpause() public onlyOwner whenPaused {
        _unpause();
    }
}

