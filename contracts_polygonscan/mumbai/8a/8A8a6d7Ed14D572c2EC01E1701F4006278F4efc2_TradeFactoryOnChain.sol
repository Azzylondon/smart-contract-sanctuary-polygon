pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract ARCHToken is ERC20 {
    event IssueReward(address token, address to, uint256 amount);
    event ReturnReward(address token, address from, uint256 amount);
    event GiftUrnedReward(address _user, uint256 _amount);

    mapping(address => mapping(address => uint256)) public tokensUserRewards;
    mapping(address => uint256) public giftedRewards;

    // Add the library methods
    using EnumerableSet for EnumerableSet.AddressSet;
    // Declare a set state variable
    EnumerableSet.AddressSet private erc20TokensSet;
    EnumerableSet.AddressSet private giftedUsersSet;

    constructor(address _address) ERC20("ARCH TOKEN", "ARCH") {
        _mint(_address, 1000000 * 10**18);
    }

    function reward(
        address _token,
        address _to,
        uint256 _amount
    ) external {
        // 1:1
        // TODO later verify sufficient funds
        erc20TokensSet.add(_token);

        tokensUserRewards[_token][_to] = _amount;

        emit IssueReward(_token, _to, _amount);
    }

    function getUsersTotalRewards(address _to) external view returns (uint256) {
        uint256 usersTotalRewards;
        for (uint256 index = 0; index < erc20TokensSet.length(); index++) {
            usersTotalRewards =
                usersTotalRewards +
                tokensUserRewards[erc20TokensSet.at(index)][_to];
        }
        return usersTotalRewards;
    }

    function getSpecificTokenRewards(address _token, address _to)
        external
        view
        returns (uint256)
    {
        return tokensUserRewards[_token][_to];
    }

    function returnRewards(address _token, address _from) external {
        uint256 _amount = tokensUserRewards[_token][_from];
        delete tokensUserRewards[_token][_from];

        emit ReturnReward(_token, _from, _amount);
    }

    function getBalance() external view returns (uint256) {
        return address(this).balance;
    }

    // // gift the urned rewards as 10%
    function giftUrnedRewards(address _user, uint256 _amount) external {
        _amount = _amount / 10;
        // TODO later verify sufficient funds
        transfer(_user, _amount);
        giftedRewards[_user] += _amount;
        giftedUsersSet.add(_user);

        emit GiftUrnedReward(_user, _amount);
    }

    function getTotalGiftedRewards() external view returns (uint256) {
        uint256 total;
        for (uint256 index = 0; index < giftedUsersSet.length(); index++) {
            total = total + giftedRewards[giftedUsersSet.at(index)];
        }
        return total;
    }
}

