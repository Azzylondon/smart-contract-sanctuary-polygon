pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "hardhat/console.sol"; // TODO remove this before you go live.
import "./Trade.sol";
import "./LP.sol";

contract TradeFactoryOnChain is
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable
{
    mapping(address => Trade[]) trades;
    mapping(address => uint256) tradeId;
    LP leveragePool;

    address AGGREGATION_ROUTER_V4;

    event TradeEvent(Trade trade);
    event TradeEventParams(
        uint256 tradeId,
        address traderAddress,
        Position position,
        address fromToken,
        address toToken,
        uint256 fromTokenAmount,
        uint256 toTokenAmount,
        uint256 equity,
        uint256 debt,
        Leverage leverage,
        Status tradeStatus
    );

    function initialize(address _leveragePoolAddress, address router)
        external
        initializer
    {
        __Ownable_init();
        leveragePool = LP(_leveragePoolAddress);
        AGGREGATION_ROUTER_V4 = router;
    }

    // Modifiers can take inputs. This modifier checks that the
    // address passed in is not the zero address.
    modifier validAddress(address _addr) {
        require(address(_addr) != address(0), "Not valid address");
        _;
    }

    modifier validAmount(uint256 _amount) {
        require(_amount > 0, "Please provide non zero amount");
        _;
    }

    function trade(Trade memory _trade, bytes calldata _callData)
        public
        validAddress(_trade.traderAddress)
        validAddress(_trade.fromToken)
        validAddress(_trade.toToken)
        validAmount(_trade.fromTokenAmount)
        validAmount(_trade.debt)
        nonReentrant
        onlyOwner
    {
        uint256 _allowance = ERC20(_trade.fromToken).allowance(
            _trade.traderAddress,
            address(this)
        );

        require(_allowance > 0, "Please approve the tokens");
        require(
            _allowance == _trade.equity,
            "Approved amount and Equity amount aren't same."
        );

        // get traders equity into this contract
        ERC20(_trade.fromToken).transferFrom(
            _trade.traderAddress,
            address(this),
            _trade.equity
        );
        if (_trade.position == Position.Long) {
            // get leverage from LP into this contract
            leveragePool.provideLeverage(_trade.fromToken, _trade.debt);

            bytes memory _decodedCallData = abi.decode(_callData, (bytes));
            // approve so oneInch can use the tokens.
            ERC20(_trade.fromToken).approve(
                AGGREGATION_ROUTER_V4,
                _trade.fromTokenAmount
            );
            // now go to oneInch and do a swap.
            (bool success, bytes memory response) = address(
                AGGREGATION_ROUTER_V4
            ).call(_decodedCallData);

            require(
                success == true,
                "Unable to proceed due to issue at oneInch side."
            );
            if (success) {
                uint256 _returnAmount = abi.decode(response, (uint256));
                _trade.toTokenAmount = _returnAmount;
            }
        } else {
            // get leverage from LP into this contract
            leveragePool.provideLeverage(_trade.toToken, _trade.debt);
        }

        _trade.tradeId = tradeId[_trade.traderAddress];
        _trade.tradeStatus = Status.open;
        _trade.fromTokenAmount = 0;
        trades[_trade.traderAddress].push(_trade);
        Trade memory eTrade = _trade;
        emit TradeEvent(eTrade);
        emit TradeEventParams(
            eTrade.tradeId,
            eTrade.traderAddress,
            eTrade.position,
            eTrade.fromToken,
            eTrade.toToken,
            eTrade.fromTokenAmount,
            eTrade.toTokenAmount,
            eTrade.equity,
            eTrade.debt,
            eTrade.leverage,
            eTrade.tradeStatus
        );

        tradeId[_trade.traderAddress]++;
    }

    function squareOffTrade(
        address _traderAddress,
        uint256 _tradeId,
        uint256 _fees,
        Status _status,
        bytes calldata _callData
    ) public validAddress(_traderAddress) onlyOwner nonReentrant {
        require(_tradeId < tradeId[_traderAddress], "invalid tradeId");
        Trade storage _trade = trades[_traderAddress][_tradeId];
        require(_trade.tradeStatus == Status.open, "Trade is not open.");
        require(
            _status == Status.close || _status == Status.liquidated,
            "Invalid trade status sent."
        );

        if (_trade.position == Position.Long) {
            // now go to oneInch and do a swap.
            bytes memory _decodedCallData = abi.decode(_callData, (bytes));
            // approve so oneInch can use the tokens.
            ERC20(_trade.toToken).approve(
                AGGREGATION_ROUTER_V4,
                _trade.toTokenAmount
            );
            (bool success, bytes memory response) = address(
                AGGREGATION_ROUTER_V4
            ).call(_decodedCallData);

            require(
                success == true,
                "Unable to proceed due to issue at oneInch side."
            );
            if (success) {
                uint256 _returnAmount = abi.decode(response, (uint256));
                _trade.fromTokenAmount = _returnAmount;
            }

            ERC20(_trade.fromToken).approve(
                address(leveragePool),
                _trade.debt + _fees
            );
            leveragePool.repay(_trade.fromToken, _trade.debt, _fees);

            ERC20(_trade.fromToken).transfer(
                _trade.traderAddress,
                _trade.fromTokenAmount - _trade.debt - _fees
            );
        } else {
            // ERC20(_trade.toToken).approve(
            //     address(leveragePool),
            //     _trade.debt + _fees
            // );
            // leveragePool.repay(_trade.toToken, _trade.debt, _fees);
            // ERC20(_trade.fromToken).transfer(
            //     _trade.traderAddress,
            //     _settlementAmount
            // );
        }

        //_trade.squareOffPrice = _squareOffPrice;
        _trade.tradeStatus = _status;

        Trade memory eTrade = _trade;
        emit TradeEvent(eTrade);
        emit TradeEventParams(
            eTrade.tradeId,
            eTrade.traderAddress,
            eTrade.position,
            eTrade.fromToken,
            eTrade.toToken,
            eTrade.fromTokenAmount,
            eTrade.toTokenAmount,
            eTrade.equity,
            eTrade.debt,
            eTrade.leverage,
            eTrade.tradeStatus
        );
    }

    function getTrade(address _traderAddress, uint256 _tradeId)
        public
        view
        validAddress(_traderAddress)
        onlyOwner
        returns (Trade memory)
    {
        require(_tradeId < tradeId[_traderAddress], "invalid tradeId");
        return trades[_traderAddress][_tradeId];
    }
}

