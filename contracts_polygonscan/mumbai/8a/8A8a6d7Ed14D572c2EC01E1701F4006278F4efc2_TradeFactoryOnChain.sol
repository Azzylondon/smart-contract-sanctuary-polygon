pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC777/ERC777.sol";
import "./tokens/ARCHToken.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./LPActions.sol";
import "./TokenManage.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

contract LP is Initializable, OwnableUpgradeable, ReentrancyGuardUpgradeable {
    event LeveragedAccount(address account, address owner);
    event Pool(
        address token,
        string name,
        string symbol,
        address from,
        uint256 amount,
        LPActions action
    );
    event Tokens(
        address token,
        string name,
        string symbol,
        uint256 totalFunds,
        uint256 totalUsedFunds
    );
    event TokenList(
        address token,
        string name,
        string symbol,
        TokenManage manageType,
        address owner
    );

    mapping(address => mapping(address => uint256)) public tokensAddressFunds;
    mapping(address => uint256) public tokenAvailableTotalFunds;
    mapping(address => uint256) public tokenTotalFunds;

    mapping(address => mapping(address => uint256))
        public tokensAddressLeverage;

    mapping(address => uint256) public totalAvailableLeveragedBal;
    mapping(address => uint256) public totalLeveragedBal;

    mapping(address => uint256) public tokensEarning;

    ARCHToken private arch;
    address private leveragedAccount;

    // Add the library methods
    using EnumerableSet for EnumerableSet.AddressSet;
    // Declare a set state variable
    EnumerableSet.AddressSet private erc20TokensSet;

    function initialize(address[] memory validTokens) public initializer {
        __Ownable_init();
        for (uint256 index = 0; index < validTokens.length; index++) {
            require(
                address(validTokens[index]) != address(0),
                "Not valid address"
            );
            erc20TokensSet.add(validTokens[index]);
            emit TokenList(
                validTokens[index],
                ERC20(validTokens[index]).name(),
                ERC20(validTokens[index]).symbol(),
                TokenManage.add,
                msg.sender
            );
        }
    }

    function init(address archTokenAddress) public {
        arch = ARCHToken(archTokenAddress);
    }

    // Modifiers can take inputs. This modifier checks that the
    // address passed in is not the zero address.
    modifier validAddress(address addr) {
        require(address(addr) != address(0), "Not valid address");
        _;
    }

    modifier validAmount(uint256 amount) {
        require(amount > 0, "Please provide non zero amount");
        _;
    }

    modifier validLeveragedAccount(address addr) {
        require(
            address(addr) == leveragedAccount,
            "Not valid leverage Account"
        );
        _;
    }

    function setLeveragedAccount(address account)
        public
        onlyOwner
        validAddress(account)
    {
        leveragedAccount = account;
        emit LeveragedAccount(account, msg.sender);
    }

    function addToken(address token) public onlyOwner validAddress(token) {
        if (!erc20TokensSet.contains(token)) {
            erc20TokensSet.add(token);
            emit TokenList(
                token,
                ERC20(token).name(),
                ERC20(token).symbol(),
                TokenManage.add,
                msg.sender
            );
        }
    }

    function removeToken(address token) public onlyOwner validAddress(token) {
        erc20TokensSet.remove(token);
        emit TokenList(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            TokenManage.remove,
            msg.sender
        );
    }

    //TODO need to remove before prod
    function getTokenList() public view returns (address[] memory) {
        address[] memory allTokens = new address[](erc20TokensSet.length());

        for (uint256 index = 0; index < erc20TokensSet.length(); index++) {
            allTokens[index] = erc20TokensSet.at(index);
        }
        return allTokens;
    }

    modifier validToken(address token) {
        require(erc20TokensSet.contains(token), "Not a valid token");
        _;
    }

    function verifySufficientTokenFunds(address token, uint256 amount)
        internal
        view
    {
        require(
            ERC20(token).balanceOf(address(this)) >= amount,
            "Insufficient funds"
        );
    }

    function verifyUserDeposit(
        address token,
        address user,
        uint256 amount
    ) internal view {
        require(
            tokensAddressFunds[token][user] >= amount,
            "insufficient funds "
        );
    }

    function deposit(address token, uint256 amount)
        external
        validAddress(token)
        validAmount(amount)
        nonReentrant
        validToken(token)
    {
        address user = msg.sender;

        require(user != address(0), "Not valid address");

        uint256 Allowance = ERC20(token).allowance(user, address(this));
        require(Allowance >= amount, "Check the token allowance");

        ERC20(token).transferFrom(user, address(this), amount);

        tokensAddressFunds[token][user] += amount;
        tokenAvailableTotalFunds[token] += amount;
        tokenTotalFunds[token] += amount;

        issueReward(token, user, amount);

        emit Tokens(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            tokenTotalFunds[token],
            totalLeveragedBal[token]
        );
        emit Pool(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            user,
            amount,
            LPActions.deposit
        );
    }

    function provideLeverage(address token, uint256 amount)
        external
        validAddress(token)
        validAmount(amount)
        nonReentrant
        validToken(token)
    {
        // TODO - need to avoid transfer

        verifySufficientTokenFunds(token, amount);

        ERC20(token).transfer(leveragedAccount, amount);

        tokensAddressLeverage[token][leveragedAccount] += amount;

        totalAvailableLeveragedBal[token] += amount;
        totalLeveragedBal[token] += amount;

        emit Tokens(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            tokenTotalFunds[token],
            totalLeveragedBal[token]
        );
        emit Pool(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            leveragedAccount,
            amount,
            LPActions.leverage
        );
    }

    function repay(
        address token,
        uint256 amount,
        uint256 other
    )
        external
        validAddress(token)
        validAmount(amount)
        validLeveragedAccount(msg.sender)
        nonReentrant
        validToken(token)
    {
        // send back to contract

        uint256 Allowance = ERC20(token).allowance(
            leveragedAccount,
            address(this)
        );
        require(Allowance >= (amount + other), "Check the token allowance");

        ERC20(token).transferFrom(
            leveragedAccount,
            address(this),
            (amount + other)
        );

        tokensAddressLeverage[token][leveragedAccount] -= amount;

        totalAvailableLeveragedBal[token] -= amount;

        tokensEarning[token] += other;

        emit Pool(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            leveragedAccount,
            other,
            LPActions.earnings
        );
        emit Pool(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            leveragedAccount,
            amount,
            LPActions.repay
        );
    }

    function withdraw(address token, uint256 amount)
        external
        validAddress(token)
        validAddress(msg.sender)
        validAmount(amount)
        nonReentrant
        validToken(token)
    {
        address to = msg.sender;
        // verify available funds in pool
        verifySufficientTokenFunds(token, amount);

        // verify deposited amount by this user
        verifyUserDeposit(token, to, amount);

        returnRewards(token, to, amount);

        ERC20(token).transfer(to, amount);

        tokensAddressFunds[token][to] -= amount;
        tokenAvailableTotalFunds[token] -= amount;

        emit Pool(
            token,
            ERC20(token).name(),
            ERC20(token).symbol(),
            to,
            amount,
            LPActions.withdraw
        );
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function getBalanceOfToken(address token) public view returns (uint256) {
        return ERC20(token).balanceOf(address(this));
    }

    function getTotalLeveragedBal(address token) public view returns (uint256) {
        return totalLeveragedBal[token];
    }

    function getAvailableLeveragedBal(address token)
        public
        view
        returns (uint256)
    {
        return totalAvailableLeveragedBal[token];
    }

    function getTokenTotalFunds(address token) public view returns (uint256) {
        return tokenTotalFunds[token];
    }

    function getTokensEarning(address token) public view returns (uint256) {
        return tokensEarning[token];
    }

    // Issuing Tokens/reward
    function issueReward(
        address token,
        address to,
        uint256 amount
    ) internal {
        // TODO for now keeping equal reward as deposited amount 1:1
        arch.transfer(to, amount);
        arch.reward(token, to, amount);

        emit Pool(
            address(arch),
            ERC20(address(arch)).name(),
            ERC20(address(arch)).symbol(),
            to,
            amount,
            LPActions.rewards
        );
    }

    function getUsersTotalRewards(address user) public view returns (uint256) {
        return arch.getUsersTotalRewards(user);
    }

    function getUsersTokenSpecificRewards(address token, address user)
        public
        view
        returns (uint256)
    {
        return arch.getSpecificTokenRewards(token, user);
    }

    function returnRewards(
        address token,
        address user,
        uint256 amount
    ) internal {
        uint256 Allowance = arch.allowance(user, address(this));
        require(Allowance >= amount, "Check the reward token allowance");

        arch.transferFrom(user, address(this), amount);
        arch.returnRewards(token, user);

        emit Pool(
            address(arch),
            ERC20(address(arch)).name(),
            ERC20(address(arch)).symbol(),
            user,
            amount,
            LPActions.repayRewards
        );
    }

    function sendUrnedRewards(address user, uint256 amount) public {
        arch.giftUrnedRewards(user, amount);
    }
}

