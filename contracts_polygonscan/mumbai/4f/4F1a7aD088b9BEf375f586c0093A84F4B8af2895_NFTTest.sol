	uint256 public mintedHives;
	mapping(uint256 => uint256) private _totalSupply;
    mapping(uint256 => uint256) private _tire; // Tire category 1- common, 2 - Drone, 3 - Queen

    mapping(uint256 => uint256) private lastClaimedTimestamp; // Last time when the reward claimed.
	mapping(uint256 => uint256) private remainingReward; // To store left over reward while converting to NFTs

    // MaxCap of all tires
    uint256 tireOneMaxCap = 180; //39200
    uint256 tireTwoMaxCap = 600; //600
    uint256 tireThreeMaxCap = 39200; //180

    //Total number of NFTs minted for each tire.
    uint256 public tireOneTotalMintedNFT = 0;
    uint256 public tireTwoTotalMintedNFT = 0;
    uint256 public tireThreeTotalMintedNFT = 0;

    //Reward rate for each tire.
    uint256 tireThreeRewardRate = 50000000000000000;
    uint256 tireTwoRewardRate = 100000000000000000;
    uint256 tireOneRewardRate = 200000000000000000;

    //Previous reward rate for each tire. (Will be used when admin changed reward rate)
    uint previousTireOneRewardRate = tireOneRewardRate;
    uint previousTireTwoRewardRate = tireTwoRewardRate;
    uint previousTireThreeRewardRate = tireThreeRewardRate;

    mapping(uint => uint) rateUpdatedTime; // When the reward get updated

    event RewardClaimed(address Sender, uint256 tokenId, uint256 amount);

	string public _baseURI = "https://hivenft.s3.ap-southeast-2.amazonaws.com/jsons/";

	mapping(uint256 => string) public _tokenURIs;

	IERC20 public hiveToken;

    address public charityAddress;
    address public adminAddress;
    address public rewardPool;
    address public treasuryPool;
    address public liquidityPool;
    address public team;

	uint256 public initialItemPrice = 10000000000000000000; //10 Hive

	constructor(address _token, address _charity, address _team, address _reward, address _treasury, address _liquidity) {
        hiveToken = IERC20(_token);
        _setURI(_baseURI);
        rateUpdatedTime[1] = block.timestamp;
        rateUpdatedTime[2] = block.timestamp;
        rateUpdatedTime[3] = block.timestamp;
        charityAddress = _charity;
        adminAddress = msg.sender;
        team = _team;
        rewardPool = _reward;
        treasuryPool = _treasury;
        liquidityPool = _liquidity;
	}

    // Returns the price of item which will be incremented t0 0.5 Hives after every 8000 NFT
	function getItemPrice() public view returns (uint256) {
		uint increment = 500000000000000000;
		uint count = mintedHives / 8000;
		return initialItemPrice + (increment * count);
	}

    //Split the total amount and transfer to respective address
    function splitAndTransferTokens(uint amount) internal {
        uint toRewardPool = amount.mul(40).div(100); //40 %
        uint toTeam = amount.mul(5).div(100); // 5 %
        uint toLiquidity = amount.mul(10).div(100); // 10 %
        uint toCharity = amount.mul(15).div(1000); // 1.5%

        uint toTreasury = amount.sub(toRewardPool).sub(toTeam).sub(toLiquidity).sub(toCharity); //Remaining %

        hiveToken.transferFrom(msg.sender, rewardPool, toRewardPool);
        hiveToken.transferFrom(msg.sender, team, toTeam);
        hiveToken.transferFrom(msg.sender, liquidityPool, toLiquidity);
        hiveToken.transferFrom(msg.sender, charityAddress, toCharity);
        hiveToken.transferFrom(msg.sender, treasuryPool, toTreasury);

    }

    //Mint a single hive
	function mintHive() public {
        uint itemPrice = getItemPrice();
        require(hiveToken.balanceOf(msg.sender) >= itemPrice, "Insufficent token balance");
        splitAndTransferTokens(itemPrice);
		buy();
	}

    //Mint multiple hive
	function mintHives(uint256 _howMany) public {
        uint itemPrice = getItemPrice();
		require(_howMany <= 5, "max 10 hives at once");
        uint256 totalPrice = itemPrice.mul(_howMany);
        require(hiveToken.balanceOf(msg.sender) >= totalPrice, "Insufficient token balance");
        splitAndTransferTokens(totalPrice);
		for (uint256 i = 0; i < _howMany; i++) {
			buy();
		}
	}

    //Internal function to handle minting
	function buy() private {
		for (uint256 i = 0; i < 9999; i++) {
			uint256 randID = random(21, 40000, uint256(uint160(address(msg.sender))) + i);
			if (randID > 20 && _totalSupply[randID] == 0) {
				_totalSupply[randID] = 1;
                allocateTire(randID);
                isOverlapRewardClaimed[rateUpdatedTime[_tire[randID]]][randID] = true;
                nextTimePeriodToPayFee[randID] = block.timestamp;
				_mint(msg.sender, randID, 1, "0x0000");
                lastClaimedTimestamp[randID] = block.timestamp;
				mintedHives = mintedHives + 1;
				return;
			}
		}
		revert();
	}

    //Allocate tire based on token ID
    function allocateTire(uint256 randID) private {
        if(randID <= 180) {
			_tire[randID] = 1;
            tireOneTotalMintedNFT += 1;
		} else if(randID > 181 && randID <= 780) {
			_tire[randID] = 2;
            tireTwoTotalMintedNFT += 1;
		} else  {
			_tire[randID] = 3;
            tireThreeTotalMintedNFT += 1;
		}
		return;
    }

    //Rebase time for reward - 1 Day
    uint256 private rebaseTime = 1 minutes;

    function setRebaseTime(uint256 time) public onlyOwner {
        rebaseTime = time;
    }

    mapping(uint => mapping(uint => bool)) isOverlapRewardClaimed;

    //Calculate the reward earned for a paricular token.
    function calculateReward(uint id, uint tire, uint currentRate, uint previousRate) private view returns(uint256){
        uint256 timeDifference = block.timestamp.sub(lastClaimedTimestamp[id]);
        uint256 numOfDays = timeDifference.div(rebaseTime);
        if(isOverlapRewardClaimed[rateUpdatedTime[tire]][id]) {
                return (numOfDays * currentRate).add(remainingReward[id]);
            } else {
                uint256 timeDifferenceTillRewardUpdate = rateUpdatedTime[tire].sub(lastClaimedTimestamp[id]);
                uint256 numOfDaysPassed = timeDifferenceTillRewardUpdate.div(rebaseTime);
                uint256 rewardsTillRewardUpdate = numOfDaysPassed.mul(previousRate);

                uint timeDifferenceAfterUpdate = block.timestamp.sub(rateUpdatedTime[tire]);
                uint256 numOfDaysAfterUpdate = timeDifferenceAfterUpdate.div(rebaseTime);
                uint rewardsAfterUpdate = numOfDaysAfterUpdate.mul(currentRate);

                return rewardsTillRewardUpdate.add(rewardsAfterUpdate).add(remainingReward[id]);
            }
    }

    // Returns the total reward earned in the token
    function rewardEarned(uint256 id) public view returns(uint256) {
        uint256 tire = _tire[id];
        require(tire > 0, "ID isn't minted yet or Invalid ID");
        if(tire == 1) {
            return calculateReward(id, tire, tireOneRewardRate, previousTireOneRewardRate);
        }
        if(tire == 2) {
            return calculateReward(id, tire, tireTwoRewardRate, previousTireTwoRewardRate);
        }
        if(tire == 3) {
            return calculateReward(id, tire, tireThreeRewardRate, previousTireThreeRewardRate);
        }
        if(tire == 4) {
            return calculateReward(id, tire, premiumRewardRate[id], premiumRewardRate[id]);
        }

        return 0;
    }

    //Claime reward
    function claimReward(uint256 id, uint toUser, uint toCharity) public {
        if(id > 20) {
            require(isMaintainanceFeePaid(id), "Please pay maintainance fee before claiming reward!");
        }

        require(balanceOf(msg.sender, id) > 0, "Reward claimer is not the owner of the NFT");
        uint256 amount = rewardEarned(id);
        require(amount > 0, "Wait till next rebase!");
        require(amount >= (toUser + toCharity), "Total of user and charity amount not equal to total reward");
        lastClaimedTimestamp[id] = block.timestamp;
        if(!isOverlapRewardClaimed[rateUpdatedTime[_tire[id]]][id]) {
            isOverlapRewardClaimed[rateUpdatedTime[_tire[id]]][id] = true;
        }

        remainingReward[id] = amount.sub(toUser + toCharity);

        if(toUser > 0) {
            hiveToken.transfer(msg.sender, toUser);
        }
        if(toCharity > 0) {
            hiveToken.transfer(charityAddress, toCharity);
        }
        emit RewardClaimed(msg.sender, id, amount);
    }

    //Convert a reward to NFT
	function convertRewardToNFT(uint256 id) public {
		require(balanceOf(msg.sender, id) > 0, "Reward claimer is not the owner of the NFT");
		uint256 totalRewards = rewardEarned(id);
		uint256 numOfNFTs = totalRewards.div(getItemPrice());
		require(numOfNFTs > 0, "Insufficent reward balance to convert to NFT");
		//require(rewardEarned(id) >= itemPrice, "Insufficient reward balance to convert to NFT");
		if(numOfNFTs > 5) {
			numOfNFTs = 5;
		}

		uint256 remainingPrice =  totalRewards.sub(numOfNFTs.mul(getItemPrice()));
		remainingReward[id] = remainingPrice;
		lastClaimedTimestamp[id] = block.timestamp;
        if(!isOverlapRewardClaimed[rateUpdatedTime[_tire[id]]][id]) {
            isOverlapRewardClaimed[rateUpdatedTime[_tire[id]]][id] = true;
        }
		
		for(uint256 i = 0; i < numOfNFTs; i++) {
			buy();
		}
	}

    //Function to mint an NFT by the owner.
    //He can specify the address and the token id
	function mint(
		address to,
		uint256 id,
		bytes memory data
	) public onlyOwner {
		require(_totalSupply[id] == 0, "this id is already owned by someone");
		_totalSupply[id] = 1;
        allocateTire(id);
        isOverlapRewardClaimed[rateUpdatedTime[_tire[id]]][id] = true;
        nextTimePeriodToPayFee[id] = block.timestamp;
        lastClaimedTimestamp[id] = block.timestamp;
		mintedHives = mintedHives + 1;
		_mint(to, id, 1, data);
	}

