pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./StoaEternalStorage.sol";
import "./Resolver.sol";
import "./STOA.sol";
import "./Staking.sol";

contract Rewarder is Ownable {
    using SafeMath for uint256;
    uint256 public genesis;

    address public resolver;
    address public token;

    uint256 public totalSupply;
    uint256 public quarterlyDecay;
    uint256 public yearlyDecay;

    uint256 public baseRatio;
    uint256 public stakeRatio;
    uint256 public commitmentRatio;
    uint256 public accomplishmentRatio;

    uint256 public baseForPeriod;
    uint256 public stakingForPeriod;
    uint256 public commitmentForPeriod;
    uint256 public accomplishmentForPeriod;

    uint256 public periodCounter;
    uint256 public periodExpires;
    uint256 public periodDuration = 90 days;

    uint256 public rewardAssignedPeriod;

    constructor(
        address _owner,
        address _stoaToken,
        address _resolver,
        uint256 _quarterlyDecay,
        uint256 _yearlyDecay,
        uint256 _baseRatio,
        uint256 _stakeRatio,
        uint256 _commitmentRatio,
        uint256 _accomplishmentRatio
    ) {
        resolver = _resolver;
        token = _stoaToken;
        quarterlyDecay = _quarterlyDecay;
        yearlyDecay = _yearlyDecay;
        baseRatio = _baseRatio;
        stakeRatio = _stakeRatio;
        commitmentRatio = _commitmentRatio;
        accomplishmentRatio = _accomplishmentRatio;
        genesis = block.timestamp;
        periodExpires = genesis + periodDuration;
        if (msg.sender != _owner) {
            transferOwnership(_owner);
        }
    }

    // Event
    event PeriodRewardsUpdated(uint256 base, uint256 stake, uint256 accomplishment, uint256 commitment);
    event PeriodExpirationUpdated(uint256 periodCounter, uint256 timestamp);
    event RewardsDurationUpdated(uint256 newDuration);

    // Constants
    bytes32 private constant CONTRACT_STOA_ETERNAL_STORAGE = "StoaEternalStorage";
    bytes32 private constant CONTRACT_STOA = "STOA";
    bytes32 private constant CONTRACT_STAKING = "Staking";
    bytes32 private constant USER_COUNT = "UserCount";

    // Views
    function getTotalSupply() public view returns (uint256) {
        return totalSupply;
    }

    function getBaseRewardEarned(address account, uint256 lastClaimed) public view returns (uint256 rewardAmount) {
        uint256 daysSinceLastClaim = (block.timestamp - lastClaimed) / 1 days;
        uint256 baseRewardPerDay = baseForPeriod.div(periodDuration / 1 days);
        return (daysSinceLastClaim * baseRewardPerDay) / stoa().getUserCount();
    }

    function getStakingRewardAvailable() public view returns (uint256 rewardAmount) {
        return stakingForPeriod;
    }

    function getStakingRewardEarned(address user) public view returns (uint256 rewardAmount) {
        return staking().earned(user);
    }

    function getPeriodDuration() public view returns (uint256 duration) {
        return periodDuration;
    }

    function getPeriodEnd() public view returns (uint256 endTimestamp) {
        return periodExpires;
    }

    function getYearsSinceGenesis() public view returns (uint256) {
        return (block.timestamp - genesis) / 365 days;
    }

    function getQuartersSinceGenesis() public view returns (uint256) {
        return (block.timestamp - genesis) / 13 weeks;
    }

    // Mutation
    function allocateReward(uint256 amount) external onlyOwner {
        // TODO route excess rewards to treasury
        totalSupply = totalSupply + amount;
        require(IERC20(token).balanceOf(address(this)) >= totalSupply, "Amount exceeds balance");
        calculateRewardsForPeriod(totalSupply);
    }

    function calculateRewardsForPeriod(uint256 rewardForPeriod) internal {
        // calculate base
        baseForPeriod = rewardForPeriod.mul(baseRatio).div(1e18);
        // calculate staking
        stakingForPeriod = rewardForPeriod.mul(stakeRatio).div(1e18);

        // calculate accomplishment
        accomplishmentForPeriod = rewardForPeriod.mul(accomplishmentRatio).div(1e18);
        // calculate commitment
        commitmentForPeriod = rewardForPeriod.mul(commitmentRatio).div(1e18);

        // updates
        staking().notifyRewardAmount(stakingForPeriod);
        updatePeriodExpires();

        // emit event
        emit PeriodRewardsUpdated(baseForPeriod, stakingForPeriod, accomplishmentForPeriod, commitmentForPeriod);
    }

    function updatePeriodExpires() internal {
        if (block.timestamp >= periodExpires) {
            periodExpires = periodExpires + periodDuration;
            periodCounter = periodCounter + 1;
            emit PeriodExpirationUpdated(periodCounter, periodExpires);
        }
    }

    function setRewardsDuration(uint256 _rewardsDuration) external onlyOwner {
        require(block.timestamp > periodExpires, "Previous rewards period must be complete before update");
        periodDuration = _rewardsDuration;
        emit RewardsDurationUpdated(periodDuration);
    }

    // System
    function stoaEternalStorage() internal view returns (StoaEternalStorage) {
        return StoaEternalStorage(stoaResolver().requireAndGetAddress(CONTRACT_STOA_ETERNAL_STORAGE));
    }

    function stoa() internal view returns (STOA) {
        return STOA(stoaResolver().requireAndGetAddress(CONTRACT_STOA));
    }

    function staking() internal view returns (Staking) {
        return Staking(stoaResolver().requireAndGetAddress(CONTRACT_STAKING));
    }

    function stoaResolver() internal view returns (Resolver) {
        return Resolver(resolver);
    }

    function stoaToken() internal view returns (IERC20) {
        return IERC20(token);
    }
}

