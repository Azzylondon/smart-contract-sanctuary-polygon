pragma solidity ^0.8.9;
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

// https://github.com/Synthetixio/synthetix/blob/sol8-everything-poc/contracts/AddressResolver.sol
contract Resolver is Ownable {
    mapping(bytes32 => address) public contracts;

    constructor(address _owner) {
        if (msg.sender != _owner) {
            transferOwnership(_owner);
        }
    }

    function importContractAddresses(bytes32[] calldata names, address[] calldata destinations) external onlyOwner {
        require(names.length == destinations.length, "Input lengths must match");

        for (uint256 i = 0; i < names.length; i++) {
            bytes32 name = names[i];
            address destination = destinations[i];

            contracts[name] = destination;
            emit ContractAddressImported(name, destination);
        }
    }

    function getContractAddress(bytes32 name) external view returns (address) {
        return contracts[name];
    }

    function requireAndGetAddress(bytes32 name) external view returns (address) {
        address _foundAddress = contracts[name];
        require(_foundAddress != address(0), string(abi.encodePacked("Missing address: ", name)));
        return _foundAddress;
    }

    event ContractAddressImported(bytes32 name, address destination);
}

