// File contracts/Issue.sol

pragma solidity ^0.8.0;



contract Issue is Ownable, ERC1155 {
    address public board;
    mapping(uint256 => string) public stockInfos;
    mapping(uint256 => bool) private isStockCreated;
    event Redeem(uint256 indexed stockId, address indexed receiver, uint256 indexed amount);
    event NewStock(uint256 indexed stockId);
    event NewIssuance(uint256 indexed stockId, uint256 indexed amount);

    modifier onlyBoard() {
        require(msg.sender == board, 'Issue: not board');
        _;
    }


    constructor(string memory uri, address _board) ERC1155(uri) {
        board = _board;
    }

    function _issueNewStock(uint256 _stockId, string memory _stockInfo) internal {
        stockInfos[_stockId] = _stockInfo;
        emit NewStock(_stockId);
    }

    function setUri(string memory uri) external onlyOwner {
        _setURI(uri);
    }

    function issueStock(uint256 _stockId, uint256 amount, string memory _stockInfo) public onlyBoard {
        if (isStockCreated[_stockId] != true) {
            isStockCreated[_stockId] = true;
            _issueNewStock(_stockId, _stockInfo);
        }

        _mint(msg.sender, _stockId, amount, "");
        emit NewIssuance(_stockId, amount);
    }

    function redeem(uint256 _stockId, uint256 amount) public {
        _burn(msg.sender, _stockId, amount);
        emit Redeem(_stockId, msg.sender, amount);
    }
}


// File contracts/Board.sol

pragma solidity ^0.8.0;




struct Proposal {
    uint256 stockId;
    uint256 amount;
    address receiver;
    string stockInfo;
}

contract Board is Ownable, ERC1155Receiver{
    address public issue;

    event SetIssue(address IssueAddress);
    event NewBoardMember(address member);
    event RemoveBoardMember(address member);
    event NewProposal(uint256 indexed proposalId, uint256 indexed stockId, address indexed receiver, uint256 amount);
    event SetMinVotes(uint256 indexed minVotes);

    uint256 public minimumVotes;
    uint256 public totalProposals;
    mapping(uint256 => Proposal) proposals;
    mapping(uint256 => bool) isProposalInValid;
    mapping(address => mapping(uint256 => bool)) isVoted;
    mapping(uint256 => uint256) proposalVotes;
    mapping(address => bool) isBoardMember;

    constructor(uint256 _minimumVotes) {
        minimumVotes = _minimumVotes;
    }

    function onERC1155BatchReceived(
        address, /* _operator */
        address, /* _from */
        uint256[] calldata, /* _ids */
        uint256[] calldata, /* _values */
        bytes calldata /* _data */
    ) override external pure returns (bytes4) {
        return IERC1155Receiver.onERC1155BatchReceived.selector;
    }

    function onERC1155Received(
        address, /* _operator */
        address, /* _from */
        uint256, /* _id */
        uint256, /* _value */
        bytes calldata /* _data */
    ) override external pure returns (bytes4) {
        return IERC1155Receiver.onERC1155Received.selector;
    }

    modifier onlyBoardMember() {
        require(isBoardMember[msg.sender], 'Board: not a board member');
        _;
    }

    function setMinVotes(uint256 _minVotes) external onlyOwner {
        minimumVotes = _minVotes;
        emit SetMinVotes(_minVotes);
    }

    function voteForProposal(uint256 proposalId) external onlyBoardMember {
        require(isVoted[msg.sender][proposalId] != true, "Board: double voting");
        proposalVotes[proposalId]++;
        isVoted[msg.sender][proposalId] = true;
    }

    function addBoardMembers(address[] calldata members) external onlyOwner {
        for (uint i = 0; i < members.length; i++) {
            _addBoardMember(members[i]);
        }
    }

    function removeBoardMembers(address[] calldata members) external onlyOwner {
        for (uint i = 0; i < members.length; i++) {
            _removeBoardMember(members[i]);
        }
    }

    function _addBoardMember(address member) internal {
        isBoardMember[member] = true;
        emit NewBoardMember(member);
    }

    function _removeBoardMember(address member) internal {
        require(isBoardMember[member], "Board: is not a board member");
        isBoardMember[member] = false;
        emit RemoveBoardMember(member);
    }

    function newStockProposal(uint256 _stockId, uint256 amount, address receiver, string memory stockInfo) onlyOwner external {
        Proposal memory proposal = Proposal(_stockId, amount, receiver, stockInfo);
        proposals[totalProposals] = proposal;
        emit NewProposal(totalProposals, _stockId, receiver, amount);
        totalProposals++;
    }

    function issueNewStock(uint256 proposalId) external {
        require(isProposalInValid[proposalId] != true, "Board: proposal is not valid");
        require(proposalVotes[proposalId] >= minimumVotes, "Board: no enough votes on the proposal");
        Proposal memory currentProposal = proposals[proposalId];
        isProposalInValid[proposalId] = true;
        Issue(issue).issueStock(currentProposal.stockId, currentProposal.amount, currentProposal.stockInfo);
        IERC1155(issue).safeTransferFrom(address(this), currentProposal.receiver, currentProposal.stockId, currentProposal.amount, "");
    }

    function setIssueAddress(address _issue) onlyOwner external {
        issue = _issue;
        emit SetIssue(_issue);
    } 
}