pragma solidity ^0.8.9;

/* solhint-disable func-name-mixedcase */

interface ITimelock {
	enum ProposalState {
		UNKNOWN,
		SCHEDULED
	}

	struct Proposal {
		ProposalState state;
		uint64 eta;
		Call[] calls;
	}

	struct Cooldown {
		uint64 start;
		uint64 period;
	}

	struct Call {
		address target;
		uint256 value;
		string signature;
		bytes data;
	}

	event Scheduled(bytes32 indexed txHash, uint256 salt, uint64 eta);
	event Canceled(bytes32 indexed txHash);
	event Executed(bytes32 indexed txHash);
	event Graced(bytes32 indexed txHash, bytes result);
	event LockChanged(uint64 indexed oldLock, uint64 indexed newDelay);
	event CooldownChanged(uint64 indexed oldCooldown, uint64 indexed newCooldown);
	event GraceAmmoAdded(address indexed target, string indexed signature, uint8 ammo);

	function MINIMUM_DELAY() external view returns (uint64);

	function MAXIMUM_DELAY() external view returns (uint64);

	function STALE_THRESHOLD() external view returns (uint64);

	function lock() external view returns (uint64);

	function cooldown() external view returns (uint64 st, uint64 cd);

	function setLock(uint64 lock) external;

	function setCooldown(uint64 cooldown) external;

	function addGraceAmmo(address target, string memory signature) external;

	function getProposal(bytes32 txHash) external view returns (Proposal memory);

	function getGraceAmmo(address target, bytes32 sigHash) external view returns (uint8);

	function schedule(Call[] calldata calls, uint256 salt) external returns (bytes32 txHash, uint64 eta);

	function cancel(bytes32 txHash) external;

	function execute(bytes32 txHash) external payable returns (bytes[] memory results);

	function grace(Call calldata call) external payable returns (bytes memory result);
}

