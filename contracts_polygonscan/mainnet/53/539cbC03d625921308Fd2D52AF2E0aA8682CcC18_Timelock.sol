pragma solidity ^0.8.9;
/* solhint-disable not-rely-on-time, reason-string, var-name-mixedcase */

import { AccessControlEnumerable } from "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import { Address } from "@openzeppelin/contracts/utils/Address.sol";

import { ITimelock } from "../interfaces/ITimelock.sol";

import "hardhat/console.sol";

/**
 * @notice Timelock with RBAC, batch execution, graced transactions and proposals' cooldown add-ons
 */
contract Timelock is AccessControlEnumerable, ITimelock {
	using Address for address;

	bytes32 public constant SCHEDULER_ROLE = keccak256("SCHEDULER_ROLE");
	bytes32 public constant EXECUTOR_ROLE = keccak256("EXECUTOR_ROLE");

	uint64 public constant override MINIMUM_DELAY = 15 hours;
	uint64 public constant override MAXIMUM_DELAY = 30 days;
	uint64 public constant override STALE_THRESHOLD = 7 days;

	uint64 public override lock;

	mapping(address => mapping(bytes32 => uint8)) private graces;
	mapping(bytes32 => Proposal) private proposals;

	Cooldown public override cooldown;

	constructor(
		address _scheduler,
		address _executor,
		uint64 _lock,
		uint64 _cooldown
	) {
		require(
			_scheduler != address(0) && _executor != address(0),
			"Timelock::constructor: scheduler and executor are the 0 address"
		);
		require(_lock >= MINIMUM_DELAY, "Timelock::constructor: delay must exceed minimum lock");
		require(_lock <= MAXIMUM_DELAY, "Timelock::constructor: delay cannot exceed maximum lock");
		require(1 hours <= _cooldown && _cooldown <= 12 hours, "Timelock::constructor: illegal cooldown period");

		lock = _lock;
		cooldown = Cooldown(0, _cooldown);

		_setupRole(DEFAULT_ADMIN_ROLE, address(this));
		_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
		_setupRole(SCHEDULER_ROLE, _scheduler);
		_setupRole(EXECUTOR_ROLE, _executor);
		_setRoleAdmin(SCHEDULER_ROLE, DEFAULT_ADMIN_ROLE);
		_setRoleAdmin(EXECUTOR_ROLE, DEFAULT_ADMIN_ROLE);
		// Roles operations will now pass through the timelock
		revokeRole(DEFAULT_ADMIN_ROLE, msg.sender);

		graces[address(0)][keccak256(abi.encodePacked("exampleFunctionSignature(uint256)"))] = 1; // for demo purposes
	}

	/// @notice restrict functions access to just this timelock
	modifier onlySelf() {
		require(msg.sender == address(this), "Timelock::onlySelf: function call must be time-locked");
		_;
	}

	receive() external payable {} // solhint-disable-line no-empty-blocks

	/// @dev Proposes a transaction batch for execution
	/// @param salt Unique identifier for the transaction when repeatedly proposed (chosen by proposer)
	function schedule(Call[] calldata calls, uint256 salt)
		external
		override
		onlyRole(SCHEDULER_ROLE)
		returns (bytes32 txHash, uint64 eta)
	{
		require(calls.length > 0, "Timelock::propose: empty proposal");
		require(block.timestamp > (cooldown.start + cooldown.period), "Timelock::propose: cooldown");

		txHash = keccak256(abi.encode(calls, salt));
		require(proposals[txHash].state == ProposalState.UNKNOWN, "Timelock::propose: existing proposal");

		proposals[txHash].state = ProposalState.SCHEDULED;
		eta = uint64(block.timestamp) + lock;
		proposals[txHash].eta = eta;

		for (uint256 i = 0; i < calls.length; i++) proposals[txHash].calls.push(calls[i]);
		cooldown = Cooldown(uint64(block.timestamp), cooldown.period);

		emit Scheduled(txHash, salt, eta);
	}

	/// @dev Cancels a pending proposal and removes it from state
	function cancel(bytes32 txHash) external override onlyRole(SCHEDULER_ROLE) {
		Proposal memory proposal = proposals[txHash];
		require(proposal.state == ProposalState.SCHEDULED, "Timelock::cancel: illegal state proposal");
		delete proposals[txHash];
		emit Canceled(txHash);
	}

	/// @dev Executes atomically all calls contained in the proposal
	function execute(bytes32 txHash) external payable override onlyRole(EXECUTOR_ROLE) returns (bytes[] memory results) {
		Proposal memory proposal = proposals[txHash];
		require(proposal.state == ProposalState.SCHEDULED, "Timelock::execute: proposal never scheduled");
		require(uint64(block.timestamp) >= proposal.eta, "Timelock::execute: proposal has not surpassed ETA");
		require(uint64(block.timestamp) <= proposal.eta + STALE_THRESHOLD, "Timelock::execute: stale proposal");

		results = new bytes[](proposal.calls.length);
		for (uint256 i = 0; i < proposal.calls.length; i++) results[i] = _execute(proposal.calls[i]);
		delete proposals[txHash];

		emit Executed(txHash);
	}

	/// @dev Graces the call if its signature has enough grace ammo left
	function grace(Call calldata call) external payable override onlyRole(EXECUTOR_ROLE) returns (bytes memory result) {
		bytes32 sigHash = keccak256(abi.encodePacked(call.signature));
		require(graces[call.target][sigHash] > 0, "Timelock::grace: out of grace ammo");

		bytes32 txHash = keccak256(abi.encode(call.target, call.value, call.signature, call.data));
		graces[call.target][sigHash]--;
		result = _execute(call);

		emit Graced(txHash, result);
	}

	function setLock(uint64 _lock) public override onlySelf {
		require(_lock >= MINIMUM_DELAY, "Timelock::setLock: delay must exceed minimum lock");
		require(_lock <= MAXIMUM_DELAY, "Timelock::setLock: delay must not exceed maximum lock");
		emit LockChanged(lock, _lock);
		lock = _lock;
	}

	function setCooldown(uint64 period) public override onlySelf {
		require(1 hours <= period && period <= 1 weeks, "Timelock::setCooldown: illegal cooldown period");
		emit CooldownChanged(cooldown.period, period);
		cooldown.period = period;
	}

	function addGraceAmmo(address target, string memory sig) public override onlySelf {
		bytes32 sigHash = keccak256(abi.encodePacked(sig));
		uint8 ammo = graces[target][sigHash];
		require(ammo < 3, "Timelock::addGraceAmmo: ammo already full");

		graces[target][sigHash]++;
		emit GraceAmmoAdded(target, sig, ammo++);
	}

	function getProposal(bytes32 txHash) external view override returns (Proposal memory) {
		return proposals[txHash];
	}

	function getGraceAmmo(address target, bytes32 sigHash) external view override returns (uint8) {
		return graces[target][sigHash];
	}

	function _execute(Call memory call) internal returns (bytes memory) {
		bytes memory callData;

		if (bytes(call.signature).length == 0) {
			callData = call.data;
		} else {
			callData = abi.encodePacked(bytes4(keccak256(bytes(call.signature))), call.data);
		}

		bytes memory returnData = call.target.functionCallWithValue(
			callData,
			call.value,
			"Timelock::_execute: execution reverted"
		);

		return returnData;
	}
}

