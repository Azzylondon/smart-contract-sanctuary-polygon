/// @title TheCommunityCanvasProject
/// @dev This contract creates a canvas from 625 NFTs and allows users to buy blocks or change pixels on unowned blocks.
contract TheCommunityCanvasProject is ERC721Enumerable, ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    using SafeMath for uint16;
    using strings for string;
    using strings for strings.slice;

    //Constants
    uint256 public constant _MAXBLOCKCOUNT = 625;
    uint256 public constant itemPrice = 250000000000000000; //0.25 MATIC;
    uint8 private constant _height = 10;
    uint8 private constant _width = 10;
    string private constant path = '<path stroke=\"#';
    string private constant dstr = '\" d=\"M';
    string private constant close = 'h1\" />';
    string private constant space = ' ';
    address payable constant withdrawAddr = payable(0xC7C44B86E5cBb36b9cEcc9A519cC32cC531ad7ae);
    string private constant svgStart = '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -0.5 10 10">';

    //To keep track of blocks created
    uint256 public blocksCreated = 0;

    //Pixel struct to hold color and price per each pixel
    struct Pixel {
        string color;
        uint32 changeCount;
    }

    //Array of all the pixel data.
    Pixel[625*10*10] public pixels;
    
    /// @notice Mints the blocks for the canvas. 50 is the max at a time.
    function generateBlocks(uint16 quantity) onlyOwner external {
        require(quantity > 0, "Minimum 1 tokens need to be minted");
	    require(quantity <= blocksRemainingToBeMinted(), "Purchase amount is greater than the token available");
		require(quantity <= 50, "Max 50 Blocks");
		
		for(uint16 i =0; i < quantity; i++)
		{
		uint256 tokenId = blocksCreated;
        _safeMint(msg.sender, tokenId, "");
        blocksCreated+=1;
		}
        
    }

     /// @notice Changes a pixel at the specified block, x, and y coordinates.
     /// @param color must be a string containing a valid hex color in all upper case without the '#' character.
    function writePixel(uint16 blockid, uint8 x, uint8 y, string memory color) external payable {
        require(blockid<blocksCreated, "Invalid Block ID");
        require(x<10, "Invalid X value.");
        require(y<10, "Invalid Y value.");
        require(getPriceOfPixelChange(blockid,x,y, msg.sender) == msg.value, "Incorrect Eth Value to change pixel");
        require(utfStringLength(color)==6, "Must enter valid length of 6 characters.");
        require(hexColorString(color)==true, "Must enter valid color hex values in upper case.");
        require(checkWritable(blockid)==true, "Block is owned by another address.");
        
        if(pixels[(blockid*_height*_width)+(y*_width)+x].changeCount==0)
            pixels[(blockid*_height*_width)+(y*_width)+x] = Pixel(color, 1);
        else
        {
             pixels[(blockid*_height*_width)+(y*_width)+x].color = color;
             pixels[(blockid*_height*_width)+(y*_width)+x].changeCount+=1;
        }
    }

    /// @notice Checks if the sender address can write to the specified block.
    function checkWritable(uint16 blockid) public view returns (bool) {
        if(ownerOf(blockid) == msg.sender)
            return true;
        else if(ownerOf(blockid) == owner())
            return true;
        else
        return false;
    }

    /// @notice Withdraws the contract balance to the specified address. 
    function withdrawAll() onlyOwner public returns(bool) {
        uint256 amount = address(this).balance;
        withdrawAddr.transfer(amount);
        return true;
    }
      
     /// @notice Gets the price of a pixel at the specified block, x, and y coordinates.
    function getPriceOfPixelChange(uint16 blockid, uint8 x, uint8 y, address requestor) public view returns (uint256) {
        require(checkWritable(blockid)==true, "Block is owned by another address.");
        if(pixels[(blockid*_height*_width)+(y*_width)+x].changeCount==0)
            return 0;
        else if(ownerOf(blockid) == requestor && ownerOf(blockid)!=owner())
            return 0;
        else
            return itemPrice*(2**(pixels[(blockid*_height*_width)+(y*_width)+x].changeCount-1));
    }
    
    /// @notice Takes an array of pixels and writes them all to the canvas.
    function writePixels(uint16[] memory blockid, uint8[] memory x, uint8[] memory y, string[] memory color) external payable {
        require(blockid.length == x.length && x.length == y.length && y.length == color.length, "Arrays must all be the same length.");
        require(blockid.length<101, "Only 100 pixels can be changed at a time");
        require(getPriceOfPixelChanges(blockid,x,y, msg.sender) == msg.value, "Incorrect Eth Value to change pixel");
        
        for(uint8 i=0;i<blockid.length;i++)
        {
            require(blockid[i]<blocksCreated, "Invalid Block ID");
            require(x[i]<10, "Invalid X value.");
            require(y[i]<10, "Invalid Y value.");
            require(utfStringLength(color[i])==6, "Must enter valid length of 6 characters.");
             require(hexColorString(color[i])==true, "Must enter valid color hex values in upper case.");
            
            if(pixels[(blockid[i]*_height*_width)+(y[i]*_width)+x[i]].changeCount==0)
                pixels[(blockid[i]*_height*_width)+(y[i]*_width)+x[i]] = Pixel(color[i], 1);
            else
            {
                 pixels[(blockid[i]*_height*_width)+(y[i]*_width)+x[i]].color = color[i];
                 pixels[(blockid[i]*_height*_width)+(y[i]*_width)+x[i]].changeCount+=1;
            }
        }
    }
    
    /// @notice Checks the price of an array of pixel changes.
    function getPriceOfPixelChanges(uint16[] memory blockid, uint8[] memory x, uint8[] memory y, address requestor) public view returns (uint256) {
        require(blockid.length == x.length && x.length == y.length, "Arrays must all be the same length.");
        uint256 toReturn = 0;
        for(uint8 i=0;i<blockid.length;i++)
            toReturn += getPriceOfPixelChange(blockid[i], x[i], y[i], requestor);
            
        return toReturn;
       
    }
    
    /// @notice Gets the current pixel color at a specified block, x and y coordinate.
     function getPixelColor(uint16 blockid, uint8 x, uint8 y) public view returns (string memory) {
        if(pixels[(blockid*_height*_width)+(y*_width)+x].changeCount==0)
            return "FFFFFF";
        else
            return pixels[(blockid*_height*_width)+(y*_width)+x].color;
    }
    
    /// @notice Validates if the string contains valid hex characters.
    function hexColorString(string memory tocheck) internal pure returns (bool){
        
        for(uint8 i = 0;i<6;i++)
        {
           
            if(compareStrings(substring(tocheck,i,i+1),"0") || compareStrings(substring(tocheck,i,i+1),"1") || compareStrings(substring(tocheck,i,i+1),"2") || compareStrings(substring(tocheck,i,i+1),"3") || compareStrings(substring(tocheck,i,i+1),"4") || compareStrings(substring(tocheck,i,i+1),"5") || compareStrings(substring(tocheck,i,i+1),"6") || compareStrings(substring(tocheck,i,i+1),"7") || compareStrings(substring(tocheck,i,i+1),"8") || compareStrings(substring(tocheck,i,i+1),"9") || compareStrings(substring(tocheck,i,i+1),"A") || compareStrings(substring(tocheck,i,i+1),"B") || compareStrings(substring(tocheck,i,i+1),"C") || compareStrings(substring(tocheck,i,i+1),"D") || compareStrings(substring(tocheck,i,i+1),"E") || compareStrings(substring(tocheck,i,i+1),"F"))
                continue;
            else
                return false;
        }
        return true;
    }
    
    /// @notice Compares if string a is the same as string b.
    function compareStrings(string memory a, string memory b) internal pure returns (bool) {
    return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
}
    
    /// @notice Returns a substring of a specified string
    function substring(string memory str, uint8 startIndex, uint8 endIndex) internal pure returns (string memory) {
    bytes memory strBytes = bytes(str);
    bytes memory result = new bytes(endIndex-startIndex);
    for(uint i = startIndex; i < endIndex; i++) {
        result[i-startIndex] = strBytes[i];
    }
    return string(result);
    }
    
    /// @notice Returns the length of a utf string
    function utfStringLength(string memory str) internal pure returns (uint length){
        uint i=0;
        bytes memory string_rep = bytes(str);

        while (i<string_rep.length)
        {
            if (string_rep[i]>>7==0)
                i+=1;
            else if (string_rep[i]>>5==0x06)
                i+=2;
            else if (string_rep[i]>>4==0x0E)
                i+=3;
            else if (string_rep[i]>>3==0x1E)
                i+=4;
            else
                //For safety
                i+=1;

            length++;
        }
    }
    
    /// @notice Checks if the sender owns any blocks
    function isOwner() public view returns (bool) {
        return (balanceOf(msg.sender)>0);
    }
    
    /// @notice Returns the number of blocks left to be minted.
    function blocksRemainingToBeMinted() public view returns (uint256) {
        return _MAXBLOCKCOUNT.sub(blocksCreated);
    }

    function isAllTokenMinted() public view returns (bool) {
        return blocksCreated == _MAXBLOCKCOUNT;
    }
    
    /// @notice Returns the uri of a block. This includes the metadata and creates the 10x10 SVG graphics for the requested block.
    function uri(uint256 tokenId) public view returns (string memory) {
        
        string[3] memory parts;
        
        parts[0] = svgStart;
        uint16 x=0;
        uint16 y=0;
        string memory strSlice;

        string[3] memory innerParts;
       	for(uint256 i =0; i < 100; i++)
		{
		    string memory color = "FFFFFF";
		    uint index = (tokenId*10*10)+(y*10)+x;
		    if(pixels[index].changeCount > 0)
		         color = pixels[index].color;
		    innerParts[0] = color;
		    innerParts[1] = toString(x);
		    innerParts[2] = toString(y);
		    x+=1;
		    if(x==10)
		    {
		        y+=1;
		        x=0;
		    }
		    strSlice = string(abi.encodePacked(strSlice, path, innerParts[0], dstr, innerParts[1], space, innerParts[2], close));
		}
        
        parts[1] = strSlice;
       
        parts[2] = '</svg>';
        
        string memory output = string(abi.encodePacked(parts[0], parts[1], parts[2]));
       
        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "Block #', toString(tokenId), '", "description": "The Community Canvas Project is a collection of 625 100x100 pixel canvas NFTs. Before an NFT is purchased, anyone can participate in drawing on the canvas.", "image": "data:image/svg+xml;base64,', Base64.encode(bytes(output)), '"}'))));
        output = string(abi.encodePacked('data:application/json;base64,', json));
    
        return output;
    }

    /// @notice Creates a string from a uint256 value.
    function toString(uint256 value) internal pure returns (string memory) {
    // Inspired by OraclizeAPI's implementation - MIT license
    // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
    
    constructor() ERC721("TheCommunityCanvasProject", "THECOMMUNITYCANVASPROJECT") Ownable() {


    }
}

/// [MIT License]
/// @title Base64
/// @notice Provides a function for encoding some bytes in base64
/// @author Brecht Devos <[emailÂ protected]>
library Base64 {
    bytes internal constant TABLE = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";

    /// @notice Encodes some bytes to the base64 representation
    function encode(bytes memory data) internal pure returns (string memory) {
        uint256 len = data.length;
        if (len == 0) return "";

        // multiply by 4/3 rounded up
        uint256 encodedLen = 4 * ((len + 2) / 3);

        // Add some extra buffer at the end
        bytes memory result = new bytes(encodedLen + 32);

        bytes memory table = TABLE;

        assembly {
            let tablePtr := add(table, 1)
            let resultPtr := add(result, 32)

            for {
                let i := 0
            } lt(i, len) {

            } {
                i := add(i, 3)
                let input := and(mload(add(data, i)), 0xffffff)

                let out := mload(add(tablePtr, and(shr(18, input), 0x3F)))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(shr(12, input), 0x3F))), 0xFF))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(shr(6, input), 0x3F))), 0xFF))
                out := shl(8, out)
                out := add(out, and(mload(add(tablePtr, and(input, 0x3F))), 0xFF))
                out := shl(224, out)

                mstore(resultPtr, out)

                resultPtr := add(resultPtr, 4)
            }

            switch mod(len, 3)
            case 1 {
                mstore(sub(resultPtr, 2), shl(240, 0x3d3d))
            }
            case 2 {
                mstore(sub(resultPtr, 1), shl(248, 0x3d))
            }

            mstore(result, encodedLen)
        }

        return string(result);
    }
