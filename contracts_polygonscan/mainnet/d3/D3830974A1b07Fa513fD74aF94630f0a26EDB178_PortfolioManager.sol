import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import '@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol';
import '@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol';

import "./ERC1155Receiver.sol";

contract PortfolioManager is ERC1155Supply, ERC1155Receiver {
  using SafeMath for uint256;

  bool public isWhitelist;

  address public owner;
  constructor(bool _isWhitelist) ERC1155("TODO: Implement URI") {
    owner = msg.sender;
    isWhitelist = _isWhitelist;
  }

  struct Portfolio {
    address owner;
    uint256 index;
    address[] tokens;
    uint256[] dubTokens;
    string name;
  }

  struct PortfolioParams {
    address[] tokens;
    uint256[] amounts;
    uint256[] dubTokens;
    uint256[] dubAmounts;
    string name;
    address creator;
  }

  mapping (address => bool) public whitelist;
  mapping (uint256 => mapping (address => uint256)) public getTokenBalanceInPortfolio;
  mapping (uint256 => mapping (uint256 => uint256)) public getDubBalanceInPortfolio;
  mapping (uint256 => mapping (address => uint256)) internal getTokenIndexByAddress;
  mapping (address => uint256) public numDubbers;
  mapping (uint256 => mapping (address => bool)) public hasDubbed;
  mapping (string => uint256) public getPortfolioByName;

  Portfolio[] public portfolios;

  function getNumDubbers(address creator) public view returns (uint256) {
    return numDubbers[creator];
  }

  function _getPortfolio(uint256 index) internal view returns (Portfolio storage) {
    require(index < portfolios.length + 1, "Portfolio index out of bounds");
    return portfolios[index - 1];
  }

  function getPortfolio(uint256 index) public view returns (Portfolio memory) {
    require(index < portfolios.length + 1, "Portfolio index out of bounds");
    return portfolios[index - 1];
  }

  /**
   * @dev Add a new portfolio to the list of portfolios.
   * @return The index of the new portfolio.
   */
  function createPaperPortfolio(PortfolioParams calldata ps) public returns (uint256) {
    uint256 index = portfolios.length + 1;
    require(index < 2**32, "Too many portfolios");
    require(ps.tokens.length == ps.amounts.length, "Length of tokens and amounts must match");
    require(ps.dubTokens.length == ps.dubAmounts.length, "Length of dubTokens and dubAmounts must match");
    // the zero portfolio should not exist.
    require(getPortfolioByName[ps.name] == 0, "Portfolio with that name already exists");
    getPortfolioByName[ps.name] = index;
    Portfolio memory portfolio = Portfolio({
      owner: ps.creator,
      index: index,
      tokens: ps.tokens,
      dubTokens: ps.dubTokens,
      name: ps.name
    });
    for (uint256 i = 0; i < ps.tokens.length; i++) {
      require(ps.amounts[i] != 0, "Amounts must be non-zero");
      require(whitelist[ps.tokens[i]] || !isWhitelist, "Token is not whitelisted");
      getTokenBalanceInPortfolio[index][ps.tokens[i]] = ps.amounts[i];
      getTokenIndexByAddress[index][ps.tokens[i]] = i+1;
    }
    for (uint256 i = 0; i < ps.dubTokens.length; i++) {
      require(ps.dubAmounts[i] != 0, "dubAmounts must be non-zero");

      getDubBalanceInPortfolio[index][ps.dubTokens[i]] = ps.dubAmounts[i];
    }
    portfolios.push(portfolio);
    return portfolio.index;
  }

  function createPortfolio(PortfolioParams calldata ps) public returns (uint256) {
      uint256 index = createPaperPortfolio(ps);
      assetsIn(index, 1 ether, ps.creator);
      return index;
  }

  function transferOwner(address newOwner, uint256 index) public {
    require(index < portfolios.length + 1, "Portfolio index out of bounds");
    Portfolio storage portfolio = _getPortfolio(index);
    require(portfolio.owner == msg.sender, "Only the owner can transfer ownership");
    require(newOwner != msg.sender, "Cannot transfer ownership to yourself");
    require(newOwner != address(0), "Cannot transfer ownership to the zero address");
    portfolio.owner = newOwner;
  }

  function whiteList(address token, bool isWhitelisted) public {
    require(msg.sender == owner, "Only the owner can whitelist tokens");
    whitelist[token] = isWhitelisted;
  }

  /**
    * @dev Number of tokens in the portfolio at the given index if amount is invested in the portfolio.
    * @param idx The index of the portfolio.
    * @param tok The token address.
    * @param amount The amount of the portfolio coin owned.
    * @return The number of tokens in the portfolio at the given index if amount is invested in the portfolio.
   */
  function getAssetExposureByAddress(uint256 idx, address tok, uint256 amount) public view returns (uint256) {
    if (this.totalSupply(idx) == 0) {
      return (getTokenBalanceInPortfolio[idx][tok] * amount) / 1 ether;
    }
    return (getTokenBalanceInPortfolio[idx][tok] * amount) / this.totalSupply(idx);
  }

  /**
    * @dev Number of dubbed tokens in the portfolio at the given index if amount is invested in the portfolio.
    * @param idx The index of the portfolio.
    * @param dubIdx The index of the dubbed sub-portfolio.
    * @param amount The amount of the portfolio coin owned.
    * @return The number of dubbed tokens in the portfolio at the given index if amount is invested in the portfolio.
   */
  function getAssetExposureByDubIndex(uint256 idx, uint256 dubIdx, uint256 amount) public view returns (uint256) {
    if (this.totalSupply(idx) == 0) {
      return (getDubBalanceInPortfolio[idx][dubIdx] * amount) / 1 ether;
    }
    return (getDubBalanceInPortfolio[idx][dubIdx] * amount) / this.totalSupply(idx);
  }

  /**
    * @dev Transfers the given amount of each token in the portfolio to the pool.
    * @param portfolioIndex The index of the portfolio.
    * @param amount The amount of portfolio coins being bought.
    * @param sender The address of the initiator of the transaction.
   */
  function transferCoinsToPool(uint256 portfolioIndex, uint256 amount, address sender) private {
    Portfolio storage portfolio = _getPortfolio(portfolioIndex);
    for (uint256 i = 0; i < portfolio.tokens.length; i++) {
      address tokenAddress = portfolio.tokens[i];
      IERC20 token = IERC20(tokenAddress);
      uint256 amountToTransfer = getAssetExposureByAddress(portfolioIndex, tokenAddress, amount);
      uint256 balance = getTokenBalanceInPortfolio[portfolioIndex][tokenAddress];
      if (amount < this.totalSupply(portfolioIndex)) {
        getTokenBalanceInPortfolio[portfolioIndex][tokenAddress] = balance.add(amountToTransfer);
      }
      console.log("Transferring %s tokens of %s from %s", amountToTransfer, tokenAddress, sender);
      require(token.transferFrom(sender, address(this), amountToTransfer), "Failed to transfer tokens");
    }
  }

  /**
    * @dev Transfers the given amount of each dub token in the portfolio to the pool.
    * @param portfolioIndex The index of the portfolio.
    * @param amount The amount of portfolio coins being bought.
    * @param sender The address of the initiator of the transaction.
   */
  function transferDubsToPool(uint256 portfolioIndex, uint256 amount, address sender) private {
    Portfolio storage portfolio = _getPortfolio(portfolioIndex);
    uint256[] memory batchTransferAmounts = new uint256[](portfolio.dubTokens.length);
    for (uint256 i = 0; i < portfolio.dubTokens.length; i++) {
      uint256 dubTokenIndex   = portfolio.dubTokens[i];
      batchTransferAmounts[i] = getAssetExposureByDubIndex(portfolioIndex, dubTokenIndex, amount);
      uint256 balance         = getDubBalanceInPortfolio[portfolioIndex][dubTokenIndex];
      getDubBalanceInPortfolio[portfolioIndex][dubTokenIndex] = balance.add(batchTransferAmounts[i]);
    }
    if (batchTransferAmounts.length > 0) {
      this.safeBatchTransferFrom(sender, address(this), portfolio.dubTokens, batchTransferAmounts, "");
    }
  }

  /**
    * @dev Transfers the given amount of each token from the pool to the recipient.
    * @param portfolioIndex The index of the portfolio.
    * @param amount The amount of portfolio coins being sold.
    * @param recipient The address of the recipient of the tokens.
   */
  function transferCoinsFromPool(uint256 portfolioIndex, uint256 amount, address recipient) private {
    Portfolio storage portfolio = _getPortfolio(portfolioIndex);
    for (uint256 i = 0; i < portfolio.tokens.length; i++) {
      address tokenAddress = portfolio.tokens[i];
      uint256 amountOwed   = getAssetExposureByAddress(portfolioIndex, tokenAddress, amount);
      uint256 balance      = getTokenBalanceInPortfolio[portfolioIndex][tokenAddress];
      getTokenBalanceInPortfolio[portfolioIndex][tokenAddress] = balance.sub(amountOwed);
      console.log("Balance of pool is %s", IERC20(tokenAddress).balanceOf(address(this)));
      console.log("Transferring %s tokens of %s to %s", amountOwed, tokenAddress, recipient);
      TransferHelper.safeTransfer(tokenAddress, recipient,  amountOwed);
    }
  }

  /**
    * @dev Transfers the given amount of each dub token from the pool to the recipient.
    * @param portfolioIndex The index of the portfolio.
    * @param amount The amount of portfolio coins being sold.
    * @param recipient The address of the recipient of the tokens.
   */
  function transferDubsFromPool(uint256 portfolioIndex, uint256 amount, address recipient) private {
    Portfolio storage portfolio = _getPortfolio(portfolioIndex);
    uint256[] memory batchTransferAmounts = new uint256[](portfolio.dubTokens.length);
    for (uint256 i = 0; i < portfolio.dubTokens.length; i++) {
      uint256 dubTokenIndex   = portfolio.dubTokens[i];
      batchTransferAmounts[i] = getAssetExposureByDubIndex(portfolioIndex, dubTokenIndex, amount);
      uint256 balance         = getDubBalanceInPortfolio[portfolioIndex][dubTokenIndex];
      getDubBalanceInPortfolio[portfolioIndex][dubTokenIndex] = balance.sub(batchTransferAmounts[i]);
    }
    if (batchTransferAmounts.length > 0) {
      this.safeBatchTransferFrom(address(this), recipient, portfolio.dubTokens, batchTransferAmounts, "");
    }
  }

  /**
    * @dev Exchanges the tokens tracked by a portfolio for pool tokens.
    * @param portfolioIndex The index of the portfolio.
    * @param amount The amount of portfolio coins being bought.
    * @param investor The address of the investor.
   */
  function assetsIn(uint256 portfolioIndex, uint256 amount, address investor) public {
    require(portfolioIndex < portfolios.length + 1, "Portfolio index out of bounds.");
    _mint(investor, portfolioIndex, amount, "");
    transferCoinsToPool(portfolioIndex, amount, investor);
    transferDubsToPool(portfolioIndex, amount, investor);
    owner = _getPortfolio(portfolioIndex).owner;
    if (investor != owner && !hasDubbed[portfolioIndex][investor]) {
      numDubbers[owner] = numDubbers[owner] + 1;
      hasDubbed[portfolioIndex][investor] = true;
    }
  }

  /**
    * @dev Exchanges the pool tokens for tokens tracked by a portfolio.
    * @param portfolioIndex The index of the portfolio.
    * @param amount The amount of portfolio coins being sold.
    * @param investor The address of the investor.
   */
  function assetsOut(uint256 portfolioIndex, uint256 amount, address investor) public {
    require(portfolioIndex < portfolios.length + 1, "Portfolio index out of bounds.");
    require(amount < this.totalSupply(portfolioIndex), "Amount out of bounds.");
    transferCoinsFromPool(portfolioIndex, amount, investor);
    transferDubsFromPool(portfolioIndex, amount, investor);
    safeTransferFrom(investor, address(this), portfolioIndex, amount, "");
    _burn(address(this), portfolioIndex, amount);
  }


  ISwapRouter public constant ROUTER = ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);
  // ISwapRouter public constant ROUTER = ISwapRouter(0x68b3465833fb72A70ecDF485E0e4C7bD8665Fc45);


  /**
    * @dev Rebalances the portfolio by swapping two assets tracked by the portfolio.
    * @param portfolioIndex The index of the portfolio.
    * @param tokenOut The address of the token being bought.
    * @param amountOutPerTokenIn The minimum acceptable amount you'd be willing to receive in exchange for 1 token of tokenIn
    * @param tokenIn The address of the token being sold.
    * @param pctIn The percentage of the token being sold.
   */
  function rebalance(
    uint256 portfolioIndex, 
    address tokenOut, 
    uint256 amountOutPerTokenIn, 
    address tokenIn, 
    uint256 pctIn) public {
    require(portfolioIndex < portfolios.length + 1, "Portfolio index out of bounds.");
    require(_getPortfolio(portfolioIndex).owner == msg.sender, "Only the portfolio owner can rebalance.");
    require(whitelist[tokenOut] || !isWhitelist, "Token is not whitelisted.");
    // Total amount of tokens being sold for the portfolio.
    uint256 amountIn     = (getTokenBalanceInPortfolio[portfolioIndex][tokenIn] * pctIn) / 1 ether;
    // Total amount of tokens being bought for the portfolio.
    uint256 amountOutMin = (amountIn * amountOutPerTokenIn) / 1 ether;
    TransferHelper.safeApprove(tokenIn, address(ROUTER), amountIn);
    uint256 amountOut = ROUTER.exactInputSingle(ISwapRouter.ExactInputSingleParams({
      tokenIn: tokenIn,
      tokenOut: tokenOut,
      fee: 3000,
      recipient: address(this),
      deadline: block.timestamp,
      amountIn: amountIn,
      amountOutMinimum: amountOutMin,
      sqrtPriceLimitX96: 0
    }));
    require(amountOut >= amountOutMin, "the swap paid too little for the token");
    console.log("amountOut", amountOut);
    Portfolio storage portfolio = _getPortfolio(portfolioIndex);
    getTokenBalanceInPortfolio[portfolioIndex][tokenOut] += amountOut;
    getTokenBalanceInPortfolio[portfolioIndex][tokenIn]  -= amountIn;
    if (getTokenIndexByAddress[portfolioIndex][tokenOut] == 0) {
      getTokenIndexByAddress[portfolioIndex][tokenOut] = portfolio.tokens.length + 1;
      portfolio.tokens.push(tokenOut);
    }
  }

}

