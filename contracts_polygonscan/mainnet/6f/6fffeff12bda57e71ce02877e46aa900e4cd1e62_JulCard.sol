import "hardhat/console.sol";
import "./libraries/TransferHelper.sol";
import "./interfaces/PriceOracle.sol";
import "./interfaces/IWETH9.sol";
import "./uniswapv2/interfaces/IUniswapV2Pair.sol";
import "./uniswapv2/interfaces/IUniswapV2Factory.sol";
import './uniswapv2/libraries/UniswapV2Library.sol';
// This is the main building block for smart contracts.
contract JulCard {
    // Some string type variables to identify the token.
    
    // A mapping is a key/value map. Here we store each account balance.
    // mapping(address => uint256) balances;
    address public immutable WETH;
    address public immutable USDT;
    address public immutable factory;
    address juld;
    bool juldPaymentEnable;

    address public priceOracle;
    uint public levelValidationPeriod = 30 days;

    address public treasuryAddress;
    address public financialAddress;
    address public masterAddress;
    
    bool public cashBackEnable;
    uint constant MAX_FEE_AMOUNT = 500; // 5%
    uint public withdrawFeePercent;
    uint public buyFeePercent;
    
    /*** Main Actions ***/
    mapping (address => uint) internal usersSpendAmountDay;
    mapping (address => uint) internal usersSpendTime;              // it is needed to calculate how much assets user sold in a day.
    mapping (address => uint) internal usersLevel;                  // current user level of each user
    mapping (address => uint) internal usersjuldUpdatedTime;        // the time juld amount is updated

    // A mapping is a key/value map. Here we store each account balance.
    mapping(address => uint256) dailyLimits;

    mapping (address => mapping (address => uint)) internal usersBalances;   // user  => ( market => balances)
    
    // An address type variable is used to store ethereum accounts.
    address public owner;

    /// @notice A list of all assets
    address[] public allMarkets;
    mapping(address => bool) _marketEnabled;
    //prevent reentrancy attack
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;
    uint256 private _status;
    // emit event

    event WithdrawFeePercentChanged(address owner, uint newPercent , uint beforePercent);
    event BuyFeePercentChanged(address owner, uint newPercent , uint beforePercent);
    event CashBackEnableChanged(address owner, bool newEnabled , bool beforeEnabled);
    event TreasuryAddressChanged(address owner, address newAddress , address beforeAddress);
    event FinancialAddressChanged(address owner, address newAddress , address beforeAddress);
    event MasterAddressChanged(address owner, address newAddress , address beforeAddress);
    event UserBalanceChanged(address indexed userAddr, address indexed market, uint amount);
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);
    event MarketEnableChanged(address owner, address market, bool bEnable, bool beforeEnabled);
    event PriceOracleChanged(address owner, address newOracleAddress, address beforePriceOracle);
    event JuldPaymentEnabled(address owner, bool juldPaymentEnable, bool bOldEnable);
    /**
     * Contract initialization.
     *
     * The `constructor` is executed only once when the contract is created.
     * The `public` modifier makes a function callable from outside the contract.
     */
    constructor(address _WETH, address _USDT, address _juldAddress, address _priceOracle, address _factory) {
        // The totalSupply is assigned to transaction sender, which is the account
        // that is deploying the contract.
        owner = msg.sender;
        WETH = _WETH;
        priceOracle = _priceOracle;
        juld = _juldAddress;
        USDT = _USDT;
        factory = _factory;
        buyFeePercent = 100; // 1%
        withdrawFeePercent = 10; // 0.1 %
        _status = _NOT_ENTERED;
    }
    /// modifier functions 
    modifier onlyOwner() {
        require(msg.sender == owner, "only owner can");
        _;
    }

    modifier marketSupported(address market) {
        bool marketExist = false;
        for (uint i = 0; i < allMarkets.length; i ++) {
            if(allMarkets[i] == market)
            {
                marketExist = true ;
            }
        }
        require(marketExist, "market not supported");
        _;
    }
    
    modifier marketEnabled(address market) {
        require(_marketEnabled[market] , "market is disabled or not supported");
        _;
    }
    /**
     * @dev Prevents a contract from calling itself, directly or indirectly.
     * Calling a `nonReentrant` function from another `nonReentrant`
     * function is not supported. It is possible to prevent this from happening
     * by making the `nonReentrant` function external, and make it call a
     * `private` function that does the actual work.
     */
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
    /// owner functions

    function setWithdrawFeePercent(uint newPercent) public onlyOwner {
        require(newPercent <= MAX_FEE_AMOUNT, "withdraw fee should be less than 5%");
        uint beforePercent = withdrawFeePercent;
        withdrawFeePercent = newPercent;
        emit WithdrawFeePercentChanged(owner, newPercent, beforePercent);
    }
    
    function setBuyFeePercent(uint newPercent) public onlyOwner {
        require(newPercent <= MAX_FEE_AMOUNT, "buy fee should be less than 5%");
        uint beforePercent = withdrawFeePercent;
        buyFeePercent = newPercent;
        emit BuyFeePercentChanged(owner, newPercent, beforePercent);
    }
    
    function setCashBackEnable(bool newEnabled) public onlyOwner {
        bool beforeEnabled = cashBackEnable;
        cashBackEnable = newEnabled;
        emit CashBackEnableChanged(owner, newEnabled, beforeEnabled);
    }

    function setTreasuryAddress(address _newTreasuryAddress) public onlyOwner {
        address beforeAddress = treasuryAddress;
        treasuryAddress = _newTreasuryAddress;
        emit TreasuryAddressChanged(owner, treasuryAddress, beforeAddress);
    }

    function setFinancialAddress(address _newFinancialAddress) public onlyOwner {
        address beforeAddress = financialAddress;
        financialAddress = _newFinancialAddress;
        emit FinancialAddressChanged(owner, _newFinancialAddress, beforeAddress);
    }
    
    function setMasterAddress(address _newAddress) public onlyOwner {
        address beforeAddress = _newAddress;
        masterAddress = _newAddress;
        emit MasterAddressChanged(owner, _newAddress, beforeAddress);
    }
    
    function transaferOwnership(address newOwner) public onlyOwner {
        address oldOwner = owner;
        owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }  
    
    function enableMarket(address market, bool bEnable) public onlyOwner {
        bool beforeEnabled = _marketEnabled[market];
        _marketEnabled[market] = bEnable;
        emit MarketEnableChanged(owner, market, bEnable, beforeEnabled);
    }
    
    function addMarket(address market) public onlyOwner {
        _addMarketInternal(market);
    }

    function setPriceOracle(address _priceOracle) public onlyOwner {
        address beforeAddress = priceOracle;
        priceOracle = _priceOracle;
        emit PriceOracleChanged(owner, priceOracle, beforeAddress);
    }

    function setJuldAsPayment(bool bEnable) public onlyOwner {
        bool bOldEnable = juldPaymentEnable;
        juldPaymentEnable = bEnable;
        emit JuldPaymentEnabled(owner, juldPaymentEnable, bOldEnable);
    }

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

    function _addUserBalance(address market, address userAddr, uint256 amount) internal marketEnabled(market) returns (bool)
    {
        uint beforeAmount = usersBalances[userAddr][market];
        usersBalances[userAddr][market] += amount ;
        onUpdateUserBalance(userAddr, market, usersBalances[userAddr][market], beforeAmount );
    }

    // deduce user assets using usd amount
    function _makePayment(address market, address userAddr, uint256 usdAmount) internal marketEnabled(market) returns (bool)
    {
        uint spendAmount = calculateAmount(market,userAddr, usdAmount);

        uint currentDate = block.timestamp / 1 days;
        uint beforeAmount = usersBalances[userAddr][market];
        uint totalSpendAmount;

        if(usersSpendTime[userAddr] != currentDate){
            usersSpendTime[userAddr] = currentDate;
            totalSpendAmount = spendAmount;
        }
        else{
            totalSpendAmount = usersSpendAmountDay[userAddr] + spendAmount;
        }

        require(withinLimits(userAddr, totalSpendAmount), "out of daily limts");
        cashBack(userAddr, spendAmount);
        usersSpendAmountDay[userAddr] = totalSpendAmount;
        onUpdateUserBalance(userAddr, market, usersBalances[userAddr][market], beforeAmount);

    }
    
    //returns today's spend amount
    function getSpendAmountToday(address userAddr) public view returns (uint256) {
       uint currentDate = block.timestamp / 1 days;
       if(usersSpendTime[userAddr] != currentDate){
           return 0;
       }
       return usersSpendAmountDay[userAddr];
    }
    
    function  onUpdateUserBalance(address userAddr, address market, uint amount, uint beforeAmount) internal returns (bool) {
        emit UserBalanceChanged(userAddr, market, amount);
        if(market != juld) return true;
        uint newLevel = getLevel(usersBalances[userAddr][market]);
        uint beforeLevel = getLevel(beforeAmount);
        if(newLevel != beforeLevel) usersjuldUpdatedTime[userAddr] = block.timestamp ;
        if(newLevel == usersLevel[userAddr]) return true;
        if(newLevel < usersLevel[userAddr]) {
            usersLevel[userAddr] = newLevel;
        }
        else{
            if(usersjuldUpdatedTime[userAddr] + levelValidationPeriod < block.timestamp)
            {
                usersLevel[userAddr] = newLevel;
            }
        }
    }

    function getLevel(uint _juldAmount) public pure returns (uint) {
        if(_juldAmount < 1000 ether) return 0 ;
        if(_juldAmount < 2500 ether) return 1 ;
        if(_juldAmount < 10000 ether) return 2 ;
        if(_juldAmount < 25000 ether) return 3 ;
        if(_juldAmount < 100000 ether) return 4 ;
        return 5 ;
    }

    function getDailyLimit(uint level) public pure returns (uint) {
        require(level <= 5, "level > 5");
        if(level == 0 ) return 100 ether ;
        if(level == 1 ) return 250 ether ;
        if(level == 2 ) return 500 ether ;
        if(level == 3 ) return 2500 ether ;
        if(level == 4 ) return 5000 ether ;
        if(level == 5 ) return 10000 ether ;
    }

    function getCashBackPercent(uint level) public pure returns (uint) {
        require(level <= 5, "level > 5");
        if(level == 0 ) return 10;
        if(level == 1 ) return 200;
        if(level == 2 ) return 300;
        if(level == 3 ) return 400;
        if(level == 4 ) return 500;
        if(level == 5 ) return 600;
    }

    // calculate aseet amount from market and required usd amount
    function calculateAmount(address market, address userAddr, uint usdAmount) public returns (uint spendAmount) {
        uint addFeeUsdAmount;
        if(treasuryAddress != address(0)){
            addFeeUsdAmount = usdAmount + usdAmount * buyFeePercent / 1000;
        }
        else{
            addFeeUsdAmount = usdAmount;
        }
        uint usdOraclePrice = PriceOracle(priceOracle).getUnderlyingPrice(market);  //decimal is 8
        require(usdOraclePrice > 0, "invalid price");

        uint assetAmountIn = addFeeUsdAmount * 10**8 / usdOraclePrice ;
        assetAmountIn = assetAmountIn + assetAmountIn / 10 ;  //price tolerance = 10%
        if(market != USDT) {
            address [] memory path;
            path[0] = market;
            path[1] = USDT;
            uint[] memory amounts = UniswapV2Library.getAmountsIn(factory, addFeeUsdAmount, path);
            require(amounts[0] <= assetAmountIn, 'price much different');
            TransferHelper.safeTransferFrom(
                path[0], msg.sender, UniswapV2Library.pairFor(factory, path[0], path[1]), amounts[0]
            );
            _swap(amounts, path, address(this));
            spendAmount = amounts[1];
        }
        else
        {
            spendAmount = addFeeUsdAmount ;
        }
        require(masterAddress != address(0), "master address is not set");
        TransferHelper.safeTransfer(USDT, masterAddress, usdAmount);
        if(spendAmount > usdAmount) TransferHelper.safeTransfer(USDT, treasuryAddress, spendAmount - usdAmount);
        return spendAmount;
    }

    // **** SWAP ****
    // requires the initial amount to have already been sent to the first pair
    function _swap(uint[] memory amounts, address[] memory path, address _to) internal virtual {
        for (uint i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0,) = UniswapV2Library.sortTokens(input, output);
            uint amountOut = amounts[i + 1];
            (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOut) : (amountOut, uint(0));
            address to = i < path.length - 2 ? UniswapV2Library.pairFor(factory, output, path[i + 2]) : _to;
            IUniswapV2Pair(UniswapV2Library.pairFor(factory, input, output)).swap(
                amount0Out, amount1Out, to, new bytes(0)
            );
        }
    }

    function withinLimits(address userAddr, uint usdAmount) public view returns (bool){
        if(usdAmount <= getUserLimit(userAddr))
            return true;
        return false;

    }

    function getUserLimit(address userAddr) public view returns (uint){
        uint dailyLimit = dailyLimits[userAddr];
        if(dailyLimit != 0) return dailyLimit ;
        return getDailyLimit(usersLevel[userAddr]);
    }

    function userBalance(address userAddr, address market) external view returns (uint256) {
        return usersBalances[userAddr][market];
    }

////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    
    function _addMarketInternal(address assetAddr) internal {
        for (uint i = 0; i < allMarkets.length; i ++) {
            require(allMarkets[i] != assetAddr, "market already added");
        }
        allMarkets.push(assetAddr);
        _marketEnabled[assetAddr] = true;
    }

    /**
     * @notice Return all of the markets
     * @dev The automatic getter may be used to access an individual market.
     * @return The list of market addresses
     */
    function getAllMarkets() public view returns (address[] memory) {
        return allMarkets;
    }

    function deposit(address market, uint amount) public marketEnabled(market) nonReentrant {
        TransferHelper.safeTransferFrom(market, msg.sender, address(this), amount);
        _addUserBalance(market, msg.sender, amount);
    }

    function depositETH() public payable marketEnabled(WETH) nonReentrant {
        IWETH9(WETH).deposit{value: msg.value}();
        _addUserBalance(WETH, msg.sender, msg.value);
    }

    function withdraw(address market, uint amount) public nonReentrant returns(uint256) {
        address userAddr = msg.sender;
        uint beforeAmount = usersBalances[userAddr][market];
        require(beforeAmount >= amount, "insufficient balance");
        usersBalances[userAddr][market] -= amount ;
        if(market == WETH)
        {
            IWETH9(WETH).withdraw(amount);
            if(treasuryAddress != address(0))
            {
                uint realAmount = amount - amount * withdrawFeePercent / 10000;
                TransferHelper.safeTransferETH(msg.sender, realAmount);
                TransferHelper.safeTransferETH(treasuryAddress, amount - realAmount);
            }
            else{
                TransferHelper.safeTransferETH(msg.sender, amount);
            }
        }
        else
        {
            if(treasuryAddress != address(0))
            {
                uint realAmount = amount - amount * withdrawFeePercent / 10000;
                TransferHelper.safeTransfer(market,msg.sender, realAmount);
                TransferHelper.safeTransfer(market,treasuryAddress, amount - realAmount);
            }
            else{
                TransferHelper.safeTransfer(market,msg.sender, amount);
            }
        }
        onUpdateUserBalance(userAddr, market,usersBalances[userAddr][market], beforeAmount );
    }

    function buyGoods(address market, uint usdAmount) external nonReentrant returns(uint256)
    {
        if(market == juld){
            require(juldPaymentEnable, "juld not supported yet");
        } 
        _makePayment(market, msg.sender, usdAmount);
    }

    function cashBack(address userAddr, uint256 usdAmount) internal returns (uint256) {
        if(!cashBackEnable) return 0;
        uint256 cashBackPercent = getCashBackPercent(usersLevel[userAddr]) ;
        uint juldAmount = usdAmount * cashBackPercent / 10000 ;
        uint juldPrice = PriceOracle(priceOracle).getUnderlyingPrice(juld);
        require(juldPrice > 0, "juld price error");
        juldAmount = juldAmount * (10 ** 8) / juldPrice ;
        // require(ERC20Interface(juld).balanceOf(address(this)) >= juldAmount , "insufficient juld");
        if(usersBalances[financialAddress][juld] > juldAmount){
            usersBalances[financialAddress][juld] = usersBalances[financialAddress][juld] - juldAmount;
            //needs extra check that owner deposited how much juld for cashBack
            _addUserBalance(juld, userAddr, juldAmount);
        }
    }
}

