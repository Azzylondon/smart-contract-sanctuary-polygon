contract StakeValue {
    
    using SafeMath for uint;

    IUniswapV2Pair private immutable LPToken = IUniswapV2Pair(0x6E53cB6942e518376E9e763554dB1A45DDCd25c4);
    IERC20 private immutable GFarmToken = IERC20(0xE5417Af564e4bFDA1c483642db72007871397896);
    GFarmPool private immutable gfarmPool = GFarmPool(0x151757c2E830C467B28Fe6C09c3174b6c76aA0c5);
    
    function yourStakedValueInUSD(address user) public view returns(uint){

        (, uint reserveUsd) = reservesLp();
        uint lpPriceUsd = reserveUsd.mul(2).div(LPToken.totalSupply());

        return gfarmPool.users(user).provided.mul(lpPriceUsd).div(1e18);
    }
    function reservesLp() private view returns(uint, uint){
        (uint112 reserves0, uint112 reserves1, ) = LPToken.getReserves();
        if(LPToken.token0() == address(GFarmToken)){
            return (reserves0, reserves1);
        }else{
            return (reserves1, reserves0);
        }
    }
}