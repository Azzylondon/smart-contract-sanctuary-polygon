        emit SetUniswapV2Router02(_contract);
    }

    function swap(uint256 _value, uint256 _amountOut, address[] calldata contracts) internal returns(uint256) {
        address _contractIn = contracts[0];
        address _contractOut = contracts[1];

        //tokensForETH
        if(_contractIn != address(0) && _contractOut == address(0)){
            uint[] memory amounts = swapTokensForExactETH(_contractIn, _amountOut, _value);
            return amounts[0]; //swap後の値

        //tokensForTokens
        }else if(_contractIn != address(0) && _contractOut != address(0)){
            uint[] memory amounts = swapTokensForExactTokens(_contractIn, _contractOut, _amountOut, _value);
