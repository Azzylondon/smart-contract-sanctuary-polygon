// File contracts3/money/Purchase.sol

pragma solidity ^0.8.0;


abstract contract Purchase is Withdrawable {

    constructor() {}

    function _buy(
        address _contract,
        uint256 _value,
        address payable _sender
    ) virtual internal whenNotPaused()
    {
        require(msg.sender == _sender, "Purchase: invalid sender");

        if(_contract != address(0)){
            require(msg.value == 0, "Purchase: only receive ERC20");
            IERC20 erc20 = IERC20(_contract);
            require(erc20.allowance(_sender, address(this)) >= _value, "Purchase: transfer amount exceeds allowance");
            erc20.transferFrom(_sender, address(this), _value);
        }else{
            require(msg.value == _value, "Purchase: invalid value");
        }
    }
}


