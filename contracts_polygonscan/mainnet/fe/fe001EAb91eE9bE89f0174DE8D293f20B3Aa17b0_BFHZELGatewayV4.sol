// File contracts3/money/BFHZELGatewayV4.sol

pragma solidity 0.8.1;





contract BFHZELGatewayV4 is Purchase ,UniswapExecutor, SignatureValidator {
    struct Campaign {
        uint8 purchaseType;
        uint8 subPurchaseType;
        uint8 proxyPurchaseType;
    }

    uint8 constant PURCHASE_NORMAL = 0;
    uint8 constant PURCHASE_ETH_BACK = 1;
    uint8 constant PURCHASE_UP20 = 2;
    uint8 constant PURCHASE_REGULAR = 3;
    uint8 constant PURCHASE_ETH_BACK_UP20 = 4;
    uint8 constant PURCHASE_UP10 = 5;

    Campaign public campaign;

    uint256 constant BASE = 10000;
    uint256 private nonce;
    uint16 public chanceDenom;
    uint256 public ethBackBasisPoint;
    bytes private salt;

    event Sold(
        address indexed user,
        address indexed referrer,
        uint8 purchaseType,
        uint32 usCent,
        uint256 grossValue,
        uint256 referralValue,
        uint256 netValue
    );

    event Swap(
        address indexed user,
        address _contractIn,
        address _contractOut,
        uint256 _value,
        uint256 _swappedVal,
        uint256 _amountOut
    );

    event CampaignUpdated(
        uint8 purchaseType,
        uint8 subPurchaseType,
        uint8 proxyPurchaseType
    );

    // mapping(ERC20contractAddress => bool)
    mapping(address => bool) public deniedERC20Contract;

    event DeniedERC20Contract(address _contract, bool _deny);

    constructor(
        address _validator, 
        address _uniswap
    ) SignatureValidator(_validator) {
        setUniswapV2Router02(_uniswap);
        setCampaign(0, 0, 0);
        updateEthBackPercentege(5000);
        updateChance(25);
        salt = bytes("ulooNg6veiv2Mieg");

        //TODO ここはあとで変えてもOK
        nonce = 489;
    }

    function setCampaign(
        uint8 _purchaseType,
        uint8 _subPurchaseType,
        uint8 _proxyPurchaseType
    )
        public
        onlyOperator()
    {
        campaign = Campaign(_purchaseType, _subPurchaseType, _proxyPurchaseType);
        emit CampaignUpdated(_purchaseType, _subPurchaseType, _proxyPurchaseType);
    }

    function updateChance(uint16 _newchanceDenom) public onlyOperator() {
        chanceDenom = _newchanceDenom;
    }

    function updateEthBackPercentege(uint256 _ethBackBasisPoint) public onlyOperator() {
        ethBackBasisPoint = _ethBackBasisPoint;
    }

    function setDeniedERC20Contract(address _contract, bool _deny) public onlyOperator{
        deniedERC20Contract[_contract] = _deny;
        emit DeniedERC20Contract(_contract, _deny);
    }

    function getRandom(uint16 max, uint256 _nonce, address _sender) public view returns (uint16) {
        return uint16(
            bytes2(
                keccak256(
                    abi.encodePacked(
                        blockhash(block.number-1),
                        _sender,
                        _nonce,
                        salt
                    )
                )
            )
        ) % max;
    }

    function _ethBack(address payable _buyer, uint256 _ethBackBasisPoint) internal returns (uint256) {
        uint256 ethBackValue = msg.value * _ethBackBasisPoint / BASE;
        _buyer.transfer(ethBackValue);
        return ethBackValue;
    }

    function _referrerBack(address payable _referrer, uint256 _referralBasisPoint) internal returns (uint256) {
        if(_referrer == address(0x0) || _referrer == msg.sender) {
            return 0;
        }
        uint256 referralValue = msg.value * _referralBasisPoint / BASE;
        _referrer.transfer(referralValue);
        return referralValue;
    }

    function encodeData(
        address _user, 
        address _referrer, 
        uint256 _referralBasisPoint, 
        uint32 _usCent,
        uint256 _value,
        address _contract
    ) public pure returns (bytes32) {
        return keccak256(
            abi.encode(
                _user, 
                _referrer, 
                _referralBasisPoint, 
                _usCent,
                _value,
                _contract
            )
        );
    }

    function encodeData(
        address _user, 
        address _referrer, 
        uint256 _referralBasisPoint, 
        uint32 _usCent,
        uint256 _value,
        address[] calldata _contracts,
        uint256 _amountOut,
        uint256 _deadline
    ) public pure returns (bytes32) {
        return keccak256(
            abi.encode(
                _user, 
                _referrer, 
                _referralBasisPoint, 
                _value,
                _contracts,
                _amountOut,
                _deadline
            )
        );
    }

    function swap(address[] calldata _contracts, uint256 _value, uint256 _amountOut) private returns(uint256) {

        //tokensForETH
        if(_contracts[0] != address(0) && _contracts[1] == address(0)){
            return swapTokensForExactETH(_contracts[0], _amountOut, _value)[0];

        //tokensForTokens
        }else if(_contracts[0] != address(0) && _contracts[1] != address(0)){
            return swapTokensForExactTokens(_contracts[0], _contracts[1], _amountOut, _value)[0];
        }

        return _value;
    }

    function buyZEL(
        address payable _referrer,
        uint256 _referralBasisPoint,
        uint32 _usCent,
        uint256 _value,
        address _contract,
        bytes memory _signature
    )
        public
        payable
    {
        require(_referralBasisPoint + ethBackBasisPoint <= BASE, "Invalid basis points");

        if(_contract != address(0)){
            require(!deniedERC20Contract[_contract], "BFHZELGatewayV4: not allowed contract");
        }

        require(validateSigConcat(
            encodeData(
                msg.sender, 
                _referrer, 
                _referralBasisPoint, 
                _usCent,
                _value,
                _contract
            ), 
            _signature
        ), "Invalid signature");

        uint8 purchaseType = campaign.purchaseType;
        uint256 referralValue = 0;
        uint256 netValue = 0;

        if (purchaseType == PURCHASE_ETH_BACK || purchaseType == PURCHASE_ETH_BACK_UP20) {
            if (getRandom(chanceDenom, nonce, msg.sender) == 0) {
                netValue = _value - _ethBack(payable(msg.sender), ethBackBasisPoint);
            } else {
                purchaseType = campaign.subPurchaseType;
                referralValue = _referrerBack(_referrer, _referralBasisPoint);
                netValue = _value - referralValue;
            }
            nonce++;
        } else {
            referralValue = _referrerBack(_referrer, _referralBasisPoint);
            netValue = _value - referralValue;
        }

        emit Sold(
            msg.sender,
            _referrer,
            purchaseType,
            _usCent,
            _value,
            referralValue,
            netValue
        );

        super._buy(_contract, _value, payable(msg.sender));
    }

    function buyZELSwap(
        address payable _referrer,
        uint256 _referralBasisPoint,
        uint32 _usCent,
        uint256 _value,
        address[] calldata _contracts,//
        uint256 _amountOut,//
        uint256 _deadline,//
        bytes memory _signature
    )
        public
        payable
    {
        require(_referralBasisPoint + ethBackBasisPoint <= BASE, "Invalid basis points");

        if(_contracts[0] != address(0)){
            require(!deniedERC20Contract[_contracts[0]], "BFHZELGatewayV4: not allowed contract");
        }

        require(validateSigConcat(
            encodeData(
                msg.sender, 
                _referrer, 
                _referralBasisPoint, 
                _usCent,
                _value,
                _contracts,//
                _amountOut,//
                _deadline//
            ), 
            _signature
        ), "Invalid signature");

        uint8 purchaseType = campaign.purchaseType;
        uint256 referralValue = 0;
        uint256 netValue = 0;

        if (purchaseType == PURCHASE_ETH_BACK || purchaseType == PURCHASE_ETH_BACK_UP20) {
            if (getRandom(chanceDenom, nonce, msg.sender) == 0) {
                netValue = _value - _ethBack(payable(msg.sender), ethBackBasisPoint);
            } else {
                purchaseType = campaign.subPurchaseType;
                referralValue = _referrerBack(_referrer, _referralBasisPoint);
                netValue = _value - referralValue;
            }
            nonce++;
        } else {
            referralValue = _referrerBack(_referrer, _referralBasisPoint);
            netValue = _value - referralValue;
        }

        emit Sold(
            msg.sender,
            _referrer,
            purchaseType,
            _usCent,
            _value,
            referralValue,
            netValue
        );

        super._buy(_contracts[0], _value, payable(msg.sender));

        //swap
        _swap(_value, _amountOut, _contracts);
    }

    function _swap(uint _value, uint256 _amountOut, address[] calldata _contracts) private {

        //swap
        uint256 realValue = swap(_value, _amountOut, _contracts);

        //余ったらsenderに返す
        if(realValue < _value){
            IERC20(_contracts[0]).transfer(msg.sender, _value - realValue);
        }

        emit Swap(
            msg.sender,
            _contracts[0],
            _contracts[1],
            _value,
            realValue, 
            _amountOut
        );
    }

