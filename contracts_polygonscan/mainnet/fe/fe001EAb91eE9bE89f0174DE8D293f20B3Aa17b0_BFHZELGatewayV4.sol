        require(erc20.approve(address(uniswap), amountIn), 'approve failed.');

        address[] memory path = new address[](2);
        path[0] = _contractIn;
        path[1] = _contractOut;

        return uniswap.swapExactTokensForTokens(amountIn, amountOutMin, path, address(this), block.timestamp);
    }

