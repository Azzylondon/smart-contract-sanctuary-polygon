        require(erc20.approve(address(uniswap), amountInMax), 'approve failed.');

        address[] memory path = new address[](2);
        path[0] = _contract;
        path[1] = uniswap.WETH();

        return uniswap.swapTokensForExactETH(amountOut, amountInMax, path, address(this), block.timestamp);
    }

    function swapTokensForExactTokens(address _contractIn, address _contractOut, uint amountOut, uint amountInMax) internal returns(uint[] memory amounts) {
        IERC20 erc20 = IERC20(_contractIn);
        require(erc20.approve(address(uniswap), amountInMax), 'approve failed.');

        address[] memory path = new address[](2);
        path[0] = _contractIn;
        path[1] = _contractOut;

