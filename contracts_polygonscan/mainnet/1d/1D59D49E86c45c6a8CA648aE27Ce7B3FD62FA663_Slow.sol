pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IWETH.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./libraries/AddressArray.sol";
import "./interfaces/ITokenERC20.sol";

contract Slow is Ownable, ReentrancyGuard {
  using SafeMath for uint256;
  using AddressArray for address[];
  
  address[] public tokens;
  address[] public routers;
  
  int256 public stackedAmount;

  uint256 public lotSizePercentage = 2000;
  
  event Stacked(address token, int256 amount, int256 totalStackedAmount);
  
  constructor(){
  }

  function setLotSizePercentage(uint256 _p) external onlyOwner {
    lotSizePercentage = _p;
  }

  function lotSize(address token) public view returns(uint256) {
    uint256 balance = IERC20(token).balanceOf(address(this));
    return balance.mul(lotSizePercentage).div(10000);
  }
  
  function tokensLength() external view returns(uint256) {
    return tokens.length;
  }
  
  function initToken(address token) external onlyOwner {
    require(IERC20(token).balanceOf(address(this)) > 0);
    require(ITokenERC20(token).decimals() >= 0);
    tokens.addValue(token);
    setAllowanceToRoutersForToken(token);
  }

  function removeToken(address token) external onlyOwner {
    tokens.removeValue(token);
    swipeToken(token);
  }

  function swipeToken(address token) public onlyOwner returns(uint256){
    uint256 balance = IERC20(token).balanceOf(address(this));
    if(balance > 0){
      IERC20(token).transfer(msg.sender, balance);
    }
    return balance;
  }

  function setAllowanceToRoutersForAllTokens() public onlyOwner {
    for(uint256 i = 0; i < tokens.length; i++){
      setAllowanceToRoutersForToken(tokens[i]);
    }
  }
  
  function setAllowanceToRoutersForToken(address token) internal{

    for(uint256 i = 0; i < routers.length; i++){
      setAllowanceToRouterForToken(routers[i], token);
    }
  }

  function setAllowanceToRouterForToken(address router, address token) internal returns(bool){
    IERC20(token).approve(router, type(uint256).max);
    return true;
  }

  function setRouter(address router) external onlyOwner {
    routers.addValue(router);
    setAllowanceToRoutersForAllTokens();
  }

  function removeRouter(address router) external onlyOwner {
    routers.removeValue(router);
  }
  
  function swipeAllTokens() public onlyOwner {
    for(uint256 i = 0; i < tokens.length; i++){
      swipeToken(tokens[i]);
    }
  }
  function swipe() external onlyOwner {
    swipeAllTokens();
    payable(msg.sender).transfer(address(this).balance);
  }

  function bestPrice(uint256 fromAmount, address fromToken, address toToken) internal view returns(uint256, address){
    uint256 maxAmount = 0;
    address foundRouter = address(0);
    for(uint256 routerIndex = 0; routerIndex < routers.length; routerIndex++){
      IUniswapV2Router02 router = IUniswapV2Router02(routers[routerIndex]);
      if(IUniswapV2Factory(router.factory()).getPair(fromToken, toToken) != address(0)){
        address[] memory path = new address[](2);
        path[0] = fromToken;
        path[1] = toToken;
        uint256[] memory expectedAmounts = router.getAmountsOut(fromAmount, path);
        if(maxAmount < expectedAmounts[1]){
          maxAmount = expectedAmounts[1];
          foundRouter = address(router);
        }
      }
    }
    return (maxAmount, foundRouter);
  }

  function swapSpecific(uint256 amount, address router, address fromToken, address toToken) public onlyOwner returns(uint256){
    if(IERC20(fromToken).balanceOf(address(this)) < amount){
      // we don't throw if there is not enough balance
      return 0;
    }
    address[] memory path = new address[](2);
    path[0] = fromToken;
    path[1] = toToken;
    uint256[] memory resulted = IUniswapV2Router02(router).swapExactTokensForTokens(amount, 0, path, address(this), block.timestamp * 2);
    return resulted[1];
  }

  function unitsDiff(uint256 amountA, address tokenA, uint256 amountB, address tokenB) public view returns(int256) {
    uint256 decimalsA = ITokenERC20(tokenA).decimals();
    uint256 decimalsB = ITokenERC20(tokenB).decimals();
    if(decimalsB > decimalsA){
      amountA = amountA * (( decimalsB - decimalsA) ** 10);
    } else {
      if(decimalsB < decimalsA){
        amountB = amountB * (( decimalsA -decimalsB) ** 10);
      }
    }
    return int256(amountA - amountB); 
  }

  function checkAndSwap(address fromToken, address toToken) public onlyOwner returns(uint256){
    uint256 amount = lotSize(fromToken);
    (uint256 best, address router) = bestPrice(amount, fromToken, toToken);
    if(router == address(0)){
      return 0;
    }
    if(best == 0){
      return 0;
    }
    uint256 result = swapSpecific(amount, router, fromToken, toToken);
    if(result == 0){
      revert("BS");
    }
    int256 diff = unitsDiff(amount, fromToken, result, toToken);
    require(diff > 0, "BSD");
    stackedAmount = stackedAmount + diff;
    
    emit Stacked(toToken, diff, stackedAmount);
    return result;
  }
  
}

