    event DEFIBASKET_UNISWAPV2_ADD_LIQUIDITY(
        uint256[] amountIn,
        address lpToken,
        uint256 liquidity
    );

    event DEFIBASKET_UNISWAPV2_REMOVE_LIQUIDITY(
