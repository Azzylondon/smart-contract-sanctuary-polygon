////// SPDX-License-Identifier-FLATTEN-SUPPRESS-WARNING: MIT
pragma solidity ^0.8.4;

////import "./DAF.sol";
////import "./DAFLib.sol";
////import "./interfaces/IAccessManager.sol";
////import "@openzeppelin/contracts/access/Ownable.sol";

contract Factory is Ownable {
	address[] public dafs;
	mapping(address => mapping(address => address)) public mapDAF;
	address public accessManager;
	event DAFCreated(address cashToken, address longToken, address newDAF, address owner);

	function countDAF() external view returns (uint256) {
		return dafs.length;
	}

	constructor(address accessManagerImplementation) {
		accessManager = accessManagerImplementation;
	}

	function createDAF(DAFLib.NewDAF memory params) public {
		/// ----- Rights -----
		bytes32 strategist = IAccessManager(accessManager).STRATEGIST_ADDER_ROLE();
		require(IAccessManager(accessManager).hasRole(strategist, msg.sender), "createDAF: not strategist");
		/// ----- Params Validation -----
		require(params.cashToken != params.longToken, "createDAF: IDENTICAL_ADDRESSES");
		(address token0, address token1) = params.cashToken < params.longToken
			? (params.cashToken, params.longToken)
			: (params.longToken, params.cashToken);
		require(token0 != address(0), "createDAF: ZERO_ADDRESS");
		require(token1 != address(0), "createDAF: ZERO_ADDRESS");
		require(mapDAF[token0][token1] == address(0), "createDAF: DAF_EXISTS");
		require(mapDAF[token1][token0] == address(0), "createDAF: DAF_EXISTS");
		/// ----- Creation -----
		DAF daf = new DAF(params);
		daf.transferOwnership(msg.sender);
		mapDAF[token0][token1] = address(daf);
		mapDAF[token1][token0] = address(daf);
		dafs.push(address(daf));
		emit DAFCreated(token0, token1, address(daf), msg.sender);
	}

	function setAccessManager(address newAccessManagerImplementation) external onlyOwner {
		accessManager = newAccessManagerImplementation;
	}
}