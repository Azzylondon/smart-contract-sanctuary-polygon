pragma solidity ^0.8.4;

////import "./DAFLib.sol";
////import "./NapDAFToken.sol";
////import "hardhat/console.sol";
////import "./interfaces/IFactory.sol";
////import "./interfaces/IAccessManager.sol";
////import "@openzeppelin/contracts/access/Ownable.sol";
////import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
////import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

/// @title  Decentralised Autonomous Funds
/// @author NapX
/// @notice Trading Manager based on external signals.
/// @notice Not audited yet. DYOR and use at your own risks.
/// @notice Security Contact : [email protected]

contract DAF is Ownable {
	bool public DEBUG = true;
	bool public paused;
	string public version = "1.0.0";
	string public nameDAF;
	uint256 public maxAUM;
	address public router;
	address public cashToken;
	address public longToken;
	address public factory;
	address public feesReceiver;
	uint256 public vaLoPrecision;
	uint256 public previousSignaL;
	uint256 public previousTimestamp;
	uint256 public signaLPrecision = 1000000;
	uint256 public managementFeesDayRateNumerator = 5;
	uint256 public managementFeesDayRateDenominator = 1000;
	uint256 public minAmountSubscription;
	mapping(address => uint256) public contributions;

	AggregatorV3Interface public oracle;
	NapDAFToken public ndt;

	event Subscription(address user, uint256 amount, uint256 timestamp);
	event Redemption(address user, uint256 amount, uint256 timestamp);
	event Swaped(address daf, uint256 signaL);

	constructor(DAFLib.NewDAF memory params) {
		require(params.cashToken != address(0), "constructor: Invalid cashToken address");
		require(params.longToken != address(0), "constructor: Invalid longToken address");
		require(params.router != address(0), "constructor: Invalid router address");
		require(params.oracle != address(0), "constructor: Invalid oracle address");
		require(params.feesReceiver != address(0), "constructor: Invalid feesReceiver address");
		cashToken = params.cashToken;
		longToken = params.longToken;
		router = params.router;
		oracle = AggregatorV3Interface(params.oracle);
		maxAUM = params.maxAUM;
		factory = msg.sender;
		feesReceiver = params.feesReceiver;
		minAmountSubscription = params.minAmountSubscription;
		nameDAF = params.nameDAF;
		ndt = new NapDAFToken(params.tokenName, params.tokenSym);
		previousTimestamp = block.timestamp;
		IERC20(cashToken).approve(router, 2**256 - 1);
		IERC20(longToken).approve(router, 2**256 - 1);
		vaLoPrecision = (10**(oracle.decimals() + IERC20Metadata(longToken).decimals() - IERC20Metadata(cashToken).decimals()));
	}

	/// ------- FUNCTIONS::READ -------
	/// @notice Compute Management Fees based on the previous timestamp, not onlyOwner because users are incentivize to call it before deposit
	/// @notice Fees are collected daily so that we new deposit are charged precisely. We should prevent front-run withdraw when calling this function
	/// @return cashFees : The amount of fees received in cashToken
	/// @return longFees : The amount of fees received in longToken
	function managementFees() public view returns (uint256 cashFees, uint256 longFees) {
		uint256 oneDay = 1 days;
		uint256 nbDays = (block.timestamp - previousTimestamp) / oneDay;
		uint256 cashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 longTokenBalance = IERC20(longToken).balanceOf(address(this));
		// if (DEBUG) console.log("        (sol) managementFees() : previousTimestamp : %s", previousTimestamp);
		// if (DEBUG) console.log("        (sol) managementFees() : nbDays : %s", nbDays);
		if (nbDays == 0) return (0, 0);
		if (cashTokenBalance > 0) cashFees = (nbDays * cashTokenBalance * managementFeesDayRateNumerator) / managementFeesDayRateDenominator;
		if (longTokenBalance > 0) longFees = (nbDays * longTokenBalance * managementFeesDayRateNumerator) / managementFeesDayRateDenominator;
	}

	/// @notice getLatestPrice returns the latest price powered by ChainLink price feeds
	/// @notice ChainLink returns an price with a given decimal precision (ex: 8 for USD/ETH). It complexify the computation within the contract.
	/// @return Price of longToken per cashToken (eg. ETH/USD)
	function getLatestPrice() public view returns (int256) {
		(, int256 price, , , ) = oracle.latestRoundData();
		return price;
	}

	/// getDAFShares compute the percentages of cash/long assets in the DAF on a dollar basis
	/// @return Percentage of cashToken and longToken
	function getDAFPosition() public view returns (uint256, uint256) {
		uint256 cashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 longTokenBalance = IERC20(longToken).balanceOf(address(this));
		if (cashTokenBalance == 0 && longTokenBalance == 0) return (0, 0);
		uint256 longTokenPrice = uint256(getLatestPrice());
		uint256 longTokenPooLValoInCashToken = (longTokenBalance * longTokenPrice) / vaLoPrecision;
		uint256 pooLGlobaL = ((longTokenBalance * longTokenPrice) / vaLoPrecision) + cashTokenBalance;
		uint256 longTokenPercentPosition = (longTokenPooLValoInCashToken * signaLPrecision) / pooLGlobaL;
		uint256 cashTokenPercentPosition = (cashTokenBalance * signaLPrecision) / pooLGlobaL;
		// if (DEBUG) console.log("        (sol) getDAFPosition() : cashTokenPercentPosition : %s %", cashTokenPercentPosition / 10000);
		// if (DEBUG) console.log("        (sol) getDAFPosition() : longTokenPercentPosition : %s %", longTokenPercentPosition / 10000);
		return (cashTokenPercentPosition, longTokenPercentPosition);
	}

	/// getCashTokenPerNDT compute the current price of 1 NDT per cashToken
	/// @return NDT price in cashToken
	function priceNDT() public view returns (uint256) {
		uint256 supply = ndt.totalSupply() / (10**ndt.decimals());
		if (supply == 0) return 0;
		uint256 cashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 longTokenBalance = IERC20(longToken).balanceOf(address(this));
		uint256 longTokenPrice = uint256(getLatestPrice());
		uint256 longTokenPooLValoInCashToken = (longTokenBalance * longTokenPrice) / vaLoPrecision;
		// if (DEBUG) console.log("        (sol) priceNDT() : supply : %s", supply);
		// if (DEBUG) console.log("        (sol) priceNDT() : longTokenPooLValoInCashToken : %s", longTokenPooLValoInCashToken);
		return ((longTokenPooLValoInCashToken + cashTokenBalance) / supply);
	}

	/// @notice getAmountOutMax compute the amount receive for `_amountIn` of token described via `path`
	function getAmountOutMax(uint256 _amountIn, address[] memory _path) internal view returns (uint256) {
		uint256[] memory amountsOutMax = IUniswapV2Router02(router).getAmountsOut(_amountIn, _path);
		return amountsOutMax[_path.length - 1];
	}

	/// @notice getFees compute the amount of fees that a given user must pay (entry/exit of DAF), based on its possible discounts
	function getFees(address _user)
		public
		view
		returns (
			uint256,
			uint256,
			uint256
		)
	{
		address accessManager = IFactory(factory).accessManager();
		uint256 entryFees = IAccessManager(accessManager).getEntryFees(_user);
		uint256 exitFees = IAccessManager(accessManager).getExitFees(_user);
		uint256 denominator = IAccessManager(accessManager).feeDenominator();
		return (entryFees, exitFees, denominator);
	}

	/// ------- FUNCTIONS::WRITE -------

	/// @notice Take user deposit `_amountIn` and return NDT tokens as share of the fund
	/// @param _amountIn Amount of cashToken to deposit
	/// @dev 	msg.sender must have approved to this contract _amountIn of cashToken
	function subscription(uint256 _amountIn) external whenNotPaused {
		require(_amountIn >= minAmountSubscription, "subscription: too low deposit amount");
		/// @dev ----- Fees Variable -----
		address accessManager = IFactory(factory).accessManager();
		require(IAccessManager(accessManager).isWhiteListed(msg.sender), "subscription: not whitelisted");
		(uint256 entryFees, , uint256 denominator) = getFees(msg.sender);
		/// @dev ----- Init Section -----
		uint256 toMint;
		uint256 nbTotalParts = ndt.totalSupply();
		uint256 previousCashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 previousLongTokenBalance = IERC20(longToken).balanceOf(address(this));
		require(_amountIn > 0, "subscription: No cashToken sent");
		require(IERC20(cashToken).allowance(msg.sender, address(this)) >= _amountIn, "subscription: cashToken not approved");
		require(IERC20(cashToken).transferFrom(msg.sender, address(this), _amountIn), "subscription: Failed to transfer cashToken to contract");
		/// @dev ----- Fees Section - Taken based on amountIn of cashToken -----
		if (entryFees > 0 && denominator >= entryFees) {
			uint256 _amountInFeesExempted = (_amountIn * (denominator - entryFees)) / denominator;
			uint256 fees = _amountIn - _amountInFeesExempted;
			if (fees > 0) require(IERC20(cashToken).transfer(feesReceiver, fees), "subscription: fees transfer failed");
			_amountIn -= fees;
		}
		/// @dev ----- ALLocation Section -----
		if (nbTotalParts == 0) {
			require(previousCashTokenBalance == 0 && previousLongTokenBalance == 0, "subscription: ndt supply doesn't match DAF balances");
			if (_amountIn > maxAUM) revert("subscription: max AUM reached");
			toMint = _amountIn;
		}
		uint256 longTokenPrice = uint256(getLatestPrice());
		if (nbTotalParts > 0 && (previousCashTokenBalance > 0 || previousLongTokenBalance > 0)) {
			uint256 longTokenPooLValoInCashToken = (previousLongTokenBalance * longTokenPrice) / vaLoPrecision;
			uint256 pooLValo = longTokenPooLValoInCashToken + previousCashTokenBalance;
			// if (DEBUG) console.log("        (sol) subscription() : pooLValo + _amountIn : %s", pooLValo + _amountIn);
			if (pooLValo + _amountIn > maxAUM) revert("subscription: max AUM reached");
			toMint = (_amountIn * nbTotalParts) / pooLValo;
			// if (DEBUG) console.log("        (sol) subscription() : _amountIn = %s and toMint = %s", _amountIn, toMint);
			if (previousSignaL > 0) {
				uint256 amountOfCashToSwap = (_amountIn * previousSignaL) / signaLPrecision;
				// if (DEBUG) console.log("        (sol) subscription : previousSignaL : %s", previousSignaL);
				// if (DEBUG) console.log("        (sol) subscription : amountOfCashToSwap : %s", amountOfCashToSwap);
				swap(cashToken, longToken, amountOfCashToSwap, address(this));
				toMint = (toMint * ((previousSignaL * 997) / 1000)) / signaLPrecision;
			}
		}
		require(toMint > 0, "subscription: You need to subscribe more parts in the fund");
		contributions[msg.sender] += toMint;
		ndt.mint(msg.sender, toMint);
		emit Subscription(msg.sender, _amountIn, block.timestamp);
	}

	/// @notice Take user NDT `_amountIn` and send back the amount of token associated
	/// @param _amountIn Amount of NDT to redeem
	/// @dev msg.sender must have approved to this contract _amountIn of NDT
	/// @notice The 0.3% of fees when swapping are charged upon msg.sender by sending amountOut directly to msg.sender
	function redemption(uint256 _amountIn) external whenNotPaused {
		uint256 cashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 longTokenBalance = IERC20(longToken).balanceOf(address(this));
		uint256 nbTotalParts = ndt.totalSupply();
		require(_amountIn > 0, "redemption: _amountIn == 0");
		require(ndt.balanceOf(msg.sender) >= _amountIn, "redemption: _amountIn > balanceOf");
		require(contributions[msg.sender] >= _amountIn, "redemption: redeem amount > contributions");
		contributions[msg.sender] -= _amountIn;
		ndt.burnFrom(msg.sender, _amountIn);
		// uint256 longTokenPooLValoInCashToken = (longTokenBalance * uint256(getLatestPrice())) / vaLoPrecision;
		// uint256 pooLValo = longTokenPooLValoInCashToken + cashTokenBalance;
		// if (DEBUG) console.log("        (sol) redemption : pooLValo : %s", pooLValo);
		// if (DEBUG) console.log("        (sol) redemption : cashTokenBalance : %s", IERC20(cashToken).balanceOf(address(this)));
		// if (DEBUG) console.log("        (sol) redemption : longTokenBalance : %s", IERC20(longToken).balanceOf(address(this)));
		/// @dev ----- Fees Section - Took on cashToken and longToken -----
		uint256 cashAmountToReimburse = (_amountIn * cashTokenBalance) / nbTotalParts;
		uint256 longAmountToSwapAndReimburse = (_amountIn * longTokenBalance) / nbTotalParts;
		(, uint256 exitFees, uint256 denominator) = getFees(msg.sender);
		if (exitFees > 0 && denominator >= exitFees) {
			(uint256 cashPos, uint256 longPos) = getDAFPosition();
			uint256 feesCash = (((cashAmountToReimburse * exitFees) / denominator) * cashPos) / signaLPrecision;
			uint256 feesLong = (((longAmountToSwapAndReimburse * exitFees) / denominator) * longPos) / signaLPrecision;
			cashAmountToReimburse -= feesCash;
			longAmountToSwapAndReimburse -= feesLong;
			require(IERC20(cashToken).transfer(feesReceiver, feesCash), "redemption: feesCash transfer failed");
			swap(longToken, cashToken, feesLong, feesReceiver);
		}
		/// ----- Transfer -----
		console.log("        (sol) cashAmountToReimburse : %s", cashAmountToReimburse);
		console.log("        (sol) longAmountToSwapAndReimburse : %s", longAmountToSwapAndReimburse);
		if (longAmountToSwapAndReimburse > 0) swap(longToken, cashToken, longAmountToSwapAndReimburse, msg.sender);
		if (cashAmountToReimburse > 0)
			require(IERC20(cashToken).transfer(msg.sender, cashAmountToReimburse), "redemption: cashTokenToReimburse transfer failed");
		// if (DEBUG) console.log("        (sol) redemption : cashTokenBalance after transfer : %s", IERC20(cashToken).balanceOf(address(this)));
		// if (DEBUG) console.log("        (sol) redemption : longTokenBalance after transfer : %s", IERC20(longToken).balanceOf(address(this)));
		emit Redemption(msg.sender, _amountIn, block.timestamp);
	}

	/// @notice Compute swap `_amountIn` of `_tokenIn` and execute swap
	/// @dev Token must have been approved (done in constructor)
	function swap(
		address _tokenIn,
		address _tokenOut,
		uint256 _amountIn,
		address _to
	) internal {
		if (_amountIn == 0) return;
		require(_tokenIn != address(0) && _tokenOut != address(0) && _to != address(0), "swap: invalid parameters");
		address[] memory path;
		path = new address[](2);
		path[0] = _tokenIn;
		path[1] = _tokenOut;
		uint256 amountOutMin = getAmountOutMax(_amountIn, path);
		IUniswapV2Router02(router).swapExactTokensForTokens(_amountIn, amountOutMin, path, _to, block.timestamp);
	}

	/// @dev I think we should keep the daily tx but no revert if same (or close) signal, so that we have a daily record of what happened and the signal given
	/// @dev Checks that should be done off-chain, also, dont trigger signal if too weak in comparaison to the current exposure

	/// @notice swap assets depending the signal triggered by off-chain scripts
	/// If _signal is identic we don't revert, just return and emit an event
	/// @param _signal (discrete) nature of signal : 0 or 1 or 0 to 100 : % of longToken
	function swapAsset(uint256 _signal) public onlyOwner {
		uint256 cashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 longTokenBalance = IERC20(longToken).balanceOf(address(this));
		emit Swaped(address(this), _signal);
		if (_signal == previousSignaL) return;
		uint256 longTokenPrice = uint256(getLatestPrice());
		require((cashTokenBalance > 0 || longTokenBalance > 0), "swapAsset: empty DAF");
		uint256 longTokenPooLValoInCashToken = (longTokenBalance * longTokenPrice) / vaLoPrecision;
		uint256 longTokenPercentShare = (longTokenPooLValoInCashToken * signaLPrecision) / (longTokenPooLValoInCashToken + cashTokenBalance);
		// if (DEBUG) console.log("        (sol) swapAsset before : cashTokenBalance : %s", cashTokenBalance);
		// if (DEBUG) console.log("        (sol) swapAsset before : longTokenBalance : %s", longTokenBalance);
		// if (DEBUG) console.log("        (sol) swapAsset before : longTokenPooLValoInCashToken : %s", longTokenPooLValoInCashToken);
		// if (DEBUG) console.log("        (sol) swapAsset before : longTokenPercentShare : %s", longTokenPercentShare);
		if (longTokenPercentShare > _signal) {
			/// @notice Here we decrease the DAF exposure
			uint256 percentSwapDelta = longTokenPercentShare - _signal;
			require(percentSwapDelta >= 0 && percentSwapDelta <= signaLPrecision, "swapAsset: Unexpected percentSwapDelta #1");
			uint256 longToSwapForCash = (percentSwapDelta * (longTokenPooLValoInCashToken + cashTokenBalance)) / signaLPrecision;
			uint256 amountToSwap = (longToSwapForCash * vaLoPrecision) / longTokenPrice;
			uint256 maxToSwap = amountToSwap > longTokenBalance ? longTokenBalance : amountToSwap;
			swap(longToken, cashToken, maxToSwap, address(this));
		} else {
			/// @dev Here we increase the DAF exposure
			uint256 percentSwapDelta = _signal - longTokenPercentShare;
			require(percentSwapDelta >= 0 && percentSwapDelta <= signaLPrecision, "swapAsset: Unexpected percentSwapDelta #2");
			uint256 cashToSwapForLong = (percentSwapDelta * (longTokenPooLValoInCashToken + cashTokenBalance)) / signaLPrecision;
			uint256 maxToSwap = cashToSwapForLong > cashTokenBalance ? cashTokenBalance : cashToSwapForLong;
			swap(cashToken, longToken, maxToSwap, address(this));
		}
		// if (DEBUG) console.log("        (sol) swapAsset after : cashTokenBalance : %s", IERC20(cashToken).balanceOf(address(this)));
		// if (DEBUG) console.log("        (sol) swapAsset after : longTokenBalance : %s", IERC20(longToken).balanceOf(address(this)));
		previousSignaL = _signal;
	}

	/// @notice withdraw Management Fees based on current fees ratio and last withdraw timestamp
	/// @dev : Make sure that fees receiver has the ability to retrieve funds
	function withdrawManagementFees() public onlyOwner {
		(uint256 cashFees, uint256 longFees) = managementFees();
		if (cashFees == 0 && longFees == 0) return;
		if (cashFees > 0) require(IERC20(cashToken).transfer(feesReceiver, cashFees), "withdrawManagementFees: failed to retrieve cashToken fees");
		if (longFees > 0) swap(longToken, cashToken, longFees, feesReceiver);
		previousTimestamp = block.timestamp;
	}

	/// @notice Set max amount of assets under management so that we avoid too big price impact due to liquidity
	/// @param _maxInCashToken max amount in cash token
	function setMaxAUM(uint256 _maxInCashToken) external onlyOwner {
		maxAUM = _maxInCashToken;
	}

	function setPaused(bool _paused) public onlyOwner {
		paused = _paused;
	}

	/// @notice Set new Management Fees Numerator used to determinate fees
	/// @param mngtFeesDailyRateNumerator : fees numerator
	function setMngtFeesDailyRateNumerator(uint256 mngtFeesDailyRateNumerator) external onlyOwner {
		managementFeesDayRateNumerator = mngtFeesDailyRateNumerator;
	}

	/// @notice Set new Management Fees Denominator used to determinate fees
	/// @param mngtFeesDailyRateDenominator : fees denominator
	function setMngtFeesDailyRateDenominator(uint256 mngtFeesDailyRateDenominator) external onlyOwner {
		managementFeesDayRateDenominator = mngtFeesDailyRateDenominator;
	}

	/// @notice Set new minimum amount to subscribe into that DAF
	/// @param _minAmountSubscription : amount in cashToken
	function setMinAmountSubscription(uint256 _minAmountSubscription) external onlyOwner {
		minAmountSubscription = _minAmountSubscription;
	}

	/// ------- MODIFIER -------

	modifier whenNotPaused() {
		require(!paused, "whenNotPaused: contract is paused");
		_;
	}

	/// ------- FUNCTIONS TO BE REMOVED IN PRODUCTION -------
	///	If executed, all circulating NDT will be worthless, unless reinjected
	/// Contributions mapping too
	function withdrawDAFAssets() public onlyOwner {
		uint256 cashTokenBalance = IERC20(cashToken).balanceOf(address(this));
		uint256 longTokenBalance = IERC20(longToken).balanceOf(address(this));
		if (cashTokenBalance > 0)
			require(IERC20(cashToken).transfer(feesReceiver, cashTokenBalance), "withdrawDAFAssets: failed to retrieve cashToken");
		if (longTokenBalance > 0) swap(longToken, cashToken, longTokenBalance, feesReceiver);
	}
}


