    return _nftAddress;
  }

  /**
        @dev Returns the owner of the NFT
        @return ownerAddress Address of the owner
     */
  function owner() public view virtual returns (address ownerAddress) {
    return IERC721(_nftAddress).ownerOf(_tokenId);
  }
}

