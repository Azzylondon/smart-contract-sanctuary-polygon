pragma solidity 0.8.9;

interface IOddEvenGame {

    function setBeneficiary(address _addr) external;

    function setDefaultAdmin(address admin) external;

    function placeBet(
        uint256 _roundIndex,
        uint256 _amt,
        uint256 _ans
    ) external;

    function terminate() external;

    function isRoundSettled(uint256 _roundIndex) external view returns (bool);

    function getCurrentRound() external view returns (uint256);

    function totalValueLocked() external view returns (uint256);

    function addSponsorFunds(uint256 _wantAmt) external;

    function claimRewards(uint256 _roundIndex) external;

    function requestRandomness(uint256 roundIndex) external returns (bytes32 requestId);

    function availableBetValue(uint256 _roundIndex, uint256 _ticket) external view returns (uint256);

    function settleRound(
        uint256 _roundIndex,
        uint256 _limit,
        uint256 _autoDistribute
    ) external;

    function overlookRound(
        uint256 _roundIndex,
        uint256 _limit,
        uint256 _autoDistribute
    ) external;

    // events of two types of games are identical.
    event Submit(
        uint256 indexed _roundIndex,
        address indexed _user,
        uint256 _betIndex,
        uint256 _betSize,
        uint256 _ticket
    );

    event DistributeRewards(
        uint256 indexed _roundIndex,
        address indexed _user,
        uint256 _roundProfit
    );
    event DistributeDividend(
        uint256 indexed _roundIndex,
        address indexed _to,
        uint256 _amt
    );

    event SettleRoundFully(
        uint256 indexed _roundIndex,
        uint256 _totalBets,
        uint256 _jackpotTicket
    );
}

