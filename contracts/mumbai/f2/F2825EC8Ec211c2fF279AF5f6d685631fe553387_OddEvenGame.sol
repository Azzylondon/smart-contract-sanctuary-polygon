pragma solidity 0.8.9;

import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/SignedSafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

import "./interfaces/IOddEvenGame.sol";

import "hardhat/console.sol";


// solhint-disable-next-line max-states-count
contract OddEvenGame is ReentrancyGuard, Pausable, AccessControl, VRFConsumerBase, IOddEvenGame{

    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint256;
    using SignedSafeMath for int256;
    using SafeERC20 for IERC20;

    // chainlink
    bytes32 internal linkKeyHash;
    uint256 internal linkFee;
    mapping(bytes32 => uint256) internal requestId2roundIndex;

    struct Bet {
        address user;
        uint256 amount;
    }

    struct BettingRound {
        uint256 winningTicket;
        uint256 sponsorFunds;
        uint256 settleCursor; // track arbitration progress
        Bet[] oddBets;
        Bet[] evenBets;
        uint256 oddBetsValue;
        uint256 evenBetsValue;

        uint256 dividends;
        EnumerableSet.AddressSet winners;
        mapping(address => uint256) winnerPendingRewards;
    }

    // configurable game parameters
    uint256 public immutable betUnitValue;
    uint8 public bettingPeriod;
    IERC20 public gameToken;
    uint256 public immutable houseEdge;

    // logic
    mapping(uint256 => BettingRound) internal rounds;
    int256 public lastSettledRoundIndex;
    bool public terminated;
    int256 public terminationRoundIndex;

    uint256 public sponsorFundsCap;
    uint256 public reserveFundsCap;
    uint256 public reserveFunds;

    // admin
    address public beneficiary;
    bytes32 public constant DEV_ROLE = keccak256("DEV_ROLE");

    // dividend
    uint256 public constant RATIO_DENOM = 1e4;
    uint256 public basicDividendRatio;

    event randomnessFulfilled(
        bytes32 requestId,
        uint256 randomness
    );

    modifier roundBettable(uint256 roundIndex){
        require(roundIndex >0 && roundIndex == getCurrentRound(), "previous round unsettled");
        // to-do : set lastSettledroundindex = 0 in addSponsorfunds;
        _;
    }

    modifier roundSettleable(uint256 roundIndex){
        require(roundIndex == getCurrentRound(), "not the current round");
        require(rounds[roundIndex].winningTicket==1 || rounds[roundIndex].winningTicket == 2, "no randomness yet");
        _;
    }

    constructor(address _linkVRFCoordinator,
                address _linkToken,
                bytes32 _linkKeyHash,
                uint256 _betUnitValue,
                uint8 _bettingPeriod,
                IERC20 _gameToken,
                uint256 _houseEdge,
                address _beneficiary
                )
        VRFConsumerBase(_linkVRFCoordinator, _linkToken)
    {
        linkKeyHash = _linkKeyHash;
        linkFee = 0.0001 * 10 ** 18; // 0.1 LINK (Varies by network)

        betUnitValue = _betUnitValue;
        bettingPeriod = _bettingPeriod;
        gameToken = _gameToken;
        houseEdge = _houseEdge;
        beneficiary = _beneficiary;
    }

    function getCurrentRound() public view returns (uint256){
        return uint256(lastSettledRoundIndex +1);
    }

    function isRoundSettled(uint256 roundIndex)
        public
        view
        override
        returns (bool)
    {
        return roundIndex <= uint256(lastSettledRoundIndex);
    }

    function availableBetValue(uint256 _roundIndex, uint256 _ticket)
        public
        view
        override
        returns (uint256)
    {
        BettingRound storage round = rounds[_roundIndex];
        require(_ticket == 0 || _ticket ==1, "illegal ticket");
        if (_roundIndex != getCurrentRound()) {
            return 0;
        }

        if(_ticket == 0){
            return round.sponsorFunds + round.oddBetsValue + round.evenBetsValue - round.evenBetsValue * (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM;
        }else{
            return round.sponsorFunds + round.oddBetsValue + round.evenBetsValue - round.oddBetsValue * (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM;
        }
    }

    function placeBet(
        uint256 _roundIndex,
        uint256 _betAmt,
        uint256 _ticket
    ) external override roundBettable(_roundIndex) {

        // may implictly include prefix zeros
        BettingRound storage round = rounds[_roundIndex];
        require(_ticket == 0 || _ticket ==1, "illegal ticket");

        // update round.betsValue;
        uint256 potentialProfit = _betAmt * betUnitValue * (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM;
        if(_ticket == 0){
            // bet on even
            require(potentialProfit < round.sponsorFunds
                    + round.oddBetsValue
                    + round.evenBetsValue
                    - round.evenBetsValue * (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM, "illegal bet amount");
            // 1, record in evenbets
            round.evenBets.push(Bet(msg.sender, _betAmt));
            // 2, update lockedinevenbets
            round.evenBetsValue += _betAmt * betUnitValue;
        }else{
            // bet on odd
            require(potentialProfit < round.sponsorFunds
                    + round.oddBetsValue
                    + round.evenBetsValue
                    - round.oddBetsValue * (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM, "illegal bet amount");
            // 1, record in evenbets
            round.oddBets.push(Bet(msg.sender, _betAmt));
            // 2, update lockedinevenbets
            round.oddBetsValue += _betAmt * betUnitValue;
        }

        emit Submit(
            _roundIndex,
            msg.sender,
            round.oddBets.length + round.evenBets.length,
            _betAmt,
            _ticket
        );
    }

    function settleRound(
        uint256 _roundIndex,
        uint256 _limit,
        uint256 _autoDistribute
    )
        external
        override
        roundSettleable(_roundIndex)
    {
        BettingRound storage round = rounds[_roundIndex];
        uint256 winningTicket = round.winningTicket;
        Bet[] storage winningBets;
        Bet[] storage losingBets;
        if(winningTicket==2){
            winningBets = round.evenBets;
            losingBets = round.oddBets;
        }else{
            winningBets = round.oddBets;
            losingBets = round.evenBets;
        }

        uint256 base = round.settleCursor;
        if (_limit == 0 || _limit > winningBets.length - base) {
            _limit = winningBets.length - base;
        }

        for (uint256 i = 0; i < _limit; i++) {
            Bet storage curr = winningBets[base+i];
            round.winners.add(curr.user);
            round.winnerPendingRewards[curr.user] = round.winnerPendingRewards[curr.user] +
                (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM * curr.amount * betUnitValue;
            //            round.roundRewards = round.roundRewards + (RATIO_DENOM - houseEdge) * 2 / RATIO_DENOM * curr.amount * betUnitValue;
        }
        round.settleCursor = base.add(_limit);


        // when the round has been fully settled, update roundRewards and distribute rewards to all winners.
        if (round.settleCursor == winningBets.length) {
            // setup next round
            BettingRound storage nextRound = rounds[_roundIndex +1];
            round.dividends = (round.oddBetsValue + round.evenBetsValue) * houseEdge / RATIO_DENOM;
            if(winningTicket==2){
                // even bets wins
                nextRound.sponsorFunds = round.sponsorFunds
                    + round.evenBetsValue
                    + round.oddBetsValue
                    - round.evenBetsValue * (RATIO_DENOM - houseEdge)*2 /RATIO_DENOM
                    - round.dividends;
            }else{
                // odd bets win
                nextRound.sponsorFunds = round.sponsorFunds
                    + round.evenBetsValue
                    + round.oddBetsValue
                    - round.oddBetsValue * (RATIO_DENOM - houseEdge)*2 /RATIO_DENOM
                    - round.dividends;
            }
            lastSettledRoundIndex = lastSettledRoundIndex + 1;

            if (_autoDistribute > 0) {
                _distributeRewards(_roundIndex);
                _distributeDividend(round.dividends);
            }
            emit SettleRoundFully(
                _roundIndex,
                winningBets.length + losingBets.length,
                round.winningTicket
            );
        }
    }

    /**
     * Requests randomness
     */
    function requestRandomness(uint256 roundIndex) public returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= linkFee, "Not enough LINK - fill contract with faucet");
        requestId = requestRandomness(linkKeyHash, linkFee);
        requestId2roundIndex[requestId] = roundIndex;
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        uint256 roundIndex = requestId2roundIndex[requestId];
        rounds[roundIndex].winningTicket = randomness % 2 + 1;
        emit randomnessFulfilled(requestId, randomness);
    }

    function depositLink(uint256 amount) external {
        LINK.transfer(address(this), amount);
    }

    function withdrawLink() external {}

    function _distributeRewards(uint256 _roundIndex) internal {
        BettingRound storage round = rounds[_roundIndex];
        address winner;
        uint256 pendingRewards;
        for (uint256 i = 0; i < round.winners.length(); i++) {
            winner = round.winners.at(i);
            pendingRewards = round.winnerPendingRewards[winner];

            require(pendingRewards > 0, "rewards already distributed");
            gameToken.safeTransfer(winner, pendingRewards);
            round.winnerPendingRewards[winner] = 0;

            emit DistributeRewards(_roundIndex, winner, pendingRewards);
        }
    }

    function _distributeDividend(uint256 _dividend) internal {
        if (_dividend == 0) {
            return;
        }
        assert(gameToken.balanceOf(address(this)) >= _dividend);
        gameToken.safeTransfer(beneficiary, _dividend);
        emit DistributeDividend( getCurrentRound(), beneficiary, _dividend);
    }

    function claimRewards(uint256 _roundIndex) external {
        address winner = _msgSender();
        require(
            rounds[_roundIndex].winnerPendingRewards[winner] > 0,
            "rewards already distributed"
        );
        gameToken.safeTransfer(
            winner,
            rounds[_roundIndex].winnerPendingRewards[winner]
        );
        rounds[_roundIndex].winnerPendingRewards[winner] = 0;
        emit DistributeRewards(
            _roundIndex,
            winner,
            rounds[_roundIndex].winnerPendingRewards[winner]
        );
    }

    function totalValueLocked() external view override returns (uint256) {
        return gameToken.balanceOf(address(this));
    }

    function setBeneficiary(address _addr) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        _setupRole(DEFAULT_ADMIN_ROLE, _addr);
        beneficiary = _addr;
    }

    function setDefaultAdmin(address admin) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setupRole(DEFAULT_ADMIN_ROLE, admin);
    }

    function terminate() external override onlyRole(DEFAULT_ADMIN_ROLE) {
        require(!terminated, "terminated already");
        terminated = true;
        terminationRoundIndex = lastSettledRoundIndex+1;
    }

    function overlookRound(
        uint256 _roundIndex,
        uint256 _limit,
        uint256 _autoDistribute
                           )
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        // the overlook process is similar as normal settlement, the only difference is that every player wins back his principle.
        BettingRound storage round = rounds[_roundIndex];

        Bet[] storage oddBets = round.oddBets;
        Bet[] storage evenBets = round.evenBets;

        uint256 oddLen = oddBets.length;
        uint256 evenLen = evenBets.length;

        uint256 base = round.settleCursor;
        uint256 end; // end is in the array.
        if(_limit ==0){
            end = oddLen + evenLen -1;
        }else{
            end = base + _limit;
            if (end >oddLen + evenLen -1){
                end = oddLen + evenLen -1;
            }
        }

        if(base < oddLen){
            if(end < oddLen){
                _overlookBets(round, oddBets, 0, oddLen - 1);
            }else{
                _overlookBets(round, oddBets, 0, oddLen - 1);
                _overlookBets(round, evenBets, 0, end - (oddLen-1));
            }
        }else{
            _overlookBets(round, evenBets, base-(oddLen-1), end -(oddLen-1));
        }

        round.settleCursor = end;

        // update roundRewards and distribute rewards to all winners, if the round has been fully settled
        if (round.settleCursor == oddLen+evenLen -1) {
            // setup next round
            BettingRound storage nextRound = rounds[_roundIndex +1];
            nextRound.dividends = 0;
            nextRound.sponsorFunds = round.sponsorFunds;
            lastSettledRoundIndex = lastSettledRoundIndex +1;

            if (_autoDistribute > 0) {
                _distributeRewards(_roundIndex);
            }
        }
    }

    function _overlookBets(BettingRound storage round, Bet[] storage bets, uint256 from, uint256 end) internal {
        for (uint256 i = from; i <= end; i++) {
            Bet storage curr = bets[i];
            round.winners.add(curr.user);
            round.winnerPendingRewards[curr.user] = round.winnerPendingRewards[curr.user]
                + curr.amount * betUnitValue;
        }
    }

    // 1, no sponsorFundsCap; 2, afterSponsorFunds added, firstRound begins; 3, rounds start at index 2;
    function addSponsorFunds(uint256 _wantAmt)
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        gameToken.safeTransferFrom(msg.sender, address(this), _wantAmt);
        if (lastSettledRoundIndex == -1){
            lastSettledRoundIndex = 0;
            rounds[1].sponsorFunds += _wantAmt;
        }else{
            rounds[getCurrentRound()].sponsorFunds = rounds[getCurrentRound()].sponsorFunds + _wantAmt;
        }
    }

}

