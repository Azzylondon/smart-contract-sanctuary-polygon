    }

    function setUsdaAmount(address _token, uint256 _amount) external override {
        _onlyGov();

        uint256 usdaAmount = usdaAmounts[_token];
        if (_amount > usdaAmount) {
            _increaseUsdaAmount(_token, _amount.sub(usdaAmount));
            return;
        }

        _decreaseUsdaAmount(_token, usdaAmount.sub(_amount));
    }

    // the governance controlling this function should have a timelock
    function upgradeVault(address _newVault, address _token, uint256 _amount) external {
        _onlyGov();
        IERC20(_token).safeTransfer(_newVault, _amount);
    }


    function depositeInsuranceFund(address _token, uint256 _amount) external {
        _onlyGov();
        IERC20(_token).safeTransferFrom(msg.sender, address(this), _amount);
        uint256 tokenAmount = _transferIn(_token);
        insuranceFund[_token] = insuranceFund[_token].add(tokenAmount);
    }

    function withdrawInsuranceFund(address _token, uint256 _amount) external  {
        _onlyGov();
        uint256 insuranceFundAmount = insuranceFund[_token];
        _validate(_amount > 0 && insuranceFundAmount >= _amount, 0);
        insuranceFund[_token] = insuranceFundAmount.sub(_amount);
        _transferOut(_token, _amount, msg.sender);
    }
    // deposit into the pool without minting USDA tokens
    // useful in allowing the pool to become over-collaterised
    function directPoolDeposit(address _token) external override nonReentrant {
        _validate(whitelistedTokens[_token], 14);
        uint256 tokenAmount = _transferIn(_token);
        _validate(tokenAmount > 0, 15);
        _increasePoolAmount(_token, tokenAmount);
        emit DirectPoolDeposit(_token, tokenAmount);
    }

    function addLiquidity(address _token, address _receiver) external override nonReentrant returns (uint256) {
        _validateManager();
        _validate(whitelistedTokens[_token], 16);
        useSwapPricing = true;

        uint256 tokenAmount = _transferIn(_token);
        _validate(tokenAmount > 0, 17);

        updateFundingAndBorrowRate(_token, _token);

        uint256 price = getMinPrice(_token);

        uint256 usdaAmount = tokenAmount.mul(price).div(PRICE_PRECISION);
        usdaAmount = adjustForDecimals(usdaAmount, _token, usda);
        _validate(usdaAmount > 0, 18);

        uint256 feeBasisPoints = vaultUtils.getBuyUsdaFeeBasisPoints(_token, usdaAmount);
        uint256 amountAfterFees = _collectSwapFees(_token, tokenAmount, feeBasisPoints);
        uint256 mintAmount = amountAfterFees.mul(price).div(PRICE_PRECISION);
        mintAmount = adjustForDecimals(mintAmount, _token, usda);

        _increaseUsdaAmount(_token, mintAmount);
        _increasePoolAmount(_token, amountAfterFees);

        IUSDA(usda).mint(_receiver, mintAmount);

        emit AddLiquidity(_receiver, _token, tokenAmount, mintAmount, feeBasisPoints);

        useSwapPricing = false;
        return mintAmount;
    }

    function removeLiquidity(address _token, address _receiver) external override nonReentrant returns (uint256) {
        _validateManager();
        _validate(whitelistedTokens[_token], 19);
        useSwapPricing = true;

        uint256 usdaAmount = _transferIn(usda);
        _validate(usdaAmount > 0, 20);

        updateFundingAndBorrowRate(_token, _token);

        uint256 redemptionAmount = getRedemptionAmount(_token, usdaAmount);
        _validate(redemptionAmount > 0, 21);

        _decreaseUsdaAmount(_token, usdaAmount);
        _decreasePoolAmount(_token, redemptionAmount);

        IUSDA(usda).burn(address(this), usdaAmount);

        // the _transferIn call increased the value of tokenBalances[usda]
        // usually decreases in token balances are synced by calling _transferOut
        // however, for usda, the tokens are burnt, so _updateTokenBalance should
        // be manually called to record the decrease in tokens
        _updateTokenBalance(usda);

        uint256 feeBasisPoints = vaultUtils.getSellUsdaFeeBasisPoints(_token, usdaAmount);
        uint256 amountOut = _collectSwapFees(_token, redemptionAmount, feeBasisPoints);
        _validate(amountOut > 0, 22);

        _transferOut(_token, amountOut, _receiver);

        emit RemoveLiquidity(_receiver, _token, usdaAmount, amountOut, feeBasisPoints);

        useSwapPricing = false;
        return amountOut;
    }

    function swap(address _tokenIn, address _tokenOut, address _receiver) external override nonReentrant returns (uint256) {
        _validate(isSwapEnabled, 23);
        _validate(whitelistedTokens[_tokenIn], 24);
        _validate(whitelistedTokens[_tokenOut], 25);
        _validate(_tokenIn != _tokenOut, 26);

        useSwapPricing = true;

        updateFundingAndBorrowRate(_tokenIn, _tokenIn);
        updateFundingAndBorrowRate(_tokenOut, _tokenOut);

        uint256 amountIn = _transferIn(_tokenIn);
        _validate(amountIn > 0, 27);

        uint256 priceIn = getMinPrice(_tokenIn);
        uint256 priceOut = getMaxPrice(_tokenOut);

        uint256 amountOut = amountIn.mul(priceIn).div(priceOut);
        amountOut = adjustForDecimals(amountOut, _tokenIn, _tokenOut);

        // adjust usdaAmounts by the same usdaAmount as debt is shifted between the assets
        uint256 usdAmount = amountIn.mul(priceIn).div(PRICE_PRECISION);
        usdAmount = adjustForDecimals(usdAmount, _tokenIn, usda);

        uint256 feeBasisPoints = vaultUtils.getSwapFeeBasisPoints(_tokenIn, _tokenOut, usdAmount);
        uint256 amountOutAfterFees = _collectSwapFees(_tokenOut, amountOut, feeBasisPoints);

        _increaseUsdaAmount(_tokenIn, usdAmount);
        _decreaseUsdaAmount(_tokenOut, usdAmount);

        _increasePoolAmount(_tokenIn, amountIn);
        _decreasePoolAmount(_tokenOut, amountOut);

        _validateBufferAmount(_tokenOut);

        _transferOut(_tokenOut, amountOutAfterFees, _receiver);

        emit Swap(_receiver, _tokenIn, _tokenOut, amountIn, amountOut, amountOutAfterFees, feeBasisPoints);

        useSwapPricing = false;
        return amountOutAfterFees;
    }

    function increasePosition(address _account, address _collateralToken, address _indexToken, uint256 _sizeDelta, bool _isLong) external override nonReentrant {
        _validate(isLeverageEnabled, 28);
        _validateGasPrice();
        _validateRouter(_account);
        _validateTokens(_collateralToken, _indexToken, _isLong);
        vaultUtils.validateIncreasePosition(_account, _collateralToken, _indexToken, _sizeDelta, _isLong);

        updateFundingAndBorrowRate(_collateralToken, _indexToken);

        bytes32 key = getPositionKey(_account, _collateralToken, _indexToken, _isLong);
        Position storage position = positions[key];

        uint256 price =  getMarketPrice(_indexToken, _sizeDelta, _isLong);

        if (position.size == 0) {
            position.averagePrice = price;
        }

        if (position.size > 0 && _sizeDelta > 0) {
            position.averagePrice = getNextAveragePrice(_indexToken, position.size, position.averagePrice, _isLong, price, _sizeDelta, position.lastIncreasedTime);
        }

        uint256 fee = _collectMarginFees(_account, _collateralToken, _indexToken, _isLong, _sizeDelta, position.size, position.entryBorrowRate);
        uint256 collateralDelta = _transferIn(_collateralToken);
        // uint256 collateralDeltaUsd = tokenToUsdMin(_collateralToken, collateralDelta);
        position.collateral = position.collateral.add(collateralDelta);
        _validate(position.collateral >= fee, 29);
        // console.log("position.collateral >= fee out ");

        position.collateral = position.collateral.sub(fee);
        position.entryBorrowRate = getEntryBorrowRate(_collateralToken, _indexToken, _isLong);
        position.entryFundingRate = cumulativeFundingRates[_indexToken];
        position.size = position.size.add(_sizeDelta);
        position.lastIncreasedTime = block.timestamp;

        _validate(position.size > 0, 30);
        uint256 collateralUsd = tokenToUsdMin(_collateralToken, position.collateral);
        // _validatePosition(position.size, collateralUsd);
        validateLiquidation(_account, _collateralToken, _indexToken, _isLong, true);

        // reserve tokens to pay profits on the position
        uint256 reserveDelta = usdToTokenMax(_collateralToken, _sizeDelta);
        position.reserveAmount = position.reserveAmount.add(reserveDelta);
        _increaseReservedAmount(_collateralToken, reserveDelta);
        // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].add(collateralDelta.sub(fee));
        _increaseCollateralsPool(_collateralToken, collateralDelta.sub(fee));

        if (_isLong) {

            // update net position
            _decreaseNetPosition(_indexToken, usdToToken(_indexToken, _sizeDelta, price));
            _decreaseGlobalNetUsd(_indexToken, _sizeDelta);
        } else {

            _increaseNetPosition(_indexToken, usdToToken(_indexToken, _sizeDelta, price));
            _increaseGlobalNetUsd(_indexToken, _sizeDelta);
        }

        emit IncreasePosition(key, _account, _collateralToken, _indexToken, collateralDelta, _sizeDelta, _isLong, price, fee);
        emit UpdatePosition(key, position.size, position.collateral, position.averagePrice, position.entryBorrowRate, position.reserveAmount, position.realisedPnl, price);
    }


    function decreasePosition(address _account, address _collateralToken, address _indexToken, uint256 _collateralDelta, uint256 _sizeDelta, bool _isLong, address _receiver) external override nonReentrant returns (uint256) {
        _validateGasPrice();
        _validateRouter(_account);
        return _decreasePosition(_account, _collateralToken, _indexToken, _collateralDelta, _sizeDelta, _isLong, _receiver);
    }

    function _decreasePosition(address _account, address _collateralToken, address _indexToken, uint256 _collateralDelta, uint256 _sizeDelta, bool _isLong, address _receiver) private returns (uint256) {
        vaultUtils.validateDecreasePosition(_account, _collateralToken, _indexToken, _collateralDelta, _sizeDelta, _isLong, _receiver);
        updateFundingAndBorrowRate(_collateralToken, _indexToken);

        bytes32 key = getPositionKey(_account, _collateralToken, _indexToken, _isLong);
        Position storage position = positions[key];
        _validate(position.size > 0, 31);
        _validate(position.size >= _sizeDelta, 32);
        _validate(position.collateral >= _collateralDelta, 33);

        uint256 collateral = position.collateral;
        // scrop variables to avoid stack too deep errors
        {
        uint256 reserveDelta = position.reserveAmount.mul(_sizeDelta).div(position.size);
        position.reserveAmount = position.reserveAmount.sub(reserveDelta);
        _decreaseReservedAmount(_collateralToken, reserveDelta);
        }

        (uint256 tokenAmountOut, uint256 tokenAmountOutAfterFee) = _reduceCollateral(_account, _collateralToken, _indexToken, _collateralDelta, _sizeDelta, _isLong);
        uint256 price = getMarketPrice(_indexToken, _sizeDelta, !_isLong);

        if (!_isLong) {

            _decreaseNetPosition(_indexToken, usdToToken(_indexToken, _sizeDelta, position.averagePrice));
            _decreaseGlobalNetUsd(_indexToken, _sizeDelta);
        } else {
            _increaseNetPosition(_indexToken, usdToToken(_indexToken, _sizeDelta, position.averagePrice));
            _increaseGlobalNetUsd(_indexToken, _sizeDelta);
        }

        if (position.size != _sizeDelta) {
            position.entryBorrowRate = getEntryBorrowRate(_collateralToken, _indexToken, _isLong);
            position.entryFundingRate = cumulativeFundingRates[_indexToken];
            position.size = position.size.sub(_sizeDelta);

            // _validatePosition(position.size,  tokenToUsdMin(_collateralToken, position.collateral));
            validateLiquidation(_account, _collateralToken, _indexToken, _isLong, true);

            emit DecreasePosition(key, _account, _collateralToken, _indexToken, _collateralDelta, _sizeDelta, _isLong, price, tokenAmountOut.sub(tokenAmountOutAfterFee));
            emit UpdatePosition(key, position.size, position.collateral, position.averagePrice, position.entryBorrowRate, position.reserveAmount, position.realisedPnl, price);
        } else {
           
            emit DecreasePosition(key, _account, _collateralToken, _indexToken, _collateralDelta, _sizeDelta, _isLong, price, tokenAmountOut.sub(tokenAmountOutAfterFee));
            emit ClosePosition(key, position.size, position.collateral, position.averagePrice, position.entryBorrowRate, position.reserveAmount, position.realisedPnl);

            delete positions[key];
        }

        if (tokenAmountOutAfterFee > 0) {

            _transferOut(_collateralToken, tokenAmountOutAfterFee, _receiver);
            return tokenAmountOutAfterFee;
        }

        return 0;
    }

    function liquidatePosition(address _account, address _collateralToken, address _indexToken, bool _isLong, address _feeReceiver) external nonReentrant {
        if (inPrivateLiquidationMode) {
            _validate(isLiquidator[msg.sender], 34);
        }

        // set includeAmmPrice to false to prevent manipulated liquidations
        includeAmmPrice = false;

        updateFundingAndBorrowRate(_collateralToken, _indexToken);

        bytes32 key = getPositionKey(_account, _collateralToken, _indexToken, _isLong);
        Position memory position = positions[key];
        _validate(position.size > 0, 35);

        (uint256 liquidationState, uint256 marginFees) = validateLiquidation(_account, _collateralToken, _indexToken, _isLong, false);
        _validate(liquidationState != 0, 36);
        if (liquidationState == 2) {
            // max leverage exceeded but there is collateral remaining after deducting losses so decreasePosition instead
            _decreasePosition(_account, _collateralToken, _indexToken, 0, position.size, _isLong, _account);
            includeAmmPrice = true;
            return;
        }
        // Partial Liquidation
        // if(liquidationState == 3) {
        //     //marginFees = pnl
        //     uint256 marginRatio = marginFees.mul(BASIS_POINTS_DIVISOR).div(position.size);
        //     uint256 liquidateAmount = position.size.mul(BASIS_POINTS_DIVISOR.sub(marginRatio.mul(BASIS_POINTS_DIVISOR).div(liquidationMarginRatio.mul(2))));
        //     uint256 margin = marginFees.sub(liquidateAmount.mul(liquidationPenaltyRate).div(BASIS_POINTS_DIVISOR));
        //     //need to test here
        //     // _reduceCollateral(_account, _collateralToken, _indexToken, position.collateral-margin, liquidateAmount, _isLong); 
        //     // return;
        // }

        uint256 feeTokens = usdToTokenMin(_collateralToken, marginFees);
        feeReserves[_collateralToken] = feeReserves[_collateralToken].add(feeTokens);
        emit CollectMarginFees(_collateralToken, marginFees, feeTokens);

        _decreaseReservedAmount(_collateralToken, position.reserveAmount);

        uint256 markPrice = getMarketPrice(_indexToken, position.size, !_isLong);
        emit LiquidatePosition(key, _account, _collateralToken, _indexToken, _isLong, position.size, position.collateral, position.reserveAmount, position.realisedPnl, markPrice);

        // if (!_isLong && marginFees < position.collateral) {
        if (marginFees < position.collateral) {
            uint256 remainingCollateral = position.collateral.sub(marginFees);
            _increasePoolAmount(_collateralToken, usdToTokenMin(_collateralToken, remainingCollateral));
            // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].sub(remainingCollateral);
            _decreaseCollateralsPool(_collateralToken, remainingCollateral);
        }

        if (!_isLong) {

            _decreaseNetPosition(_indexToken, usdToToken(_indexToken, position.size, markPrice));
            _decreaseGlobalNetUsd(_indexToken, position.size);
        } else {
            _increaseNetPosition(_indexToken, usdToToken(_indexToken, position.size, markPrice));
            _increaseGlobalNetUsd(_indexToken, position.size);
        }

        delete positions[key];

        // pay the fee receiver using the pool, we assume that in general the liquidated amount should be sufficient to cover
        // the liquidation fees
        uint256 liquidationFee = usdToTokenMin(_collateralToken, liquidationFeeUsd);
        _decreasePoolAmount(_collateralToken, liquidationFee);
        _transferOut(_collateralToken, liquidationFee, _feeReceiver);

        includeAmmPrice = true;
    }

