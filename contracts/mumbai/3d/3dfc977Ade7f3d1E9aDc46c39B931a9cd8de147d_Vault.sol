    function _decreaseUsdaAmount(address _token, uint256 _amount) private {
        uint256 value = usdaAmounts[_token];
        // since USDA can be minted using multiple assets
        // it is possible for the USDA debt for a single asset to be less than zero
        // the USDA debt is capped to zero for this case
        if (value <= _amount) {
            usdaAmounts[_token] = 0;
            emit DecreaseUsdaAmount(_token, value);
            return;
        }
        usdaAmounts[_token] = value.sub(_amount);
        emit DecreaseUsdaAmount(_token, _amount);
    }

