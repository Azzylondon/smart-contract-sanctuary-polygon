    function _reduceCollateral(address _account, address _collateralToken, address _indexToken, uint256 _collateralDelta, uint256 _sizeDelta, bool _isLong) private returns (uint256, uint256) {
        bytes32 key = getPositionKey(_account, _collateralToken, _indexToken, _isLong);
        Position storage position = positions[key];
        
        uint256 fee = _collectMarginFees(_account, _collateralToken, _indexToken, _isLong, _sizeDelta, position.size, position.entryBorrowRate);
        bool hasProfit;
        uint256 adjustedDelta;

        // scope variables to avoid stack too deep errors
        {
        (bool _hasProfit, uint256 delta) = getDelta(_indexToken, position.size, position.averagePrice, _isLong, position.lastIncreasedTime, true);
        hasProfit = _hasProfit;
        // get the proportional change in pnl
        adjustedDelta = _sizeDelta.mul(delta).div(position.size);
        }

        uint256 tokenAmountOut = _isLong?  usdToTokenMin(_collateralToken, adjustedDelta):  usdToTokenMax(_collateralToken, adjustedDelta) ;
        // transfer profits out
        if (hasProfit && adjustedDelta > 0) {
            position.realisedPnl = position.realisedPnl + int256(adjustedDelta);

            _decreasePoolAmount(_collateralToken, tokenAmountOut);
        }

        if (!hasProfit && adjustedDelta > 0) {
            position.collateral = position.collateral.sub(tokenAmountOut);
            _decreaseCollateralsPool(_collateralToken, tokenAmountOut);
            // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].sub(tokenAmountOut);

            // transfer realised losses to the pool 

            _increasePoolAmount(_collateralToken, tokenAmountOut);

            position.realisedPnl = position.realisedPnl - int256(adjustedDelta);
            tokenAmountOut = 0;
        }

        // reduce the position's collateral by _collateralDelta
        // transfer _collateralDelta out
        if (_collateralDelta > 0) {
            if(!hasProfit && position.collateral < _collateralDelta) {
                tokenAmountOut = position.collateral;
                position.collateral = 0;

                // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].sub(tokenAmountOut);
                _decreaseCollateralsPool(_collateralToken, tokenAmountOut);
            } else {
                tokenAmountOut = tokenAmountOut.add(_collateralDelta);
                position.collateral = position.collateral.sub(_collateralDelta);

                // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].sub(_collateralDelta);
                _decreaseCollateralsPool(_collateralToken, _collateralDelta);
            }
            
            
            // _decreaseCollateralsPool(_collateralToken, _collateralDelta);
            
        }

        // if the position will be closed, then transfer the remaining collateral out
        if (position.size == _sizeDelta) {
            
            tokenAmountOut = tokenAmountOut.add(position.collateral);

            // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].sub(position.collateral);
            _decreaseCollateralsPool(_collateralToken, position.collateral);
            position.collateral = 0;
            // collateralsPool[_collateralToken] = 0;
        }

        // if the usdOut is more than the fee then deduct the fee from the usdOut directly
        // else deduct the fee from the position's collateral
        uint256 tokenAmountOutAfterFee = tokenAmountOut;
        if (tokenAmountOut > fee) {
            tokenAmountOutAfterFee = tokenAmountOut.sub(fee);

        } else {
            position.collateral = position.collateral.sub(fee);

            // collateralsPool[_collateralToken] = collateralsPool[_collateralToken].sub(fee);
            _decreaseCollateralsPool(_collateralToken, fee);
        }

        emit UpdatePnl(key, hasProfit, adjustedDelta);
        
        return (tokenAmountOut, tokenAmountOutAfterFee);
    }

