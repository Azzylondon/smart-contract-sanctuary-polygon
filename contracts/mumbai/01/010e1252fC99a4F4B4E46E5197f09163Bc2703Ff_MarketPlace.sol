pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract MarketPlace is ERC721Holder, Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;

    Counters.Counter private _itemIds;
    Counters.Counter private _tradeIds;

    IERC721 public immutable AUTH_NFT;
    IERC721 public immutable VIN_PASS;
    uint256 private _serviceFee;
    uint256 private _bidIncRate;

    event Listed(
        uint256 indexed itemId,
        uint256 tokenId,
        uint256 basePrice,
        uint256 startTime,
        uint256 expirationTime,
        address seller,
        SaleKind saleKind
    );

    event CancelListing(
        uint256 indexed itemId,
        uint256 indexed tokenId,
        address indexed seller
    );

    event Sold(
        uint256 indexed itemId,
        uint256 indexed tokenId,
        address indexed buyer,
        address seller
    );

    event Bid(uint256 indexed itemId, uint256 amount, address indexed bidder);

    event MakeOffer(
        uint256 indexed itemId,
        uint256 amount,
        address indexed offerer
    );

    event TradeOffer(
        uint256 indexed tradeId,
        uint256 tokenId,
        uint256 amount,
        address trader
    );

    event WithdrawTradeOffer(uint256 indexed tradeid, address trader);

    event WithdrawServiceFee(address indexed withdrawer, uint256 amount);

    modifier accessVINPass(address caller) {
        require(VIN_PASS.balanceOf(caller) != 0, "No access without VIN Pass");
        _;
    }

    enum SaleKind {
        fixedSale,
        AuctionSale,
        exchangeSale
    }

    struct ItemInfo {
        uint256 tokenId;
        uint256 basePrice;
        uint256 startTime;
        uint256 expirationTime;
        address seller;
        SaleKind saleKind;
    }

    struct BidInfo {
        address bidder;
        uint256 bid;
    }

    struct OfferInfo {
        address offerer;
        uint256 price;
    }

    struct TradeInfo {
        uint256 itemId;
        uint256 amount;
        uint256 nftId;
        address trader;
    }

    mapping(uint256 => ItemInfo) public marketItems;
    mapping(uint256 => BidInfo) public biddingDetails;
    mapping(uint256 => TradeInfo) public tradeDetails;
    mapping(uint256 => uint256[]) public tradeIds;
    mapping(address => uint256[]) public userTradeoffers;

    constructor(address authNft, address vinPass) {
        require(authNft != address(0), "Invalid NFT address");
        require(vinPass != address(0), "Invalid VIN Pass address");
        AUTH_NFT = IERC721(authNft);
        VIN_PASS = IERC721(vinPass);
        _serviceFee = 500;
        _bidIncRate = 2; // 2%
    }

    function wineListing(
        uint256 _tokenId,
        uint256 _basePrice,
        uint256 _startTime,
        uint256 _expirationTime,
        address _seller,
        SaleKind _saleKind
    ) external accessVINPass(msg.sender) {
        _checkListingDetails(
            _tokenId,
            _basePrice,
            _startTime,
            _expirationTime,
            _seller,
            _saleKind
        );
        _itemIds.increment();
        uint256 _itemId = _itemIds.current();

        marketItems[_itemId] = ItemInfo(
            _tokenId,
            _basePrice,
            _startTime,
            _expirationTime,
            msg.sender,
            _saleKind
        );

        emit Listed(
            _itemId,
            _tokenId,
            _basePrice,
            _startTime,
            _expirationTime,
            msg.sender,
            _saleKind
        );

        AUTH_NFT.safeTransferFrom(msg.sender, address(this), _tokenId);
    }

    function cancelListing(uint256 _itemId)
        external
        accessVINPass(msg.sender)
        nonReentrant
    {
        ItemInfo memory _item = marketItems[_itemId];

        require(
            _item.seller == msg.sender,
            "cancelListing: Unauthorized access"
        );

        if (_item.saleKind == SaleKind.AuctionSale) {
            BidInfo memory _bidDetail = biddingDetails[_itemId];
            if (_bidDetail.bidder != address(0)) {
                delete biddingDetails[_itemId];

                _payment(_bidDetail.bidder, _bidDetail.bid);
            }
        }

        emit CancelListing(_itemId, _item.tokenId, msg.sender);

        delete (marketItems[_itemId]);

        AUTH_NFT.safeTransferFrom(address(this), msg.sender, _item.tokenId);
    }

    function buyWine(uint256 _itemId)
        external
        payable
        accessVINPass(msg.sender)
        nonReentrant
    {
        ItemInfo memory _item = marketItems[_itemId];
        uint256 amountPaid = msg.value;

        require(msg.sender != _item.seller, "buyWine: Buyer == Seller");
        require(
            _item.saleKind == SaleKind.fixedSale,
            "buyWine: Not an auction"
        );
        require(amountPaid >= _item.basePrice, "BuyWine: Price not met");

        uint256 excessAmount = amountPaid - _item.basePrice;
        if (excessAmount != 0) {
            _payment(msg.sender, excessAmount);
        }

        uint256 serviceFee_ = _computeServiceFee(amountPaid - excessAmount);
        uint256 priceToPay = amountPaid - excessAmount - serviceFee_;

        emit Sold(_itemId, _item.tokenId, msg.sender, _item.seller);

        delete marketItems[_itemId];
        _payment(_item.seller, priceToPay);

        AUTH_NFT.safeTransferFrom(address(this), msg.sender, _item.tokenId);
    }

    function placeBid(uint256 _itemId)
        external
        payable
        accessVINPass(msg.sender)
    {
        ItemInfo memory _item = marketItems[_itemId];
        uint256 bidOffered = msg.value;

        require(msg.sender != _item.seller, "placeBid: Bidder == Seller");
        require(
            _item.saleKind == SaleKind.AuctionSale,
            "placeBid: Not an Auction"
        );

        require(
            block.timestamp <= _item.expirationTime,
            "placeBid: Auction closed"
        );

        BidInfo memory _bidDetail = biddingDetails[_itemId];

        uint256 _previousBid = _bidDetail.bid;

        if (_previousBid == 0) {
            require(bidOffered >= _item.basePrice, "placeBid: Price not met");
        } else {
            require(
                _isIncreasedBid(bidOffered, _previousBid),
                "placeBid: bid diff failed"
            );
        }
        address _previousBidder = _bidDetail.bidder;

        biddingDetails[_itemId] = BidInfo(msg.sender, bidOffered);

        emit Bid(_itemId, bidOffered, msg.sender);

        // return previous bid to user
        _payment(_previousBidder, _previousBid);
    }

    function claimNFT(uint256 _itemId)
        external
        accessVINPass(msg.sender)
        nonReentrant
    {
        ItemInfo memory _item = marketItems[_itemId];
        BidInfo memory _bidDetail = biddingDetails[_itemId];

        require(
            _item.saleKind == SaleKind.AuctionSale,
            "claimNFT: Not an Auction"
        );
        require(
            msg.sender == _bidDetail.bidder || msg.sender == _item.seller,
            "claimNFT: Unauthorized access"
        );
        require(
            block.timestamp > _item.expirationTime,
            "claimNFT: Auction not ended"
        );

        emit Sold(_itemId, _item.tokenId, _bidDetail.bidder, _item.seller);

        uint256 serviceFee_ = _computeServiceFee(_bidDetail.bid);
        uint256 priceToPay = _bidDetail.bid - serviceFee_;

        delete biddingDetails[_itemId];
        delete marketItems[_itemId];

        // transfer payment to seller
        _payment(_item.seller, priceToPay);

        AUTH_NFT.safeTransferFrom(
            address(this),
            _bidDetail.bidder,
            _item.tokenId
        );
    }

    function createtradeOffer(uint256 _itemId, uint256 _tokenId)
        external
        payable
        accessVINPass(msg.sender)
    {
        ItemInfo memory _item = marketItems[_itemId];
        uint256 priceOffered = msg.value;
        address _sender = msg.sender;

        require(_item.tokenId != 0, "tradeOffer: itemId not exist");
        require(
            _item.saleKind == SaleKind.fixedSale,
            "tradeOffer: Not a fixedSale"
        );
        require(_sender != _item.seller, "tradeOffer: Offerer == Seller");
        _tradeIds.increment();
        uint256 _tradeId = _tradeIds.current();

        tradeIds[_itemId].push(_tradeId);
        userTradeoffers[_sender].push(_tradeId);

        TradeInfo storage _tradeoffer = tradeDetails[_tradeId];
        _tradeoffer.itemId = _itemId;
        _tradeoffer.amount = priceOffered;
        _tradeoffer.nftId = _tokenId;
        _tradeoffer.trader = _sender;

        AUTH_NFT.safeTransferFrom(_sender, address(this), _tokenId);
        emit TradeOffer(_tradeId, _tokenId, priceOffered, _sender);
    }

    function withdrawtradeOffer(uint256 _tradeId)
        external
        accessVINPass(msg.sender)
    {
        TradeInfo memory _tradeoffer = tradeDetails[_tradeId];
        address caller = msg.sender;
        address _trader = _tradeoffer.trader;
        uint256 id = _tradeoffer.nftId;

        require(
            _trader != address(0),
            "withdrawtradeOffer: trade not exist or already withdraw "
        );
        require(_trader == caller, "withdrawtradeOffer: Unauthorized access");

        uint256 amount = _tradeoffer.amount;
        if (amount > 0) {
            _payment(caller, amount);
        }

        delete tradeDetails[_tradeId];
        AUTH_NFT.safeTransferFrom(address(this), caller, id);

        emit WithdrawTradeOffer(_tradeId, _trader);
    }

    function acceptTradeOffer(uint256 _tradeId)
        external
        accessVINPass(msg.sender)
    {
        TradeInfo memory _tradeoffer = tradeDetails[_tradeId];
        uint256 _itemId = _tradeoffer.itemId;
        require(_itemId != 0, "acceptTradeOffer: _tradeId not exist");
        ItemInfo memory _item = marketItems[_itemId];
        require(msg.sender == _item.seller, "acceptOffer: Unauthorized access");
        uint256 _listerdnftId = _item.tokenId;
        uint256 _tradernftId = _tradeoffer.nftId;
        uint256 amount = _tradeoffer.amount;
        address _lister = _item.seller;
        address _trader = _tradeoffer.trader;

        AUTH_NFT.safeTransferFrom(address(this), _trader, _listerdnftId);

        AUTH_NFT.safeTransferFrom(address(this), _lister, _tradernftId);

        if (amount > 0) {
            _payment(_lister, amount);
        }

        delete tradeDetails[_tradeId];
        delete marketItems[_itemId];

        emit Sold(_itemId, _listerdnftId, _trader, _lister);
    }

    function withdrawServiceFee() external onlyOwner nonReentrant {
        uint256 _balance = address(this).balance;
        require(_balance != 0, "withdrawServiceFee: Zero service fee");

        emit WithdrawServiceFee(msg.sender, _balance);
        _payment(msg.sender, _balance);
    }

    function _payment(address _receiver, uint256 _amount) internal {
        (bool isSuccess, ) = _receiver.call{value: _amount}("");
        require(isSuccess == true, "_payment: Payment failed");
    }

    function _checkListingDetails(
        uint256 _tokenId,
        uint256 _basePrice,
        uint256 _startTime,
        uint256 _expirationTime,
        address _seller,
        SaleKind _saleKind
    ) internal view {
        require(_tokenId != 0, "listWine: TokenId cannot be zero");
        require(_seller != address(0), "listWine: Invalid seller");
        require(_basePrice != 0, "listWine: Base price cannot be zero");
        require(_startTime >= block.timestamp, "listWine: Invalid start time");

        if (_saleKind == SaleKind.AuctionSale) {
            require(
                _expirationTime > _startTime,
                "listWine: Expires before start time"
            );
        }
    }

    function _isIncreasedBid(uint256 _amount, uint256 _prevbid)
        internal
        view
        returns (bool)
    {
        return _amount >= ((_prevbid * (_bidIncRate + 100)) / 100);
    }

    function _computeServiceFee(uint256 _amount)
        internal
        view
        returns (uint256)
    {
        return (_amount * _serviceFee) / 10000;
    }
}

