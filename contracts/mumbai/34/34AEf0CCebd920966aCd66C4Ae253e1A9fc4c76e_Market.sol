import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";

import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";

import "hardhat/console.sol";

contract Market is Ownable, Pausable {
    ISwapRouter public immutable swapRouter;
    uint24 public poolFee;

    mapping(address => mapping(uint256 => Sale)) public sales;

    event SaleCreated(
        address indexed token,
        uint256 indexed tokenId,
        uint256 amount,
        uint256 price,
        address priceToken,
        address seller,
        address receiver
    );

    event Purchased(
        address indexed token,
        uint256 indexed tokenId,
        address buyer,
        address seller,
        uint256 amount,
        uint256 price,
        address priceToken,
        address payableToken,
        uint256 payableAmount,
        address receiver
    );

    struct Sale {
        address token;
        uint256 tokenId;
        uint256 amount;
        uint256 price;
        address priceToken;
        address seller;
        address receiver;
    }

    constructor(ISwapRouter _swapRouter) {
        swapRouter = _swapRouter;
        poolFee = 3000;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function swapExactOutputSingle(
        address tokenIn,
        address tokenOut,
        address recipient,
        uint256 amountOut,
        uint256 amountInMaximum
    ) internal returns (uint256 amountIn) {
        TransferHelper.safeTransferFrom(
            tokenIn,
            _msgSender(),
            address(this),
            amountInMaximum
        );

        console.log("amountInMaximum %s %s", tokenIn, amountInMaximum);

        TransferHelper.safeApprove(
            tokenIn,
            address(swapRouter),
            amountInMaximum
        );

        console.log("amountInMaximum %s %s", tokenIn, amountInMaximum);

        ISwapRouter.ExactOutputSingleParams memory params = ISwapRouter
            .ExactOutputSingleParams({
                tokenIn: tokenIn,
                tokenOut: tokenOut,
                fee: poolFee,
                recipient: recipient,
                deadline: block.timestamp,
                amountOut: amountOut,
                amountInMaximum: amountInMaximum,
                sqrtPriceLimitX96: 0
            });

        amountIn = swapRouter.exactOutputSingle(params);

        console.log("ExactOutputSingleParams %s", amountIn);

        if (amountIn < amountInMaximum) {
            TransferHelper.safeApprove(tokenIn, address(swapRouter), 0);
            TransferHelper.safeTransfer(
                tokenIn,
                _msgSender(),
                amountInMaximum - amountIn
            );
        }
    }

    function register(
        address token,
        uint256 tokenId,
        uint16 amount,
        uint256 price,
        address priceToken,
        address receiver
    ) public {
        require(amount > 0, "Market: invalid amount");
        require(priceToken != address(0), "Market: invalid price token");
        require(receiver != address(0), "Market: receiver the zero address");
        require(
            IERC1155(token).balanceOf(_msgSender(), tokenId) >= amount,
            "Market: amount exceeds balance"
        );

        Sale memory sale = Sale(
            token,
            tokenId,
            amount,
            price,
            priceToken,
            _msgSender(),
            receiver
        );

        sales[token][tokenId] = sale;

        emit SaleCreated(
            sale.token,
            sale.tokenId,
            sale.amount,
            sale.price,
            sale.priceToken,
            sale.seller,
            sale.receiver
        );
    }

    function buy(
        address token,
        uint256 tokenId,
        uint256 amount,
        address payableToken,
        uint256 payableMaxAmount,
        address receiver
    ) public {
        Sale memory sale = sales[token][tokenId];

        require(amount > 0, "Market: invalid amount");
        require(payableToken != address(0), "Market: invalid payable token");
        require(receiver != address(0), "Market: receiver the zero address");
        require(
            IERC1155(sale.token).balanceOf(sale.seller, sale.tokenId) >= amount,
            "Market: amount exceeds balance"
        );

        uint256 destAmount = sale.price * amount;

        (address royaltyReceiver, uint256 royaltyAmount) = IERC2981(sale.token)
            .royaltyInfo(sale.tokenId, destAmount);

        // If saller is owner of the token
        if (royaltyReceiver == sale.receiver) {
            swapExactOutputSingle(
                payableToken,
                sale.priceToken,
                sale.receiver,
                destAmount,
                payableMaxAmount
            );
        } else {
            uint256 receiverAmount = destAmount - royaltyAmount;
            uint256 royaltyPerscent = royaltyAmount / destAmount;

            swapExactOutputSingle(
                payableToken,
                sale.priceToken,
                royaltyReceiver,
                royaltyAmount,
                payableMaxAmount * royaltyPerscent
            );

            swapExactOutputSingle(
                payableToken,
                sale.priceToken,
                sale.receiver,
                receiverAmount,
                payableMaxAmount * (1 - royaltyPerscent)
            );
        }

        IERC1155(token).safeTransferFrom(
            sale.seller,
            receiver,
            sale.tokenId,
            amount,
            ""
        );

        emit Purchased(
            sale.token,
            sale.tokenId,
            sale.seller,
            _msgSender(),
            sale.amount,
            sale.price,
            sale.priceToken,
            payableToken,
            payableMaxAmount,
            receiver
        );
    }
}

