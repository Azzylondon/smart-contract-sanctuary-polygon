pragma solidity ^0.8.8;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "./interfaces/IProtocol.sol";
import "./interfaces/IWithdrawBatcher.sol";
import "./lib/WithdrawalBatcherLib.sol";
import "./tunnel/FxBaseChildTunnel.sol";
import {WithdrawBatch} from "./type/Batch.sol";
import {RedeemedData, RedemptionData} from "./type/Tunnel.sol";
import {Errors} from "./lib/helpers/Error.sol";
import "hardhat/console.sol";

contract WithdrawBatcher is
    Initializable,
    IWithdrawBatcher,
    FxBaseChildTunnel,
    ReentrancyGuardUpgradeable,
    AccessControlUpgradeable
{
    using WithdrawalBatcherLib for WithdrawBatch;

    bytes32 public constant GOVERNOR_ROLE = keccak256("GOVERNOR_ROLE");

    IERC20L2 private _usdc;
    IL2Factory private _factory;
    IWhitelist private _whitelist;
    uint256 private _currentBatch;

    mapping(uint256 => WithdrawBatch) private _batch;

    modifier onlyWhitelisted() {
        require(
            _whitelist.whitelisted(_msgSender()),
            Errors.AC_USER_NOT_WHITELISTED
        );
        _;
    }

    function initialize(
        address admin,
        address usdcAddress,
        address factoryAddress,
        address whitelistAddress,
        address fxChild
    ) public virtual initializer {
        _usdc = IERC20L2(usdcAddress);
        _factory = IL2Factory(factoryAddress);
        _whitelist = IWhitelist(whitelistAddress);
        _setupRole(DEFAULT_ADMIN_ROLE, admin);
        FxBaseChildTunnel.setInitialParams(fxChild);
    }

    function redeem(bytes32[] memory protocols, uint256[] memory amounts)
        public
        virtual
        override
        onlyWhitelisted
        nonReentrant
        returns (bool)
    {
        address user = _msgSender();
        for (uint256 i = 0; i < protocols.length; i++) {
            address ta = _factory.fetchTokenAddressL2(protocols[i]);
            require(
                IERC20(ta).allowance(user, address(this)) >= amounts[i],
                Errors.VL_INSUFFICIENT_ALLOWANCE
            );
            require(
                IERC20(ta).balanceOf(user) >= amounts[i],
                Errors.VL_INSUFFICIENT_BALANCE
            );
            IERC20(ta).transferFrom(user, address(this), amounts[i]);
            address pa = _factory.fetchProtocolAddressL2(protocols[i]);
            if (pa != address(0)) {
                instantRedeem(user, protocols[i], ta, pa, amounts[i]);
            } else {
                batch(user, protocols[i], amounts[i]);
            }
        }
        return true;
    }

    function execute()
        public
        virtual
        override
        onlyRole(GOVERNOR_ROLE)
        returns (bool)
    {
        uint256 batchId = _currentBatch;
        WithdrawBatch storage b = _batch[_currentBatch];
        require(b.status == BatchStatus.LIVE, Errors.VL_BATCH_NOT_ELLIGIBLE);

        RedemptionData memory tunneldata;

        uint256[] memory amounts = new uint256[](b.protocols.length);

        for (uint256 i = 0; i < b.protocols.length; i++) {
            amounts[i] = b.tokens[b.protocols[i]];
            IERC20L2(_factory.fetchTokenAddressL2(b.protocols[i])).withdraw(
                b.tokens[b.protocols[i]]
            );
        }

        tunneldata.batchId = batchId;
        tunneldata.protocols = b.protocols;
        tunneldata.amounts = amounts;

        b.status = BatchStatus.BATCHED;
        _currentBatch += 1;

        _sendMessageToRoot(abi.encode(tunneldata));

        return true;
    }

    function mockProcessMessageFromRoot(
        address _sender,
        bytes32[] memory protocol,
        uint256[] memory amount,
        uint256 batchId
    ) public {
        RedeemedData memory data;
        data.batchId = batchId;
        data.protocols = protocol;
        data.amounts = amount;

        _processMessageFromRoot(0, _sender, abi.encode(data));
    }

    function usdc() public view virtual override returns (IERC20L2) {
        return _usdc;
    }

    function currentBatch() public view virtual override returns (uint256) {
        return _currentBatch;
    }

    function factory() public view virtual override returns (IL2Factory) {
        return _factory;
    }

    function whitelist() public view override returns (IWhitelist) {
        return _whitelist;
    }

    function batchStatus(uint256 batchId)
        public
        view
        override
        returns (BatchStatus)
    {
        return _batch[batchId].status;
    }

    function fetchUserDeposit(
        bytes32 protocol,
        uint256 batchId,
        address user
    ) public view override returns (uint256) {
        WithdrawBatch storage b = _batch[batchId];
        return b.individualUser[protocol][user];
    }

    function fetchUsersInBatch(bytes32 protocol, uint256 batchId)
        public
        view
        override
        returns (address[] memory)
    {
        WithdrawBatch storage b = _batch[batchId];
        return b.users[protocol];
    }

    function fetchTotalDepositInBatch(bytes32 protocol, uint256 batchId)
        public
        view
        override
        returns (uint256)
    {
        WithdrawBatch storage b = _batch[batchId];
        return b.tokens[protocol];
    }

    function fetchProtocolsInBatch(uint256 batchId)
        public
        view
        override
        returns (bytes32[] memory)
    {
        WithdrawBatch storage b = _batch[batchId];
        return b.protocols;
    }

    function batch(
        address user,
        bytes32 protocol,
        uint256 amount
    ) internal virtual returns (bool) {
        bool update = _batch[_currentBatch].insert(user, protocol, amount);
        emit Batched(user, protocol, amount, _currentBatch);
        return update;
    }

    function instantRedeem(
        address user,
        bytes32 protocol,
        address tokenAddress,
        address protocolAddress,
        uint256 amount
    ) internal virtual returns (bool) {
        IERC20(tokenAddress).transfer(protocolAddress, amount);
        uint256 redeemed = IProtocol(protocolAddress).redeemProtocolToken(
            user,
            amount
        );
        emit Redeemed(user, protocol, amount, redeemed, _currentBatch);
        return true;
    }

    function _processMessageFromRoot(
        uint256 _stateId,
        address _sender,
        bytes memory _data
    ) internal override nonReentrant validateSender(_sender) {
        RedeemedData memory data = abi.decode(_data, (RedeemedData));
        WithdrawBatch storage b = _batch[data.batchId];

        require(
            b.status == BatchStatus.BATCHED,
            Errors.AC_BATCH_ALREADY_PROCESSED
        );

        for (uint256 i = 0; i < b.protocols.length; i++) {
            bytes32 protocol = b.protocols[i];
            address tokenAddress = _factory
                .fetchProtocolInfo(protocol)
                .stablecoinL2;
            for (uint256 j = 0; j < b.users[protocol].length; j++) {
                address user = b.users[protocol][j];
                uint256 percent = (b.individualUser[protocol][user] * 10**18) /
                    b.tokens[protocol];
                uint256 value = (percent * data.amounts[i]) / 10**18;
                IERC20(tokenAddress).transfer(user, value);
            }
        }
        b.status = BatchStatus.DISTRIBUTED;
    }
}

