pragma solidity ^0.8.0;

import "../VCMarketplaceBase.sol";

abstract contract VCMarketplaceFixedPrice is VCMarketplaceBase {
    struct ListingFixedPrice {
        address owner;
        uint256 price;
    }

    event ListedFixedPrice(address owner, IERC721 token, uint256 tokenId, uint256 price);
    event UnlistedFixedPrice(address owner, IERC721 token, uint256 tokenId, bool purchased);
    event Purchased(address purchaser, address owner, IERC721 token, uint256 tokenId, uint256 price);

    mapping(IERC721 => mapping(uint256 => ListingFixedPrice)) public fixedPriceListings;

    /**
     * @dev Lists a token in the Marketplace for a given price, which requires
     * sending the token from the seller to the Market. It can optionally
     * recieve a list of beneficiary projects with their corresponding fees in
     * bips.
     */
    function listFixedPrice(
        IERC721 _token,
        uint256 _tokenId,
        uint256 _price,
        uint256[] memory _projectIds,
        uint256[] memory _feesInBips
    ) public whenNotPaused onlyWhitelistedTokens(_token) {
        require(_projectIds.length == _feesInBips.length, "MARKETPLACE: unexpected project fees details");
        _requireActiveProjects(_projectIds);
        _setFees(_token, _tokenId, _projectIds, _feesInBips);
        _list(_token, _tokenId);
        fixedPriceListings[_token][_tokenId] = ListingFixedPrice({owner: msg.sender, price: _price});
        emit ListedFixedPrice(msg.sender, _token, _tokenId, _price);
    }

    /**
     * @dev Removes the token listing from the Marketplace and sends back the
     * asset to the seller or token owner.
     */
    function unlist(IERC721 _token, uint256 _tokenId) public onlyWhitelistedTokens(_token) {
        ListingFixedPrice memory listing = fixedPriceListings[_token][_tokenId];
        require(listing.owner == msg.sender, "MARKETPLACE: Caller is not token owner");
        _unlist(_token, _tokenId, false);
        _token.transferFrom(address(this), msg.sender, _tokenId);
    }

    function purchase(IERC721 _token, uint256 _tokenId) public whenNotPaused {
        ListingFixedPrice memory listing = fixedPriceListings[_token][_tokenId];
        require(listing.owner != address(0), "MARKETPLACE: tokenId not for sale");

        uint256 price = listing.price;
        uint256 resultingAmount = _chargeFee(_token, _tokenId, currency, price);
        resultingAmount -= _chargeRoyalty(_token, _tokenId, currency, price);

        currency.transferFrom(msg.sender, listing.owner, resultingAmount);
        _token.transferFrom(address(this), msg.sender, _tokenId);
        _unlist(_token, _tokenId, true);

        emit Purchased(msg.sender, listing.owner, _token, _tokenId, price);
    }

    function _unlist(
        IERC721 _token,
        uint256 _tokenId,
        bool _purchased
    ) internal {
        delete fixedPriceListings[_token][_tokenId];
        emit UnlistedFixedPrice(msg.sender, _token, _tokenId, _purchased);
    }

    // returns listed tokens that belongs to a given collection, esto puede ser lo que vaya en `See all albums`
    // o que usando eventos cada vez q se lista un album, los ponemos en una base de datos y leemos esa base de datos
    // usando algo como thegraph.
    // function getListedAlbums(uint256 almbumId) external view returns (uint256[] memory result) {

    // }
}

