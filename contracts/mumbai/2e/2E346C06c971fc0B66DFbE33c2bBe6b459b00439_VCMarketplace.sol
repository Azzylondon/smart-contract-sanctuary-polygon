abstract contract VCMarketplaceBase is FeeBeneficiary, Pausable {
    mapping(IERC721 => bool) public isWhitelistedToken;

    /**
     * @dev The Marketplace currency.
     */
    IERC20 public currency;

    /**
     * @dev Sets the whitelisted tokens to be traded at the Marketplace.
     */
    constructor(IERC721[] memory _tokens) {
        whitelistTokens(_tokens);
    }

    modifier onlyWhitelistedTokens(IERC721 _token) {
        require(isWhitelistedToken[_token], "MARKETPLACE: Token address not whitelisted");
        _;
    }

    function pause(bool _paused) public onlyRole(DEFAULT_ADMIN_ROLE) {
        if (_paused) _pause();
        else _unpause();
    }

    function setCurrency(IERC20 _currency) public onlyRole(DEFAULT_ADMIN_ROLE) {
        currency = _currency;
    }

    /**
     * @dev Adds tokens to the whitelist of tradable assets.
     */
    function whitelistTokens(IERC721[] memory _tokens) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < _tokens.length; i++) {
            isWhitelistedToken[_tokens[i]] = true;
        }
    }

    /**
     * @dev Removes tokens from the whitelist of tradable assets.
     */
    function blacklistTokens(IERC721[] memory _tokens) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < _tokens.length; i++) {
            isWhitelistedToken[_tokens[i]] = false;
        }
    }

    /**
     * @dev Allows the withdrawal of any `ERC20` token from the Marketplace to
     * any account. Can only be called by the owner.
     */
    function withdrawTo(
        address _token,
        address _to,
        uint256 _amount
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC20(_token).transfer(_to, _amount);
    }

    /**
     * @dev Transfers the listed asset from `sender` to the Marketplace.
     */
    function _list(IERC721 _token, uint256 _tokenId) internal {
        require(_token.ownerOf(_tokenId) == msg.sender, "MARKETPLACE: Caller is not token owner");
        _token.transferFrom(msg.sender, address(this), _tokenId);
    }

    /**
     * @dev Computes the royalty amount and transfers it from sender to the
     * asset creator, i.e. the royalty beneficiary.
     */
    function _chargeRoyalty(
        IERC721 _token, // fixme: is this ok?
        uint256 _tokenId,
        IERC20 _currency,
        uint256 _totalAmount
    ) internal returns (uint256) {
        (address receiver, uint256 royaltyAmount) = IERC2981(address(_token)).royaltyInfo(_tokenId, _totalAmount);
        // fixme: porque no ve _transferFeeFromSender? deberia ser internal?
        // _transferFeeFromSender(_currency, receiver, royaltyAmount);
        _currency.transferFrom(msg.sender, receiver, royaltyAmount);
        return royaltyAmount;
    }

    /**
     * @dev Computes the royalty amount and transfers it from the Marketplace
     * to the asset creator, i.e. the royalty beneficiary.
     */
    function _transferRoyalty(
        IERC721 _token, // fixme: is this ok?
        uint256 _tokenId,
        IERC20 _currency,
        uint256 _totalAmount
    ) internal returns (uint256) {
        (address receiver, uint256 royaltyAmount) = IERC2981(address(_token)).royaltyInfo(_tokenId, _totalAmount);
        // fixme: porque no ve _transferFeeIfRequired? deberia ser internal?
        // _transferFeeIfRequired(_currency, receiver, royaltyAmount);
        if (receiver != address(this)) {
            _currency.transfer(receiver, royaltyAmount);
        }
        return royaltyAmount;
    }
}

