pragma solidity ^0.8.9;

import "./extensions/VCMarketplaceFixedPrice.sol";
import "./extensions/VCMarketplaceAuction.sol";
import "../starter/VCStarter.sol";

contract VCMarketplace is VCMarketplaceFixedPrice, VCMarketplaceAuction {
    constructor(
        IERC721[] memory _whitelistedTokens,
        address _pool,
        address _starter,
        uint256 _defaultPoolFee,
        uint256 _maxStarterFee
    ) VCMarketplaceBase(_whitelistedTokens) FeeBeneficiary(_pool, _starter, _defaultPoolFee, _maxStarterFee) {
        currency = VCStarter(_starter).currency();
    }

    function _onTransferFee(uint256 _projectId, uint256 _fee) internal override {
        VCStarter(starter).receiveFromMarketplace(_projectId, _fee);
    }

    function updateCurrency(IERC20 _currency) external onlyRole(STARTER_ROLE){
        currency = _currency;
    }
}

