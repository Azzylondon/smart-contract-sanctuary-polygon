struct Donation {
    uint256 amount;
    address donator;
}

contract DonationSplitter is Initializable {
    VCStarter public starter;
    address public lab;
    Donation[] public donations;
    uint256 public totalDonations;

    constructor() {}

    function init(address _lab) public initializer {
        starter = VCStarter(msg.sender);
        lab = _lab;
    }

    function onCampaignSuccess() external {
        delete donations;
        totalDonations = 0;
        starter.currency().transfer(lab, totalDonations);
    }

    function onCampaignCancelled() external {
        for (uint256 i = 0; i < donations.length; i++) {
            starter.currency().transfer(donations[i].donator, donations[i].amount);
        }
        delete donations;
    }

    function donate(address _donator, uint256 _amount) external {
        donations.push(Donation({amount: _amount, donator: _donator}));
        totalDonations += _amount;
    }
}

