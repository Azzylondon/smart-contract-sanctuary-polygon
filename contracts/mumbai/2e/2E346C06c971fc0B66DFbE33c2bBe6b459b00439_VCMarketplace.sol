pragma solidity ^0.8.9;

import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../utils/DonationSplitter.sol";
import "../marketplace/VCMarketplace.sol";

enum CampaignStatus {
    ACTIVE,
    FINISHED,
    CANCELLED
}

struct Project {
    uint256 id;
    address lab;
    address donationSplitter;
    uint256 marketplaceGain;
    uint256 target;
    uint256 deadline;
    CampaignStatus status;
}

contract VCStarter is Ownable {
    Project[] private _projects;
    address private _marketplace;
    address public pool;
    address public donationSplitterTemplate;
    IERC20 public currency;

    constructor(
        IERC20 _currency,
        address _pool,
        address _donationSplitterTemplate
    ) {
        currency = _currency;
        pool = _pool;
        donationSplitterTemplate = _donationSplitterTemplate;
    }

    function setMarketplace(address _newMarketplace) external onlyOwner {
        _marketplace = _newMarketplace;
        for (uint256 i = 0; i < _projects.length; i++) {
            VCMarketplace(_newMarketplace).addProject(_projects[i].id);
        }
    }

    function setCurrency(IERC20 _currency) public onlyOwner {
        currency = _currency;
        VCMarketplace(_marketplace).updateCurrency(_currency);
    }

    function createCampaign(
        address _lab,
        uint256 _target,
        uint256 _deadline
    ) external {
        address donationSplitter = Clones.clone(donationSplitterTemplate);
        DonationSplitter(donationSplitter).init(_lab);
        Project memory project = Project({
            id: _projects.length,
            lab: _lab,
            donationSplitter: donationSplitter,
            marketplaceGain: 0,
            target: _target,
            deadline: _deadline,
            status: CampaignStatus.ACTIVE
        });
        _projects.push(project);
        if (_marketplace != address(0)) {
            VCMarketplace(_marketplace).addProject(project.id);
        }
    }

    function _updateCampaignStatus(Project storage _project) internal {
        require(_project.status == CampaignStatus.ACTIVE, "STARTER: Only active projects can be updated");
        if (DonationSplitter(_project.donationSplitter).totalDonations() >= _project.target) {
            _project.status = CampaignStatus.FINISHED;
        }
        if (_project.deadline < block.timestamp) {
            _project.status = CampaignStatus.CANCELLED;
        }
    }

    function supportProject(uint256 _projectId, uint256 _donation) external {
        _updateCampaignStatus(_projects[_projectId]);
        require(_projects[_projectId].status != CampaignStatus.FINISHED, "STARTER: Campaign has already finished");
        require(
            _projects[_projectId].status != CampaignStatus.CANCELLED,
            "STARTER: Campaign has already been cancelled"
        );
        address donationSplitter = _projects[_projectId].donationSplitter;
        currency.transfer(donationSplitter, _donation);
        DonationSplitter(DonationSplitter(donationSplitter)).donate(msg.sender, _donation);
    }

    function receiveFromMarketplace(uint256 _projectId, uint256 _donation) external {
        address donationSplitter = _projects[_projectId].donationSplitter;
        currency.transfer(donationSplitter, _donation);
        DonationSplitter(DonationSplitter(donationSplitter)).donate(_marketplace, _donation);
    }

    function closeProject(uint256 _projectId) external onlyOwner {
        _updateCampaignStatus(_projects[_projectId]);
        Project memory project = _projects[_projectId];
        require(project.status != CampaignStatus.ACTIVE, "STARTER: Campaign is still active");
        if (project.status != CampaignStatus.CANCELLED) {
            DonationSplitter(project.donationSplitter).onCampaignCancelled();
            currency.transfer(pool, project.marketplaceGain);
        } else {
            DonationSplitter(project.donationSplitter).onCampaignSuccess();
            currency.transfer(project.lab, project.marketplaceGain);
        }
    }
}

