    function swapPULSEForExactTokens(
        uint256 amountOut,
        address[] calldata path,
        address to,
        uint256 deadline
    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (uint256[] memory amounts)
    {
        require(path[0] == WPULSE, "PulseChainRouter: INVALID_PATH");
        amounts = PulseChainLibrary.getAmountsIn(factory, amountOut, path);
        require(
            amounts[0] <= msg.value,
            "PulseChainRouter: EXCESSIVE_INPUT_AMOUNT"
        );
        IWPULSE(WPULSE).deposit{value: amounts[0]}();
        assert(
            IWPULSE(WPULSE).transfer(
                PulseChainLibrary.pairFor(factory, path[0], path[1]),
                amounts[0]
            )
        );
        _swap(amounts, path, to);
        // refund dust PULSE, if any
        if (msg.value > amounts[0])
            TransferHelper.safeTransferPULSE(msg.sender, msg.value - amounts[0]);
    }

    // **** SWAP (supporting fee-on-transfer tokens) ****
    // requires the initial amount to have already been sent to the first pair
    function _swapSupportingFeeOnTransferTokens(
        address[] memory path,
        address _to
    ) internal virtual {
        for (uint256 i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0, ) = PulseChainLibrary.sortTokens(input, output);
            IPulseChainPair pair = IPulseChainPair(
                PulseChainLibrary.pairFor(factory, input, output)
            );
            uint256 amountInput;
            uint256 amountOutput;
            {
                // scope to avoid stack too deep errors
                (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();
                (uint256 reserveInput, uint256 reserveOutput) = input == token0
                    ? (reserve0, reserve1)
                    : (reserve1, reserve0);
                amountInput =
                    IERC20(input).balanceOf(address(pair)) -
                    (reserveInput);
                amountOutput = PulseChainLibrary.getAmountOut(
                    amountInput,
                    reserveInput,
                    reserveOutput,
                    pair.swapFee()
                );
            }
            (uint256 amount0Out, uint256 amount1Out) = input == token0
                ? (uint256(0), amountOutput)
                : (amountOutput, uint256(0));
            address to = i < path.length - 2
                ? PulseChainLibrary.pairFor(factory, output, path[i + 2])
                : _to;
            (uint amount0In,uint amount1In)=pair.swap(amount0Out, amount1Out, to, new bytes(0));
            emit RouterSwap(msg.sender,amount0In,amount1In, amount0Out, amount1Out,to,PulseChainLibrary.pairFor(factory, input, output));
        }
    }

    function swapExactTokensForTokensSupportingFeeOnTransferTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external virtual override ensure(deadline) {
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            PulseChainLibrary.pairFor(factory, path[0], path[1]),
            amountIn
        );
        uint256 balanceBefore = IERC20(path[path.length - 1]).balanceOf(to);
        _swapSupportingFeeOnTransferTokens(path, to);
        require(
            IERC20(path[path.length - 1]).balanceOf(to) - (balanceBefore) >=
                amountOutMin,
            "PulseChainRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
    }

    function swapExactPULSEForTokensSupportingFeeOnTransferTokens(
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external payable virtual override ensure(deadline) {
        require(path[0] == WPULSE, "PulseChainRouter: INVALID_PATH");
        uint256 amountIn = msg.value;
        IWPULSE(WPULSE).deposit{value: amountIn}();
        assert(
            IWPULSE(WPULSE).transfer(
                PulseChainLibrary.pairFor(factory, path[0], path[1]),
                amountIn
            )
        );
        uint256 balanceBefore = IERC20(path[path.length - 1]).balanceOf(to);
        _swapSupportingFeeOnTransferTokens(path, to);
        require(
            IERC20(path[path.length - 1]).balanceOf(to) - (balanceBefore) >=
                amountOutMin,
            "PulseChainRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
    }

    function swapExactTokensForPULSESupportingFeeOnTransferTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) external virtual override ensure(deadline) {
        require(
            path[path.length - 1] == WPULSE,
            "PulseChainRouter: INVALID_PATH"
        );
        TransferHelper.safeTransferFrom(
            path[0],
            msg.sender,
            PulseChainLibrary.pairFor(factory, path[0], path[1]),
            amountIn
        );
        _swapSupportingFeeOnTransferTokens(path, address(this));
        uint256 amountOut = IERC20(WPULSE).balanceOf(address(this));
        require(
            amountOut >= amountOutMin,
            "PulseChainRouter: INSUFFICIENT_OUTPUT_AMOUNT"
        );
        IWPULSE(WPULSE).withdraw(amountOut);
        TransferHelper.safeTransferPULSE(to, amountOut);
    }

