// File: contracts\PulseChainRouter.sol

contract PulseChainRouter is IPulseChainRouter02 {
    //using SafeMath for uint256;

    address public immutable override factory;
    address public immutable override WPULSE;
    //events  
    event RouterSwap(
        address indexed sender,
        uint amount0In,
        uint amount1In,
        uint amount0Out,
        uint amount1Out,
        address indexed to,
        address indexed pair
    );
    
    event addLiquid(
         address indexed sender,
         uint amount0,
         uint amount1,
         uint lpAmountMinted,
         address indexed to,
         address indexed pair);
    event removeLiquid(
         address indexed sender,
         uint amount0,
         uint amount1,
         uint lpAmountBurned,
         address indexed to,
         address indexed pair
         );
    modifier ensure(uint256 deadline) {
        require(deadline >= block.timestamp, "PulseChainRouter: EXPIRED");
        _;
    }

    constructor(address _factory, address _WPULSE) {
        factory = _factory;
        WPULSE = _WPULSE;
    }

    receive() external payable {
        assert(msg.sender == WPULSE); // only accept PULSE via fallback from the WPULSE contract
    }

    // **** ADD LIQUIDITY ****
    function _addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin
    ) internal virtual returns (uint256 amountA, uint256 amountB) {
        // create the pair if it doesn't exist yet
        if (IPulseChainFactory(factory).getPair(tokenA, tokenB) == address(0)) {
            IPulseChainFactory(factory).createPair(tokenA, tokenB);
        }
        (uint256 reserveA, uint256 reserveB) = PulseChainLibrary.getReserves(
            factory,
            tokenA,
            tokenB
        );
        if (reserveA == 0 && reserveB == 0) {
            (amountA, amountB) = (amountADesired, amountBDesired);
        } else {
            uint256 amountBOptimal = PulseChainLibrary.quote(
                amountADesired,
                reserveA,
                reserveB
            );
            if (amountBOptimal <= amountBDesired) {
                require(
                    amountBOptimal >= amountBMin,
                    "PulseChainRouter: INSUFFICIENT_B_AMOUNT"
                );
                (amountA, amountB) = (amountADesired, amountBOptimal);
            } else {
                uint256 amountAOptimal = PulseChainLibrary.quote(
                    amountBDesired,
                    reserveB,
                    reserveA
                );
                assert(amountAOptimal <= amountADesired);
                require(
                    amountAOptimal >= amountAMin,
                    "PulseChainRouter: INSUFFICIENT_A_AMOUNT"
                );
                (amountA, amountB) = (amountAOptimal, amountBDesired);
            }
        }
    }

    function addLiquidity(
        address tokenA,
        address tokenB,
        uint256 amountADesired,
        uint256 amountBDesired,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    )
        external
        virtual
        override
        ensure(deadline)
        returns (
            uint256 amountA,
            uint256 amountB,
            uint256 liquidity
        )
    {
        (amountA, amountB) = _addLiquidity(
            tokenA,
            tokenB,
            amountADesired,
            amountBDesired,
            amountAMin,
            amountBMin
        );
        address pair = PulseChainLibrary.pairFor(factory, tokenA, tokenB);
        uint256 Bal_tokenA = IERC20(tokenA).balanceOf(pair);
        uint256 Bal_tokenB = IERC20(tokenB).balanceOf(pair);
        TransferHelper.safeTransferFrom(tokenA, msg.sender, pair, amountA);
        TransferHelper.safeTransferFrom(tokenB, msg.sender, pair, amountB);
        uint256 Bal_tokA = IERC20(tokenA).balanceOf(pair);
        uint256 Bal_tokB = IERC20(tokenB).balanceOf(pair);
        require(
            Bal_tokA - Bal_tokenA == amountA &&
                Bal_tokB - Bal_tokenB == amountB,
            "Amount Verification Failed"
        );
        uint amount0;
        uint amount1;
        (liquidity,amount0,amount1) = IPulseChainPair(pair).mint(to);
        //emit addLiquid(msg.sender,amountA,amountB,tokenA,tokenB,liquidity,to,pair);
        emit addLiquid(msg.sender,amount0,amount1,liquidity,to,pair);
    }

    function addLiquidityPULSE(
        address token,
        uint256 amountTokenDesired,
        uint256 amountTokenMin,
        uint256 amountPULSEMin,
        address to,
        uint256 deadline
    )
        external
        payable
        virtual
        override
        ensure(deadline)
        returns (
            uint256 amountToken,
            uint256 amountPULSE,
            uint256 liquidity
        )
    {
        (amountToken, amountPULSE) = _addLiquidity(
            token,
            WPULSE,
            amountTokenDesired,
            msg.value,
            amountTokenMin,
            amountPULSEMin
        );
        address pair = PulseChainLibrary.pairFor(factory, token, WPULSE);
        uint256 Bal_token = IERC20(token).balanceOf(pair);
        uint256 Bal_WPULSE = IERC20(WPULSE).balanceOf(pair);
        TransferHelper.safeTransferFrom(token, msg.sender, pair, amountToken);
        IWPULSE(WPULSE).deposit{value: amountPULSE}();
        assert(IWPULSE(WPULSE).transfer(pair, amountPULSE));
        uint256 Bal_tok = IERC20(token).balanceOf(pair);
        uint256 Bal_Wth = IERC20(WPULSE).balanceOf(pair);
        require(
            Bal_tok - Bal_token == amountToken &&
                Bal_Wth - Bal_WPULSE == amountPULSE,
            "Amount Verification Failed"
        );
        uint amount0;
        uint amount1;
        (liquidity,amount0,amount1)  = IPulseChainPair(pair).mint(to);
        // refund dust PULSE, if any
        if (msg.value > amountPULSE)
        {
           TransferHelper.safeTransferPULSE(msg.sender, msg.value - amountPULSE);
        }
        
        emit addLiquid(msg.sender,amount0,amount1,liquidity,to,pair);
    }

    // **** REMOVE LIQUIDITY ****
    function removeLiquidity(
        address tokenA,
        address tokenB,
        uint256 liquidity,
        uint256 amountAMin,
        uint256 amountBMin,
        address to,
        uint256 deadline
    )
        public
        virtual
        override
        ensure(deadline)
        returns (uint256 amountA, uint256 amountB)
    {
        address pair = PulseChainLibrary.pairFor(factory, tokenA, tokenB);
        // uint256 Bal_token = IPulseChainPair(pair).balanceOf(pair);
        IPulseChainPair(pair).transferFrom(msg.sender, pair, liquidity); // send liquidity to pair
        // uint256 Bal_lptoken = IPulseChainPair(pair).balanceOf(pair);
        // require(Bal_lptoken - Bal_token == liquidity,"LP Amount Verification Failed");
        (uint256 amount0, uint256 amount1) = IPulseChainPair(pair).burn(to);
        (address token0, ) = PulseChainLibrary.sortTokens(tokenA, tokenB);
        (amountA, amountB) = tokenA == token0
            ? (amount0, amount1)
            : (amount1, amount0);
        require(
            amountA >= amountAMin,
            "PulseChainRouter: INSUFFICIENT_A_AMOUNT"
        );
        require(
            amountB >= amountBMin,
            "PulseChainRouter: INSUFFICIENT_B_AMOUNT"
        );
        uint liquid=liquidity;
        emit removeLiquid(msg.sender,amount0,amount1,liquid,to,pair);
    }

    function removeLiquidityPULSE(
        address token,
        uint256 liquidity,
        uint256 amountTokenMin,
        uint256 amountPULSEMin,
        address to,
        uint256 deadline
    )
        public
        virtual
        override
        ensure(deadline)
        returns (uint256 amountToken, uint256 amountPULSE)
    {
        (amountToken, amountPULSE) = removeLiquidity(
            token,
            WPULSE,
            liquidity,
            amountTokenMin,
            amountPULSEMin,
            address(this),
            deadline
        );
        //address pair = PulseChainLibrary.pairFor(factory, token, WPULSE);
        uint256 Bal_token = IERC20(token).balanceOf(to);
        TransferHelper.safeTransfer(token, to, amountToken);
        uint256 Bal_lptoken = IERC20(token).balanceOf(to);
        require(
            Bal_lptoken - Bal_token == amountToken,
            "LP Amount Verification Failed"
        );
        IWPULSE(WPULSE).withdraw(amountPULSE);
        TransferHelper.safeTransferPULSE(to, amountPULSE);
        
    }

