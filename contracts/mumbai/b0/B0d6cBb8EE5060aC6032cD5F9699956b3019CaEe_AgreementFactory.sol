pragma solidity ^0.8.10;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@opengsn/contracts/src/BaseRelayRecipient.sol";
import "./ERC721A/ERC721A.sol";
import "./ERC721A/extensions/ERC721ABurnable.sol";

contract Agreement is ERC721A, Pausable, AccessControl, ERC721ABurnable, BaseRelayRecipient {
    //Roles:
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE"); // Pauser role.
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE"); // Minter role.

    // Vars:
    string public _contractURI = ""; // URI of the contract.
    uint256 public immutable _maxTokens; // Maximum number of tokens that can be created.
    uint256 public immutable _endTime; // Timestamp when the agreement ends.
    bool public immutable visibility; // Visibility of the agreement.

    // Events:
    event ContractPaused(address pauser);
    event ContractUnpaused(address unpauser);
    event TokenBurned(uint256 tokenId);

    // Errors:
    error EndTimeMustBeGreaterThanToday();
    error ContractURINotEmpty();
    error AgreementHasEnded();


    constructor(
        string memory name,
        string memory contractIPFSHash, // This must be hash of IPFS without schema 'ipfs://'
        uint256 maxTokens,
        uint256 endTime,
        bool _visibility,
        address owner,
        address forwarder
    ) ERC721A(name, 'AGMNT') {
        _setTrustedForwarder(forwarder);
        if (endTime > 0 && endTime <= block.timestamp) revert EndTimeMustBeGreaterThanToday();
        if (bytes(contractIPFSHash).length <= 0) revert ContractURINotEmpty();
        _grantRole(DEFAULT_ADMIN_ROLE, owner); // Grant admin role to the deployer.
        _grantRole(PAUSER_ROLE, owner); // Grant the pauser role to the deployer.
        _grantRole(MINTER_ROLE, owner); // Grant the minter role to the deployer.
        _contractURI = string(bytes.concat(bytes('ipfs://'),bytes(contractIPFSHash))); // Setting the contract URI.
        _maxTokens = maxTokens; // Setting the maximum number of tokens that can be created.
        _endTime = endTime; // Setting the end time.
        visibility = _visibility; // Setting the visibility.
        _safeMint(owner, _maxTokens);
    }

    function _baseURI() internal view override returns (string memory) {
        return _contractURI;
    }


    function contractURI() public view returns (string memory) {
        return _baseURI();
    }

    /**
     * @dev This overrides to start with tokenID 1.
     */
    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    /**
     * @dev In this version we want that all tokens returns contractURI.
     */
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();

        return _baseURI();
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
        emit ContractPaused(_msgSender()); // Emitting the pause event.

    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
        emit ContractUnpaused(_msgSender()); // Emitting the unpause event.

    }

    function checkTime() external {
        if(block.timestamp >= _endTime) { 
            pause();
        }
    }

    function supportsInterface(bytes4 interfaceId)
        public
        pure
        override(ERC721A, AccessControl)
        returns (bool)
    {
        return
            interfaceId == 0x01ffc9a7 || // ERC165 interface ID for ERC165.
            interfaceId == 0x80ac58cd || // ERC165 interface ID for ERC721.
            interfaceId == 0x5b5e139f; // ERC165 interface ID for ERC721Metadata.
    }

    function _beforeTokenTransfers(address from, address to, uint256 startTokenId, uint256 quantity)
        internal
        whenNotPaused
        override (ERC721A)
    {
        // If agreement has expired transfers are't allowed
        if (_endTime > 0 && block.timestamp > _endTime) revert AgreementHasEnded();
        super._beforeTokenTransfers(from, to, startTokenId, quantity);
    }

    function versionRecipient() external pure override returns (string memory) {
        return "2.2.6";
    }

        /**
     * return the sender of this call.
     * if the call came through our trusted forwarder, return the original sender.
     * otherwise, return `msg.sender`.
     * should be used in the contract anywhere instead of msg.sender
     */
    function _msgSender() internal override(BaseRelayRecipient, Context) view returns (address ret) {
        if (msg.data.length >= 20 && isTrustedForwarder(msg.sender)) {
            // At this point we know that the sender is a trusted forwarder,
            // so we trust that the last bytes of msg.data are the verified sender address.
            // extract sender address from the end of msg.data
            assembly {
                ret := shr(96,calldataload(sub(calldatasize(),20)))
            }
        } else {
            ret = msg.sender;
        }
    }

    /**
     * return the msg.data of this call.
     * if the call came through our trusted forwarder, then the real sender was appended as the last 20 bytes
     * of the msg.data - so this method will strip those 20 bytes off.
     * otherwise (if the call was made directly and not through the forwarder), return `msg.data`
     * should be used in the contract instead of msg.data, where this difference matters.
     */
    function _msgData() internal override(BaseRelayRecipient, Context) view returns (bytes calldata ret) {
        if (msg.data.length >= 20 && isTrustedForwarder(msg.sender)) {
            return msg.data[0:msg.data.length-20];
        } else {
            return msg.data;
        }
    }

    function _msgSenderERC721A() internal view override returns (address) {
        return _msgSender();
    }

}

