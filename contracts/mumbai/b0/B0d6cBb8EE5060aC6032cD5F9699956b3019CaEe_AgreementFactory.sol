pragma solidity ^0.8.4;

import "./Agreement.sol";
import "@opengsn/contracts/src/BaseRelayRecipient.sol";
import "./AgreementsPayMaster.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract AgreementFactory is BaseRelayRecipient  {
    using EnumerableSet for EnumerableSet.AddressSet;

    // Vars:
    AgreementsPayMaster private paymaster;

    // Storage:
    EnumerableSet.AddressSet private _newAgreements; // Mapping of agreements deployed:
    mapping (address => string) public baseURIs; // Mapping of the baseURI of the agreements deployed:

    // Events:
    event AgreementCreated(
        Agreement agreement, 
        string name, 
        string baseTokenURI,
        uint8 maxTokens, 
        uint256 endTime, 
        bool isPrivate); // Event emitted when an agreement is created.

    // Constructor:
    constructor(address _forwarder, address payable _paymaster) {
        _setTrustedForwarder(_forwarder);
        paymaster = AgreementsPayMaster(_paymaster);
    }

    function createAgreement(
        string memory name,
        string memory contractURI,
        uint8 maxTokens,
        uint256 endTime,
        bool isPrivate,
        bytes32 _salt
    ) public {
        Agreement agreement = new Agreement{salt: _salt}(
            name,
            contractURI,
            maxTokens,
            endTime,
            isPrivate,
            _msgSender(),
            trustedForwarder()
        );
        _newAgreements.add(address(agreement));
        baseURIs[address(agreement)] = string(bytes.concat(bytes('ipfs://'),bytes(contractURI)));
        paymaster.addNewAgreement(address(agreement));
        emit AgreementCreated(agreement, name, contractURI, maxTokens, endTime, isPrivate);
    }

    function getAgreements() public view returns(address[] memory) {
        return _newAgreements.values();
    }

    function versionRecipient() external pure override returns (string memory) {
        return "2.2.6";
    }
}

