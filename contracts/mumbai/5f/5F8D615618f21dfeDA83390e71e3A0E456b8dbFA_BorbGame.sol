pragma solidity ^0.8.16;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./TokenPlus.sol";

contract Pool is ReentrancyGuard, Ownable {
    uint256 public constant MIN_USDT_PRICE = 100; //читать как 1,00

    IERC20 internal usd;
    TokenPlus public tokenPlus;
    uint256 private blockedUsdCount;
    address public house;
    uint256 public totalInvested;

    event BuyPriceChanged(uint256 indexed price);
    event SellPriceChanged(uint256 indexed price);
    event InvestmentAdded(address indexed user, uint256 indexed amount);
    event Withdrawed(address indexed user, uint256 indexed amount);
    event ReferalRewardEarned(
        address indexed from,
        address indexed to,
        uint256 amount
    );

    error NotEnoughtTokenPlusBalanceError();
    error NotEnoughtPoolBalanceError();
    error MinimumAmountError();

    constructor(
        address _usd,
        address _house,
        string memory _rewardTokenName,
        string memory _rewardTokenSymbol
    ) {
        usd = IERC20(_usd);
        tokenPlus = new TokenPlus(_rewardTokenName, _rewardTokenSymbol);
        house = _house;
    }

    function canBet(uint256 _amount) external view returns (bool) {
        return usd.balanceOf(address(this)) - blockedUsdCount >= _amount;
    }

    function makeBet(
        address _from,
        uint256 _amount,
        uint256 _potentialReward
    ) external onlyOwner {
        blockedUsdCount += _potentialReward;
        usd.transferFrom(_from, address(this), _amount);
    }

    /*
     * Вызываем эту функцию в случае победы. Перечисляем игроку указанную сумму и распределяем комиссию
     */
    function transferReward(
        address _to,
        uint256 _amount,
        uint256 _houseFee,
        address ref
    ) external onlyOwner {
        if (usd.balanceOf(address(this)) >= _amount) {
            blockedUsdCount -= _amount;
            usd.transfer(_to, _amount - _houseFee);
            if (ref != address(0) && _houseFee / 2 != 0) {
                usd.transfer(house, _houseFee / 2);
                usd.transfer(ref, _houseFee / 2);
                emit ReferalRewardEarned(ref, _to, _houseFee / 2);
            } else {
                usd.transfer(house, _houseFee);
            }
        } else {
            revert NotEnoughtPoolBalanceError();
        }
    }

    /*
     * Вызываем эту функцию в случае проигрыша
     */
    function unlock(
        uint256 _amount,
        uint256 _houseFee,
        address user,
        address ref
    ) external onlyOwner {
        blockedUsdCount -= (_amount - _houseFee);
        if (ref != address(0) && _houseFee / 2 != 0) {
            usd.transfer(house, _houseFee / 2);
            usd.transfer(ref, _houseFee / 2);
            emit ReferalRewardEarned(ref, user, _houseFee / 2);
        } else {
            usd.transfer(house, _houseFee);
        }
    }

    /*
     * Внести средства на счёт пула, вносить можно от одного usdt или 1*10**6
     */
    function makeDeposit(uint256 _amount) external payable {
        if (_amount < 1 * 10**6) {
            revert MinimumAmountError();
        }
        tokenPlus.mint(msg.sender, (_amount * 100) / getTokenPlusBuyPrice());
        totalInvested += _amount;
        usd.transferFrom(msg.sender, address(this), _amount);
        emit BuyPriceChanged(getTokenPlusBuyPrice());
        emit InvestmentAdded(msg.sender, _amount * getTokenPlusBuyPrice());
    }

    /*
     *  Получает цену для покупки токена. Если цена слишком низкая, то устанавливает минимальную
     */
    function getTokenPlusBuyPrice() public view returns (uint256) {
        uint256 currentFreePoolBalance = usd.balanceOf(address(this)) -
            blockedUsdCount;

        if (
            totalInvested == 0 ||
            currentFreePoolBalance == 0 ||
            tokenPlus.totalSupply() == 0
        ) {
            return MIN_USDT_PRICE;
        }
        uint256 price = (currentFreePoolBalance * 100) / totalInvested;
        return price < MIN_USDT_PRICE ? MIN_USDT_PRICE : price;
    }

    /*
     *  Получает цену для продажи токена.
     */
    function getTokenPlusSellPrice() public view returns (uint256) {
        if (totalInvested == 0) {
            return MIN_USDT_PRICE;
        }
        uint256 currentFreePoolBalance = usd.balanceOf(address(this)) -
            blockedUsdCount;
        return (currentFreePoolBalance * 100) / totalInvested;
    }

    /*
     *  Забрать указанное количество USD
     */
    function withdraw(uint256 _tokenPlusAmount) external nonReentrant {
        if (tokenPlus.balanceOf(msg.sender) < _tokenPlusAmount) {
            revert NotEnoughtTokenPlusBalanceError();
        }
        uint256 usdToWithdraw = (_tokenPlusAmount * getTokenPlusSellPrice()) /
            100;
        if (usd.balanceOf(address(this)) < usdToWithdraw) {
            revert NotEnoughtPoolBalanceError();
        }
        totalInvested -= _tokenPlusAmount; //т.к. у нас цена 1 к 1, то таким образом можем поправить
        tokenPlus.burn(msg.sender, _tokenPlusAmount);
        usd.transfer(msg.sender, usdToWithdraw);

        emit SellPriceChanged(getTokenPlusSellPrice());
        emit Withdrawed(msg.sender, usdToWithdraw);
    }
}

