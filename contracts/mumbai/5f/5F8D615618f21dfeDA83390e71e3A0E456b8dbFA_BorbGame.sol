pragma solidity ^0.8.16;

import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./Pool.sol";

import "hardhat/console.sol";

contract BorbGame {
    uint256 public constant REWARD_PERCENT_MIN = 20;
    uint256 public constant REWARD_PERCENT_MAX = 80;

    enum BetType {
        Up,
        Down
    }

    enum Currency {
        BTC,
        ETH,
        SOL,
        BNB,
        ADA,
        DOT,
        MATIC,
        DOGE,
        ATOM,
        AVAX
    }

    struct Bet {
        //фиксим данные во время ставки
        address user;
        int256 lockPrice;
        uint256 lockTimestamp;
        uint80 roundId; //запоминаем раунд в котором зафиксили цену. потом будем подбирать увеличивая на 1
        uint256 amount;
        uint256 potentialReward;
        BetType betType;
        Currency currency;
        bool claimed; // default false
    }

    IERC20 public usdToken;

    AggregatorV3Interface public priceFeedBTC;
    AggregatorV3Interface public priceFeedETH;
    AggregatorV3Interface public priceFeedSOL;
    AggregatorV3Interface public priceFeedBNB;
    AggregatorV3Interface public priceFeedADA;
    AggregatorV3Interface public priceFeedDOT;
    AggregatorV3Interface public priceFeedMATIC;
    AggregatorV3Interface public priceFeedDOGE;
    AggregatorV3Interface public priceFeedATOM;
    AggregatorV3Interface public priceFeedAVAX;

    Pool public pool;
    address public owner;
    address public calculator; // адрес бэкэнда, который будет изменять текущий процент выигрыша
    uint256 public currentRewardPercent;
    uint256 public maxBetAmount;
    uint256 public minBetAmount;

    // k - timeframe id, v - total seconds of timeframe
    mapping(uint8 => uint256) private allowedTimeframes;
    //k-timeframe, v-array of bets
    mapping(uint8 => Bet[]) public bets;
    //k-user address v- timeframe- array of betId
    mapping(address => mapping(uint8 => uint256[])) public usersBets;
    //k-user v-his referer
    mapping(address => address) public referals;
    //k-user v-isKnown
    mapping(address => bool) public users;

    event NewUserAdded(address indexed user, address ref);
    event NewBetAdded(
        address indexed user,
        uint256 betId,
        BetType betType,
        Currency currency,
        uint8 timeframe,
        uint256 amount,
        uint256 potentialReward,
        uint80 roundId,
        int256 lockPrice,
        uint256 lockedAt
    );
    event BetClaimed(
        address indexed user,
        uint256 indexed timeframe,
        uint256 indexed betId,
        int256 closePrice
    );

    error NotAnOwnerError();
    error NotACalculatorError();
    error TimeframeNotExsistError();
    error NotEnoughtPoolBalanceError();
    error NotEnoughtUserBalanceError();
    error BetRangeError(uint256 min, uint256 max);
    error MinBetValueError();
    error BetAllreadyClaimedError();
    error ClaimBeforeTimeError(uint256 betTime, uint256 currentTime);
    error IncorrectKnownRoundIdError();
    error ClosePriceNotFoundError();
    error RewardPercentRangeError(uint256 min, uint256 max);
    error IncorrectRoundIdError();
    error IncorrectPriceFeedNumber();

    modifier onlyOwner() {
        if (msg.sender != owner) {
            revert NotAnOwnerError();
        }
        _;
    }

    modifier onlyCalculator() {
        if (msg.sender != calculator) {
            revert NotACalculatorError();
        }
        _;
    }

    modifier timeframeExsists(uint8 _timeframe) {
        if (allowedTimeframes[_timeframe] == 0) {
            revert TimeframeNotExsistError();
        }
        _;
    }

    //0xD4a33860578De61DBAbDc8BFdb98FD742fA7028e, ##
    constructor(
        address[10] memory _priceFeeds,
        address _calculator,
        address _usd,
        address _house,
        string memory _rewardTokenName,
        string memory _rewardTokenSymbol
    ) {
        owner = msg.sender;

        priceFeedBTC = AggregatorV3Interface(_priceFeeds[0]);
        priceFeedETH = AggregatorV3Interface(_priceFeeds[1]);
        priceFeedSOL = AggregatorV3Interface(_priceFeeds[2]);
        priceFeedBNB = AggregatorV3Interface(_priceFeeds[3]);
        priceFeedADA = AggregatorV3Interface(_priceFeeds[4]);
        priceFeedDOT = AggregatorV3Interface(_priceFeeds[5]);
        priceFeedMATIC = AggregatorV3Interface(_priceFeeds[6]);
        priceFeedDOGE = AggregatorV3Interface(_priceFeeds[7]);
        priceFeedATOM = AggregatorV3Interface(_priceFeeds[8]);
        priceFeedAVAX = AggregatorV3Interface(_priceFeeds[9]);

        calculator = _calculator;
        pool = new Pool(_usd, _house, _rewardTokenName, _rewardTokenSymbol);
        usdToken = IERC20(_usd);
        currentRewardPercent = 80;
        //5 минут, 15 минут, 30 минут, 1 час, 4 часа и 1 день.
        allowedTimeframes[1] = 5 minutes;
        allowedTimeframes[2] = 15 minutes;
        allowedTimeframes[3] = 30 minutes;
        allowedTimeframes[4] = 1 hours;
        allowedTimeframes[5] = 4 hours;
        allowedTimeframes[6] = 24 hours;
        maxBetAmount = 100 * 10**6;
        minBetAmount = 100;
    }

    function makeBet(
        uint256 _amount,
        BetType _betType,
        uint8 _timeframe,
        Currency _currency,
        address _ref
    ) external timeframeExsists(_timeframe) {
        uint256 potentialReward = getReward(_amount);   
        if (!pool.canBet(potentialReward)) {
            revert NotEnoughtPoolBalanceError();
        }
        if (usdToken.balanceOf(msg.sender) < _amount) {
            revert NotEnoughtUserBalanceError();
        }
        if (_amount > maxBetAmount || _amount < minBetAmount) {
            revert BetRangeError(minBetAmount, maxBetAmount);
        }
        //если юзер новый, то запомним его и реферала
        if (users[msg.sender] == false) {
            if (msg.sender != _ref) {
                referals[msg.sender] = _ref;
            }
            users[msg.sender] = true;
            emit NewUserAdded(msg.sender, _ref);
        }

        (uint80 roundId, int256 price, , , ) = getPriceFeed(_currency)
            .latestRoundData();

        bets[_timeframe].push(
            Bet(
                msg.sender,
                price,
                block.timestamp,
                roundId,
                _amount,
                potentialReward,
                _betType,
                _currency,
                false
            )
        );
        uint256 betId = bets[_timeframe].length - 1;
        usersBets[msg.sender][_timeframe].push(betId);

        pool.makeBet(msg.sender, _amount, potentialReward);

        emit NewBetAdded(
            msg.sender,
            betId,
            _betType,
            _currency,
            _timeframe,
            _amount,
            potentialReward,
            roundId,
            price,
            block.timestamp
        );
    }

    /**
     * Эту функцию вызывает либо пользователь, либо админ чтобы подвести итог ставки.
     * Более экономный вариант - когда сначала с фронта вызывается функция getCloseRoundId,
     * которая сделает бесплатный перебор и вернёт roundId в котором нужная цена закрытия.
     * Очень расточительный по газу вариант - вызвать эту функцию с _knownRoundId=0,
     * тогда будет вызвана функция getClosePrice которая сделает перебор ончейн
     */
    function claim(
        uint256 _betId,
        uint8 _timeframe,
        uint80 _knownRoundId
    ) external {
        Bet memory currentBet = bets[_timeframe][_betId];
        if (currentBet.claimed) {
            revert BetAllreadyClaimedError();
        }
        (uint80 latestRoundId, , , , ) = getPriceFeed(
            currentBet.currency
        ).latestRoundData();
        if (_knownRoundId > latestRoundId) {
            revert IncorrectKnownRoundIdError();
        }
        if (
            block.timestamp <
            currentBet.lockTimestamp +
                allowedTimeframes[_timeframe]
        ) {
            revert ClaimBeforeTimeError(
                currentBet.lockTimestamp +
                    allowedTimeframes[_timeframe],
                block.timestamp
            );
        }

        int256 closePrice = 0;
        uint256 houseFee = (currentBet.amount * 100) / 10000;

        //если знаем ид раунда - надо проверить предыдущий и указанный
        if (_knownRoundId != 0) {
            closePrice = getClosePriceByRoundId(
                _betId,
                _timeframe,
                _knownRoundId
            );
        } else {
            //если не знаем - перебираем последовательно от текущего
            closePrice = getClosePrice(_betId, _timeframe, latestRoundId);
        }
        bets[_timeframe][_betId].claimed = true;
        address ref = referals[currentBet.user];
        //if user win (user bet up and lockPrice<closePrice or user bet down and lockPrice>closePrice)
        if (
            (currentBet.betType == BetType.Up &&
                currentBet.lockPrice < closePrice) ||
            (currentBet.betType == BetType.Down &&
                currentBet.lockPrice > closePrice)
        ) {
            pool.transferReward(
                currentBet.user,
                currentBet.potentialReward,
                houseFee,
                ref
            );
        } else {
            pool.unlock(
                currentBet.amount,
                houseFee,
                currentBet.user,
                ref
            );
        }
        emit BetClaimed(
            currentBet.user,
            _timeframe,
            _betId,
            closePrice
        );
    }

    /*
     * Функция для установки адреса токена USDC
     */
    function setUsdToken(address _token) external onlyOwner {
        usdToken = IERC20(_token);
    }

    /*
     * Функция для установки адреса оракула цены
     */
    function setOracle(address _oracle, Currency _currency) external onlyOwner {
        if (_currency > Currency.AVAX) {
            revert IncorrectPriceFeedNumber();
        }
        if (_currency == Currency.BTC) {
            priceFeedBTC = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.ETH) {
            priceFeedETH = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.SOL) {
            priceFeedSOL = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.BNB) {
            priceFeedBNB = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.ADA) {
            priceFeedADA = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.DOT) {
            priceFeedDOT = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.MATIC) {
            priceFeedMATIC = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.DOGE) {
            priceFeedDOGE = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.ATOM) {
            priceFeedATOM = AggregatorV3Interface(_oracle);
            return;
        }
        if (_currency == Currency.AVAX) {
            priceFeedATOM = AggregatorV3Interface(_oracle);
            return;
        }
    }

    /*
     * функция получает оракула для указанной валюты
     */
    function getPriceFeed(Currency _currency)
        public
        view
        returns (AggregatorV3Interface)
    {
        if (_currency > Currency.AVAX) {
            revert IncorrectPriceFeedNumber();
        }
        if (_currency == Currency.BTC) {
            return priceFeedBTC;
        }
        if (_currency == Currency.ETH) {
            return priceFeedETH;
        }
        if (_currency == Currency.SOL) {
            return priceFeedSOL;
        }
        if (_currency == Currency.BNB) {
            return priceFeedBNB;
        }
        if (_currency == Currency.ADA) {
            return priceFeedADA;
        }
        if (_currency == Currency.DOT) {
            return priceFeedDOT;
        }
        if (_currency == Currency.MATIC) {
            return priceFeedMATIC;
        }
        if (_currency == Currency.DOGE) {
            return priceFeedDOGE;
        }
        if (_currency == Currency.ATOM) {
            return priceFeedATOM;
        }
        if (_currency == Currency.AVAX) {
            return priceFeedAVAX;
        }
        return priceFeedBTC;
    }

    /*
     *Бэкэнд вычисляет процент и записывает его в контракт
     */
    function updateRewardPercent(uint256 _newPercent) external onlyCalculator {
        if (
            _newPercent < REWARD_PERCENT_MIN || _newPercent > REWARD_PERCENT_MAX
        ) {
            revert RewardPercentRangeError(
                REWARD_PERCENT_MIN,
                REWARD_PERCENT_MAX
            );
        }
        currentRewardPercent = _newPercent;
    }

    /*
     *Устанавливает макс и мин размер ставки
     */
    function setMinAndMaxBetAmount(uint256 _newMin, uint256 _newMax)
        external
        onlyOwner
    {
        if (_newMin < 100) {
            revert MinBetValueError();
        }
        maxBetAmount = _newMax;
        minBetAmount = _newMin;
    }

    /*
     * Вызывается чтобы узнать возможный выигрыш с учетом комиссии
     */
    function getReward(uint256 _amount) public view returns (uint256 reward) {
        uint256 houseFee = (_amount * 100) / 10000;
        uint256 potentialReward = (_amount - houseFee) +
            (_amount * currentRewardPercent * 100) /
            10000;
        return potentialReward;
    }

    /*
     * Получает адрес того, кто пригласил юзера
     */
    function getReferer() public view returns (address) {
        return referals[msg.sender];
    }

    /*
     * Получает доступные таймфреймы
     */
    function getAllowedTimeframes() public view returns (uint256[] memory) {
        uint256[] memory timeframes = new uint256[](6);
        for (uint8 i = 1; i <= 6; i++) {
            timeframes[i - 1] = (allowedTimeframes[i]);
        }
        return timeframes;
    }

    /*
     * Эту функцию надо вызывать с фронта чтобы бесплатно получить roundId для указанной ставки
      В ней мы находим roundId из которого возьмем цену закрытия
     */
    function getCloseRoundId(uint256 _betId, uint8 _timeframe)
        public
        view
        timeframeExsists(_timeframe)
        returns (uint80)
    {
        AggregatorV3Interface priceFeed = getPriceFeed(
            bets[_timeframe][_betId].currency
        );
        (uint80 latestRoundId, , , , ) = priceFeed.latestRoundData();
        uint80 roundId = bets[_timeframe][_betId].roundId;
        uint256 priceTime = 0;
        uint256 closeTime = bets[_timeframe][_betId].lockTimestamp +
            allowedTimeframes[_timeframe];
        do {
            if (latestRoundId < roundId) {
                revert ClosePriceNotFoundError();
            }
            roundId += 1;
            (, , priceTime, , ) = priceFeed.getRoundData(roundId);
        } while (priceTime < closeTime);
        return roundId;
    }

    /*
     * Находит цену закрытия путём последовательного перебора
     */
    function getClosePrice(
        uint256 _betId,
        uint8 _timeframe,
        uint256 _latestRoundId
    ) public view timeframeExsists(_timeframe) returns (int256) {
        uint80 roundId = bets[_timeframe][_betId].roundId;
        int256 closePrice = 0;
        uint256 priceTime = 0;
        uint256 closeTime = bets[_timeframe][_betId].lockTimestamp +
            allowedTimeframes[_timeframe];
        do {
            if (_latestRoundId < roundId) {
                revert ClosePriceNotFoundError();
            }
            roundId += 1;
            (, closePrice, priceTime, , ) = getPriceFeed(
                bets[_timeframe][_betId].currency
            ).getRoundData(roundId);
        } while (priceTime < closeTime);
        return closePrice;
    }

    /*
     * Вернет цену закрытия по известной ид ставки, таймфрейму и ид раунда если все данные корректны
     * если ей подсунуть некорректный раунд ид, то ревертнится
     */
    function getClosePriceByRoundId(
        uint256 _betId,
        uint8 _timeframe,
        uint80 _roundId
    ) public view timeframeExsists(_timeframe) returns (int256) {
        if (_roundId <= 1) {
            revert IncorrectRoundIdError();
        }
        uint256 closeTime = bets[_timeframe][_betId].lockTimestamp +
            allowedTimeframes[_timeframe];
        AggregatorV3Interface priceFeed = getPriceFeed(
            bets[_timeframe][_betId].currency
        );
        (, , uint256 prevTime, , ) = priceFeed.getRoundData(_roundId - 1);
        (, int256 currentClosePrice, uint256 currentTime, , ) = priceFeed
            .getRoundData(_roundId);
        //если время указанного _roundId больше времени закрытия, а время предыдущего меньше, то указанный - нужный интервал
        if (currentTime < closeTime || prevTime >= closeTime) {
            revert IncorrectRoundIdError();
        }

        return currentClosePrice;
    }
}

