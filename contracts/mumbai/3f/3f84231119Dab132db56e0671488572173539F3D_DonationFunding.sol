            abi.encodePacked(
                "data:image/svg+xml;base64,",
                Base64.encode(svg)
            ));
    }

    function getTokenURI(uint256 tokenId, string memory _name) public view returns (string memory){
        bytes memory dataURI = abi.encodePacked(
            '{',
                '"name": "', _name, ' #' , tokenId.toString() , '",',
                '"description": "Fund donation appreciation NFT ",',
                '"image": "', generateSVG(_name), '"',
            '}'
        );
        return string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(dataURI)
            )
        );
    }

    function mint(string memory _name) public {
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        _safeMint(msg.sender, newItemId);
        _setTokenURI(newItemId, getTokenURI(newItemId, _name));
    }

    /**
     * @dev fetches all stored memos
     */
    function getMemos() public view returns (Memo[] memory) {
        return memos;
    }

    /**
     * @dev buy a fund for owner (sends an ETH tip and leaves a memo)
     * @param _name name of the fund purchaser
     * @param _message a nice message from the purchaser
     */
    function fundNfp(string memory _name, string memory _message) public payable {
        // Must accept more than 0 ETH for a fund.
        require(msg.value > 0, "can't donate 0 MATIC!");

        // Create and assign NFT
        mint(_name);

        // Add the memo to storage!
        memos.push(Memo(
            msg.sender,
            block.number,
            block.timestamp,
            _name,
            _message,
            msg.value
        ));


        // Emit a NewMemo event with details about the memo.
        emit NewMemo(
            msg.sender,
            block.number,
            block.timestamp,
            _name,
            _message,
            msg.value
        );
    }

    /**
     * @dev send the entire balance stored in this contract to the owner
     */
    function withdrawTips() public {
        require(contractOwner.send(address(this).balance));
    }
    
}

