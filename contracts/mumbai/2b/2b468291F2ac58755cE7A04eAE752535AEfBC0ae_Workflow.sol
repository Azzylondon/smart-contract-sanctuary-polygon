contract Workflow is KeeperCompatibleInterface {
    AggregatorV3Interface internal maticPriceFeed;
    AggregatorV3Interface internal daiPriceFeed;
    // IQuickswapLiquidity quickswapLiquidity;
    /**
    * Use an interval in seconds and a timestamp to slow execution of Upkeep
    */
    address private constant WMATIC = 0x9c3C9283D3e44854697Cd22D3Faa240Cfb032889;
    address private constant ROUTER = 0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff;// deployed at Polygon mainnet and testnet

    address public creator;
    address public tokenA;
    address public tokenB;
    uint public amountTokenA;
    uint public amountTokenB;
    uint256 public immutable interval;
    uint256 public lastTimeStamp;
    int public lastTokenAPrice;
    int public prevTokenAPrice;
    int public lastTokenBPrice;
    int public prevTokenBPrice;
    bool public newWorkflowCreated;
    uint public counter;

    event TokensSwapped(address tokenIn, address tokenOut, address to);
    event LiquidityAddedTo(uint amountA, uint amountB, uint liquidity);
    event TestEvent(address from, address tokenA, address tokenB, uint amountA, uint amountB);


    event PriceDataUpdated(int, int);

    /**
     * Network: Mumbai Testnet
     * ETH/USD 0x0715A7794a1dc8e42615F059dD6e406A6594651A
     * BTC/USD 0x007A22900a3B98143368Bd5906f8E17e9867581b
     */

    constructor(
        // IQuickswapLiquidity _quickswapLiquidity
    ) {
        // quickswapLiquidity = _quickswapLiquidity;
        interval = 100; //5min
        lastTimeStamp = block.timestamp;
        maticPriceFeed = AggregatorV3Interface(0xd0D5e3DB44DE05E9F294BB0a3bEEaF030DE24Ada);
        daiPriceFeed = AggregatorV3Interface(0x0FCAa9c899EC5A91eBc3D5Dd869De833b06fB046);
        counter = 0;
        newWorkflowCreated = false;
        creator = msg.sender;
        tokenA = 0x9c3C9283D3e44854697Cd22D3Faa240Cfb032889;
        tokenB = 0xcB1e72786A6eb3b44C2a2429e317c8a2462CFeb1;
        amountTokenA = 260000000000000;
        amountTokenB = 100000000000000;
    }

    function addLiquidityToQuickswap(
        address from,
        address _tokenA,
        address _tokenB,
        uint _amountA,
        uint _amountB
    ) public {
        // console.log(msg.sender);
        IERC20(_tokenA).transferFrom(from, address(this), _amountA);
        IERC20(_tokenB).transferFrom(from, address(this), _amountB);

        IERC20(_tokenA).approve(ROUTER, _amountA);
        IERC20(_tokenB).approve(ROUTER, _amountB);

        emit TestEvent(from, _tokenA, _tokenB, _amountA, _amountB);

        // (uint amountA, uint amountB, uint liquidity) = IUniswapV2Router02(ROUTER)
        //     .addLiquidity(
        //         _tokenA,
        //         _tokenB,
        //         _amountA,
        //         _amountB,
        //         10000000,
        //         10000000,
        //         from,
        //         block.timestamp + 3600 * 24
        //     );

        // emit LiquidityAddedTo(amountA, amountB, liquidity);
    }

    function _swap(
        address from,
        address _tokenIn,
        address _tokenOut,
        uint _amountIn,
        uint _amountOutMin
    ) public {
        IERC20(_tokenIn).transferFrom(from, address(this), _amountIn);
        IERC20(_tokenIn).approve(ROUTER, _amountIn);

        address[] memory path;
        if (_tokenIn == WMATIC || _tokenOut == WMATIC) {
            path = new address[](2);
            path[0] = _tokenIn;
            path[1] = _tokenOut;
        } else {
            path = new address[](3);
            path[0] = _tokenIn;
            path[1] = WMATIC;
            path[2] = _tokenOut;
        }

        IUniswapV2Router02(ROUTER).swapExactTokensForTokens(
            _amountIn,
            _amountOutMin,
            path,
            from,
            block.timestamp + 3600 * 24
        );

        emit TokensSwapped(_tokenIn, _tokenOut, creator);
    }

    // function create(
    //     address _tokenA,
    //     address _tokenB,
    //     uint _amountTokenA,
    //     uint _amountTokenB
    // ) public {
    //     tokenA = _tokenA;
    //     tokenB = _tokenB;
    //     amountTokenA = _amountTokenA;
    //     amountTokenB = _amountTokenB;
    //     lastTokenAPrice = getLatestMaticPrice();
    //     lastTokenBPrice = getLatestDaiPrice();
    //     // creator = msg.sender;
    //     newWorkflowCreated = true;
    // }

      /**
     * Get the latest ETH and BTC prices
     */
    function getLatestMaticPrice() public view returns (int) {
        (
            /*uint80 roundID*/,
            int price,
            /*uint startedAt*/,
            /*uint timeStamp*/,
            /*uint80 answeredInRound*/
        ) = maticPriceFeed.latestRoundData();
        return price;
    }

    function getLatestDaiPrice() public view returns (int) {
        (
            /*uint80 roundID*/,
            int price,
            /*uint startedAt*/,
            /*uint timeStamp*/,
            /*uint80 answeredInRound*/
        ) = daiPriceFeed.latestRoundData();
        return price;
    }

    function checkUpkeep(bytes calldata /* checkData */) external view override returns (bool upkeepNeeded, bytes memory /* performData */) {
        upkeepNeeded = (block.timestamp - lastTimeStamp) > interval;
        // We don't use the checkData in this example. The checkData is defined when the Upkeep was registered.
    }

    function performUpkeep(bytes calldata /* performData */) external override {
        //Revalidating the upkeep in the performUpkeep function
        // emit TestEvent(creator, tokenA, tokenB, amountTokenA, amountTokenB);
        addLiquidityToQuickswap(creator, tokenA, tokenB, amountTokenA, amountTokenB);
        lastTimeStamp = block.timestamp;

        // if ((block.timestamp - lastTimeStamp) > interval) {
            // counter = counter + 1;
            // prevTokenAPrice = lastTokenAPrice;
            // prevTokenBPrice = lastTokenBPrice;
            // if (newWorkflowCreated) {
            //     lastTokenAPrice = getLatestMaticPrice();
            //     lastTokenBPrice = getLatestDaiPrice();

                // if ((lastTokenAPrice - prevTokenAPrice) > prevTokenAPrice / 100)
                // _swap(creator, tokenA, tokenB, amountTokenA, 100000000);
                // if ((prevTokenBPrice - lastTokenBPrice) > prevTokenBPrice / 100)
                // quickswapLiquidity.addLi1quidityToQuickswap(creator, tokenA, tokenB, amountTokenA, amountTokenB);
            // }
        // }
    }
}

