pragma solidity >=0.4.22 <0.9.0;

uint256 constant _TYPE_SALEID_ = 0x01;
uint256 constant _TYPE_AUCTIONID_ = 0x02;

// SPDX-License-Identifier: MIT
pragma solidity >=0.4.22 <0.9.0;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../curate/ICurate.sol";
import "./lib/MarketContants.sol";
import "hardhat/console.sol";

contract Market is IERC721Receiver, Ownable {

    
    using Counters for Counters.Counter;

    Counters.Counter private auctionIds;

    address private usdcAddress; // usdc + 알파; 0xE097d6B3100777DC31B34dC2c58fB524C2e76921
    address private usdtAddress;  // 0xA02f6adc7926efeBBd59Fd43A84f4E0c0c91e832

    constructor( address _curateAddress) {
        cutateAddress = _curateAddress;
    }

    enum auctionStatus { Non , auctioning , auctionend }

    struct auctionListing_info {
        uint256 _info_auctionId;
        uint256 _info_curateId;
        address _info_seller;
        address _info_nftAddress;
        uint256 _info_tokenid;
        uint256 _info_startPrice;
        address _info_highestbidder;
        uint256 _info_highestbid;
        uint256 _info_endTime;
        auctionStatus _info_auctionStatus;
    }

    mapping (uint256 => uint256) internal _auctionIndex;   
    auctionListing_info[] internal _auctionidList;

    mapping(address => mapping(uint256 => auctionListing_info)) _auctionInfo;
    mapping(address => mapping(uint256 => uint256)) _bidsAmount; /* nftaddres => (autionid => amount) */

    uint256 private auctionEndTime;

    event auctionStartEvent(address , uint256 , auctionListing_info);
    event auctionBidEvent (uint256 , auctionListing_info);
    event auctionEndEvent (uint256 , address ,address , uint256);
    event auctionCancelEvent (uint256 , address);

    modifier isStart(address _NFTAddress , uint256 _TokenID , uint256 _startPrice , uint256 _curateid) {
        require(msg.sender == IERC721(_NFTAddress).ownerOf(_TokenID) , "MSG.SENDER is not the owner");
        require(ICurate(cutateAddress).isCurateEnd(_curateid) , "cutateid is not Curated");
        require(_startPrice > 0 , "StartPrice must be greater than 0");
        _;
    }

    modifier isBid(uint256 _auctionid , uint256 _BidPrice) {
        require(_auctionidList[_auctionIndex[_auctionid]]._info_auctionStatus == auctionStatus.auctioning , "NFT not started");
        require(_auctionidList[_auctionIndex[_auctionid]]._info_highestbid < _BidPrice , "msg.value must be greater than highestbid");
        require(_auctionidList[_auctionIndex[_auctionid]]._info_endTime > block.timestamp , "auction ended" );
        _;
    }

    modifier idEnd(uint256 _auctionid) {
        require(_auctionidList[_auctionIndex[_auctionid]]._info_auctionStatus == auctionStatus.auctioning, "Action not started");  
        require(_auctionidList[_auctionIndex[_auctionid]]._info_endTime <= block.timestamp , "Auction is still ongoing");
        _;
    }

    modifier onlyAuctionSeller(uint256 _auctionid) {
        require ( _auctionidList[_auctionIndex[_auctionid]]._info_seller  == msg.sender , "MSG.SENDER is not NFT Seller.");
        require ( _auctionidList[_auctionIndex[_auctionid]]._info_highestbidder == address(0) , "Aleady biding");
        _;
    }
    
    function onERC721Received(address , address , uint256 , bytes calldata ) public virtual override returns (bytes4) {
	
        return this.onERC721Received.selector;
    }

    function setStableAddress (address _usdcAddress , address _usdtAddress) public onlyOwner {

        usdcAddress = _usdcAddress;
        usdtAddress = _usdtAddress;
    }

    function auctionListing(uint256 _curateid , address _nftaddress , uint256 _tokenid , uint256 _startPrice , uint _auctionPeriod) public 
        isStart(_nftaddress , _tokenid , _startPrice , _curateid)
    {
        uint256 auctionId = auctionIds.current() + 1;
        auctionIds.increment();

        /* auctionPeriod 별 분기 구현. 1, 3, 7일 분기. 테스트를 위해 일단 보류. */

        auctionListing_info memory _info = auctionListing_info({
            _info_auctionId         : auctionId,
            _info_curateId          : _curateid,
            _info_seller            : msg.sender,
            _info_nftAddress        : _nftaddress,
            _info_tokenid           : _tokenid,
            _info_startPrice        : _startPrice,
            _info_highestbidder     : address(0),
            _info_highestbid        : 0, 
            _info_endTime           : block.timestamp + 1 days,
            _info_auctionStatus     : auctionStatus.auctioning
        });

        _auctionidList.push(_info);
        _auctionIndex[auctionId] = _auctionidList.length - 1;

        IERC721(_nftaddress).safeTransferFrom(msg.sender, address(this), _tokenid);

        emit auctionStartEvent(_nftaddress , _tokenid , _info);
    }
    
    function getAuctionListbyID(uint256 _auctionid) public view returns(auctionListing_info memory _auction) {

        if (_auctionidList.length == 0) {
            return auctionListing_info(0 , 0 , address(0) , address(0), 0 , 0, address(0) , 0 , 0 , auctionStatus.Non);
        }

        auctionListing_info memory c = _auctionidList[_auctionIndex[_auctionid]];

        if (c._info_auctionId != _auctionid) {

            return  auctionListing_info(0 , 0 , address(0) , address(0), 0 , 0, address(0) , 0 , 0 , auctionStatus.Non);
        }
        
        return c;
    }

    function getAuctionAmount ( address _bidder , uint256 _auctionId ) public view returns ( uint256 _amount) {
        return _bidsAmount[_bidder][_auctionId];
    }
    
    function auctionCancel (uint256 _auctionid ) public 
        onlyAuctionSeller(_auctionid)
    {
        
        auctionListing_info memory c = _auctionidList[_auctionIndex[_auctionid]];
       
        IERC721(c._info_nftAddress).safeTransferFrom(address(this) , msg.sender , c._info_tokenid);
        
        ICurate(cutateAddress).cancelProposal(c._info_curateId);
        
        removeListing(_auctionid , _TYPE_AUCTIONID_);

        emit auctionCancelEvent(_auctionid , msg.sender );
    }

    function bid( uint256 _auctionid , uint256 _BidPrice) public  
        isBid(_auctionid , _BidPrice)
    {
        
        auctionListing_info storage c = _auctionidList[_auctionIndex[_auctionid]];

        if (c._info_highestbidder != address(0)){
            
            uint256 amount = _bidsAmount[c._info_highestbidder][c._info_auctionId];
            _bidsAmount[c._info_highestbidder][c._info_auctionId] = 0;
            IERC20(usdcAddress).transfer(c._info_highestbidder, amount);   
        }

        c._info_highestbidder = msg.sender;
        c._info_highestbid = _BidPrice;

        _bidsAmount[msg.sender][c._info_auctionId] = _BidPrice;

        IERC20(usdcAddress).transferFrom(msg.sender, address(this), _BidPrice);

        emit auctionBidEvent(_auctionid , c);
    }

    function end ( uint256 _auctionid ) public
        onlyOwner 
        idEnd(_auctionid)
    {
        auctionListing_info storage c = _auctionidList[_auctionIndex[_auctionid]];
        address seller = c._info_seller;
        address buyer = c._info_highestbidder;
        uint256 buyPrice = c._info_highestbid;
        if (c._info_highestbidder == address(0)){ 

            IERC721(c._info_nftAddress).safeTransferFrom(address(this), c._info_seller , c._info_tokenid);

        } else {
            
            IERC721(c._info_nftAddress).safeTransferFrom(address(this), c._info_highestbidder , c._info_tokenid);

            uint256 amount = _bidsAmount[c._info_highestbidder][c._info_auctionId];
            _bidsAmount[c._info_highestbidder][c._info_auctionId] = 0;
            
            uint256 sellerAmount = amount * 8 / 100;

            IERC20(usdcAddress).transfer(c._info_highestbidder, sellerAmount);
    
        }

        ICurate(cutateAddress).cancelProposal(c._info_curateId);

        removeListing(_auctionid , _TYPE_AUCTIONID_);
       

       emit auctionEndEvent(_auctionid , seller, buyer , buyPrice);
    }
    
    /**************  sale  ************/

    mapping ( address => uint256 ) _FeeAmount;

    mapping ( address => mapping ( uint256 => marketListing_info )) _DepositNFT;

    Counters.Counter private _saleId;

    address private cutateAddress;

    struct marketListing_info {
        uint256 _info_saleId;
        address _info_seller;
        address _info_buyer;
        address _info_nftAddress;
        uint256 _info_tokenid;
        uint256 _info_curateid;
        uint256 _info_price;
        uint256 _info_saledate;
        saleStatus _info_salestatus;
    }

    enum saleStatus { Non , saling , saled } 

    mapping (uint256 => uint256) internal _saleIndex;   
    marketListing_info[] internal _saleidList;

    event marketListingEvent(address  , uint256  , marketListing_info );  // param (nftaddress , tokenid , maketStruct info)
    event buyNFTEvent(uint256 , address  ,address  , uint256 ); // param( saleid , seller , buyer , buyprice)
    event cancelMarketListingEvent (uint256  , address ); // param (saleid , canceller)

    modifier onlySeller (uint256 _saleid ) {
        if (_saleidList.length == 0) {

        }
        require ( _saleidList[_saleIndex[_saleid]]._info_seller == msg.sender  , "MSG.SENDER is not NFT Seller.");
        _;
    } 

    modifier isNFTSale (uint256 _curateid , uint256 _Price ) {
        require (_Price > 0 , "Price must be greater than 0 ");
        _;
    }

    modifier isListing(address _NFTAddress , uint256 _TokenId , address account , uint256 _curateid) {
        require (account == IERC721(_NFTAddress).ownerOf(_TokenId) , "Seller is not Owner");
        require (ICurate(cutateAddress).isCurateEnd(_curateid) , "cutateid is not Curated");
        _;
    }

    modifier isNFTBuy (uint256 _saleid , uint256 _price) {
        require (IERC20(usdcAddress).balanceOf(msg.sender) > _price , "msg.sender balance is short" );
        _;
    }
    
    function marketListing (address _nftAddress , uint256 _tokenId , uint256 _Price , uint256 _curateid) public 
        isListing(_nftAddress , _tokenId , msg.sender , _curateid)
        isNFTSale(_curateid , _Price)
    {

        uint256 saleid = _saleId.current() + 1;  // 최초 1부터 시작
        _saleId.increment();
        
        marketListing_info memory _info = marketListing_info({
            _info_saleId     : saleid,
            _info_seller     : msg.sender,
            _info_buyer      : address(0),
            _info_nftAddress : _nftAddress,
            _info_tokenid    : _tokenId,
            _info_curateid   : _curateid,
            _info_price      : _Price,
            _info_saledate   : 0,
            _info_salestatus : saleStatus.saling
        });
        
        _saleidList.push(_info);
        _saleIndex[saleid] = _saleidList.length - 1;

        IERC721(_nftAddress).safeTransferFrom( msg.sender , address(this), _tokenId);
        
        emit marketListingEvent(_nftAddress, _tokenId, _info);
    }
    
    function getMarketListByID (uint256 _saleid) public view returns(marketListing_info memory _info) {

        if (_saleidList.length == 0) {
            return marketListing_info(0 , address(0) , address(0) , address(0), 0 , 0 , 0 , 0 , saleStatus.Non);
        }

        marketListing_info memory c = _saleidList[_saleIndex[_saleid]];

        if (c._info_saleId != _saleid) {
            return marketListing_info(0 , address(0) , address(0) , address(0), 0 , 0 , 0 , 0 , saleStatus.Non);
        }

        return c;
    }

    function cancelMarketListing (uint256 _saleid) public
        onlySeller(_saleid ) 
    {

        marketListing_info memory c = _saleidList[_saleIndex[_saleid]];
       
        IERC721(c._info_nftAddress).safeTransferFrom(address(this) , msg.sender , c._info_tokenid);
        
        ICurate(cutateAddress).cancelProposal(c._info_curateid);
        
        removeListing(_saleid, _TYPE_SALEID_);

        emit cancelMarketListingEvent(_saleid , msg.sender); 
    }

    function buyNFT ( uint256 _saleid , uint256 _price) public  
       isNFTBuy( _saleid , _price)
    {   

        marketListing_info storage c = _saleidList[_saleIndex[_saleid]];

        c._info_buyer = msg.sender;
        c._info_saledate = block.timestamp;
        c._info_salestatus = saleStatus.saled;
        
        uint256 sellerAmount = _price * 8 / 100;

        IERC20(usdcAddress).transferFrom(msg.sender, address(this), _price - sellerAmount);

        IERC20(usdcAddress).transferFrom(msg.sender, c._info_seller, sellerAmount);

        IERC721(c._info_nftAddress).safeTransferFrom(address(this) , msg.sender , c._info_tokenid);

        ICurate(cutateAddress).cancelProposal(c._info_curateid);

        removeListing(_saleid, 1);
        /**
            20% 분배.
         */
        emit buyNFTEvent(_saleid , c._info_seller ,c._info_buyer , c._info_price);
    }

    function removeListing (uint256 _typeid , uint256 _type ) internal {

        if (_type == _TYPE_SALEID_) {
            uint256 lastSaleIdIndex = _saleidList.length - 1;
            uint256 saleIndex = _saleIndex[_typeid]; /* 삭제시킬 array index */

            marketListing_info memory lastInfo = _saleidList[lastSaleIdIndex];

            _saleidList[saleIndex] = lastInfo;

            _saleIndex[lastInfo._info_saleId] = saleIndex;

            _saleidList.pop();

            delete _saleIndex[_typeid];

        }else if (_type == _TYPE_AUCTIONID_) {
            uint256 lastAuctionIdIndex = _auctionidList.length -1;
            uint256 auctionIndex = _auctionIndex[_typeid];

            auctionListing_info memory lastInfo = _auctionidList[lastAuctionIdIndex];

            _auctionidList[auctionIndex] = lastInfo;

            _auctionIndex[lastInfo._info_auctionId] = auctionIndex;

            _auctionidList.pop();
            delete _auctionIndex[_typeid];
        }
    }

