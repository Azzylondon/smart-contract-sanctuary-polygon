pragma solidity 0.8.0;
import "../series/ISeriesController.sol";
import "../configuration/IAddressesProvider.sol";

/**
 @title Swap
 @author The Siren Devs
 @notice Onchain options protocol for minting, exercising, and claiming calls and puts
 @notice Manages the lifecycle of individual Series
 @dev The id's for bTokens and wTokens on the same Series are consecutive uints
 */
interface ISwap {
    struct Swap {
        uint64 productId;
        address poolAddress;
        ISwap.SwapStatus status;
        uint40 createSwap;
        uint40 expirationDate;
        uint256 bidSize;
        uint256 minBidPrice;
        ISwap.ProductType productType;
    }

    /// @notice All possible states a Swap can be in with regard to its expiration date
    enum SwapStatus {
        OPEN,
        CLOSED
    }

    /// @notice All possible ProductTypes a Swap can have
    enum ProductType {
        /**
         * New option token cans be created.
         * Existing positions can be closed.
         * bTokens cannot be exercised
         * wTokens cannot be claimed
         */
        OPTION
    }

    ///////////////////// EVENTS /////////////////////

    /// @notice Emitted when the owner creates a new Swap
    event SwapCreated(uint256 swapId, ISwap.Swap newSwap);

    /// @notice Emitted when the SeriesController gets initialized
    event SwapUpdated(uint256 swapId, ISwap.SwapStatus);

    ///////////////////// VIEW/PURE FUNCTIONS /////////////////////

    function initialize(IAddressesProvider _addressesProvider) external;

    function createSwap(
        uint64 seriesId,
        uint256 minBidPrice,
        uint40 createSwap,
        uint40 expirationDate,
        uint256 bidSize,
        address poolAddress
    ) external;

    function settleSwap(
        uint256 swapId,
        uint256 buyAmount,
        uint256 nonce,
        address referrer,
        uint256 sellAmount,
        address signer,
        bytes32 r,
        bytes32 s,
        uint8 v
    ) external;

    function checkBid(
        uint256 buyAmount,
        uint256 nonce,
        address referrer,
        uint256 sellAmount,
        address signer,
        uint256 swapId,
        bytes32 r,
        bytes32 s,
        uint8 v
    ) external view returns (bool);

    function getSwap(uint256 swapId)
        external
        view
        returns (
            uint64,
            address,
            ISwap.SwapStatus,
            uint40,
            uint256,
            uint256,
            ISwap.ProductType
        );

    function getProduct(uint256 productId)
        external
        view
        returns (ISeriesController.Series memory);
}

