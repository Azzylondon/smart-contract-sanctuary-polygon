contract Swap is ISwap, OwnableUpgradeable, Proxiable {
    /// @dev An array of all the Swap structs ever created by the ParadigmSwap
    ISwap.Swap[] internal allSwaps;

    /// @notice Monotonically incrementing index, used when creating Swaps.
    uint64 public latestIndex;

    /// @dev The address for the AddressesProvider
    IAddressesProvider addressesProvider;

    function initialize(IAddressesProvider _addressesProvider) public override {
        addressesProvider = _addressesProvider;

        __Ownable_init();

        // emit SwapInitialized(lpToken, address(0x0), address(_seriesController));
    }

    function createSwap(
        uint64 productId,
        uint256 minBidPrice,
        uint40 createSwap,
        uint40 expirationDate,
        uint256 bidSize,
        address poolAddress
    ) external override {
        //TODO add requires that we want to have here

        // store variable in memory for reduced gas costs when reading
        uint64 _latestIndex = latestIndex;
        ISwap.ProductType productType = ISwap.ProductType.OPTION;
        SwapStatus _status = ISwap.SwapStatus.OPEN;
        ISwap.Swap memory _swap = ISwap.Swap(
            productId,
            poolAddress,
            _status,
            createSwap,
            expirationDate,
            bidSize,
            minBidPrice,
            productType
        );

        // add to the array so the Series data can be accessed in the future
        allSwaps.push(_swap);

        // Emit the event
        emit SwapCreated(_latestIndex, _swap);

        // don't forget to increment our series index
        _latestIndex = _latestIndex + 1;

        // now that we're done incrementing our memory _latestIndex, update the storage variable latestIndex
        latestIndex = _latestIndex;
    }

    function settleSwap(
        uint256 swapId,
        uint256 buyAmount,
        uint256 nonce,
        address referrer,
        uint256 sellAmount,
        address signer,
        bytes32 r,
        bytes32 s,
        uint8 v
    ) external override {
        //TODO add requires here
        // I think we should make a generic settlement piece here instead of calling to the amm
        (
            uint64 _productId,
            address poolAddress,
            ,
            uint256 expirationDate,
            ,
            ,

        ) = getSwap(swapId);
        IMinterAmm(poolAddress).bTokenDirectAuctionBuy(
            _productId,
            expirationDate,
            buyAmount,
            nonce,
            sellAmount,
            signer,
            r,
            s,
            v
        );
    }

    function checkBid(
        uint256 buyAmount,
        uint256 nonce,
        address referrer,
        uint256 sellAmount,
        address signer,
        uint256 swapId,
        bytes32 r,
        bytes32 s,
        uint8 v
    ) external view override returns (bool) {
        // [ 'SIGNER_ALLOWANCE_LOW',
        //   'SIGNER_BALANCE_LOW']
        uint256 pricePerOption = (sellAmount * 1e18) / buyAmount;
        (
            uint64 _productId,
            address poolAddress,
            ,
            ,
            ,
            uint256 minBidPrice,

        ) = getSwap(swapId);

        ISeriesController.Series memory series = getProduct(_productId);

        require(minBidPrice <= pricePerOption, "PRICE_TOO_LOW");

        return true;
    }

    function getSwap(uint256 swapId)
        public
        view
        override
        returns (
            uint64 productId,
            address poolAddress,
            ISwap.SwapStatus status,
            uint40 expirationDate,
            uint256 bidSize,
            uint256 minBidPrice,
            ISwap.ProductType productType
        )
    {
        //Return swap + series
        ISwap.Swap memory _swap = allSwaps[swapId];
        return (
            _swap.productId,
            _swap.poolAddress,
            _swap.status,
            _swap.expirationDate,
            _swap.bidSize,
            _swap.minBidPrice,
            _swap.productType
        );
    }

    function getProduct(uint256 productId)
        public
        view
        override
        returns (ISeriesController.Series memory)
    {
        return (
            ISeriesController(addressesProvider.getSeriesController()).series(
                productId
            )
        );
    }
}

