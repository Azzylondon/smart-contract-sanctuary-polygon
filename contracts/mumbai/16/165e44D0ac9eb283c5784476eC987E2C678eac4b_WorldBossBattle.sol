pragma solidity >=0.8.14;


// import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/v3.4.0/contracts/access/AccessControl.sol";


contract GameAccessControls is AccessControl {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    event AdminRoleGranted(address indexed account, address indexed sender);

    event AdminRoleRemoved(address indexed account, address indexed sender);

    event MinterRoleGranted(address indexed account, address indexed sender);

    event MinterRoleRemoved(address indexed account, address indexed sender);

    // set the deployer admin role
    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    // Admin role Add, Revoke and Check
    function addAdminRole(address _address) external {
        grantRole(DEFAULT_ADMIN_ROLE, _address);
        emit AdminRoleGranted(_address, _msgSender());
    }

    function revokeAdminRole(address _address) external {
        revokeRole(DEFAULT_ADMIN_ROLE, _address);
        emit AdminRoleRemoved(_address, _msgSender());
    }

    function hasAdminRole(address _address) external view returns (bool) {
        return hasRole(DEFAULT_ADMIN_ROLE, _address);
    }

    // Minter role Add, Revoke and check
    function addMinterRole(address _address) external {
        grantRole(MINTER_ROLE, _address);
        emit MinterRoleGranted(_address, _msgSender());
    }

    function revokeMinterRole(address _address) external {
        revokeRole(MINTER_ROLE, _address);
        emit MinterRoleRemoved(_address, _msgSender());
    }

    function hasMinterRole(address _address) external view returns (bool) {
        return hasRole(MINTER_ROLE, _address);
    }
}

