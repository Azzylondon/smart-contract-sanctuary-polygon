pragma solidity >=0.8.14;





contract BattleData {
     // Structure of the battle in the game
    struct Battle {
        string battleId;
        string[] bossId;
        uint256 bossCount;
        uint256 startedTime;
        uint256 endTime;
        bool status;
    }

    //mapping struct 
    mapping(string => Battle) public battles;

    //array of all created battle
    string[] public battleList;

    GameAccessControls public accessControls;

    // TODO 1. create battles in the game
    // 2. enable or disable battle for the game
    // 3. get battle details by id

    //events to be emitted when battle is created, disabled and enable
    event CreateBattle(address indexed adminAddress, string BattleId, string[] BossId);
    event DisableBattle(string BattleId, bool status);
    event EnableBattle(address indexed playerAddress, string BattleId);

    function makeAdmin(address normal_address) public {
        accessControls.addAdminRole(normal_address);
    }

    function checkadmin(address input_address) public view{
        accessControls.hasAdminRole(input_address);
    }

    //function to create battle requires battleId and bossId
    function createBattle(string memory _battleId, string[] memory _bossId) public {
        require(accessControls.hasAdminRole(msg.sender) != false, "NFTStaking: Unauthorized to create battle.");        
        address adminAddress = msg.sender;
        Battle storage battleInstance = battles[_battleId];
        battleInstance.battleId = _battleId; 
        battleInstance.startedTime = block.timestamp;
        battleInstance.status = true;
        for(uint i = 0; i < _bossId.length; i++){
            battleInstance.bossId.push(_bossId[i]);
        }
        emit CreateBattle(adminAddress, _battleId, _bossId);
        battleList.push(_battleId);    
    }

    //function to check the battleStatus i.e true or false
    function checksBattleStatus(string memory _battleId)public view returns (uint256){
         if(battles[_battleId].status == true){
            return 1;
         }
         return 0;
    }

    //function to change true staus of battle to false i.e make inactive
    function disableBattle(string memory _battleId) public{
        require(battles[_battleId].status = true);
        battles[_battleId].status = false;
        emit DisableBattle(_battleId, battles[_battleId].status);
    }

     //function to change false staus of battle to true i.e make active
    function enableBattle(string memory _battleId) public{
        address adminAddress = msg.sender;
        require(battles[_battleId].status = false);
        battles[_battleId].status = true;
        emit EnableBattle(adminAddress, _battleId);
    }

    //function to get information of battle by its Id
    function battleDetailsById(string memory _battleId) public view returns(string memory, uint256, uint256, bool) {
        return( battles[_battleId].battleId, battles[_battleId].bossCount, battles[_battleId].startedTime, battles[_battleId].status);
    }

    //function to see all the battle that has been created 
    function returnBattleList () public view returns (string []  memory) {
        uint256 lengthOfallBattle = battleList.length;
        string [] memory createdBattle = new string [] (lengthOfallBattle);
        for(uint256 i = 0; i< lengthOfallBattle; i++){
            createdBattle[i] = battleList[i];
        }
        return (createdBattle);    
    }

    // Structure of the boss in the 

    //add, delete, update in another contract 
    
    struct Boss {
        string bossId;
        string name;
        uint256 maxHp;
        string uri;
        uint BossPointer;
    }
    //mapping of structure of boss
    mapping(address => Boss) public bosses;
    
    //variable to store every boss that is created
    string[] public bossList;

    //events to be emitted when boss is created and updated 
    event CreateBoss(address indexed adminAddress, string bossId);
    event UpdateBoss(address indexed adminAddress, string bossId);

    //function to create a new boss
    function newBoss (address adminAddress, string memory bossId, string memory name, uint256 maxHp, string memory uri) public {
        bosses[adminAddress].bossId = bossId;
        bosses[adminAddress].name = name;
        bosses[adminAddress].maxHp = maxHp;
        bosses[adminAddress].uri = uri;
        emit CreateBoss(adminAddress, bossId);
        bossList.push(bossId);
        bosses[adminAddress].BossPointer = bossList.length - 1;
    }

    //function to update the boss character
    function updateBoss (address adminAddress, string memory bossId, string memory name, uint256 maxHp, string memory uri) public {
        bosses[adminAddress].bossId = bossId;
        bosses[adminAddress].name = name;
        bosses[adminAddress].maxHp = maxHp;
        bosses[adminAddress].uri = uri;
        emit UpdateBoss(adminAddress, bossId);   
