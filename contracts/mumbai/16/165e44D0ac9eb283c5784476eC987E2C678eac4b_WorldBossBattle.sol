// File: contracts/WorldBossBattle.sol


pragma solidity >=0.8.14;






    
// import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/v3.4.0/contracts/utils/Context.sol";
// import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/v3.4.0/contracts/cryptography/ECDSA.sol";
// import "https://github.com/OpenZeppelin/openzeppelin-contracts/blob/v3.4.0/contracts/access/Ownable.sol";




contract WorldBossBattle is Context, WorldBattleSession, BattleOps, BattleData {
    using ECDSA for bytes32;

    // Events for NFT Staking
    event SetNFTContract(
        address indexed _oldNFTContract,
        address indexed _nftContract,
        address sender
    );
    event SetGamePausedState(
        bool isPaused,
        address sender
    );
    event NFTStaked(
        address indexed _nftOwner,
        uint256 _tokenId,
        string _battleId
    );
    event NFTUnstaked (
        address indexed _nftReceiver,
        uint256 _tokenId,
        string _battleId
    );
    event NFTRestaked(
        address indexed _player, 
        uint256 _tokenId, 
        string _battleId, 
        uint256 cooldownTime
    );

    ICDHNFTInventory public cdhNFT;
    ITOWER public towerToken;
    // GameAccessControls public accessControls;
    address public nftHolder;

    uint256 public lastInteractionTime;

    struct Staker {
        uint256[] tokenIds;
        mapping(uint256 => uint256) tokenIndex;
        uint256 balance; // might not be required
        uint256 stakedTime;
        mapping(uint256 => uint256) cooldownTime;  
    }

    address[] stakersAddress; //list of staker address

    //below is nested mapping, have two string acts as unique key they are index and count index denote the "stakersAddress" index where value counts how many times staking is done
    mapping(address => mapping(string => uint256)) public mappingArray;  
    
    //Staker struct mapping 
    mapping(address => Staker) public stakers;

    //tokenId is mapped to address
    mapping(uint256 => address) public tokenOwner;


    mapping(uint256 => string) public tokenTobattleId; //maps tokenId to battleId

    
    bool public isGamePaused;

    constructor(
        ICDHNFTInventory _cdhNft,
        ITOWER _towerToken, 
        GameAccessControls _accessControl, 
        address _nftHolder 
    ) {
        cdhNFT = _cdhNft;   //for the inventory of NFT where NFT is created
        towerToken = _towerToken; //to get the tower token 
        accessControls = _accessControl;  //for the access control
        nftHolder = _nftHolder; //to hold the staked NFT
        lastInteractionTime = block.timestamp;
        isGamePaused = true;

        setSigner(_msgSender());
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    // Staking contract operations
    function setTowerToken(address _towerToken) external {
        require(accessControls.hasAdminRole(_msgSender()), "NFTStaking: Unauthorized.");
        require(_towerToken != address(0));

        towerToken = ITOWER(_towerToken);
    }
    
    function setCDHNFTContractAddress(address _nftContract) external {
        require(accessControls.hasAdminRole(_msgSender()), "NFTStaking: Unauthorized.");
        require(_nftContract != address(0));

        emit SetNFTContract(address(cdhNFT), _nftContract, _msgSender());
        cdhNFT = ICDHNFTInventory(_nftContract);
    }

    function setGamePausedState(bool isPaused) external {
        isGamePaused = isPaused;
        emit SetGamePausedState(isPaused, _msgSender());
    }

    function getAllToken(address _player) public view {
        uint256[] memory allTokens = cdhNFT.getAllTokens(_player);
        for(uint i =0 ; i < allTokens.length; i++){
            console.log("tokens are:", allTokens[i]);
        }
    }

    function setNFTHolderAddress(address _holder) external {
        nftHolder = _holder;
    }


    // public methods for staking operations
    function stake(uint256 _tokenId, string memory _battleId)override external {
        internalStake(_msgSender(), _tokenId, _battleId);
    }

    function stakeAll(string memory _battleId) override external {
        internalStakeAll(_msgSender(), _battleId);
    }

    function stakeTokens(uint256[] memory tokenIds, string memory _battleId) override external {
        address player = _msgSender();
        for(uint i = 0; i < tokenIds.length; i++){
            internalStake(player, tokenIds[i], _battleId);
        }
    }

    function getStakedTokensFor(address _player) public view returns (uint256[] memory tokenIds){
        return stakers[_player].tokenIds;
    }

    function getStakedTokens() external view returns (uint256[] memory tokenIds){
        return getStakedTokensFor(_msgSender());
    }

    function unstake(uint256 _tokenId, string memory _battleId) override external {
        require(tokenOwner[_tokenId] == _msgSender(), "NFTunStaking: Unauthorized.");
        internalUnstake(_msgSender(), _tokenId, _battleId);
    }

    function unstakeAll(string memory _battleId) override external {
        internalUnstakeAll(_msgSender(), _battleId);
    }

    function unstakeTokens(uint256[] memory tokenIds, string memory _battleId) override external {
        address player = _msgSender();
        for(uint i = 0; i < tokenIds.length; i++){
            internalUnstake(player, tokenIds[i], _battleId);
        }
    }


    // internal methods for staking operations
    function checkReqiredTokenBalance(uint256 _nftCount, uint256 _balance) public view returns (bool) {
        uint256 requiredBalance = _nftCount * 100;
        console.log( _balance >= requiredBalance);
        return _balance >= requiredBalance;
        
    }

    function internalStake(address _player, uint256 _tokenId, string memory _battleId) internal {
        Staker storage staker = stakers[_player];
        

        uint256[] storage existingStakedTokens = stakers[_player].tokenIds;
        uint256 existingStakedTokensCount = existingStakedTokens.length;

        console.log("count: ", existingStakedTokensCount);
        uint256 towerBalance = towerToken.balanceOf(_player);
        console.log("tower balance: ", towerBalance);

        //checking battle has been created or not by the admin 
        require(checksBattleStatus(_battleId) != 0, "NFTStaking: battle has not been created");


        if(checkReqiredTokenBalance(existingStakedTokensCount + 1, towerBalance)){
            staker.balance = towerBalance;
            console.log("staker balance", staker.balance);
            staker.tokenIds.push(_tokenId);
            for(uint256 i = 0; i < staker.tokenIds.length; i++){
                console.log("tokendId Stored:", staker.tokenIds[i]);
            }
            staker.tokenIndex[_tokenId] =  staker.tokenIds.length -1;
            console.log("token Index", staker.tokenIndex[_tokenId]);
            tokenOwner[_tokenId] = _player;
            staker.cooldownTime[_tokenId] = block.timestamp;
            console.log("timestamp:", staker.cooldownTime[_tokenId]);
            console.log("staking token ...");
            ICDHNFTInventory(cdhNFT).safeTransferFrom(
                _player,
                nftHolder,
                _tokenId,
                1,
                "0x"
            );

            //mapping players address 
            tokenTobattleId[_tokenId] = _battleId;
            console.log("mapping of token to battleId:", tokenTobattleId[_tokenId]);



            emit NFTStaked(_player, _tokenId, _battleId);


            //if its the first time in the mapping that will be denoted by index, it will push into the array for first time if its not it will increase the counter value 
            if(mappingArray[_player]["index"]== 0){
                stakersAddress.push(_player);
                for(uint256 i =0 ; i< stakersAddress.length -1; i++){
                    console.log("all ofstakersAddress:", stakersAddress[i]);
                }
                mappingArray[_player]["index"]= stakersAddress.length;
                mappingArray[_player]["count"] = 1;
            }
                mappingArray[_player]["count"]++ ; //else
                console.log("index of stakerAddress: ",mappingArray[_player]["index"]);
                console.log("count of stakerAddress: ", mappingArray[_player]["count"]);
        } else {
            revert("Not enough Tower Tokens.");
        }
    }

    function internalStakeAll(address _player, string memory _battleId) internal {
        uint256[] memory allTokens = cdhNFT.getAllTokens(_player);
        uint256 nftBalance = allTokens.length;
        if (nftBalance > 0 ){
            for (uint i = 0; i < nftBalance; i++){
                internalStake(_player, allTokens[i], _battleId);
            }
        }
    }

    function internalStakeBatch(address _player, uint256[] memory _tokenIds, string memory _battleId) internal {
        for (uint i = 0 ; i< _tokenIds.length; i++){
            internalStake(_player, _tokenIds[i], _battleId);
        }
    }

    function internalUnstake(address _player, uint256 _tokenId, string memory _battleId) internal {

        Staker storage staker = stakers[_player];
        require(checksBattleStatus(_battleId) != 0, "NFTStaking: battle has not been created");

        uint256 lastIndex = staker.tokenIds.length - 1;
        uint256 lastIndexKey = staker.tokenIds[lastIndex];
        
        uint256 tokenIdIndex = staker.tokenIndex[_tokenId];

        staker.tokenIds[tokenIdIndex] = lastIndexKey;
        staker.tokenIndex[lastIndexKey] = tokenIdIndex;
        staker.cooldownTime[_tokenId] = 0;
        
        if (staker.tokenIds.length > 0) {
            staker.tokenIds.pop();
            delete staker.tokenIndex[_tokenId];
        }
        ICDHNFTInventory(cdhNFT).safeTransferFrom(
                nftHolder,
                _player,
                _tokenId,
                1,
                "0x"
            );
        emit NFTUnstaked(_player, _tokenId, _battleId);
        // delete mapping 
        delete tokenTobattleId[_tokenId]; 

        //delete from mapping of mappingArray by checking count status 
        if (mappingArray[_player]["index"] == 0){
            console.log("not found");
        }
        else {
            uint256 count = mappingArray[_player]["count"];
            if (count == 1){
                uint256 index = mappingArray[_player]["index"];
                mappingArray[_player]["index"] = 0;
                mappingArray[_player]["count"] = 0;
                address popedAddress = stakersAddress[stakersAddress.length - 1];
                stakersAddress.pop();
                stakersAddress[index-1] = popedAddress;
                mappingArray[popedAddress]["index"] = index;
            }
            else{
                mappingArray[_player]["count"]--;
            }

        }
        for(uint256 i = 0; i < staker.tokenIds.length; i++){
                console.log("tokendId Stored:", staker.tokenIds[i]);
            }

            console.log("token Index", staker.tokenIndex[_tokenId]);
            
            for(uint256 i =0 ; i< stakersAddress.length -1; i++){
                    console.log("all ofstakersAddress:", stakersAddress[i]);
            }
            
            console.log("index of stakerAddress: ",mappingArray[_player]["index"]);
            console.log("count of stakerAddress: ", mappingArray[_player]["count"]);
    }

    function internalUnstakeAll(address _player, string memory _battleId) internal {
        uint256[] memory allTokens = cdhNFT.getAllTokens(_player);
        uint256 nftCounts = allTokens.length;
        if (nftCounts > 0 ){
            for (uint i = 0; i < nftCounts; i++){
                internalUnstake(_player, allTokens[i], _battleId);
            }
        }
    }

    function internalUnstakeBatch(address _player, uint256[] memory _tokenIds, string memory _battleId) internal {
        for (uint i = 0; i < _tokenIds.length; i++) {
            if (tokenOwner[_tokenIds[i]] == msg.sender) {
                internalUnstake(_player, _tokenIds[i], _battleId);
            }
        }
    }

    function internalRestake(address _player, uint256 _tokenId, string memory _battleId) internal {
        require(tokenOwner[_tokenId] == _player, "NFTStaking: Unauthorized.");
        //is nft staked
        require(tokenOwner[_tokenId] != address(0x0), "NFTStaking: Token Id not staked");
        Staker storage staker = stakers[_player];
        staker.cooldownTime[_tokenId] = block.timestamp;
        emit NFTRestaked(_player, _tokenId, _battleId,staker.cooldownTime[_tokenId] );
    }


    function restake(uint256 _tokenId, string memory _battleId) external{
        internalRestake(_msgSender(), _tokenId, _battleId);
    }

    function restakeAll(string memory _battleId) external{
        address player = _msgSender();
        uint256[] memory allTokens = cdhNFT.getAllTokens(player);
        uint256 nftCounts = allTokens.length;
        if (nftCounts > 0 ){
            for (uint i = 0; i < nftCounts; i++){
                this.restake(allTokens[i], _battleId);
            }
        }
    }
    function restakeTokens(uint256[] memory _tokenIds, string memory _battleId) external{
        for (uint i = 0; i < _tokenIds.length; i++ ){
            internalRestake(_msgSender(), _tokenIds[i], _battleId);
        }
    }

    function stakeWithoutBattleId(uint256 _tokenId) external{
        internalStake(_msgSender(), _tokenId, battleList[battleList.length - 1]);
    }

    function stakeAllWithoutBattleId() external{
        uint256[] memory allTokens = cdhNFT.getAllTokens(_msgSender());
        uint256 nftBalance = allTokens.length;
        if (nftBalance > 0 ){
            for (uint i = 0; i < nftBalance; i++){
                internalStake(_msgSender(), allTokens[i], battleList[battleList.length - 1]);
            }
        }
    }

    function stakeTokensWithoutBattleId(uint256[] memory _tokenIds) external{
        for(uint i = 0; i < _tokenIds.length; i++){
            internalStake(msg.sender, _tokenIds[i],  battleList[battleList.length - 1]);
        }
    }

    function restakeWithoutBattleId(uint256 _tokenId)override external {
        internalRestake(_msgSender(), _tokenId, battleList[battleList.length - 1]);
    }

    function restakeTokensWithoutBattleId(uint256[] memory _tokenIds)override external {
        for (uint i = 0; i < _tokenIds.length; i++ ){
            internalRestake(_msgSender(), _tokenIds[i],battleList[battleList.length - 1]);
        }

    }

    function restakeAllWithoutBattleId()override external{
        uint256[] memory allTokens = cdhNFT.getAllTokens(msg.sender);
        uint256 nftCounts = allTokens.length;
        if (nftCounts > 0 ){
            for (uint i = 0; i < nftCounts; i++){
                this.restake(allTokens[i], battleList[battleList.length - 1]);
            }
        }
    }

    function toReturnCooldownTime(address _player, uint256 _tokenId) public view returns(uint256){
        Staker storage staker = stakers[_player];
        return staker.cooldownTime[_tokenId];
    }

