pragma solidity ^0.8.0;

// khai b├ío 1 struct b├¬n ngo├ái contract
// struct nh├á t├ái trß╗ú
struct Funder {
    address addr;
    uint256 amount;
}

//contract huy ─æß╗Öng vß╗æn tß╗½ cß╗Öng ─æß╗ông
contract CrowdFunding {
    // c├íc struct c├│ thß╗â ─æ╞░ß╗úc ─æß╗ïnh ngh─⌐a b├¬n trong contract
    // l├ám cho ch├║ng chß╗ë hiß╗ân thß╗ï b├¬n trong contract v├á trong c├íc contract c├│ nguß╗ôn gß╗æc
    struct Campaign {
        // struct chiß║┐n dß╗ïch
        address payable beneficiary; // ng╞░ß╗¥i thß╗Ñ h╞░ß╗ƒng
        uint256 fundingGoal; // mß╗Ñc ti├¬u t├ái trß╗ú
        uint256 numFunders; // sß╗æ ng╞░ß╗¥i t├ái trß╗ú
        uint256 amount;
        mapping(uint256 => Funder) funders;
    }

    uint256 numCampain;
    mapping(uint256 => Campaign) campaigns;

    // tß║ío mß╗¢i 1 Campaign
    function newCampaign(address payable beneficiary, uint256 fundingGoal)
        public
        returns (uint256 campaignId)
    {
        campaignId = numCampain++;
        Campaign storage campaign = campaigns[campaignId];
        campaign.beneficiary = beneficiary;
        campaign.fundingGoal = fundingGoal;
    }

    //xem kß║┐t quß║ú vß╗½a tß║ío
    function getCampain(uint256 campaignId)
        public
        view
        returns (
            address beneficiary,
            uint256 fundingGoal,
            uint256 numFunders,
            uint256 amount
        )
    {
        Campaign storage campaign = campaigns[campaignId];
        beneficiary = campaign.beneficiary;
        fundingGoal = campaign.fundingGoal;
        numFunders = campaign.numFunders;
        amount = campaign.amount;
    }

    // h├ám ─æ├│ng g├│p
    function contribute(uint256 campaignId) public payable {
        Campaign storage campaign = campaigns[campaignId];

        // insert struct Funder
        campaign.funders[campaign.numFunders++] = Funder({
            addr: msg.sender,
            amount: msg.value
        });

        campaign.amount += msg.value;
    }

    // function kiß╗âm tra mß╗Ñc ti├¬u ─æ├ú ─æß║ít ─æ╞░ß╗úc hay ch╞░a ?
    function checkGoalReached(uint256 campaignID)
        public
        returns (bool reached)
    {
        Campaign storage campaign = campaigns[campaignID];
        if (campaign.amount < campaign.fundingGoal) return false;
        uint256 amount = campaign.amount;
        campaign.amount = 0;
        campaign.beneficiary.transfer(amount);
        return true;
    }
}