pragma solidity =0.8.13;

interface IAnimeTokenStakingPool {
  // Views

  function balanceOf(address account) external view returns (uint256);

  function earned(address account) external view returns (uint256);

  function lastTimeRewardApplicable() external view returns (uint256);

  function rewardPerToken() external view returns (uint256);

  function totalSupply() external view returns (uint256);

  function getAverageTotalSupply() external view returns (uint256);

  function estimateDuration(uint256 _type, address account) external;

  function vestingSchedule() external view returns (uint256);

  function getRewardRate() external view returns (uint256);

  function canBuyNft(uint256 _type, address account)
    external
    view
    returns (bool);

  // Mutative

  function claimStudioReward() external;

  function buyNft(uint256 _type) external;

  function stake(uint256 amount) external;

  function setPaused(bool _paused) external;

  function setPriceOfType(uint256 _type, uint256 _price) external;

  function setNonceOfType(uint256 _type, uint256 _nonce) external;

  function setMaxSerialsOfType(uint256 _type, uint256 _maxSerial) external;

  function setMinRewardOfType(uint256 _type, uint256 _minReward) external;

  function setProductNft(address _productNft) external;

  function setInitialTime(uint256 _time) external;

  function setInitialTime() external;

  function setAnimeStudio(address _animeStudio) external;

  function exit() external;
}

