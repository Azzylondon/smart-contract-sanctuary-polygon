import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../interfaces/IEMERC721A.sol";
import "../interfaces/IEMERC1155.sol";
import "../enums/PackageType.sol";
import "../structs/Package.sol";
import "../enums/Steps.sol";
import "hardhat/console.sol";

contract EMPackageSaleManager is Pausable, Ownable {
    event PFPCreated(address indexed woner, uint256 pfpId);

    IEMERC721A EMPFP;
    IEMERC1155 EMPackage;

    mapping (uint256 => bool) hasClaimedAstronautByPackage;
    mapping (uint256 => uint256) public packageTopfp;
    mapping (uint256 => uint256) public pfpToPackage;
    uint256 public constant MAX_SUPPLY = 13000;
    uint256 public constant MAX_PRIVATE = 5000;
    uint256 public privateCollectorPrice = 0.16 ether;
    uint256 public publicCollectorPrice = 0.2 ether;
    uint256 public privateInvestorPrice = 0.08 ether;
    uint256 public publicInvestorPrice = 0.09 ether;
    uint256 public privatePlayerPrice = 0.02 ether;
    uint256 public publicPlayerPrice = 0.03 ether;
    uint8 public mintPerTransaction = 5;
    uint8 public mintPerWallet = 10;
    uint256 public privateSaleCount = 0;
    uint256 public totalPackagesCount = 0;
    bytes32 public merkleRoot;
    uint256 public privateSaleStartTime = 1651701600;
    uint256 public privateSaleEndTime = 1652219999;
    uint256 public publicSaleStartTime = 1652220000;
    uint256 public publicSaleEndTime = 1652911198;
    bool public isActive = false;
    address EMRefundManager;

    constructor(
        IEMERC721A pfp_,
        IEMERC1155 package_,
        bytes32 merkleRoot_){
        EMPFP = pfp_;
        merkleRoot = merkleRoot_;
        EMPackage = IEMERC1155(package_);
    }

function buyPackage(
    PackageType _packageType,
    uint8 _quantity,
    bool _refundable,
	bytes32[] calldata _proof
    )
    external
	payable
    whenNotPaused
    {
        require(isActive, "not active");
        require(currentTime() >= privateSaleStartTime, "Private sale has not started yet");
        require(currentTime() <= publicSaleEndTime, "Public sale is over");
        require(_quantity <= mintPerTransaction, "EMPackageSaleManager: You can't mint more than 10 tokens in one time");
        require(_quantity + EMPackage.getUserPackagesCount(msg.sender) <= mintPerWallet, "(Sale): You can't mint more than 20 tokens");
        uint256 uinitPrice;
        if(currentTime() <= privateSaleEndTime) {
            require(privateSaleCount + _quantity <= MAX_PRIVATE, "EMPackageSaleManager(Private Sale): Max supply exceeded");
            require(isWhiteListed(msg.sender, _proof), "EMPackageSaleManager(Private Sale): Not whiteListed");
            uinitPrice = privatePlayerPrice;
            if(_packageType == PackageType.COLLECTOR)
                uinitPrice = privateCollectorPrice;
            if(_packageType == PackageType.INVESTOR)
                uinitPrice = privateInvestorPrice;
            require(msg.value >= uinitPrice * _quantity, "EMPackageSaleManager(Private Sale): Not enough funds");
            privateSaleCount += _quantity;
        }else{
            require(EMPackage.getTotalPackagesCount() + _quantity <= MAX_SUPPLY, "EMPackageSaleManager(Public Sale): Max supply exceeded");
            uinitPrice = privatePlayerPrice;
            if(_packageType == PackageType.COLLECTOR)
                uinitPrice = publicCollectorPrice;
            if(_packageType == PackageType.INVESTOR)
                uinitPrice = publicInvestorPrice;
            require(msg.value >= uinitPrice * _quantity, "EMPackageSaleManager(Public Sale): Not enough funds");
        } 
        
        uint256 nextPfpId = EMPFP.currentIndex();
        uint256 nextPackageId = EMPackage.getTotalPackagesCount() + 1;
        EMPFP.safeMint(msg.sender, _quantity);
        EMPackage.mintPackage(msg.sender, uint8(_packageType), _quantity, _refundable, uinitPrice);
        for (uint8 index = 0; index < _quantity; index++) {
            uint256 pfpId = nextPfpId + index;
            uint256 packageId = nextPackageId + index;
            packageTopfp[packageId] = pfpId;
            pfpToPackage[pfpId] = packageId;
            emit PFPCreated(msg.sender, pfpId);
        }
        totalPackagesCount += _quantity;
    }

    function payBack(address _owner, uint256 _packageId) external {
        require(msg.sender == EMRefundManager, "EMPackageSaleManager(payBack): Not EMRefundManager");
        require(EMPackage.ownerOf(_packageId) == _owner, "You are not the owner of this package");
        Package memory pack = EMPackage.getPackageByOwner(_owner, _packageId);
        EMPackage.setPackageRefund(_owner, _packageId);
        payable(_owner).transfer(pack.price);
    }

    function currentTime() internal view returns (uint256) {
        return block.timestamp;
    }

    function getStep() public view returns (Steps stape) {
        if(currentTime() < privateSaleStartTime)
            return Steps.BEFORE;
        if(privateSaleStartTime <= currentTime() && currentTime() <= privateSaleEndTime)
            return Steps.PRIVATE_SALE;
        if(privateSaleEndTime <= currentTime() && currentTime() <= publicSaleStartTime)
            return Steps.BEFORE_PUBLIC_SALE;
        if(publicSaleStartTime <= currentTime() && currentTime() <= publicSaleEndTime)
            return Steps.PUBLIC_SALE;
        if(currentTime() > publicSaleEndTime)
            return Steps.SALETIME_OVER;
    }

    function isWhiteListed(address _account, bytes32[] calldata _proof) internal view returns (bool){
        return verify(leaf(_account), _proof);
    }

    function isUserWhiteListed(address _account, bytes32[] calldata _proof) external view returns (bool){
        return verify(leaf(_account), _proof);
    }

    function leaf(address _account) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked(_account));
    }

    function verify(bytes32 _leaf, bytes32[] memory _proof) internal view returns (bool) {
        return MerkleProof.verify(_proof, merkleRoot, _leaf);
    }
    function getPackageIdByPFP(uint256 _pfpId) external view returns(uint256){
        return pfpToPackage[_pfpId];
    }
    function getPFPIdByPackage(uint256 _packageId) external view returns(uint256){
        return packageTopfp[_packageId];
    }
    function setPrivateCollectorPrice(uint256 price) external onlyOwner {
        privateCollectorPrice = price * 1e18;
    }
    function setPublicCollectorPrice(uint256 price) external onlyOwner {
        publicCollectorPrice = price * 1e18;
    }
    function setPrivateInvestorPrice(uint256 price) external onlyOwner {
        privateInvestorPrice = price * 1e18;
    }
    function setPublicInvestorPrice(uint256 price) external onlyOwner {
        publicInvestorPrice = price * 1e18;
    }
    function setPrivatePlayerPrice(uint256 price) external onlyOwner {
        privatePlayerPrice = price * 1e18;
    }
    function setPublicPlayerPrice(uint256 price) external onlyOwner {
        publicPlayerPrice = price * 1e18;
    }
    function setMerkleRoot(bytes32 _newMerkleRoot) external onlyOwner {
        merkleRoot = _newMerkleRoot;
    }
    function setPrivateSaleStartTime(uint256 _time) external onlyOwner {
        privateSaleStartTime = _time;
    }
    function setPrivateSaleEndTime(uint256 _time) external onlyOwner {
        privateSaleEndTime = _time;
    }
    function setPublicSaleStartTime(uint256 _time) external onlyOwner {
        publicSaleStartTime = _time;
    }
    function setPublicSaleEndTime(uint256 _time) external onlyOwner {
        publicSaleEndTime = _time;
    }
    function setMintPerTransaction(uint8 _nb) external onlyOwner {
        mintPerTransaction = _nb;
    }
    function setMintPerWallet(uint8 _nb) external onlyOwner {
        mintPerWallet = _nb;
    }
    function setEMPFP(address _pfp) external onlyOwner {
        EMPFP = IEMERC721A(_pfp);
    }
    function setEMPackage(address _package) external onlyOwner {
        EMPackage = IEMERC1155(_package);
    }
    function setEMRefundManager(address _refundManager) external onlyOwner {
        EMRefundManager = _refundManager;
    }
    function setIsActive(bool _active) external onlyOwner {
        isActive = _active;
    }

    function withdraw() external {
        require(msg.sender == EMRefundManager, "EMPackageSaleManager(payBack): Not EMRefundManager");
        uint256 balance = address(this).balance;
        require(balance > 0, "Balance should be more than zero");
        (bool sent, ) = payable(msg.sender).call{value: balance}("");
        require(sent, "EMPackageRefundManager(withdraw): Failed to send Ether");
    }
}

