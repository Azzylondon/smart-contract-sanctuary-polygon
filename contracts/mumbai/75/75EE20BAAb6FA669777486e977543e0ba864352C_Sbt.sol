import "hardhat/console.sol";

contract Sbt {
    struct MetadataStruct {
        string name;
        string imageCID;
        string description;
        string organization;
    }

    mapping(uint256 => MetadataStruct) private _tokenMetadata;
    mapping(uint256 => mapping(address => uint256)) private _balances;

    function supportsInterface(bytes4 _interfaceId) external pure returns (bool) {
        return _interfaceId == 0x01ffc9a7 || _interfaceId == 0x4e2312e0;
    }

    function tokenURI(uint256 _tokenId) external view returns (string memory) {
        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{',
                            '"name": "', _tokenMetadata[_tokenId].name, '",'
                            '"description": "', _tokenMetadata[_tokenId].description, '",',
                            '"image": "ipfs://', _tokenMetadata[_tokenId].imageCID, '",',
                            '"imageUrl": "ipfs://', _tokenMetadata[_tokenId].imageCID, '",',
                            '"attributes": [',
                                '{',
                                    '"trait_type": "organization",',
                                    '"value": "', _tokenMetadata[_tokenId].organization, '"',
                                '},',
                                '{',
                                    '"trait_type": "card id",',
                                    '"value": "', _tokenId, '"',
                                '},',
                                '{',
                                    '"trait_type": "card name",',
                                    '"value": "', _tokenMetadata[_tokenId].name, '"',
                                '}',
                            ']',
                        '"}'
                    )
                )
            )
        );
        string memory output = string(
            abi.encodePacked("data:application/json;base64,", json)
        );
        return output;
    }

    function balanceOf(address _account, uint256 _id) public view returns (uint256) {
        require(_account != address(0), "_account cannot be the zero address.");
        return _balances[_id][_account];
    }

    function balanceOfBatch(address[] memory _accounts, uint256[] memory _ids) external view returns (uint256[] memory) {
        require(_accounts.length == _ids.length, "_accounts and _ids must have the same length.");

        uint256[] memory batchBalances = new uint256[](_accounts.length);

        for (uint256 i = 0; i < _accounts.length; ++i) {
            batchBalances[i] = balanceOf(_accounts[i], _ids[i]);
        }

        return batchBalances;
    }

    // TODO: _tokenIdの型が問題ないかを確認する
    function mint(
        address[] memory _accounts,
        uint256 _tokenId,
        string memory _name,
        string memory _imageCID,
        string memory _description,
        string memory _organization
    ) external {
        _mint(_accounts, _tokenId);
        _setTokenMetadata(_tokenId, _name, _imageCID, _description, _organization);
    }

    function burn(uint256 _tokenId) external {
        uint256 balance = _balances[_tokenId][msg.sender];
        require(balance == 1, "Don't own this token.");
        unchecked {
            _balances[_tokenId][msg.sender]--;
        }
    }

    function _mint(address[] memory _accounts, uint256 _tokenId) internal {
        for (uint256 i = 0; i < _accounts.length; i++) {
            require(_accounts[i] != address(0), "Account cannot be the zero address.");
            require(_balances[_tokenId][_accounts[i]] == 0, "Already minted to this user.");

            _balances[_tokenId][_accounts[i]]++;
        }
    }

    function _setTokenMetadata(
        uint256 _tokenId,
        string memory _name,
        string memory _imageCID,
        string memory _description,
        string memory _organization
    ) internal {
        require(bytes(_tokenMetadata[_tokenId].name).length == 0, 'Metadata has already been set.');

        _tokenMetadata[_tokenId] = MetadataStruct({
            name: _name,
            imageCID: _imageCID,
            description: _description,
            organization: _organization
        });
    }
}

