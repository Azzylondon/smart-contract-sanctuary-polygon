import "./owner/Ownable.sol";
import "./library/SafeMath.sol";

contract TGETokensManagement is Ownable {
    using SafeMath for uint256;

    /**
     * details of rounds
     * roundName : Round number
     * TotalSaleTokens : Total tokens for a sale
     * TotalSoldTokens : Total sold tokens from a sale
     * SPRDMaticValue : CHP price for MATIC purchase (price should be in USD(8 decimal))
     * SPRDUSDValue : CHP price for usd purchase (price should be in USD(18 decimal))
     * startDate : Round start date
     * endDate : Round end date
     */
    struct RoundInfo {
        string RoundName;
        uint256 TotalSaleTokens;
        uint256 TotalSoldTokens;
        uint256 CliffDays;
        uint256 LockingDaysOfPurchase;
        uint256 TokenUnlockedPercentage;
        uint256 InstantTokenUnlockedPercentage;
        uint256 SubTransactionCount;
        uint256 SPRDMaticValue;
        uint256 SPRDUSDValue;
    }
    uint256 public numRounds;
    mapping(uint256 => RoundInfo) internal rounds;

    event NewRound(
        string RoundName,
        uint256 TotalSaleTokens,
        uint256 CliffDays,
        uint256 LockingDaysOfPurchase,
        uint256 TokenUnlockedPercentage,
        uint256 InstantTokenUnlockedPercentage,
        uint256 SubTransactionCount,
        uint256 SPRDMaticValue,
        uint256 SPRDUSDValue
    );
    event RoundDetailsUpdated(
        string RoundName,
        uint256 TotalSaleTokens,
        uint256 CliffDays,
        uint256 LockingDaysOfPurchase,
        uint256 TokenUnlockedPercentage,
        uint256 InstantTokenUnlockedPercentage,
        uint256 SubTransactionCount,
        uint256 SPRDMaticValue,
        uint256 SPRDUSDValue
    );

    /**
     * Round information : roundName, TotalSaleTokens, TotalSoldTokens, SPRDMaticValue, SPRDUSDValue, startDate, endDate
     */
    function getRoundDetails(uint256 _roundId)
        public
        view
        returns (RoundInfo memory)
    {
        return rounds[_roundId];
    }

    /**
     * Add round information
     * - `_roundName` : Round number
     * - `_TotalSaleTokens` : Total tokens for a sale
     * -  `_SPRDMaticValue` : CHP price for MATIC purchase
     * - `_SPRDUSDValue` : CHP price for usd purchase
     * - `_startDate` : Round start date
     * -  `_endDate` : Round end date
     */
    function addRoundDetails(
        string memory _RoundName,
        uint256 _TotalSaleTokens,
        uint256 _CliffDays,
        uint256 _LockingDaysOfPurchase,
        uint256 _TokenUnlockedPercentage,
        uint256 _InstantTokenUnlockedPercentage,
        uint256 _SubTransactionCount,
        uint256 _SPRDMaticValue,
        uint256 _SPRDUSDValue
    ) external onlyOwner returns (uint256 roundId) {
        require(_TotalSaleTokens > 0, "Sale tokens should not be zero");
        require(
            _SPRDMaticValue > 0 && _SPRDUSDValue > 0,
            "Wrong CHP Token price!"
        );
        roundId = ++numRounds;
        RoundInfo memory round;
        round = RoundInfo(
            _RoundName,
            _TotalSaleTokens,
            0,
            _CliffDays,
            _LockingDaysOfPurchase,
            _TokenUnlockedPercentage,
            _InstantTokenUnlockedPercentage,
            _SubTransactionCount,
            _SPRDMaticValue,
            _SPRDUSDValue
        );
        rounds[roundId] = round;
        emit NewRound(
            _RoundName,
            _TotalSaleTokens,
            _CliffDays,
            _LockingDaysOfPurchase,
            _TokenUnlockedPercentage,
            _InstantTokenUnlockedPercentage,
            _SubTransactionCount,
            _SPRDMaticValue,
            _SPRDUSDValue
        );
    }

    /**
     * Update round information
     * - `_roundId` : Id of round
     * - `_roundName` : Round number
     * - `_TotalSaleTokens` : Total tokens for a sale
     * -  `_SPRDMaticValue` : CHP price for MATIC purchase
     * - `_SPRDUSDValue` : CHP price for usd purchase
     * - `_startDate` : Round start date
     * -  `_endDate` : Round end date
     */
    function updateRoundDetails(
        uint256 _roundId,
        string memory _RoundName,
        uint256 _TotalSaleTokens,
        uint256 _CliffDays,
        uint256 _LockingDaysOfPurchase,
        uint256 _TokenUnlockedPercentage,
        uint256 _InstantTokenUnlockedPercentage,
        uint256 _SubTransactionCount,
        uint256 _SPRDMaticValue,
        uint256 _SPRDUSDValue
    ) external onlyOwner returns (bool) {
        RoundInfo storage round = rounds[_roundId];
        round.RoundName = _RoundName;
        round.TotalSaleTokens = _TotalSaleTokens;
        round.LockingDaysOfPurchase = _LockingDaysOfPurchase;
        round.TokenUnlockedPercentage = _TokenUnlockedPercentage;
        round.InstantTokenUnlockedPercentage = _InstantTokenUnlockedPercentage;
        round.SubTransactionCount = _SubTransactionCount;
        round.SPRDMaticValue = _SPRDMaticValue;
        round.SPRDUSDValue = _SPRDUSDValue;

        emit RoundDetailsUpdated(
            _RoundName,
            _TotalSaleTokens,
            _CliffDays,
            _LockingDaysOfPurchase,
            _TokenUnlockedPercentage,
            _InstantTokenUnlockedPercentage,
            _SubTransactionCount,
            _SPRDMaticValue,
            _SPRDUSDValue
        );
        return true;
    }
}

