import "./owner/Ownable.sol";
import "./library/SafeMath.sol";
import "hardhat/console.sol";

contract TokenomicsWalletManagement is Ownable {
    using SafeMath for uint256;

    /**
     * Withdrawal details of single phase of sale
     * WithdrawalDate : Token redeem date
     * TotalTokens : Maaximum token available to reedem in particuler slot
     * ClaimedTokens : Claimed tokens from that slot
     */
    struct VestingSlot {
        uint256 WithdrawalDate;
        uint256 TotalTokens;
        uint256 ClaimedTokens;
    }

    /**
     * Withdrawal details of sale
     * beneficiary : Sale address (e.g Marketing wallet)
     * TotalWithdrawalAmount : Total loked tokens for a sale
     * TotalClaimed : Total Claimed tokens from a sale
     * TotalSlots : Total phases of sale
     */
    struct WalletInfo {
        address Beneficiary;
        uint256 TotalWithdrawalAmount;
        uint256 TotalClaimed;
        uint256 TokenUnlockedPercentage;
        uint256 CliffDays;
        uint256 LockingDays;
        uint256 TotalVestingSlots;
        mapping(uint256 => VestingSlot) vestingSlots;
    }

    mapping(string => WalletInfo) internal wallets;

    // uint256 public day = 86400; // day in seconds
    uint256 public day = 300; // day in seconds(for testing only)

    event WalletInfoAdded(
        string walletName,
        uint256 withdrawalDate,
        uint256 totalTokens
    );
    event WalletInfoUpdated(
        string walletName,
        uint256 slotId,
        uint256 withdrawalDate,
        uint256 totalTokens
    );
    event TokenWithdrawal(
        address indexed user,
        string walletName,
        uint256 amount
    );

    constructor() public {
        WalletInfo storage team = wallets["team"];
        team.TotalWithdrawalAmount = 120000000000000000000000000;

        WalletInfo storage advisors = wallets["advisors"];
        advisors.TotalWithdrawalAmount = 60000000000000000000000000;

        WalletInfo storage ecosystemAndNetwork = wallets["ecosystemAndNetwork"];
        ecosystemAndNetwork.TotalWithdrawalAmount = 80000000000000000000000000;

        WalletInfo storage treasury = wallets["treasury"];
        treasury.TotalWithdrawalAmount = 100000000000000000000000000;

        WalletInfo storage partnerships = wallets["partnerships"];
        partnerships.TotalWithdrawalAmount = 90000000000000000000000000;

        WalletInfo storage marketingAndDevelopment = wallets[
            "marketingAndDevelopment"
        ];
        marketingAndDevelopment
            .TotalWithdrawalAmount = 100000000000000000000000000;

        WalletInfo storage stakingAndCommunity = wallets["stakingAndCommunity"];
        stakingAndCommunity.TotalWithdrawalAmount = 180000000000000000000000000;
    }

    /**
     * Sale information : Beneficiary address, Total tokens to withdraw, Total claimed till now, Total phases of sale
     */
    function getWalletInfo(string memory _walletName)
        public
        view
        returns (
            address Beneficiary,
            uint256 TotalWithdrawalAmount,
            uint256 TotalClaimed,
            uint256 TokenUnlockedPercentage,
            uint256 CliffDays,
            uint256 LockingDays,
            uint256 TotalVestingSlots
        )
    {
        WalletInfo memory wallet = wallets[_walletName];
        Beneficiary = wallet.Beneficiary;
        TotalWithdrawalAmount = wallet.TotalWithdrawalAmount;
        TotalClaimed = wallet.TotalClaimed;
        TokenUnlockedPercentage = wallet.TokenUnlockedPercentage;
        CliffDays = wallet.CliffDays;
        LockingDays = wallet.LockingDays;
        TotalVestingSlots = wallet.TotalVestingSlots;
    }

    /**
     * Add sale slot(phase) information
     * - `_saleName` : Name of the sale
     * - `_withdrawalDate` : Redeem date
     * -  `_totalTokens` : Tokens allocation for that slot
     */
    function addWalletInfo(
        string memory _walletName,
        address _Beneficiary,
        uint256 _TokenUnlockedPercentage,
        uint256 _CliffDays,
        uint256 _LockingDays,
        uint256 _TotalVestingSlots
    ) public onlyOwner returns (bool) {
        WalletInfo storage wallet = wallets[_walletName];

        require(wallet.TotalWithdrawalAmount != 0, "Wrong wallet name.");
        require(
            _Beneficiary != address(0),
            "Beneficiary address should not be zero address"
        );

        wallet.Beneficiary = _Beneficiary;
        wallet.TokenUnlockedPercentage = _TokenUnlockedPercentage;
        wallet.CliffDays = _CliffDays;
        wallet.LockingDays = _LockingDays;
        wallet.TotalVestingSlots = _TotalVestingSlots;

        storeWalletVestingDetails(_walletName);
        return true;
    }

    /**
     * Add sale slot(phase) information
     * - `_saleName` : Name of the sale
     * - `_withdrawalDate` : Redeem date
     * -  `_totalTokens` : Tokens allocation for that slot
     */
    function updateWalletInfo(string memory _walletName, address _Beneficiary)
        public
        onlyOwner
        returns (bool)
    {
        WalletInfo storage wallet = wallets[_walletName];

        require(wallet.TotalWithdrawalAmount != 0, "Wrong wallet name.");
        require(
            _Beneficiary != address(0),
            "Beneficiary address should not be zero address"
        );

        wallet.Beneficiary = _Beneficiary;
        return true;
    }

    /**
     * function to store user's presale purchase details
     * - `_totalTokens` : Bought tokens
     **/

    function storeWalletVestingDetails(string memory _walletName)
        internal
        returns (bool)
    {
        WalletInfo storage wallet = wallets[_walletName];

        for (uint256 i = 0; i < wallet.TotalVestingSlots; i++) {
            if (i == 0) {
                wallet.vestingSlots[i] = VestingSlot(
                    block.timestamp.add(day.mul(wallet.CliffDays)),
                    wallet
                        .TotalWithdrawalAmount
                        .mul(wallet.TokenUnlockedPercentage)
                        .div(1000000),
                    0
                );
            } else {
                wallet.vestingSlots[i] = VestingSlot(
                    block.timestamp.add(day.mul(wallet.CliffDays)).add(
                        day.mul(i).mul(wallet.LockingDays)
                    ),
                    wallet
                        .TotalWithdrawalAmount
                        .mul(wallet.TokenUnlockedPercentage)
                        .div(1000000),
                    0
                );
            }
        }
        return true;
    }

    /**
     * Update sale slot(phase) information
     * - `_saleName` : Name of the sale
     * - `_withdrawalDate` : Redeem date
     * -  `_totalTokens` : Tokens allocation for that slot
     * - `_slotId` : Id of slot which want to update
     */
    function updateWalletVestingSlot(
        string memory _walletName,
        uint256 _withdrawalDate,
        uint256 _vestingSlotId
    ) public onlyOwner returns (bool) {
        WalletInfo storage wallet = wallets[_walletName];

        require(wallet.TotalWithdrawalAmount != 0, "Wrong wallet name.");
        // update slot info
        wallet.vestingSlots[_vestingSlotId].WithdrawalDate = _withdrawalDate;
        return true;
    }

    /**
     * get sale slot(phase) information
     * - `_saleName` : Name of the sale
     * - `_slotId` : Id of slot which want to get information of
     *
     * This will returns WithdrawalDate(Redeem date),  TotalTokens to withdraw, Count of Claimed Tokens
     */
    function getWalletVestingSlotInfo(
        string memory _walletName,
        uint256 _vestingSlotId
    ) public view returns (VestingSlot memory) {
        return (wallets[_walletName].vestingSlots[_vestingSlotId]);
    }

    /**
     * Get total number of tokens available to withdraw from sale
     */
    function getAvailableTokensToWithdraw(string memory _walletName)
        public
        view
        returns (uint256)
    {
        WalletInfo storage wallet = wallets[_walletName];
        uint256 availableTokens;
        for (uint256 i = 0; i < wallet.TotalVestingSlots; i++) {
            if (wallet.vestingSlots[i].WithdrawalDate < block.timestamp) {
                availableTokens += wallet.vestingSlots[i].TotalTokens.sub(
                    wallet.vestingSlots[i].ClaimedTokens
                );
            }
        }
        return availableTokens;
    }

    /**
     * Update slot information after successful withdrawal
     */
    function updateWithdrawalRecord(string memory _walletName, uint256 _amount)
        internal
        returns (bool)
    {
        WalletInfo storage wallet = wallets[_walletName];
        uint256 amount = _amount;
        for (uint256 i = 0; i < wallet.TotalVestingSlots; i++) {
            uint256 slotTokens = wallet.vestingSlots[i].TotalTokens.sub(
                wallet.vestingSlots[i].ClaimedTokens
            );
            if (
                slotTokens != 0 &&
                wallet.vestingSlots[i].WithdrawalDate < block.timestamp
            ) {
                if (slotTokens > amount) {
                    wallet.vestingSlots[i].ClaimedTokens = wallet
                        .vestingSlots[i]
                        .ClaimedTokens
                        .add(amount);
                    amount = 0;
                    break;
                } else if (slotTokens <= amount) {
                    wallet.vestingSlots[i].ClaimedTokens = wallet
                        .vestingSlots[i]
                        .ClaimedTokens
                        .add(slotTokens);
                    amount -= slotTokens;
                }
            }
        }
        wallet.TotalClaimed += _amount;
        return true;
    }
}

