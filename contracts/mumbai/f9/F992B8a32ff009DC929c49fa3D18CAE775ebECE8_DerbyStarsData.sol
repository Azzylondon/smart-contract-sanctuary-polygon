pragma solidity ^0.8.2;

import "hardhat/console.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {IDerbyStarsData} from "./interfaces/IDerbyStarsData.sol";

contract DerbyStarsData is IDerbyStarsData, Ownable {
    // TODO : BATCH COUNT
    uint16 public constant TRAIT_FAMILY_BATCH_LIMIT = 10;
    uint16 public constant TRAIT_BATCH_LIMIT = 100;

    bytes32[] public traitFamilyName;
    mapping(bytes32 => bool) public traitFamilyNameSet;
    mapping(bytes32 => mapping(uint256 => string)) public trait;

    // XXX : do we have to separate color from traitFamilyName?
    string[] public colorGroupName;
    mapping(bytes32 => bool) public colorGroupNameSet;
    mapping(uint256 => uint256[]) public colorGroupByPart;
    mapping(uint256 => string[]) color;

    mapping(uint256 => uint256) public specialSkillIdsToTiers;
    mapping(uint256 => uint256[]) public specialSkillTiersToIds;
    mapping(uint256 => uint256) public normalSkillIdsToTiers;
    mapping(uint256 => uint256[]) public normalSkillTiersToIds;

    mapping(uint256 => mapping(uint256 => uint256[])) bodyPartTypesToRarityGroups;
    mapping(uint256 => uint256) bodyPartIdsToRarity;

    uint256 public constant FEETYPE_MAX = 4;
    mapping(uint256 => uint256[]) public feeByRarity;

    constructor() Ownable() {}

    // XXX : Do we really need this?
    function addTraitFamily(string[] calldata _familynames)
        external
        virtual
        override
        onlyOwner
    {
        uint256 len = _familynames.length;
        require(
            len <= TRAIT_FAMILY_BATCH_LIMIT,
            "No more than 10 items can be processed at a time."
        );
        for (uint256 i = 0; i < len; ) {
            bytes32 hashedname = keccak256(abi.encodePacked(_familynames[i]));
            if (!traitFamilyNameSet[hashedname]) {
                traitFamilyName.push(hashedname);
                traitFamilyNameSet[hashedname] = true;
            }
            unchecked {
                i++;
            }
        }
    }

    function addTrait(
        string calldata _familyname,
        uint256[] calldata _ids,
        string[] calldata _vals
    ) external virtual override onlyOwner {
        bytes32 hashedfn = keccak256(abi.encodePacked(_familyname));
        require(traitFamilyNameSet[hashedfn], "Trait family name not exists!");
        uint256 len = _ids.length;
        require(
            len <= TRAIT_BATCH_LIMIT,
            "No more than 10 items can be processed at a time."
        );
        require(len == _vals.length, "Id and value array length mismatched!");

        for (uint256 i = 0; i < len; ) {
            trait[hashedfn][_ids[i]] = _vals[i];
            unchecked {
                i++;
            }
        }
    }

    // XXX : Do we really need this?
    function addColorGroup(string[] calldata _colorgroups, uint256 part)
        external
        virtual
        override
        onlyOwner
    {
        uint256 len = _colorgroups.length;
        require(
            len <= TRAIT_FAMILY_BATCH_LIMIT,
            "No more than 10 items can be processed at a time."
        );

        for (uint256 i = 0; i < len; ) {
            bytes32 hashedname = keccak256(abi.encodePacked(_colorgroups[i]));
            if (!colorGroupNameSet[hashedname]) {
                colorGroupByPart[part].push(colorGroupName.length);
                colorGroupName.push(_colorgroups[i]);
                colorGroupNameSet[hashedname] = true;
            }
            unchecked {
                i++;
            }
        }
    }

    function addColor(uint256 _colorgroupid, string[] calldata _colors)
        external
        virtual
        override
        onlyOwner
    {
        require(
            colorGroupName.length > _colorgroupid,
            "Color group id not exists!"
        );
        uint256 len = _colors.length;
        require(
            len <= TRAIT_BATCH_LIMIT,
            "No more than 100 items can be processed at a time."
        );

        for (uint256 i = 0; i < len; ) {
            color[_colorgroupid].push(_colors[i]);
            unchecked {
                i++;
            }
        }
    }

    function addBodyPartTypes(uint256 _bodyPartType, BodyPartRarityGroup[] memory _groups) external virtual override onlyOwner {
        for (uint256 i = 0; i < _groups.length; i++) {
            bodyPartTypesToRarityGroups[_bodyPartType][_groups[i].rarity] = _groups[i].ids;
        }
    }

    function addBodyParts(BodyPart[] memory _bodyParts) external virtual override onlyOwner {
        for (uint256 i = 0; i < _bodyParts.length; i++) {
            bodyPartIdsToRarity[_bodyParts[i].id] = _bodyParts[i].rarity;
        }
    }

    function addSpecialSkills(Skill[] memory _skills, SkillSet[] memory _skillSets) external virtual override onlyOwner {
        for (uint256 i = 0; i < _skills.length; i++) {
            specialSkillIdsToTiers[_skills[i].id] = _skills[i].tier;
        }
        for (uint256 i = 0; i < _skillSets.length; i++) {
            specialSkillTiersToIds[_skillSets[i].tier] = _skillSets[i].ids;
        }
    }

    function addNormalSkills(Skill[] memory _skills, SkillSet[] memory _skillSets) external virtual override onlyOwner {
        for (uint256 i = 0; i < _skills.length; i++) {
            normalSkillIdsToTiers[_skills[i].id] = _skills[i].tier;
        }
        for (uint256 i = 0; i < _skillSets.length; i++) {
            normalSkillTiersToIds[_skillSets[i].tier] = _skillSets[i].ids;
        }
    }

    function getTrait(string calldata _familyname, uint256 _id)
        public
        view
        virtual
        override
        returns (string memory)
    {
        bytes32 hashedfn = keccak256(abi.encodePacked(_familyname));
        require(traitFamilyNameSet[hashedfn], "Trait family name not exists!");
        // TODO : key exist check

        return trait[hashedfn][_id];
    }

    function getColorsLength(uint256 _colorgroupid)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return color[_colorgroupid].length;
    }

    function getColor(uint256 _colorgroupid, uint256 _id)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            colorGroupName.length > _colorgroupid,
            "Color group id not exists!"
        );
        require(color[_colorgroupid].length > _id, "Index out of range!");

        return color[_colorgroupid][_id];
    }

    function getColorGroups(uint256 part)
        public
        view
        virtual
        override
        returns (uint256[] memory)
    {
        return colorGroupByPart[part];
    }

    function getSpecialSkillTier(uint256 _id) external view virtual override returns (uint256) {
        return specialSkillIdsToTiers[_id];
    }

    function getSpecialSkillIds(uint256 _tier) external view override returns (uint256[] memory) {
        return specialSkillTiersToIds[_tier];
    }

    function getNormalSkillTier(uint256 _id) external view virtual override returns (uint256) {
        return normalSkillIdsToTiers[_id];
    }

    function getNormalSkillIds(uint256 _tier) external view virtual override returns (uint256[] memory) {
        return normalSkillTiersToIds[_tier];
    }

    function getBodyPartRarityGroupIds(uint256 _bodyPartType, uint256 _rarity) external view virtual override returns (uint256[] memory) {
        return bodyPartTypesToRarityGroups[_bodyPartType][_rarity];
    }

    function getBodyPartRarity(uint256 bodyPartId) external view virtual override returns (uint256) {
        return bodyPartIdsToRarity[bodyPartId];
    }

    function setBreedingFee(uint256 _type, uint256[] calldata _fees) external virtual override onlyOwner {
        require(_type < FEETYPE_MAX, "Type out of range!");
        require(feeByRarity[_type].length == 0, "Fee already initialized!");

        for (uint256 i = 0; i < _fees.length; ) {
            feeByRarity[_type].push(_fees[i]);
            unchecked {
                i++;
            }
        }
    }

    function modifyBreedingFee(uint256 _type, uint256 _tier, uint256 _fee) external virtual override onlyOwner {
        require(_type < FEETYPE_MAX, "Type out of range!");
        require(_tier < feeByRarity[_type].length, "Index in Type array out of range!");

        feeByRarity[_type][_tier] = _fee;
    }

    function getBreedingFee(uint256 _type, uint256 _tier) external virtual override view returns (uint256) {
        require(_type < FEETYPE_MAX, "Type out of range!");
        require(_tier < feeByRarity[_type].length, "Index in Type array out of range!");

        return feeByRarity[_type][_tier];
    }
}

