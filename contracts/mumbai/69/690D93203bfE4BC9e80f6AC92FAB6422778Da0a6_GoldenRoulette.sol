pragma solidity ^0.8.7;

/* import "@openzeppelin/contracts/access/Ownable.sol"; */
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/KeeperCompatibleInterface.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./IHexTechToken.sol";
import "./IHexTechCertificateOfDeposit.sol";
import "hardhat/console.sol";

enum BetType {
    One,
    Two,
    Five,
    Ten,
    Twenty,
    Fourty
}

contract GoldenRoulette is VRFConsumerBaseV2, KeeperCompatibleInterface, Context {

    using SafeMath for uint256;

    VRFCoordinatorV2Interface public COORDINATOR;
    LinkTokenInterface public LINKTOKEN;
    IHexTechToken private hxtToken;
    IHexTechCertificateOfDeposit private hxtCertificateOfDeposit;

    // Your subscription ID.
    uint64 public s_subscriptionId;

    // The gas lane to use, which specifies the maximum gas price to bump to.
    // For a list of available gas lanes on each network,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
    bytes32 immutable s_keyHash;

    // Depends on the number of requested values that you want sent to the
    // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
    // so 100,000 is a safe default for this example contract. Test and adjust
    // this limit based on the network that you select, the size of the request,
    // and the processing of the callback request in the fulfillRandomWords()
    // function.
    uint32 immutable s_callbackGasLimit = 1000000;
    uint32 immutable callGasLimit = 1000000;

    // The default is 3, but you can set this higher.
    uint16 immutable s_requestConfirmations = 3;

    // For this example, retrieve 2 random values in one request.
    // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.
    uint32 immutable s_numWords = 2;

    uint256[] public s_randomWords;
    uint256 public s_requestId;
    address s_owner;

    /// Need to delete for prod

    uint256 testBaseAmount = 5000 * 1E18;

    struct Globals {
        uint256 totalTicketsBought;
        mapping(BetType => uint256) typeBought;
        uint256 totalHXTSpent;
        uint256 currentHextechDay;
    }

    struct SnapShot {
        /* uint256 hxtPrice; */
        uint256 totalTicketsBoughtToday;
        uint256 totalHXTSpentToday;
        // Use a mapping here
        mapping(BetType => address[]) bettersList;
        Result result;
    }

    struct Result {
        address winnerAddress;
        uint256 rouletteOutcome;
        bool winnerBool;
        uint256 randomRoulette;
        uint256 randomAddress;
    }

    mapping(uint8 => BetType) public BetTypes;
    mapping(uint256 => uint256) public baseAmount;
    mapping(address => mapping(uint256 => uint256)) public ticketsBoughtCount;
    mapping(address => mapping(uint256 => mapping(BetType => uint256))) public ticketsBought;
    mapping(uint256 => SnapShot) public snapshots;
    mapping(uint256 => Result) public results;
    mapping(uint256 => bool) public betActive;

    mapping (uint256 => bool) _rollRequestsCompleted;
    mapping (uint256 => address) _rollRequestsSender;
    mapping (uint256 => Result) _rollRequestsResults;
    mapping (uint256 => uint256) _rollRequestsTime;

    Globals public globals;


    event ReturnedRandomness(
        uint256[] randomWords
    );

    event NewGlobals(
        uint256 totalTicketsBought,
        BetType typeBought,
        uint256 typeTotal,
        uint256 totalHXTSpent,
        uint256 indexed currentHextechDay
    );

    event NewSnapShot(
        /* uint256 hxtPrice, */
        uint256 totalTicketsBoughtToday,
        uint256 totalHXTSpentToday,
        address[] bettersOnTwo,
        address[] bettersOnFive,
        address[] bettersOnTen,
        address[] bettersOnTwenty,
        address[] bettersOnFourty,
        address indexed winnerAddress,
        uint256 indexed rouletteOutcome,
        bool winnerBool,
        uint256 indexed currentHextechDay
    );

    event TicketPurchased(
        address indexed better,
        BetType _betType,
        uint256 amountSpent,
        uint256 indexed currentHextechDay
    );

    event NoMoreBetsCalled(
        uint256 requestId,
        address indexed caller,
        uint256 timestamp,
        uint256 indexed currentHextechDay
    );



   /**
   * @notice Constructor inherits VRFConsumerBaseV2
   * @param _subscriptionId - the subscription ID that this contract uses for funding requests
   * @param _vrfCoordinator - coordinator, check https://docs.chain.link/docs/vrf-contracts/#configurations
   * @param _keyHash - the gas lane to use, which specifies the maximum gas price to bump to
   */
    constructor(
        uint64 _subscriptionId,
        address _vrfCoordinator,
        address _link,
        bytes32 _keyHash,
        IHexTechToken _hxtToken,
        IHexTechCertificateOfDeposit _hxtCertificateOfDeposit
    ) VRFConsumerBaseV2(_vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        LINKTOKEN = LinkTokenInterface(_link);
        s_keyHash = _keyHash;
        s_owner = _msgSender();
        s_subscriptionId = _subscriptionId;
        hxtToken = _hxtToken;
        hxtCertificateOfDeposit = _hxtCertificateOfDeposit;

        // Set up the roulette odds
        BetTypes[0] = BetType.Fourty;
        for (uint8 i = 1; i < 25; i++) {
            BetTypes[i] = BetType.One;
        }
        for (uint8 i = 25; i < 39; i++) {
            BetTypes[i] = BetType.Two;
        }
        for (uint8 i = 39; i < 46; i++) {
            BetTypes[i] = BetType.Five;
        }
        for (uint8 i = 46; i < 50; i++) {
            BetTypes[i] = BetType.Ten;
        }
        for (uint8 i = 50; i < 52; i++) {
            BetTypes[i] = BetType.Twenty;
        }

        betActive[globals.currentHextechDay] = true;   

        createNewSubscription();
    }

    /**
    * @notice Checks if the contract requires work to be done
    */
    function checkUpkeep(
        bytes memory /* checkData */
    )
    public override
    returns (
    bool upkeepNeeded,
    bytes memory /* performData */
    )
    {   
        console.log(globals.currentHextechDay);
        console.log(hxtCertificateOfDeposit.currentHextechDay());
        upkeepNeeded = globals.currentHextechDay < hxtCertificateOfDeposit.currentHextechDay();
        console.log(upkeepNeeded);
        // We don't use the checkData in this example. The checkData is defined when the Upkeep was registered.
    }

    /**
    * @notice Performs the work on the contract, if instructed by :checkUpkeep():
    */
    function performUpkeep(
        bytes calldata /* performData */
    ) external override {
        // add some verification
        (bool upkeepNeeded, ) = checkUpkeep("");
        require(upkeepNeeded, "Time interval not met");

        _noMoreBets();
        // We don't use the performData in this example. The performData is generated by the Keeper's call to your checkUpkeep function
    }


    // Create a new subscription when the contract is initially deployed.
    function createNewSubscription() private onlyOwner {
        // Create a subscription with a new subscription ID.
        address[] memory consumers = new address[](1);
        consumers[0] = address(this);
        s_subscriptionId = COORDINATOR.createSubscription();
        // Add this contract as a consumer of its own subscription.
        COORDINATOR.addConsumer(s_subscriptionId, consumers[0]);
    }


    function topUpSubscription(uint256 amount) external onlyOwner {
        LINKTOKEN.transferAndCall(address(COORDINATOR), amount, abi.encode(s_subscriptionId));
    }

    function _increaseGlobals(
        BetType _betType,
        uint256 _amount
    )
    internal
    {
        globals.totalTicketsBought++;
        globals.totalHXTSpent = globals.totalHXTSpent.add(_amount);
        globals.typeBought[_betType]++;
        console.log(globals.typeBought[_betType]);

        _logGlobals(_betType);
    }

    function _logGlobals(
        BetType _betType
    )
    private
    {
        emit NewGlobals(
            globals.totalTicketsBought,
            _betType,
            globals.typeBought[_betType],
            globals.totalHXTSpent,
            globals.currentHextechDay
        );
    }

    function _updateBettersList(
        address _better,
        BetType _betType
    )
    internal 
    {
        uint256 _day = globals.currentHextechDay;

        SnapShot storage snapshot = snapshots[_day];

        snapshot.bettersList[_betType].push(_better);
    }

    function _increaseTicketsBought(
        address _better,
        uint256 _currentHextechDay,
        BetType _betType
    )
    internal
    {
        ticketsBoughtCount[_better][_currentHextechDay]++;
        ticketsBought[_better][_currentHextechDay][_betType]++;
    }

    /**
     * @notice allows volunteer to offload snapshots
     * to save on gas during next start/end bond
     */
    /* function manualDailySnapshot()
        external
    {
        _dailySnapshotPoint(hxtCertificateOfDeposit.currentHextechDay());
    } */

    function _dailySnapshotPoint(
        uint256 _updateDay
    )
    internal
    {
        
        for (uint256 _day = globals.currentHextechDay; _day < _updateDay; _day++) {

            SnapShot storage snapshot = snapshots[_day];
            Result memory result = results[_day];

            snapshot.totalTicketsBoughtToday = globals.totalTicketsBought;
            snapshot.totalHXTSpentToday = globals.totalHXTSpent;
            snapshot.result = result;        

            globals.currentHextechDay++;

            betActive[globals.currentHextechDay] = true;   

            emit NewSnapShot(
                /* hxtPrice, */
                snapshot.totalTicketsBoughtToday,
                snapshot.totalHXTSpentToday,
                snapshot.bettersList[BetType.Two],
                snapshot.bettersList[BetType.Five],
                snapshot.bettersList[BetType.Ten],
                snapshot.bettersList[BetType.Twenty],
                snapshot.bettersList[BetType.Fourty],
                result.winnerAddress,
                result.rouletteOutcome,
                result.winnerBool,
                _day
            );
        }
    }

    function purchaseGoldenTicket(
        BetType _betType
    ) 
    external 
    {
        // redondant check as it's done in the _collectToken function
        require(_betType != BetType.One, "HXT: You can't bet on One");
        require(betActive[globals.currentHextechDay] == true, "HXT: No more bets");
        require(1 <= globals.currentHextechDay && globals.currentHextechDay <= 365, "HXT: Emission phase not active");

        
        uint256 amountSpent = testBaseAmount.mul(1 + ticketsBoughtCount[_msgSender()][globals.currentHextechDay]);

        _collectToken(_msgSender(), amountSpent);
        _increaseGlobals(_betType, amountSpent);
        _updateBettersList(_msgSender(), _betType);
        _increaseTicketsBought(_msgSender(), globals.currentHextechDay, _betType);

        emit TicketPurchased(
            _msgSender(),
            _betType,
            amountSpent,
            globals.currentHextechDay
        );
    }

    function _collectToken(
        address _better,
        uint256 _amount
    )
    internal
    {
         // Since you can't burn tokens from others, you have to transfer them to the contract
        require(hxtToken.transferFrom(_better, address(this), _amount), 'HXT: Transfer failed');
        
        /// Need to readd for prod 

        /* // Burn the tokens transferred using balanceOf, as 1% of the _bondAmount are burned in the transfer 
        // Because of the transfer burn tax
        hxtToken.burn(hxtToken.balanceOf(address(this))); */
    }

    /**
     * @notice external function to call noMoreBets
     */
    function callNoMoreBets()
    external
    {
        _noMoreBets();
    }

    /**
     * @notice internal function to call noMoreBets
     */
    function _noMoreBets() 
    internal 
    {
        require(globals.currentHextechDay < hxtCertificateOfDeposit.currentHextechDay(), "HXT: Needs to be a new day");
        require(globals.currentHextechDay <= 365, "HXT: Emission phase not active");

        uint256 requestId = requestRandomNumbers();

        _rollRequestsSender[requestId] = _msgSender();
        _rollRequestsCompleted[requestId] = false;
        _rollRequestsTime[requestId] = block.timestamp;

        betActive[globals.currentHextechDay] = false;

        emit NoMoreBetsCalled(
            requestId,
            _msgSender(),
            block.timestamp,
            globals.currentHextechDay
        );
    }

    /**
     * Creates a randomness request for Chainlink VRF
     * @return requestId id of the created randomness request
     */
    function requestRandomNumbers() internal returns (uint256 requestId) {
        requestId = COORDINATOR.requestRandomWords(
            s_keyHash,
            s_subscriptionId,
            s_requestConfirmations,
            s_callbackGasLimit,
            s_numWords
        );
        s_requestId = requestId;
        
        // Chainlink Coordinator emits RandomWordsRequested event
    }

    /**
   * @notice Callback function used by VRF Coordinator
   *
   * @param requestId - id of the request
   * @param randomWords - array of random results from VRF Coordinator
   */
  function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
    require(_rollRequestsCompleted[requestId] == false);

    s_randomWords = randomWords;
    uint256 s_randomRouletteRange = randomWords[0] % 52;
    uint256 s_randomAddressRange;
    uint256 _day = globals.currentHextechDay;
    SnapShot storage snapshot = snapshots[_day];
    Result memory result = results[_day];

    if (BetTypes[uint8(s_randomRouletteRange)] == BetType.One) 
    {      
        result.rouletteOutcome = 1;
    }
    else if (BetTypes[uint8(s_randomRouletteRange)] == BetType.Two)
    {
        result.rouletteOutcome = 2;
    }
    else if (BetTypes[uint8(s_randomRouletteRange)] == BetType.Five) 
    {
        result.rouletteOutcome = 5;
    }
    else if (BetTypes[uint8(s_randomRouletteRange)] == BetType.Ten) 
    {
        result.rouletteOutcome = 10;
    } 
    else if (BetTypes[uint8(s_randomRouletteRange)] == BetType.Twenty) 
    {
        result.rouletteOutcome = 20;
    } 
    else if (BetTypes[uint8(s_randomRouletteRange)] == BetType.Fourty) 
    {
        result.rouletteOutcome = 40;
    }

    if (snapshot.bettersList[BetTypes[uint8(s_randomRouletteRange)]].length != 0 && BetTypes[uint8(s_randomRouletteRange)] != BetType.One) {
        s_randomAddressRange = randomWords[1] % snapshot.bettersList[BetTypes[uint8(s_randomRouletteRange)]].length;

        result.winnerAddress = snapshot.bettersList[BetTypes[uint8(s_randomRouletteRange)]][s_randomAddressRange];
        result.winnerBool = true;
    } 

    result.randomRoulette = s_randomRouletteRange;
    result.randomAddress = s_randomAddressRange; 

    results[_day] = result;

    _rollRequestsResults[requestId] = result;
    _rollRequestsCompleted[requestId] = true;

    _dailySnapshotPoint(hxtCertificateOfDeposit.currentHextechDay());

    emit ReturnedRandomness(randomWords);
  }

  modifier onlyOwner() {
    require(_msgSender() == s_owner);
    _;
  }

}

