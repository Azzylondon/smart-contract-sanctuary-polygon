pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721RoyaltyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "../interfaces/ISocialTokenDeployer.sol";
import "../extensions/NativeMetaTransaction.sol";
import "../MerchandisingManager.sol";
import "../Structs.sol";
import "hardhat/console.sol";

contract Merchandising is
    ERC721RoyaltyUpgradeable,
    ERC721URIStorageUpgradeable,
    NativeMetaTransaction,
    OwnableUpgradeable
{
    using Counters for Counters.Counter;
    using SafeERC20 for IERC20;

    /// @notice The address of the social token manager
    address public immutable socialTokenManager;

    Counters.Counter private _tokenIdCounter;
    Counters.Counter private _merchIdCounter;

    /// @dev The address of the merchandising manager contract
    MerchandisingManager public manager;

    /// @dev The uri of the collection metadata
    string private _metadataURI;

    /// @notice The funding token
    address public fundingToken;

    /// @notice The registered merchandise to sell (merchandise whitout NFT relation)
    mapping(uint256 => MerchandiseToSell) public registeredMerchandiseToSell;

    /// @notice The registered merchandise to claim (merchandise with NFT relation through Merkle Tree)
    mapping(uint256 => MerchandiseToClaim) public registeredMerchandiseToClaim;

    /// @dev Whether a token was claimed already
    mapping(uint256 => mapping(bytes32 => bool)) private _tokenAlreadyClaimedForMerchandising;

    event MerchandisingBought(
        uint256 indexed merchId,
        uint256 amount,
        uint256[] tokenIds,
        address indexed buyer
    );

    /**
     * @dev modifier to make method signature more explicit
     */
    modifier onlyManager() {
        require(msg.sender == address(manager), "Only Manager allowed");
        _;
    }

    /**
     * @dev The initializer modifier is to avoid someone initializing
     *      the implementation contract after deployment
     */
    constructor(address socialTokenManager_) initializer {
        socialTokenManager = socialTokenManager_;
    }

    function initialize(
        string memory metadataURI_,
        string memory name_,
        string memory symbol_,
        address defaultRoyaltyReceiver_,
        uint64 defaultRoyaltyPercentage_,
        address artist_,
        address fundingToken_
    ) external initializer {
        _metadataURI = metadataURI_;
        fundingToken = fundingToken_;

        __Ownable_init();
        __ERC721_init(name_, symbol_);
        __EIP712_init(name_);

        _setDefaultRoyalty(defaultRoyaltyReceiver_, defaultRoyaltyPercentage_);
        _transferOwnership(artist_);

        manager = MerchandisingManager(msg.sender);
    }

    function contractURI() public view returns (string memory uri) {
        return _metadataURI;
    }

    /**
     * @dev To register new merchandising to sell
     * @param uris_ The new merchandising uris
     * @param prices_ The new merchandising prices
     * @param totalSupplies_ The new merchandising total supplies
     * @param socialTokens_ The new merchandising social tokens if allowed
     * @param socialTokenPrices_ The new merchandising prices in social token
     */
    function registerMerchandisingToSell(
        string[] calldata uris_,
        uint256[] calldata prices_,
        uint256[] calldata totalSupplies_,
        address[] calldata socialTokens_,
        uint256[] calldata socialTokenPrices_,
        address[] calldata sellingTokens_
    ) external onlyManager returns (uint256[] memory merchIds) {
        require(uris_.length == prices_.length, "Quantities mismatch");
        require(prices_.length == totalSupplies_.length, "Quantities mismatch");
        require(totalSupplies_.length == socialTokens_.length, "Quantities mismatch");
        require(socialTokens_.length == socialTokenPrices_.length, "Quantities mismatch");

        merchIds = new uint256[](uris_.length);
        address artist = owner();

        for (uint256 i; i < uris_.length; i++) {
            require(totalSupplies_[i] > 0, "Wrong total supply");
            require(bytes(uris_[i]).length > 0, "Wrong uri");

            _merchIdCounter.increment();
            merchIds[i] = _merchIdCounter.current();

            // save the merchandise definition
            MerchandiseToSell storage merchandise = registeredMerchandiseToSell[merchIds[i]];

            merchandise.uri = uris_[i];
            merchandise.price = prices_[i];
            merchandise.totalSupply = totalSupplies_[i];
            merchandise.sellingTokens = sellingTokens_[i];
            // console.log("#####", sellingTokens_[i]);
            

            if (socialTokens_[i] != address(0)) {
                // verify social token owner
                address tokenOwner = ISocialTokenDeployer(socialTokenManager).socialTokenOwner(
                    socialTokens_[i]
                );
                require(tokenOwner == artist, "Invalid social token");

                merchandise.socialToken = socialTokens_[i];
                merchandise.socialTokenPrice = socialTokenPrices_[i];
            }
        }
    }

    /**
     * @dev To register new merchandising to claim (with another NFT)
     * @param uris_ The new merchandising uris
     * @param merkleRoots_ The new merchandising merkle roots
     * @param merkleTreeURIs_ The new merchandising merkle trees IPFS URIs
     */
    function registerMerchandisingToClaim(
        string[] memory uris_,
        bytes32[] memory merkleRoots_,
        string[] memory merkleTreeURIs_
    ) public onlyManager returns (uint256[] memory merchIds) {
        require(uris_.length == merkleRoots_.length, "Quantities mismatch");
        require(uris_.length == merkleTreeURIs_.length, "Quantities mismatch");

        merchIds = new uint256[](uris_.length);

        for (uint256 i; i < uris_.length; i++) {
            require(bytes(uris_[i]).length > 0, "Wrong uri");

            _merchIdCounter.increment();
            merchIds[i] = _merchIdCounter.current();

            // save the merchandise definition
            MerchandiseToClaim storage merchandise = registeredMerchandiseToClaim[merchIds[i]];

            merchandise.uri = uris_[i];
            merchandise.merkleRoot = merkleRoots_[i];
            merchandise.merkleTreeURI = merkleTreeURIs_[i];
        }
    }

    /**
     * @dev To increase or decrease the merchandise total supply
     * @param merchIds_ The merchandise ids
     * @param newTotalSupply_ The new totals supply
     */
    function increaseMerchandisingTotalSupply(uint256[] memory merchIds_, uint256[] memory newTotalSupply_)
        public
        onlyManager
    {
        require(merchIds_.length == newTotalSupply_.length, "Quantities mismatch");

        uint256[] memory oldTotalSupply = new uint256[](merchIds_.length);

        for (uint256 i; i < merchIds_.length; i++) {
            require(isRegisteredToSell(merchIds_[i]), "Not registered Id");

            oldTotalSupply[i] = registeredMerchandiseToSell[merchIds_[i]].totalSupply;

            // allow only increments
            require(newTotalSupply_[i] > oldTotalSupply[i], "Wrong total supply");

            registeredMerchandiseToSell[merchIds_[i]].totalSupply = newTotalSupply_[i];
        }
    }

    /**
     * @notice Allows artists to update the Merkle Root
     * @param merchId_ The merchandise id
     * @param merkleRoot_ The new Merkle Root
     * @param merkleTreeURI_ The new Merkle Tree IPFS URI
     */
    function updateMerkleRoot(
        uint256 merchId_,
        bytes32 merkleRoot_,
        string calldata merkleTreeURI_
    ) external onlyManager {
        require(isRegisteredToClaim(merchId_), "Not registered Id");

        // update the Merkle Root
        registeredMerchandiseToClaim[merchId_].merkleRoot = merkleRoot_;
        registeredMerchandiseToClaim[merchId_].merkleTreeURI = merkleTreeURI_;
    }

    /**
     * @notice To buy merchandising
     *         (transfer the merchandise price to the artist and mint the merchNFT to the buyer)
     *         emits a MerchandisingBought event by merchandising id
     * @param merchIds_ The merchandise to buy ids
     * @param amounts_ The amounts of each merchandise to buy
     * @param buyer_ The address of the buyer
     */
    function buyMerchandising(
        uint256[] calldata merchIds_,
        uint256[] calldata amounts_,
        bool[] calldata withSocialToken_,
        address buyer_
    ) external onlyManager {
        require(merchIds_.length == amounts_.length, "Quantities mismatch");
        require(withSocialToken_.length == amounts_.length, "Quantities mismatch");

        for (uint256 i; i < merchIds_.length; i++) {
            if (withSocialToken_[i]) {
                _buyMerchandiseWithSocialToken(merchIds_[i], amounts_[i], buyer_);
            } else {
                _buyMerchandiseWithFundingToken(merchIds_[i], amounts_[i], buyer_);
            }
        }
    }

    /**
     * @notice Allows claiming the merchandise with the corresponding Merkle Proof
     * @param merchId_ The merchandise for claiming id
     * @param collection_ The collection of the token to claim from
     * @param tokenId_ The id of the token to claim from
     * @param merklePath_ The proof
     */
    function claimMerchandise(
        uint256 merchId_,
        address collection_,
        uint256 tokenId_,
        bytes32[] calldata merklePath_,
        address claimer_
    ) external onlyManager returns (uint256 mintedTokenId) {
        require(isRegisteredToClaim(merchId_), "Invalid Id for claiming");

        MerchandiseToClaim memory merchandise = registeredMerchandiseToClaim[merchId_];

        // validate the proof
        bytes32 nextTreeLevelHash = keccak256(abi.encodePacked(collection_, tokenId_));

        require(!_tokenAlreadyClaimedForMerchandising[merchId_][nextTreeLevelHash], "Token already claimed");

        // avoid claiming twice
        _tokenAlreadyClaimedForMerchandising[merchId_][nextTreeLevelHash] = true;

        // traverse the merkle path to the root
        for (uint256 i; i < merklePath_.length; i++) {
            if (nextTreeLevelHash <= merklePath_[i]) {
                nextTreeLevelHash = keccak256(abi.encodePacked(nextTreeLevelHash, merklePath_[i]));
            } else {
                nextTreeLevelHash = keccak256(abi.encodePacked(merklePath_[i], nextTreeLevelHash));
            }
        }

        // the last level must be the merkle root if the proof is correct
        require(nextTreeLevelHash == merchandise.merkleRoot, "Invalid proof provided");

        // check the owner of the NFT
        require(IERC721(collection_).ownerOf(tokenId_) == claimer_, "Only token owner allowed");

        // mint the NFT
        mintedTokenId = _mintToken(claimer_, merchandise.uri);
    }

    /**
     * @dev Allows to burn the token to claim the physical asset
     * @param tokenId_ The token to burn
     * @param caller_ Must be the token owner
     */
    function burnMerchandise(uint256 tokenId_, address caller_)
        external
        onlyManager
        returns (string memory tokenMetadataURI)
    {
        require(caller_ == ownerOf(tokenId_), "Only owner allowed");

        tokenMetadataURI = tokenURI(tokenId_);

        _burn(tokenId_);
    }

    /**
     * @dev To buy a single merchandise
     */
    function _buyMerchandiseWithFundingToken(
        uint256 merchId_,
        uint256 amount_,
        address buyer_
    ) internal {
        require(isRegisteredToSell(merchId_), "Invalid Id for selling");

        uint256[] memory tokenIds = new uint256[](amount_);

        MerchandiseToSell memory merchandise = registeredMerchandiseToSell[merchId_];

        require(merchandise.price > 0, "Non-buyable with funding token");
        require(merchandise.totalSupply - merchandise.mintedSupply >= amount_, "Not enough remining tokens");

        address artist = owner();
        registeredMerchandiseToSell[merchId_].mintedSupply += amount_;

        for (uint256 j; j < amount_; j++) {
            IERC20(fundingToken).safeTransferFrom(buyer_, artist, merchandise.price);

            tokenIds[j] = _mintToken(buyer_, merchandise.uri);
        }

        emit MerchandisingBought(merchId_, amount_, tokenIds, buyer_);
    }

    /**
     * @dev To buy a single merchandise
     */
    function _buyMerchandiseWithSocialToken(
        uint256 merchId_,
        uint256 amount_,
        address buyer_
    ) internal {
        require(isRegisteredToSell(merchId_), "Invalid Id for selling");

        uint256[] memory tokenIds = new uint256[](amount_);

        MerchandiseToSell memory merchandise = registeredMerchandiseToSell[merchId_];

        require(merchandise.socialToken != address(0), "Non-buyable with social token");
        require(merchandise.totalSupply - merchandise.mintedSupply >= amount_, "Not enough remining tokens");

        address artist = owner();
        registeredMerchandiseToSell[merchId_].mintedSupply += amount_;

        for (uint256 j; j < amount_; j++) {
            IERC20(merchandise.socialToken).safeTransferFrom(buyer_, artist, merchandise.socialTokenPrice);

            tokenIds[j] = _mintToken(buyer_, merchandise.uri);
        }

        emit MerchandisingBought(merchId_, amount_, tokenIds, buyer_);
    }

    /**
     * @dev To mint a merchandise NFT token
     */
    function _mintToken(address to_, string memory uri_) internal returns (uint256 tokenId) {
        // increment first to avoid using 0 as the first id
        _tokenIdCounter.increment();

        tokenId = _tokenIdCounter.current();

        _mint(to_, tokenId);
        _setTokenURI(tokenId, uri_);
    }

    function isRegisteredToSell(uint256 merchId_) public view returns (bool isRegistered) {
        return registeredMerchandiseToSell[merchId_].totalSupply > 0;
    }

    function isRegisteredToClaim(uint256 merchId_) public view returns (bool isRegistered) {
        return bytes(registeredMerchandiseToClaim[merchId_].uri).length > 0;
    }

    function _msgSender() internal view virtual override returns (address sender) {
        if (msg.sender == address(this)) {
            bytes memory array = msg.data;
            uint256 index = msg.data.length;

            // solhint-disable-next-line no-inline-assembly
            assembly {
                // load the 32 bytes word from memory with the address on the lower 20 bytes, and mask those.
                sender := and(mload(add(array, index)), 0xffffffffffffffffffffffffffffffffffffffff)
            }
        } else {
            return super._msgSender();
        }
    }

    /**
     * @dev Override isApprovedForAll to auto-approve OS's proxy contract
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override
        returns (bool isOperator)
    {
        // if OpenSea's ERC721 Proxy Address is detected, auto-return true
        if (_operator == address(0x58807baD0B376efc12F5AD86aAc70E78ed67deaE)) {
            return true;
        }

        // otherwise, use the default ERC721.isApprovedForAll()
        return super.isApprovedForAll(_owner, _operator);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function _burn(uint256 tokenId)
        internal
        virtual
        override(ERC721RoyaltyUpgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
        _resetTokenRoyalty(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721RoyaltyUpgradeable, ERC721Upgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

