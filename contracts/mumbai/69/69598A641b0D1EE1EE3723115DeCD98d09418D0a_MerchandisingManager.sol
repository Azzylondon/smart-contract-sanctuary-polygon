        require(whitelistedERC20Tokens[definition_.fundingToken], "Non-approved funding token");

        // deploy the licenses collection
        bytes memory calldata_ = abi.encodeWithSignature(
            "initialize(string,string,string,address,uint64,address,address)",
            definition_.metadataURI,
            definition_.collectionName,
            definition_.collectionSymbol,
            artist_,
            0,
            artist_,
            definition_.fundingToken
        );

        merchandisingCollection = address(new BeaconProxy(_merchandisingBeacon, calldata_));

        // save the collection
        registeredCollectionPerAccount[msg.sender] = merchandisingCollection;

        emit MerchandisingCollectionRegistered(merchandisingCollection, msg.sender);
    }

    /// @dev Override to add onlyRole modifer
    function whitelistERC20Token(address token_) public override onlyRole(ADMIN_ROLE) {
        super.whitelistERC20Token(token_);
    }

    /// @dev Override to add onlyRole modifer
    function blacklistERC20Token(address token_) public override onlyRole(ADMIN_ROLE) {
        super.blacklistERC20Token(token_);
    }

    /// @dev Override to add onlyRole modifer
    function whitelistCollection(address token_) public override onlyRole(ADMIN_ROLE) {
        super.whitelistCollection(token_);
    }

    /// @dev Override to add onlyRole modifer
    function blacklistCollection(address token_) public override onlyRole(ADMIN_ROLE) {
        super.blacklistCollection(token_);
    }

    function updateApprovedSigner(address approvedSigner_) external onlyRole(UPGRADER_ROLE) {
        approvedSigner = approvedSigner_;
    }

    function _authorizeUpgrade(address) internal view override onlyRole(UPGRADER_ROLE) {}

    function getMerchandisingBeacon() external returns (address _merchandisingBeacon) {}
}

