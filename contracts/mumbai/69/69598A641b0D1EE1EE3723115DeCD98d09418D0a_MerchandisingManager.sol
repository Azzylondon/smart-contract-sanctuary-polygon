import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol";
import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "./beacon-implementations/Merchandising.sol";
import "./extensions/CollectionWhitelist.sol";
import "./extensions/ERC20TokenWhitelist.sol";
import "./extensions/EIP712Signature.sol";
import "./Structs.sol";

/**
 * @title The factory to handle the multiple collections deployments
 */
contract MerchandisingManager is
    UUPSUpgradeable,
    AccessControlUpgradeable,
    CollectionWhitelist,
    ERC20TokenWhitelist,
    EIP712Signature
{
    /* solhint-disable no-empty-blocks,not-rely-on-time */

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    address private immutable _merchandisingBeacon;

    /// @notice The address allowed to sign registration of songs
    address public approvedSigner;

    /// @notice A mapping to check if a collection was deployed for an Account
    mapping(address => address) public registeredCollectionPerAccount;

    /**
     * @dev Emitted when a new collection is deployed
     * @param collection The address of the new deployed collection
     * @param artist The address of the artist
     */
    event MerchandisingCollectionRegistered(address collection, address indexed artist);

    event MerchandisingToSellRegistered(
        address indexed collection,
        uint256[] merchIds,
        string[] uris,
        uint256[] prices
    );

    event MerchandisingToClaimRegistered(
        address indexed collection,
        uint256[] merchIds,
        string[] uris,
        bytes32[] merkleRoots
    );

    event MerchandiseClaimed(
        uint256 indexed merchId,
        address indexed artist,
        uint256 mintedTokenId,
        address collection,
        uint256 tokenId,
        address claimer
    );

    event MerchandisingTotalSupplyIncreased(uint256[] merchIds, uint256[] newTotalSupply);

    event MerkleRootUpdated(uint256 merchId, bytes32 merkleRoot, string merkleTreeURI);

    event MerchandiseRedeemed(address artist, address collection, uint256 tokenId, string tokenURI);

    /**
     * @dev Initializes the domain separator and parameter caches.
     *
     * The meaning of `name` and `version` is specified in
     * https://eips.ethereum.org/EIPS/eip-712#definition-of-domainseparator[EIP 712]:
     *
     * @param name_ the user readable name of the signing domain, i.e. the name of the DApp or the protocol.
     * @param version_ the current major version of the signing domain.
     */
    constructor(
        address socialTokenDeployer_,
        string memory name_,
        string memory version_,
        address upgrader
    ) initializer EIP712Signature(name_, version_) {
        // _merchandisingBeacon = merchandisingBeacon_;
        UpgradeableBeacon _tokenBeacon = new UpgradeableBeacon(address(new Merchandising(socialTokenDeployer_)));
        _tokenBeacon.transferOwnership(upgrader);
        _merchandisingBeacon = address(_tokenBeacon);
    }

    function initialize(address approvedSigner_, address owner_) external initializer {
        approvedSigner = approvedSigner_;

        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);

        assembly {
            sstore(_ADMIN_SLOT, owner_)
        }
    }

    /**
     * @notice Allows to deploy the merchandising collection and register merchandising to sell
     * @param v_ The v component of the signature
     * @param r_ The r component of the signature
     * @param s_ The s component of the signature
     * @param deadline_ The timestamp when the signature expires
     */
    function registerNewMerchandisingToSell(
        uint8 v_,
        bytes32 r_,
        bytes32 s_,
        CollectionDefinition calldata definition_,
        string[] memory uris_,
        uint256[] memory prices_,
        uint256[] memory totalSupplies_,
        address[] memory sellingTokens_,
        uint64 deadline_
    ) external {
        // verify the signature
        //require(verify(v_, r_, s_, deadline_, msg.sender), "Invalid signature");

        // deploy the new merchandising collection
        _deployMerchandisingBeaconProxy(definition_, msg.sender);

        // register the merchadise to sell
        registerMerchandisingToSell(uris_, prices_, totalSupplies_, sellingTokens_);
    }

    /**
     * @dev To register new merchandising to sell
     * @param uris_ The new merchandising uris
     * @param prices_ The new merchandising prices
     * @param totalSupplies_ The new merchandising total supplies
     */
    function registerMerchandisingToSell(
        string[] memory uris_,
        uint256[] memory prices_,
        uint256[] memory totalSupplies_,
        address[] memory sellingTokens_
    ) public {
        address collection = registeredCollectionPerAccount[msg.sender];
        require(collection != address(0), "Non-existent collection");

        address[] memory socialTokens_ = new address[](uris_.length);
        uint256[] memory socialTokenPrices_ = new uint256[](uris_.length);

        // register the merchandise
        uint256[] memory ids = Merchandising(collection).registerMerchandisingToSell(
            uris_,
            prices_,
            totalSupplies_,
            socialTokens_,
            socialTokenPrices_,
            sellingTokens_
        );

        emit MerchandisingToSellRegistered(collection, ids, uris_, prices_);
    }

    /**
     * @notice Allows to deploy the merchandising collection and register merchandising to claim
     * @param v_ The v component of the signature
     * @param r_ The r component of the signature
     * @param s_ The s component of the signature
     * @param uris_ The new merchandising uris
     * @param merkleRoots_ The new merchandising merkle roots
     * @param merkleTreeURIs_ The new merchandising merkle trees IPFS URIs
     * @param deadline_ The timestamp when the signature expires
     */
    function registerNewMerchandisingToClaim(
        uint8 v_,
        bytes32 r_,
        bytes32 s_,
        CollectionDefinition calldata definition_,
        string[] memory uris_,
        bytes32[] memory merkleRoots_,
        string[] memory merkleTreeURIs_,
        uint64 deadline_
    ) external {
        // verify the signature
        // require(verify(v_, r_, s_, deadline_, msg.sender), "Invalid signature");

        // deploy the new merchandising collection
        _deployMerchandisingBeaconProxy(definition_, msg.sender);

        // register the merchadise to claim
        registerMerchandisingToClaim(uris_, merkleRoots_, merkleTreeURIs_);
    }

    /**
     * @dev To register new merchandising to claim (with another NFT)
     * @param uris_ The new merchandising uris
     * @param merkleRoots_ The new merchandising merkle roots
     * @param merkleTreeURIs_ The new merchandising merkle trees IPFS URIs
     */
    function registerMerchandisingToClaim(
        string[] memory uris_,
        bytes32[] memory merkleRoots_,
        string[] memory merkleTreeURIs_
    ) public {
        address collection = registeredCollectionPerAccount[msg.sender];
        require(collection != address(0), "Non-existent collection");

        // register the merchandise
        uint256[] memory ids = Merchandising(collection).registerMerchandisingToClaim(
            uris_,
            merkleRoots_,
            merkleTreeURIs_
        );

        emit MerchandisingToClaimRegistered(collection, ids, uris_, merkleRoots_);
    }

    /**
     * @dev To increase or decrease the merchandise total supply
     * @param merchIds_ The merchandise ids
     * @param newTotalSupply_ The new totals supply
     */
    function increaseMerchandisingTotalSupply(uint256[] memory merchIds_, uint256[] memory newTotalSupply_)
        public
    {
        address collection = registeredCollectionPerAccount[msg.sender];
        require(collection != address(0), "Non-existent collection");

        // increase the supply
        Merchandising(collection).increaseMerchandisingTotalSupply(merchIds_, newTotalSupply_);

        emit MerchandisingTotalSupplyIncreased(merchIds_, newTotalSupply_);
    }

    /**
     * @notice Allows artists to update the Merkle Root
     * @param merchId_ The merchandise id
     * @param merkleRoot_ The new Merkle Root
     * @param merkleTreeURI_ The new Merkle Tree IPFS URI
     */
    function updateMerkleRoot(
        uint256 merchId_,
        bytes32 merkleRoot_,
        string calldata merkleTreeURI_
    ) external {
        address collection = registeredCollectionPerAccount[msg.sender];
        require(collection != address(0), "Non-existent collection");

        // update the merkle info
        Merchandising(collection).updateMerkleRoot(merchId_, merkleRoot_, merkleTreeURI_);

        emit MerkleRootUpdated(merchId_, merkleRoot_, merkleTreeURI_);
    }

    /**
     * @notice To buy merchandising
     * @param artist_ The artist to get the collection
     * @param merchIds_ The merchandise to buy ids
     * @param amounts_ The amounts of each merchandise to buy
     */
    function buyMerchandising(
        address artist_,
        uint256[] calldata merchIds_,
        uint256[] calldata amounts_
    ) external {
        address collection = registeredCollectionPerAccount[artist_];
        require(collection != address(0), "Non-existent collection");

        bool[] memory withSocialToken_ = new bool[](merchIds_.length);

        // buy the tokens
        Merchandising(collection).buyMerchandising(merchIds_, amounts_, withSocialToken_, msg.sender);
    }

    /**
     * @notice Allows claiming the merchandise with the corresponding Merkle Proof
     * @param artist_ The artist to get the collection
     * @param merchId_ The merchandise for claiming id
     * @param collection_ The collection of the token to claim from
     * @param tokenId_ The id of the token to claim from
     * @param merklePath_ The proof
     */
    function claimMerchandise(
        address artist_,
        uint256 merchId_,
        address collection_,
        uint256 tokenId_,
        bytes32[] calldata merklePath_
    ) external {
        address collection = registeredCollectionPerAccount[artist_];
        require(collection != address(0), "Non-existent collection");

        // buy the tokens
        uint256 mintedTokenId = Merchandising(collection).claimMerchandise(
            merchId_,
            collection_,
            tokenId_,
            merklePath_,
            msg.sender
        );

        emit MerchandiseClaimed(merchId_, artist_, mintedTokenId, collection_, tokenId_, msg.sender);
    }

    /**
     * @dev Allows to burn the token to claim the physical asset
     * @param artist_ The artist owning the collection
     * @param tokenId_ The token to burn
     */
    function redeemMerchandise(address artist_, uint256 tokenId_) external {
        address collection = registeredCollectionPerAccount[artist_];
        require(collection != address(0), "Non-existent collection");

        // burn the token
        string memory uri = Merchandising(collection).burnMerchandise(tokenId_, msg.sender);

        emit MerchandiseRedeemed(artist_, collection, tokenId_, uri);
    }

    function verify(
        uint8 v_,
        bytes32 r_,
        bytes32 s_,
