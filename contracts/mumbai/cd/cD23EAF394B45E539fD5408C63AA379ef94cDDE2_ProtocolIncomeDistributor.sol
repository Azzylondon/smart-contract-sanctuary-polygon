interface IProtocolIncomeDistributor {
    event Distributed(uint256 time, uint256 tokenAmount);

    event ManulDistributed(uint256 week,uint256 blockNumber,uint256 veSupply,uint256 remainAmount1,uint256 remainAmount2,uint256 remainAmount3,uint256 remainAmount4);

    event Claimed(
        address indexed recipient,
        uint256 veSupply,
        uint256 amount1,
        uint256 amount2,
        uint256 amount3,
        uint256 amount4
        
    );

    function claimable(address _addr) external view returns (uint256,uint256[] memory);
}



contract ProtocolIncomeDistributor is
    IProtocolIncomeDistributor,
    ReentrancyGuard,
    Ownable
{
    using SafeERC20 for IERC20;

    struct ClaimRecord {
        uint256 week;
        uint256 blockNumber;
        uint256 veSupply;
        uint256 remainAmount1;
        uint256 remainAmount2;
        uint256 remainAmount3;
        uint256 remainAmount4;
    }

    uint256 public  WEEK = 7 * 600;
    uint256 public  TOKEN_CHECKPOINT_DEADLINE = 600;
    
    // uint256 public constant WEEK = 2 * 600;
    // uint256 public constant TOKEN_CHECKPOINT_DEADLINE = 600;

    uint256 public startTime;
    // uint256 public timeCursor;
    // mapping(address => uint256) public timeCursorOf;
    // mapping(address => mapping(uint256 => bool)) public claimRecords;

    mapping(uint256 => ClaimRecord) public systemClaimRecords;
    mapping(address => uint256) public userLastClaimWeek;


    IVeDefed public veDEFED;
    address public protocolIncomeVault;


    constructor(address _veDefedAddress,address _protocolIncomeVault){
        veDEFED = IVeDefed(_veDefedAddress);
        
        protocolIncomeVault = _protocolIncomeVault;
        uint256 t = (block.timestamp / WEEK) * WEEK;
        startTime = t;
    }

     function setTokenDeadline(uint256 _deadline) external onlyOwner{
        TOKEN_CHECKPOINT_DEADLINE = _deadline;
        WEEK = 7 * TOKEN_CHECKPOINT_DEADLINE;
    }


     function setVeDefedAddress(address _veDefedAddress) external onlyOwner{
        veDEFED = IVeDefed(_veDefedAddress);
    }


     function setProtocolIncomeVault(address _protocolIncomeVault) external onlyOwner{
        protocolIncomeVault = _protocolIncomeVault;
    }


    function manulDistribute(uint256 time,uint256[] calldata amount) external onlyOwner {
        uint256 week = (time / WEEK) * WEEK;
        require(systemClaimRecords[week].blockNumber == 0, "Already distributed this week");
        uint256 totalSupply = veDEFED.totalSupplyAt(block.number);
        ClaimRecord memory record = ClaimRecord(week, block.number,totalSupply,amount[0],amount[1],amount[2],amount[3]);
        systemClaimRecords[week] = record;
        emit ManulDistributed(week, block.number, totalSupply,amount[0],amount[1],amount[2],amount[3]);
        
    }

    function claim() external  nonReentrant {
        uint256 lastWeek = userLastClaimWeek[msg.sender];
        if(lastWeek == 0){
            lastWeek = startTime;
        }
        
        (uint256 currentWeek, uint256[] memory totalDisAmount) = claimable(msg.sender);
        ClaimRecord memory claimRecord = systemClaimRecords[lastWeek];
        claimRecord.remainAmount1 -= totalDisAmount[0];
        claimRecord.remainAmount2 -= totalDisAmount[1];
        claimRecord.remainAmount3 -= totalDisAmount[2];
        claimRecord.remainAmount4 -= totalDisAmount[3];
        systemClaimRecords[lastWeek] = claimRecord;

        userLastClaimWeek[msg.sender] = currentWeek;
        
        if(totalDisAmount[0] > 0 ||totalDisAmount[1] > 0 ||totalDisAmount[2] > 0 ||totalDisAmount[3] > 0 ){
            ProtocolIncomeVaultInterface(protocolIncomeVault).claim(msg.sender,totalDisAmount);
        }
        
        emit Claimed(msg.sender, claimRecord.veSupply ,totalDisAmount[0],totalDisAmount[1],totalDisAmount[2],totalDisAmount[3]);
    }



    function claimable(address _addr) public view override returns(uint256,uint256[] memory){
        uint256 lastWeek = userLastClaimWeek[_addr];
        uint256[] memory totalDisAmount = new uint256[](4);
        uint256[] memory currentDisAmount = new uint256[](4);
        uint256 thisWeek = block.timestamp / WEEK * WEEK;
        if(lastWeek == 0){
            lastWeek = startTime;
        }
        if(lastWeek > thisWeek){
            return (lastWeek,totalDisAmount);
        }

        uint256 interval = thisWeek - lastWeek;
        uint256 cycle = interval / WEEK +1;
        
        if(cycle > 52) {
            cycle = 52;
        }
       
        for(uint256 i = 0; i< cycle;i++){
            ClaimRecord memory claimRecord = systemClaimRecords[lastWeek];
            uint256 balanceOf = veDEFED.balanceOfAt(_addr, claimRecord.blockNumber);
            if(claimRecord.veSupply == 0){
                lastWeek += WEEK;
                continue;
            }
            if(balanceOf >= claimRecord.veSupply){
                currentDisAmount[0] = claimRecord.remainAmount1;
                currentDisAmount[1] = claimRecord.remainAmount2;
                currentDisAmount[2] = claimRecord.remainAmount3;
                currentDisAmount[3] = claimRecord.remainAmount4;
            }else{
                uint256 supply = claimRecord.veSupply / (10**18);
                if(supply == 0){
                    lastWeek += WEEK;
                    continue;
                }
                uint256 simpleBalance = balanceOf /(10**18);
                currentDisAmount[0]  = simpleBalance * claimRecord.remainAmount1 / supply;
                currentDisAmount[1]  = simpleBalance * claimRecord.remainAmount2 / supply;
                currentDisAmount[2]  = simpleBalance * claimRecord.remainAmount3 / supply;
                currentDisAmount[3]  = simpleBalance * claimRecord.remainAmount4 / supply;
            }
            totalDisAmount[0] += currentDisAmount[0];
            totalDisAmount[1] += currentDisAmount[1];
            totalDisAmount[2] += currentDisAmount[2];
            totalDisAmount[3] += currentDisAmount[3];
            lastWeek += WEEK;
        }
        return (lastWeek,totalDisAmount);
    }

}

