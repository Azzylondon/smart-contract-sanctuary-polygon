        uint256 pt1xx, uint256 pt1xy,
        uint256 pt1yx, uint256 pt1yy,
        uint256 pt2xx, uint256 pt2xy,
        uint256 pt2yx, uint256 pt2yy
    ) public view returns (
        uint256, uint256,
        uint256, uint256
    ) {
        if (
            pt1xx == 0 && pt1xy == 0 &&
            pt1yx == 0 && pt1yy == 0
        ) {
            if (!(
            pt2xx == 0 && pt2xy == 0 &&
            pt2yx == 0 && pt2yy == 0
            )) {
                assert(_isOnCurve(
                        pt2xx, pt2xy,
                        pt2yx, pt2yy
                    ));
            }
            return (
            pt2xx, pt2xy,
            pt2yx, pt2yy
            );
        } else if (
            pt2xx == 0 && pt2xy == 0 &&
            pt2yx == 0 && pt2yy == 0
        ) {
            assert(_isOnCurve(
                    pt1xx, pt1xy,
                    pt1yx, pt1yy
                ));
            return (
            pt1xx, pt1xy,
            pt1yx, pt1yy
            );
        }

        assert(_isOnCurve(
                pt1xx, pt1xy,
                pt1yx, pt1yy
            ));
        assert(_isOnCurve(
                pt2xx, pt2xy,
                pt2yx, pt2yy
            ));

        uint256[6] memory pt3 = _ECTwistAddJacobian(
            pt1xx, pt1xy,
            pt1yx, pt1yy,
            1,     0,
            pt2xx, pt2xy,
            pt2yx, pt2yy,
            1,     0
        );

