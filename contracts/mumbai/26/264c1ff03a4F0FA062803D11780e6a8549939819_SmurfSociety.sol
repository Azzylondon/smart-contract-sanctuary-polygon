pragma solidity ^0.8.17;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol";
import "./libraries/ProofVerifier.sol";
import {Constants} from "./libraries/Constants.sol";

contract SmurfSociety is Initializable, ERC1155Upgradeable, AccessControlUpgradeable, PausableUpgradeable, ERC1155BurnableUpgradeable, ERC1155SupplyUpgradeable, ERC2981Upgradeable, ProofCommon {
    using SafeMathUpgradeable for uint256;
    // Roles
    bytes32 constant URI_SETTER_ROLE         = keccak256("URI_SETTER_ROLE");
    bytes32 constant PAUSER_ROLE             = keccak256("PAUSER_ROLE");
    bytes32 constant MINTER_ROLE             = keccak256("MINTER_ROLE");

    // Contract variables
    string public       __contractUri;          // The contract URI json link
    uint96 public       __maxSmurfPerHolder;    // Maximum number of smurfs per holder
    uint96 public       __maxSmurfSupply;       // Maximum number of smurfs tokens
    uint256 public      __pricePerSmurf;        // The price per smurf
    uint96 public       __maxRecipesForMint;    // Number of allowed recipes to be mintable (25)
    uint96 public       __maxRecipesPerUser;    // Number of allowed single recipe a single user can obtain (1)

    // Main addresses
    address internal    __walletSignature;      // The address used to sign the Proofs
    address internal    __walletWithdraw;       // The address used to withdraw all funds

    // Verifier
    ProofVerifier       __proofVerifier;

    // Events
    event Mint_Recipe_Failed(address indexed _from, uint _recipe_id, bytes _signature);
    event Mint_Recipe_Success(address indexed _from, uint _recipe_id, bytes _signature);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(string memory _newuri, uint96 _maxSmurfPerHolder, uint96 _maxSmurfSupply, uint256 _pricePerSmurf, uint96 _maxRecipesForMint, uint96 _maxRecipesPerUser, uint96 _royaltyValue, address _walletSignature, address _walletWithdraw, address _proofVerifier) initializer public {
        __ERC1155_init(_newuri);
        __AccessControl_init();
        __Pausable_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);

        // Collection configs
        _setMaxSmurfPerHolder(_maxSmurfPerHolder);
        _setMaxSmurfSupply(_maxSmurfSupply);
        _setPricePerSmurf(_pricePerSmurf);
        _setMaxRecipesForMint(_maxRecipesForMint);
        _setMaxRecipesPerUser(_maxRecipesPerUser);

        // Internal configs
        _setDefaultRoyalty(_walletWithdraw, _royaltyValue);
        _setWalletSignature(_walletSignature);
        _setWalletWithdraw(_walletWithdraw);

        __proofVerifier = ProofVerifier(_proofVerifier);
    }

    function mintSmurf(uint quantity) public payable whenNotPaused {
        require(quantity>0, "Smurf: quantity must be greater than 0");
        require((balanceOf(msg.sender, Constants.SMURF) + quantity) <= __maxSmurfPerHolder, "Smurf: total quantity exceeds limit allowed");
        require(msg.value >= SafeMathUpgradeable.mul(__pricePerSmurf, quantity), "Smurf: insufficient funds for selected quantity");
        require((quantity + totalSupply(Constants.SMURF)) <= __maxSmurfSupply, "Smurf: selected quantity exceeds remaining supply");

        _mint(msg.sender, Constants.SMURF, quantity, "");
    }

    function dropSmurf(address _to) public onlyRole(MINTER_ROLE) whenNotPaused {
        require((balanceOf(_to, Constants.SMURF) + 1) <= __maxSmurfPerHolder, "Smurf: total quantity exceeds limit allowed");
        require((totalSupply(Constants.SMURF) + 1) <= __maxSmurfSupply, "Smurf: selected quantity exceeds remaining supply");

        _mint(_to, Constants.SMURF, 1, "");
    }

    function dropIngredient(address _to, uint96 _ingredient, uint8 quantity) public onlyRole(MINTER_ROLE) whenNotPaused {
        require(_ingredient >= Constants.INDEX_INGREDIENT_START && _ingredient <= Constants.INDEX_INGREDIENT_STOP, "Smurf: ingredient ID must fall within the accepted bounds");

        _mint(_to, _ingredient, quantity, "");
    }

    function dropBox(address _to, uint[] memory _ingredients, uint[] memory _quantities) public onlyRole(MINTER_ROLE) whenNotPaused {
        require(_ingredients.length == _quantities.length, "Smurf: ids and amounts length mismatch");
        for(uint256 i=0; i < _ingredients.length; i++){
            require(_ingredients[i] >= Constants.INDEX_INGREDIENT_START && _ingredients[i] <= Constants.INDEX_INGREDIENT_STOP, "Smurf: ingredient ID must fall within the accepted bounds");
        }

        _mintBatch(_to, _ingredients, _quantities, "");
    }

    function mintRecipe(Proof memory _proof, uint[32] memory _input, uint96[] memory _ingredients, bytes memory _signature) public whenNotPaused {
        // Ingredients
        require(balanceOf(msg.sender, Constants.SMURF) > 0, "Smurf: you need a statuette to be able to test a recipe");
        require(balanceOf(msg.sender, Constants.CLAY) > 0, "Smurf: you need blue clay to be able to test a recipe");
        for(uint256 i=0; i < _ingredients.length; i++){
            require(_ingredients[i] >= Constants.INDEX_INGREDIENT_START && _ingredients[i] <= Constants.INDEX_INGREDIENT_STOP, "Smurf: ingredient ID must fall within the accepted bounds");
            require(balanceOf(msg.sender,_ingredients[i]) > 0, "Smurf: you are missing an ingredient to test the recipe");
        }

        // Recipe
        require(_input[0] >= Constants.INDEX_RECIPE_START && _input[0] <= Constants.INDEX_RECIPE_STOP, "Smurf: recipe ID must fall within the accepted bounds");
        require((balanceOf(msg.sender, _input[0]) + 1) <= __maxRecipesPerUser, "Smurf: tested recipe exceeds allotted supply per user");
        require(totalSupply(_input[0]) + 1 <= __maxRecipesForMint, "Smurf: tested recipe exceeds allotted supply");

        // Signature
        bool signatureCheck = checkSignature(__walletSignature,msg.sender, _input, _signature);
        bool proofCheck = __proofVerifier.verifyTx(_proof, _input);

        if (proofCheck && signatureCheck) {
            _mint(msg.sender, _input[0], 1, "");

            burnBatch(msg.sender,getTokensToBurnForRecipe(_ingredients),getQuantitiesToBurnForRecipe(_ingredients));

            emit Mint_Recipe_Success(msg.sender, _input[0], _signature);
        } else {
            burn(msg.sender, Constants.CLAY, 1);

            emit Mint_Recipe_Failed(msg.sender, _input[0], _signature);
        }
    }

    function getTokensToBurnForRecipe(uint96[] memory _ingredients) internal pure returns(uint[] memory) {
        uint[] memory tokensToBurn = new uint[](_ingredients.length + 2);

        tokensToBurn[0]=Constants.SMURF;
        tokensToBurn[1]=Constants.CLAY;

        for(uint256 ingredient=0; ingredient < _ingredients.length; ingredient++){
            tokensToBurn[2+ingredient] = _ingredients[ingredient];
        }

        return tokensToBurn;
    }

    function getQuantitiesToBurnForRecipe(uint96[] memory _ingredients) internal pure returns(uint[] memory) {
        uint[] memory quantitiesToBurn = new uint[](_ingredients.length + 2);

        quantitiesToBurn[0]=1;
        quantitiesToBurn[1]=1;

        for(uint256 ingredient=0; ingredient < _ingredients.length; ingredient++){
            quantitiesToBurn[2+ingredient] = 1;
        }

        return quantitiesToBurn;
    }

    function checkSignature( address _walletSignature, address _sender, uint[32] memory _input, bytes memory _signature) internal view returns (bool) {
        bytes32 hash = keccak256(abi.encodePacked(_sender, _input));
        return SignatureCheckerUpgradeable.isValidSignatureNow(_walletSignature, hash, _signature);
    }

    // Setters
    function _setMaxSmurfPerHolder(uint96 _maxSmurfPerHolder) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __maxSmurfPerHolder = _maxSmurfPerHolder;
    }

    function _setMaxSmurfSupply(uint96 _maxSmurfSupply) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __maxSmurfSupply = _maxSmurfSupply;
    }

    function _setPricePerSmurf(uint256 _pricePerSmurf) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __pricePerSmurf = _pricePerSmurf;
    }

    function _setMaxRecipesForMint(uint96 _maxRecipesForMint) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __maxRecipesForMint = _maxRecipesForMint;
    }

    function _setMaxRecipesPerUser(uint96 _maxRecipesPerUser) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __maxRecipesPerUser = _maxRecipesPerUser;
    }

    function _setWalletSignature(address _walletSignature) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __walletSignature = _walletSignature;
    }

    function _setWalletWithdraw(address _walletWithdraw) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        __walletWithdraw = _walletWithdraw;
    }

    function setRoyalties(address _royaltyAddress, uint96 _royaltyValue) public onlyRole(DEFAULT_ADMIN_ROLE) whenNotPaused {
        _setDefaultRoyalty(_royaltyAddress, _royaltyValue);
    }

    function setURI(string memory newuri) public onlyRole(URI_SETTER_ROLE) whenNotPaused {
        _setURI(newuri);
    }

    function setContractUri(string memory _contractUri) external onlyRole(URI_SETTER_ROLE) whenNotPaused {
        __contractUri = _contractUri;
    }

    // Getters
    function uri(uint256 id) override public view returns (string memory) {
        require(id >= 0, "Smurf: Nonexistent token");
        string memory tokenUri = super.uri(id);
        return string(abi.encodePacked(tokenUri, StringsUpgradeable.toString(id)));
    }

    function contractURI() public view returns (string memory) {
        return __contractUri;
    }

    function balanceOfIngredients(address wallet) public view returns (uint256[] memory) {
        uint[] memory balances = new uint[](Constants.INDEX_INGREDIENT_STOP-Constants.INDEX_INGREDIENT_START+1);
        for(uint256 ingredient=Constants.INDEX_INGREDIENT_START; ingredient <= Constants.INDEX_INGREDIENT_STOP; ingredient++){
            balances[ingredient-Constants.INDEX_INGREDIENT_START]=balanceOf(wallet, ingredient);
        }
        return balances;
    }

    function balanceOfRecipes(address wallet) public view returns (uint256[] memory) {
        uint[] memory balances = new uint[](Constants.INDEX_RECIPE_STOP-Constants.INDEX_RECIPE_START+1);
        for(uint256 ingredient=Constants.INDEX_RECIPE_START; ingredient <= Constants.INDEX_RECIPE_STOP; ingredient++){
            balances[ingredient-Constants.INDEX_RECIPE_START]=balanceOf(wallet, ingredient);
        }
        return balances;
    }

    // Internals
    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
    internal
    whenNotPaused
    override(ERC1155Upgradeable, ERC1155SupplyUpgradeable)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC1155Upgradeable, AccessControlUpgradeable, ERC2981Upgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function withdraw() external payable onlyRole(DEFAULT_ADMIN_ROLE) returns (bool success) {
        (success,) = payable(__walletWithdraw).call{value: address(this).balance}("");
    }
}

