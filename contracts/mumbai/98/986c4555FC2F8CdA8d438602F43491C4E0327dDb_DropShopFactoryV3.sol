pragma solidity ^0.8.4;

import '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./DropShopNFTERC1155.sol";

import "hardhat/console.sol";

contract DropShopFactoryV3 is ReentrancyGuardUpgradeable, OwnableUpgradeable {
  address[] private deployedDropShopNFTERC1155;

   event NewContractCreated(address contractAddress);
  
  constructor(){
    __Ownable_init();
  }
  // Create a NFT contract
  function createNFTContract() public nonReentrant onlyOwner {
    DropShopNFTERC1155 dropShopNFTERC1155 = new DropShopNFTERC1155();
    address newdropShopNFTERC1155 = address(dropShopNFTERC1155);
    dropShopNFTERC1155.initialize("DropShopNFTERC1155","DPNFT","https://gateway.pinata.cloud/ipfs/?/{id}.json", "https://gateway.pinata.cloud/ipfs/QmTfE2qX3Bje2n2dRmuqi6x3tK4CFnQiHVDZmF4AfhNNLb/contract_metadata.json", msg.sender,msg.sender,1000000,10000);
    
    addDeployedNFTContract(newdropShopNFTERC1155);
    emit NewContractCreated(newdropShopNFTERC1155);
  }
  
  // Get all created campaigns
  function getDeployedNFTContract() 
  public view returns (address[]  memory){
    return deployedDropShopNFTERC1155;
  }
  
  /** Setter functions */
  function addDeployedNFTContract(address newNFTContract) private {
    deployedDropShopNFTERC1155.push(newNFTContract);
  }
}

