pragma solidity =0.8.15;

import "./ERC721.sol";
import "./utils/Ownable.sol";

contract SunsetNFT is ERC721, Ownable {
    constructor() ERC721("Sunset NFT", "SUNSET") Ownable() {}

    function mint(address to, uint256 tokenId) public onlyOwner {
        _mint(to, tokenId);
    }

    function safeMint(address to, uint256 tokenId) public onlyOwner {
        _safeMint(to, tokenId);
    }

    function safeMint(address to, uint256 tokenId, bytes memory _data) public onlyOwner {
        _safeMint(to, tokenId, _data);
    }

    function _baseURI() internal pure override returns (string memory) {
        return "ipfs://QmZJiLbU9HXS8dPmKPgM8FoXFEfG1tF4kWUGmnD2x8YCdt/";
    }
}

