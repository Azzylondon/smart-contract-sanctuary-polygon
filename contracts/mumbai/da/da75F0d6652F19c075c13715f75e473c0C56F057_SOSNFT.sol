        temporaryValue = tokenURIs[result];
        _purgeGotVals(result);
        return temporaryValue;
    }
    function _purgeGotVals(uint256 index) internal{
        for(uint256 i = index; i < tokenURIs.length - 1; i++){
            tokenURIs[i] = tokenURIs[i + 1];
        }
        tokenURIs.pop();
    }
