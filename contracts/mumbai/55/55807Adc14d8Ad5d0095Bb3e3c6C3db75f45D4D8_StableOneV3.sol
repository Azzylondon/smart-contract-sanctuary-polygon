        uint256 _numOfDays
    ) public view returns(uint256){
        if(_totalInvestment==0 || _prevInvestment==0){
            return 0;
        }
        // uint256 weightedAvg = (_oldAvg.mul(_prevInvestment).div(_totalInvestment).add(_prevInvestment.mul(_numOfDays).div(_totalInvestment))).mul(1000);
        uint256 firstEquation = (_oldAvg*_prevInvestment)/_totalInvestment;
        uint256 secondEquation = (_prevInvestment*_numOfDays*1000)/_totalInvestment;
        uint256 weightedAvg = firstEquation + secondEquation;
        return weightedAvg;
    }

    function _invest(
        address _addr,
        address _ref,
        uint256 _amount
    ) private returns (bool){
        require(msg.value >= MIN_INVESTMENT, "Minimum investment is 0.05 Matic");
        require(_ref != _addr, "Ref address cannot be same with caller");

        Investor storage _investor = investors[_addr];
        uint256 _prevInvestment = _investor.totalInvestment;
        if (_investor.addr == address(0)) {
            _addInvestor(_addr, _ref, _amount);
            investors[_addr].oldAvgCalculationDate = block.timestamp;
        }

        _investor.totalDeposit = _investor.totalDeposit.add(
            _amount
        );
        
        totalInvested = totalInvested.add(_amount);
        activeInvested = activeInvested.add(_amount);
        investors[_addr].totalInvestment = investors[_addr].totalInvestment.add(_amount);

        uint256 numOfDays = block.timestamp.sub(_investor.oldAvgCalculationDate).div(TIME_STEP_DRAW);
        if(numOfDays>=1){
        uint256 weightedAvg = calculateWeightedAverage(
            _investor.oldAvg,
            _prevInvestment,
            _investor.totalInvestment,
            numOfDays
        );

        _investor.oldAvg = weightedAvg;
        _investor.totalAvg = calculateWeightedAverage(
            _investor.totalAvg,
            _prevInvestment,
            _investor.totalInvestment,
            numOfDays
        );
        _investor.oldAvgCalculationDate = block.timestamp;
        }

        _investor.investmentCount = _investor.investmentCount.add(1);
        _investor.oldAvgCalculationDate = block.timestamp;

        _sendRewardOnInvestment(_amount);
        return true;
    }

    function _sendReferralReward(
        address payable _ref,
        uint256 level,
        uint256 _amount
    ) private {
        uint256 reward;
        if (level == 1) {
            reward = _amount.mul(REFERENCE_LEVEL1_RATE).div(1000);
        } else if (level == 2) {
            reward = _amount.mul(REFERENCE_LEVEL2_RATE).div(1000);
        } else if (level == 3) {
            reward = _amount.mul(REFERENCE_LEVEL3_RATE).div(1000);
        } else if (level == 4) {
            reward = _amount.mul(REFERENCE_LEVEL4_RATE).div(1000);
        } else if (level == 5) {
            reward = _amount.mul(REFERENCE_LEVEL5_RATE).div(1000);
        }

        totalReferralReward = totalReferralReward.add(reward);
        _ref.transfer(reward);
    }

    function _reinvest(address _addr,uint256 _amount) private returns(bool){
        Investor storage _investor = investors[_addr];

        uint256 _prevInvestment = _investor.totalInvestment;

        _investor.totalDeposit = _investor.totalDeposit.add(
            _amount
        );
        _investor.totalReinvest = _investor.totalReinvest.add(
            _amount
        );
        totalReinvested = totalReinvested.add(_amount);

        activeInvested = activeInvested.add(_amount);
        totalInvested = totalInvested.add(_amount);

        investors[_addr].totalInvestment = investors[_addr].totalInvestment.add(_amount);

        uint256 numOfDays = block.timestamp.sub(_investor.oldAvgCalculationDate).div(TIME_STEP_DRAW);
        if(numOfDays>=1){
            uint256 weightedAvg = calculateWeightedAverage(
                _investor.oldAvg,
                _prevInvestment,
                _investor.totalInvestment,
                numOfDays
            );

            _investor.oldAvg = weightedAvg;
            _investor.totalAvg = calculateWeightedAverage(
                _investor.totalAvg,
                _prevInvestment,
                _investor.totalInvestment,
                numOfDays
            );
            _investor.oldAvgCalculationDate = block.timestamp;
        }
        _investor.investmentCount = _investor.investmentCount.add(1);

        _sendRewardOnReinvestment(_amount);

        return true;
    }

    function _addInvestor(
        address _addr,
        address _ref,
        uint256 _amount
    ) private {
        investors[_addr].addr = _addr;
        address refAddr = _ref;
        investors[_addr].ref = _ref;
        for (uint256 i = 0; i < 5; i++) {
            if (investors[refAddr].addr != address(0)) {
                investors[refAddr].refs[i] = investors[refAddr].refs[i].add(1);
                _sendReferralReward(payable(refAddr), (i + 1), _amount);
            } else break;
            refAddr = investors[refAddr].ref;
        }  
        investors[_addr].startedDate = block.timestamp;
    }

    function _sendRewardOnInvestment(uint256 _amount) private {
        require(_amount > 0, "Amount must be greater than 0");

        uint256 rewardForPrimaryBenificiary = _amount
            .mul(PRIMARY_BENIFICIARY_INVESTMENT_PERC)
