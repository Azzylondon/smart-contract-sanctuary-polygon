                ? -getAmount1DeltaWithRoundUp(sqrtRatioAX96, sqrtRatioBX96, uint128(-liquidity), false).toInt256()
                : getAmount1DeltaWithRoundUp(sqrtRatioAX96, sqrtRatioBX96, uint128(liquidity), true).toInt256();
    }
