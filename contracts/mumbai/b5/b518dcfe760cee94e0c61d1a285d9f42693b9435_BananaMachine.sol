pragma solidity ^0.8.17;

import { ERC20Burnable } from "openzeppelin-contracts/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import { VRFConsumer } from "./VRFConsumer.sol";

contract BananaMachine is VRFConsumer {
    uint8 private constant _MEGA_PRIZE_REEL_POSITION = 1;

    ERC20Burnable private immutable _BANANA;
    
    uint256 public costPerSpin;
    uint8 public itemsPerReel;
    uint16 public miniPrizeBasisPoints;
    uint16 public megaPrizeBasisPoints;
    uint16 public burnBasisPoints;

    mapping(uint256 => address) public spins;

    event SpinInProgress(
        address indexed from,
        uint256 indexed requestId,
        uint256 cost,
        uint256 miniPrize,
        uint256 megaPrize,
        uint256 amountBurned
    );

    event SpinComplete(
        address indexed from,
        uint256 indexed requestId,
        bytes4 indexed positions,
        uint256 winnings
    );

    constructor(
        address banana,
        uint256 _costPerSpin,
        uint8 _itemsPerReel,
        uint16 _miniPrizeBasisPoints,
        uint16 _megaPrizeBasisPoints,
        uint16 _burnBasisPoints,
        address coordinator,
        address link,
        bytes32 keyHash
    ) VRFConsumer(coordinator, link, keyHash) {
        _BANANA = ERC20Burnable(banana);
        costPerSpin = _costPerSpin;
        itemsPerReel = _itemsPerReel;
        miniPrizeBasisPoints = _miniPrizeBasisPoints;
        megaPrizeBasisPoints = _megaPrizeBasisPoints;
        burnBasisPoints = _burnBasisPoints; 
    }

    function spin() external {
        uint256 _costPerSpin = costPerSpin;
        uint256 burnAmount = calculatePercentage(_costPerSpin, burnBasisPoints);
        uint256 requestId = _requestRandomWords();
        spins[requestId] = msg.sender;
        _BANANA.transferFrom(msg.sender, address(this), _costPerSpin);
        _BANANA.burn(burnAmount);
        (uint256 miniPrize, uint256 megaPrize) = prizes();
        emit SpinInProgress(msg.sender, requestId, _costPerSpin, miniPrize, megaPrize, burnAmount);
    }

    function prizes() public view returns (uint256, uint256) {
        uint256 balance = _BANANA.balanceOf(address(this));
        uint256 miniPrize = calculatePercentage(balance, miniPrizeBasisPoints);
        uint256 megaPrize = calculatePercentage(balance, megaPrizeBasisPoints);
        return (miniPrize, megaPrize);
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        uint8 _itemsPerReel = itemsPerReel;
        uint8 position1 = uint8(randomWords[0] % _itemsPerReel);
        uint8 position2 = uint8(randomWords[1] % _itemsPerReel);
        uint8 position3 = uint8(randomWords[2] % _itemsPerReel);
        uint8 position4 = uint8(randomWords[3] % _itemsPerReel);
        bytes4 positions = bytes4(abi.encodePacked(position1, position2, position3, position4));
        uint256 winnings;

        if (position1 == position2 && position2 == position3 && position3 == position4) {
            (uint256 miniPrize, uint256 megaPrize) = prizes();
            winnings = position1 == _MEGA_PRIZE_REEL_POSITION ? megaPrize : miniPrize;
            _BANANA.transfer(spins[requestId], winnings);
        }

        emit SpinComplete(spins[requestId], requestId, positions, winnings);
        delete spins[requestId];
    }

    function setCostPerSpin(uint256 _costPerSpin) external onlyOwner {
        costPerSpin = _costPerSpin;
    }

    function setItemsPerReel(uint8 _itemsPerReel) external onlyOwner {
        itemsPerReel = _itemsPerReel;
    }

    function setMiniPrizeBasisPoints(uint16 _miniPrizeBasisPoints) external onlyOwner {
        miniPrizeBasisPoints = _miniPrizeBasisPoints;
    }

    function setMegaPrizeBasisPoints(uint16 _megaPrizeBasisPoints) external onlyOwner {
        megaPrizeBasisPoints = _megaPrizeBasisPoints;
    }

    function setBurnBasisPoints(uint16 _burnBasisPoints) external onlyOwner {
        burnBasisPoints = _burnBasisPoints;
    }

    function withdrawBanana() external onlyOwner {
        uint256 balance = _BANANA.balanceOf(address(this));
        _BANANA.transfer(owner(), balance);
    }
}

function calculatePercentage(uint256 value, uint256 basisPoints) pure returns (uint256) {
    return value * basisPoints / 10000;
}

