pragma solidity ^0.8.17;

import { VRFCoordinatorV2Interface } from "chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import { LinkTokenInterface } from "chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import { VRFConsumerBaseV2 } from "chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import { Ownable } from "openzeppelin-contracts/contracts/access/Ownable.sol";
import { console } from "forge-std/console.sol";

// errors

abstract contract VRFConsumer is VRFConsumerBaseV2, Ownable {
    uint32 private constant _NUMBER_OF_WORDS = 4;

    VRFCoordinatorV2Interface private immutable _COORDINATOR;
    LinkTokenInterface private immutable _LINK;

    bytes32 public keyHash;
    uint64 public subscriptionId;
    uint16 public minimumConfirmations;
    uint32 public callbackGasLimit;

    constructor(address coordinator, address link, bytes32 _keyHash) VRFConsumerBaseV2(coordinator) {
        _COORDINATOR = VRFCoordinatorV2Interface(coordinator);
        _LINK = LinkTokenInterface(link);
        keyHash = _keyHash;
        minimumConfirmations = 3;
        callbackGasLimit = 100000;
        createSubscription();
    }

    function _requestRandomWords() internal returns (uint256) {
        return _COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            minimumConfirmations,
            callbackGasLimit,
            _NUMBER_OF_WORDS
        );
    }

    function createSubscription() public onlyOwner {
        require(subscriptionId == 0, "Cancel subscription first");
        uint64 _subscriptionId = _COORDINATOR.createSubscription();
        subscriptionId = _subscriptionId;
        _COORDINATOR.addConsumer(_subscriptionId, address(this));
    }

    function cancelSubscription() external onlyOwner {
        _COORDINATOR.cancelSubscription(subscriptionId, address(this));
        subscriptionId = 0;
    }
    
    function fundSubscription() external onlyOwner {
        uint256 balance = _LINK.balanceOf(address(this));
        _LINK.transferAndCall(
            address(_COORDINATOR),
            balance,
            abi.encode(subscriptionId)
        );
    }

    function setKeyHash(bytes32 _keyHash) external onlyOwner {
        keyHash = _keyHash;
    }

    function setMinimumConfirmations(uint16 _minimumConfirmations) external onlyOwner {
        minimumConfirmations = _minimumConfirmations;
    }

    function setCallbackGasLimit(uint32 _callbackGasLimit) external onlyOwner {
        callbackGasLimit = _callbackGasLimit;
    }

    function withdrawLink() external onlyOwner {
        uint256 balance = _LINK.balanceOf(address(this));
        _LINK.transfer(owner(), balance);
    }
}