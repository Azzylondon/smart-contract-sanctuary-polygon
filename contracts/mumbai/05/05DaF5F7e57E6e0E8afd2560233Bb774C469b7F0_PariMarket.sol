import { Price } from "./structs/Price.sol";

import { MetapoolContract } from "./MetapoolContract.sol";

import "./Errors.sol";

contract PariPoolContract is Context {

  using SafeMath for uint;
  using EnumerableSet for EnumerableSet.Bytes32Set;

  mapping(bytes32 => Pari) private _paris;
  mapping(bytes32 => Pool) private _pools;

  mapping(bytes32 => EnumerableSet.Bytes32Set) private _unreleasedParis;
  mapping(bytes32 => EnumerableSet.Bytes32Set) private _unresolvedPools;
  mapping(address => mapping(address => EnumerableSet.Bytes32Set)) private _unclaimedParis;

  mapping(bytes32 => uint[2**8]) private _prizefunds;

  uint8 constant public VIGORISH_PERCENT = 1;
  uint8 constant private TOTAL = 0;
  uint8 constant private RELEASED = 255;
  uint256 constant private PRICE_FEED_PHASE_OFFSET = 64;

  // solhint-disable-next-line
  address immutable public METAPOOL_CONTRACT;

  MetapoolContract immutable internal metapoolContract;

  constructor(
    address metapoolContract_
  )
  {
    METAPOOL_CONTRACT = metapoolContract_;

    metapoolContract = MetapoolContract(metapoolContract_);
  }

  function getPari(
    bytes32 pariid
  )
    external
    view
    returns (
      Pari memory
    )
  {
    return _paris[pariid];
  }

  function _placePari(
    Metapool memory metapool,
    address bettor,
    uint8 position,
    uint amount
  )
    internal
  {
    _updatePari(metapool, bettor, position, amount);
    _updatePool(metapool, bettor, position, amount);
  }

  function _updatePari(
    Metapool memory metapool,
    address bettor,
    uint8 position,
    uint amount
  )
    private
  {

    uint sinceopen = SafeMath.mod(block.timestamp, metapool.schedule);
    if (sinceopen >= metapool.positioning) {
      console.log("revert PlacePariAfterPositioning");
      revert PlacePariAfterPositioning();
    }

    uint openDate = SafeMath.sub(block.timestamp, sinceopen);

    bytes32 poolid = keccak256(abi.encode(metapool.metapoolid, openDate));
    bytes32 pariid = keccak256(abi.encode(poolid, bettor, position));

    Pari storage pari = _paris[pariid];
    if (pari.pariid == 0x0) {
      console.log("Create PariId");
      console.logBytes32(pariid);

      pari.pariid = pariid;
      pari.poolid = poolid;
      pari.bettor = bettor;
      pari.position = position;
      pari.createdAt = block.timestamp;

      _unclaimedParis[bettor][metapool.erc20].add(pariid);

      emit PariCreated(
        pariid,
        poolid,
        bettor,
        position,
        block.timestamp,
        metapool.erc20,
        metapool.metapoolid
      );
    }

    pari.wager = SafeMath.add(pari.wager, amount);

    emit IncreasePariWager(pariid, amount);
  }

  function _updatePool(
    Metapool memory metapool,
    address bettor,
    uint8 position,
    uint amount
  )
    private
  {

    uint sinceopen = SafeMath.mod(block.timestamp, metapool.schedule);
    if (sinceopen >= metapool.positioning) {
      console.log("revert UpdatePoolAfterPositioning");
      revert UpdatePoolAfterPositioning();
    }

    uint openDate = SafeMath.sub(block.timestamp, sinceopen);

    bytes32 poolid = keccak256(abi.encode(metapool.metapoolid, openDate));
    bytes32 pariid = keccak256(abi.encode(poolid, bettor, position));

    Pool storage pool = _pools[poolid];
    if (pool.poolid == 0x0) {
      _openPool(metapool);
    }

    _updatePrizefund(poolid, pariid, metapool.erc20, bettor, position, amount);

    if (!_unreleasedParis[poolid].contains(pariid)) {
      _unreleasedParis[poolid].add(pariid);
    }

  }

  function _openPool(
    Metapool memory metapool
  )
    private
  {

    uint sinceopen = SafeMath.mod(block.timestamp, metapool.schedule);
    uint openDate = SafeMath.sub(block.timestamp, sinceopen);
    uint positioning = metapool.positioning;
    bytes32 metapoolid = metapool.metapoolid;

    if (sinceopen >= positioning) {
      console.log("revert OpenPoolAfterPositioning");
      revert OpenPoolAfterPositioning();
    }

    bytes32 poolid = keccak256(abi.encode(metapoolid, openDate));

    Pool storage pool = _pools[poolid];
    if (pool.poolid == 0x0) {

      console.log("Create Pool");
      console.logBytes32(poolid);

      Price memory openPrice = _getPriceLatest(metapool.pricefeed);

      if (openPrice.timestamp < openDate) {
        console.log("revert PoolOpenPriceTimestampTooEarly");
        revert PoolOpenPriceTimestampTooEarly({
          openDate: openDate,
          openPrice: openPrice.timestamp
        });
      }
      if (openPrice.timestamp >= SafeMath.add(openDate, positioning)) {
        console.log("revert PoolOpenPriceTimestampTooLate");
        revert PoolOpenPriceTimestampTooLate({
          openDate: openDate,
          openPrice: openPrice.timestamp
        });
      }

      uint resolutionDate = SafeMath.add(openDate, metapool.schedule);
      address erc20 = metapool.erc20;

      pool.poolid = poolid;
      pool.metapoolid = metapoolid;
      pool.openPrice = openPrice;
      pool.openDate = openDate;
      pool.resolutionDate = resolutionDate;
      pool.createdAt = block.timestamp;
      pool.erc20 = erc20;

      _unresolvedPools[metapoolid].add(poolid);

      emit PoolCreated(
        poolid,
        metapoolid,
        _msgSender(),
        erc20,
        openPrice,
        openDate,
        resolutionDate,
        block.timestamp
      );
    }

  }

  function getPool(
    bytes32 poolid
  )
    external
    view
    returns (
      Pool memory
    )
  {

    return _pools[poolid];

  }


  function settleNoContest(
    bytes32 poolid
  )
    external onlyOffChainCallable
  {

    console.log("block.timestamp:", block.timestamp);
    console.log("!!!settle No Contest!!!");

    console.log("Poolid:");
    console.logBytes32(poolid);

    Pool storage pool = _pools[poolid];
    if (pool.resolved) {
      console.log("revert CannotSettleResolvedPool");
      revert CannotSettleResolvedPool();
    }

    Metapool memory metapool = metapoolContract.getMetapool(pool.metapoolid);
    uint lockDate = SafeMath.add(pool.openDate, metapool.positioning);
    if (block.timestamp < lockDate) {
      console.log("revert CannotSettlePoolDuringPositioning");
      revert CannotSettlePoolDuringPositioning({
        now: block.timestamp,
        lockDate: lockDate
      });
    }

    if (!_isNoContestEmptyPools(poolid)) {
      console.log("revert CannotResolveAsNoContestPool");
      revert CannotResolveAsNoContestPool();
    }

    pool.resolved = true;
    pool.resolvedAt = block.timestamp;
    pool.resolution = uint8(EPosition.NoContest);

    emit PoolResolvedNoContest(
      poolid,
      _msgSender(),
      block.timestamp,
      uint8(EPosition.NoContest)
    );

  }

  function settle(
    bytes32 poolid,
    uint80 resolutionPriceid,
    uint80 controlPriceid
  )
    external onlyOffChainCallable
  {

    console.log("block.timestamp:", block.timestamp);
    console.log("!!!settle!!!");

    console.log("Poolid:");
    console.logBytes32(poolid);

    Pool storage pool = _pools[poolid];
    if (pool.resolved) {
      console.log("revert CannotSettleResolvedPool");
      revert CannotSettleResolvedPool();
    }

    uint resolutionDate = pool.resolutionDate;
    if (resolutionDate >= block.timestamp) {
      console.log("revert CannotSettlePoolBeforeResolution");
      revert CannotSettlePoolBeforeResolution({
        now: block.timestamp,
        resolutionDate: resolutionDate
      });
    }

    Metapool memory metapool = metapoolContract.getMetapool(pool.metapoolid);
    Price memory resolutionPrice = _getPrice(metapool.pricefeed, resolutionPriceid);
    Price memory controlPrice = _getPrice(metapool.pricefeed, controlPriceid);

    if (!_isValidResolution(
      metapool,
      pool,
      resolutionPrice,
      controlPrice
    )) {
      console.log("revert InvalidPoolResolution");
      revert InvalidPoolResolution({
        resolutionPrice: resolutionPrice.roundid,
        controlPrice: controlPrice.roundid,
        resolutionDate: resolutionDate
      });
    }

    uint8 resolution = _calculateOutcome(poolid, pool.openPrice, resolutionPrice);

    pool.resolutionPrice = resolutionPrice;
    pool.resolved = true;
    pool.resolvedAt = block.timestamp;
    pool.resolution = resolution;

    _unresolvedPools[pool.metapoolid].remove(poolid);

    emit PoolResolved(
      poolid,
      resolutionPrice,
      _msgSender(),
      block.timestamp,
      resolution
    );

  }

  function _calculateOutcome(
    bytes32 poolid,
    Price memory openPrice,
    Price memory resolutionPrice
  )
    private
    returns (uint8)
  {

    uint8 outcome = uint8(EPosition.Undefined);

    if (resolutionPrice.value > openPrice.value) {

      outcome = uint8(EPosition.Up);

    } else if (resolutionPrice.value < openPrice.value) {

      outcome = uint8(EPosition.Down);

    } else if (resolutionPrice.value == openPrice.value) {

      outcome = uint8(EPosition.Equal);

    }

    if (outcome == uint8(EPosition.Undefined)) {

      outcome = uint8(EPosition.NoContest);

    } else if (_isNoContestPool(poolid, outcome)) {

      outcome = uint8(EPosition.NoContest);

    }

    return outcome;

  }

  function _isNoContestPool(bytes32 poolid, uint8 winnning)
    private
    returns (bool)
  {
    uint prizefundWin = _prizefund(poolid, winnning);
    uint prizefundTotal = _prizefundTotal(poolid);

    return prizefundWin == 0 || prizefundWin == prizefundTotal;
  }

  function _isNoContestEmptyPools(bytes32 poolid)
    private
    returns (bool)
  {
    uint prizefundUp = _prizefund(poolid, uint8(EPosition.Up));
    uint prizefundDown = _prizefund(poolid, uint8(EPosition.Down));
    uint prizefundEqual = _prizefund(poolid, uint8(EPosition.Equal));
    uint prizefundTotal = _prizefundTotal(poolid);

    return (
      prizefundUp == prizefundTotal ||
      prizefundDown == prizefundTotal ||
      prizefundEqual == prizefundTotal
    );
  }

  function _roundingErrorFunds(
    bytes32 poolid
  )
    internal
    view
    returns (uint)
  {

    uint unreleased = 0;
    uint prizefundReleased = _prizefundReleased(poolid);
    uint prizefundTotal = _prizefundTotal(poolid);

    if (
      _unreleasedParis[poolid].length() == 0 &&
      prizefundReleased != prizefundTotal
    ) {

      // unreleased funds that locked after last bettor claimed payout
      unreleased = SafeMath.sub(
        prizefundTotal,
        prizefundReleased
      );

    }

    return unreleased;

  }

  function _releasePrizefund(
    Pari storage pari
  )
    private
    returns (uint)
  {

    bytes32 poolid = pari.poolid;
    bytes32 pariid = pari.pariid;

    _unreleasedParis[poolid].remove(pariid);

    uint pariprize = SafeMath.add(pari.payout, pari.commission);
    if (pariprize != 0) {
      _prizefunds[poolid][RELEASED] = SafeMath.add(_prizefunds[poolid][RELEASED], pariprize);
    }

    uint roundingError = _roundingErrorFunds(poolid);
    if (roundingError != 0) {
      _prizefunds[poolid][RELEASED] = SafeMath.add(_prizefunds[poolid][RELEASED], roundingError);
    }

    if (_prizefunds[poolid][RELEASED] > _prizefunds[poolid][TOTAL]) {
      console.log("revert InsufficientPricefund");
      revert InsufficientPricefund({
        released: _prizefunds[poolid][RELEASED],
        total: _prizefunds[poolid][TOTAL]
      });
    }

    emit PoolPrizefundReleased(
      poolid,
      pariid,
      pari.payout,
      pari.commission,
      roundingError
    );

    return roundingError;
  }

  function _prizefundTotal(bytes32 poolid)
    private
    view
    returns (uint)
  {
    return _prizefunds[poolid][TOTAL];
  }

  function _prizefundReleased(bytes32 poolid)
    private
    view
    returns (uint)
  {
    return _prizefunds[poolid][RELEASED];
  }

  function _prizefund(bytes32 poolid, uint8 position)
    private
    view
    returns (uint)
  {
    if (position == TOTAL) return 0;
    if (position == RELEASED) return 0;

    return _prizefunds[poolid][position];
  }

  function _updatePrizefund(
    bytes32 poolid,
    bytes32 pariid,
    address erc20,
    address bettor,
    uint8 position,
    uint amount
  )
    private
  {

    _prizefunds[poolid][position] = SafeMath.add(_prizefunds[poolid][position], amount);
    _prizefunds[poolid][TOTAL] = SafeMath.add(_prizefunds[poolid][TOTAL], amount);

    emit PoolPrizefundAdd(
      poolid,
      pariid,
      erc20,
      bettor,
      position,
      amount
    );

  }

  function getPariPrize(bytes32 pariid)
    public
    view
    returns (
      uint,
      uint
    )
  {

    Pari storage pari = _paris[pariid];
    Pool storage pool = _pools[pari.poolid];

    uint payout = 0;
    uint commission = 0;

    if (pari.claimed || !pool.resolved) return ( payout, commission );

    bool win = pool.resolution == pari.position;
    bool nocontest = pool.resolution == uint8(EPosition.NoContest);

    if (win || nocontest) {

      uint vigorish = VIGORISH_PERCENT;
      if (nocontest) vigorish = 0;

      uint positionPrizefund = _prizefund(pari.poolid, pari.position);
      uint totalPrizefund = _prizefundTotal(pari.poolid);

      (uint paripayout, uint paricommission) = _calculatePrize(
        pari.wager,
        positionPrizefund,
        totalPrizefund,
        vigorish
      );

      payout = SafeMath.add(payout, paripayout);
      commission = SafeMath.add(commission, paricommission);
    }

    return ( payout, commission );

  }

  function _claimePari(
    bytes32 pariid,
    address erc20
  )
    internal
    returns (
      uint,
      uint,
      uint
  ) {

    console.log("Claime PariId:");
    console.logBytes32(pariid);

    Pari storage pari = _paris[pariid];
    Pool storage pool = _pools[pari.poolid];

    if (pool.erc20 != erc20) {
      console.log("revert ERC20PariPoolMismatch");
      revert ERC20PariPoolMismatch({
        poolERC20: pool.erc20,
        pariERC20: erc20
      });
    }

    address bettor = _msgSender();
    if (pari.bettor != bettor) {
      console.log("revert BettorPariMismatch");
      revert BettorPariMismatch({
        pariOwner: pari.bettor,
        bettor: bettor
      });
    }

    if (pari.claimed) {
      console.log("revert CannotClaimeClaimedPari");
      revert CannotClaimeClaimedPari();
    }

    if (!pool.resolved) {
      console.log("revert CannotClaimePariUnresolvedPool");
      revert CannotClaimePariUnresolvedPool();
    }

    (uint payout, uint commission) = getPariPrize(pariid);
    if (payout != 0) pari.payout = payout;
    if (commission != 0) pari.commission = commission;
    pari.claimed = true;

    _unclaimedParis[bettor][erc20].remove(pariid);

    emit PariClaimed(pariid, bettor, erc20, payout, commission);

    uint roundingerror = _releasePrizefund(pari);

    return ( payout, commission, roundingerror );

  }

  function _calculatePrize(
    uint wager,
    uint positionfunds,
    uint totalfunds,
    uint vigorish
  )
    private
    pure
    returns (
      uint,
      uint
    )
  {

    uint commission = 0;
    uint payout = SafeMath.div(
      SafeMath.mul(
        totalfunds,
        wager
      ),
      positionfunds
    );

    if (vigorish > 0 && vigorish < 100) {

      uint com = SafeMath.mul(payout, vigorish);
      commission = SafeMath.div(com, 100);
      uint rest = SafeMath.mod(com, 100);
      if (rest != 0) commission = SafeMath.add(commission, 1); // ceil

      payout = SafeMath.sub(payout, commission);

    }

    return ( payout, commission );

  }

  function _getPrice(
    address pricefeed,
    uint80 _roundid
  )
    private
    view
    returns (
      Price memory
    )
  {

    AggregatorV3Interface _pricefeed = AggregatorV3Interface(pricefeed);

    (
      uint80 roundid,
      int256 answer,
      /*uint256 startedAt*/,
      uint256 updatedAt,
      /*uint80 answeredInRound*/
    ) = _pricefeed.getRoundData(_roundid);

    return Price({
      roundid: roundid,
      value: answer,
      timestamp: updatedAt
    });

  }

  function _getPriceLatest(
    address pricefeed
  )
    private
    view
    returns (
      Price memory
    )
  {

    AggregatorV3Interface _pricefeed = AggregatorV3Interface(pricefeed);

    (
      uint80 roundid,
      int256 answer,
      /* uint256 startedAt */,
      uint256 updatedAt,
      /* uint80 answeredInRound */
    ) = _pricefeed.latestRoundData();

    return Price({
      roundid: roundid,
      value: answer,
      timestamp: updatedAt
    });

  }

  function _isValidResolution(
    Metapool memory metapool,
    Pool storage pool,
    Price memory resolutionPrice,
    Price memory controlPrice
  )
    private
    view
    returns (bool)
  {

    // not confirmed round
    if (resolutionPrice.timestamp == 0) return false;
    if (controlPrice.timestamp == 0) return false;

    if (resolutionPrice.timestamp >= pool.resolutionDate) return false;
    if (resolutionPrice.timestamp <= pool.openPrice.timestamp) return false;
    if (controlPrice.timestamp < pool.resolutionDate) return false;

    uint lockDate = SafeMath.add(pool.openDate, metapool.positioning);
    if (resolutionPrice.timestamp < lockDate) return false;

    (uint16 rpPhaseId, uint64 rpAggrRoundId) = parseRoundid(resolutionPrice.roundid);
		(uint16 cpPhaseId, uint64 cpAggrRoundId) = parseRoundid(controlPrice.roundid);

	  if (
      // same phase             // continuous rounds
      rpPhaseId == cpPhaseId && (rpAggrRoundId + 1) == cpAggrRoundId
    ) return true;

	  if (
      // continuous phases            // control roundid is first in control phase
      (rpPhaseId + 1) == cpPhaseId && cpAggrRoundId == 1
    ) return true;

    return false;
  }

  function parseRoundid(
    uint256 roundId
  )
		internal
		view
		returns (uint16, uint64)
  {
    uint16 phaseId = uint16(roundId >> PRICE_FEED_PHASE_OFFSET);
    uint64 aggregatorRoundId = uint64(roundId);

    return (phaseId, aggregatorRoundId);
  }

  function unresolvedPools(
    bytes32 metapoolid
  )
    public
    view
    returns (
      bytes32[] memory
    )
  {
    return _unresolvedPools[metapoolid].values();
  }

  function unclaimedParis(
    address bettor,
    address erc20
  )
    public
    view
    returns (bytes32[] memory)
  {

    return _unclaimedParis[bettor][erc20].values();

  }

  modifier onlyOffChainCallable() {
    if (_msgSender() != tx.origin) {
      console.log("revert OnlyOffChainCallesAllowed", tx.origin, _msgSender());
      revert OnlyOffChainCallesAllowed();
    }
    _;
  }

