  );
  event PoolCreated(
    bytes32 indexed poolid,
    bytes32 metapoolid,
    address openedBy,
    address erc20,
    Price openPrice,
    uint openDate,
    uint resolutionDate,
    uint createdAt
  );
  event PoolPrizefundAdd(
    bytes32 poolid,
