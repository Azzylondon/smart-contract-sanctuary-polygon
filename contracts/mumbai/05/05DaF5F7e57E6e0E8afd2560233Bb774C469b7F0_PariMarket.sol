    address bettor,
    uint8 position,
    uint amount
  );
  event PoolPrizefundReleased(
    bytes32 poolid,
    bytes32 pariid,
    uint payout,
    uint commission,
    uint roundingError
  );
  event PariCreated(
    bytes32 indexed pariid,
    bytes32 poolid,
    address bettor,
    uint8 position,
    uint createdAt,
    address erc20,
    bytes32 metapoolid
  );
