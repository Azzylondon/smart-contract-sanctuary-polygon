pragma solidity ^0.8.2;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./EliteWorldNFT.sol";

contract BaseEliteWorld is EliteWorldNFT {
    using SafeMath for uint256;

    struct ItemTemp {
        uint256 id;
        uint256 localID;
    }

    struct Item {
        string tokenURI;
        uint256 idType;
        uint256 id;
        uint256 price;
        uint256 point;
        uint256 maxLevel;
        bool isSold;
    }

    Item[] public items;
    mapping(uint256 => uint256) public itemRegistry;
    mapping(uint256 => bool) public freeItems;
    mapping(uint256 => uint256) public itemTypeRegistry;
    mapping(uint256 => uint256) public freeItemTypeRegistry;
    mapping(uint256 => mapping(address => bool)) public itemOf;
    mapping(uint256 => mapping(address => bool)) public itemBlock;
    mapping(address => bool) public isBuyFree;
    mapping(uint256 => uint256) public levels;
    mapping(uint256 => uint256) internal _itemOfOwnerByToken;

    mapping(address => bool) internal approvers;

    event Withdraw(address indexed account, uint256 amount);
    event WithdrawPoint(address indexed account, uint256 amount);
    event PresentNft(address indexed account, uint256 id);

    // ============ Modifiers ============

    modifier itemNotExist(uint256 _id) {
        require(itemRegistry[_id] == 0, "Item Already Exist");
        _;
    }

    // ============ Helper ============

    function itemExist(uint256 _id) public view {
        require(itemRegistry[_id] > 0, "Item Not Exist");
    }

    function getItemIdLocal(uint256 _index) public view returns (uint256) {
        return items[_index].id;
    }

    function getItemId(uint256 _id) public view returns (uint256) {
        itemExist(_id);
        return itemRegistry[_id] - 1;
    }

    function getItemsLength() external view returns (uint256) {
        return items.length;
    }

    function getFreeItemsByType(uint256 _idType)
        external
        view
        returns (uint256[] memory)
    {
        uint256[] memory result = new uint256[](freeItemTypeRegistry[_idType]);
        uint256 counter = 0;
        for (uint256 i = 0; i < items.length; i++) {
            if (items[i].idType == _idType && freeItems[items[i].id]) {
                result[counter] = i;
                counter++;
            }
        }
        return result;
    }

    function getItemsByType(uint256 _idType)
        external
        view
        returns (ItemTemp[] memory)
    {
        ItemTemp[] memory result = new ItemTemp[](itemTypeRegistry[_idType]);
        uint256 counter = 0;
        for (uint256 i = 0; i < items.length; i++) {
            if (items[i].idType == _idType && items[i].isSold) {
                result[counter] = ItemTemp({id: i, localID: items[i].id});
                counter++;
            }
        }
        return result;
    }

    function balanceOfType(uint256 _idType, address owner)
        external
        view
        returns (uint256[] memory)
    {
        uint256 count;
        uint256 balance = balanceOf(owner);

        for (uint256 i = 0; i < balance; i++) {
            if (
                items[_itemOfOwnerByToken[tokenOfOwnerByIndex(owner, i)]]
                    .idType == _idType
            ) {
                count++;
            }
        }

        uint256[] memory result = new uint256[](count);

        uint256 counter;
        for (uint256 i = 0; i < balance; i++) {
            if (
                items[_itemOfOwnerByToken[tokenOfOwnerByIndex(owner, i)]]
                    .idType == _idType
            ) {
                result[counter] = i;
                counter++;
            }
        }

        return result;
    }

    function balanceOfTypeLocal(uint256 _idType, address owner)
        external
        view
        returns (ItemTemp[] memory)
    {
        uint256 count;
        uint256 balance = balanceOf(owner);

        for (uint256 i = 0; i < balance; i++) {
            if (
                items[_itemOfOwnerByToken[tokenOfOwnerByIndex(owner, i)]]
                    .idType == _idType
            ) {
                count++;
            }
        }

        ItemTemp[] memory result = new ItemTemp[](count);

        uint256 counter;
        for (uint256 i = 0; i < balance; i++) {
            if (
                items[_itemOfOwnerByToken[tokenOfOwnerByIndex(owner, i)]]
                    .idType == _idType
            ) {
                uint256 tokenId = tokenOfOwnerByIndex(owner, i);
                result[counter] = ItemTemp({
                    id: tokenId,
                    localID: items[_itemOfOwnerByToken[tokenId]].id
                });
                counter++;
            }
        }

        return result;
    }

    function itemOfOwnerByToken(uint256 tokenId)
        external
        view
        returns (uint256)
    {
        require(totalSupply() > tokenId, "token id out of bounds");
        return _itemOfOwnerByToken[tokenId];
    }

    // ============ Ownable ============

    function _beforeMintItem(uint256 tokenId, uint256 idItem) internal {
        _itemOfOwnerByToken[tokenId] = idItem;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);

        if (from == address(0)) {
            itemOf[items[_itemOfOwnerByToken[tokenId]].id][to] = true;
            levels[tokenId] = 1;
        } else if (to == address(0)) {
            delete itemOf[items[_itemOfOwnerByToken[tokenId]].id][from];
            delete levels[tokenId];
        } else if (from != to) {
            delete itemOf[items[_itemOfOwnerByToken[tokenId]].id][from];
            itemOf[items[_itemOfOwnerByToken[tokenId]].id][to] = true;
        }
    }

    function setItem(
        uint256 _id,
        string memory _tokenURI,
        uint256 _idType,
        uint256 _price,
        uint256 _point,
        uint256 _maxLevel,
        bool _isSold,
        bool _freeItem
    ) public itemNotExist(_id) onlyOwner {
        items.push(
            Item(_tokenURI, _idType, _id, _price, _point, _maxLevel, _isSold)
        );
        itemRegistry[_id] = items.length;
        if (_isSold) {
            itemTypeRegistry[_idType] = itemTypeRegistry[_idType].add(1);
        }
        if (_freeItem && _isSold) {
            freeItems[_id] = true;
            freeItemTypeRegistry[_idType] = freeItemTypeRegistry[_idType].add(
                1
            );
        } else {
            freeItems[_id] = false;
        }
    }

    function editItem(
        uint256 _id,
        uint256 _price,
        uint256 _point,
        uint256 _maxLevel,
        bool _isSold
    ) public onlyOwner {
        uint256 id = getItemId(_id);
        Item storage item = items[id];
        item.price = _price;
        item.point = _point;
        item.maxLevel = _maxLevel;
        if (!item.isSold && _isSold) {
            itemTypeRegistry[item.idType] = itemTypeRegistry[item.idType].add(
                1
            );
        } else if (freeItems[_id] && !_isSold) {
            itemTypeRegistry[item.idType] = itemTypeRegistry[item.idType].sub(
                1
            );
        }
        item.isSold = _isSold;
    }

    function setFreeItem(uint256 _id, bool _freeItem) public onlyOwner {
        uint256 id = getItemId(_id);
        Item memory item = items[id];
        require(item.isSold, "Cannot change item is not sold");
        if (!freeItems[_id] && _freeItem) {
            freeItemTypeRegistry[item.idType] = freeItemTypeRegistry[
                item.idType
            ].add(1);
        } else if (freeItems[_id] && !_freeItem) {
            freeItemTypeRegistry[item.idType] = freeItemTypeRegistry[
                item.idType
            ].sub(1);
        }
        freeItems[_id] = _freeItem;
    }

    /*
     * @dev withdraw token from vault
     */
    function withdraw(
        address addressToken,
        address _to,
        uint256 _amount
    ) public onlyOwner {
        IEliteWorldVault(tokenVault).withdraw(addressToken, _to, _amount);
    }

    function setApprover(address _approver, bool _isApproved) public onlyOwner {
        approvers[_approver] = _isApproved;
    }

    function setItemOf(
        address _oldOwner,
        address _tokenOwner,
        uint256 _tokenId
    ) external {
        require(approvers[msg.sender], "Caller is not approver");
        if (_oldOwner != address(0)) {
            delete itemOf[_itemOfOwnerByToken[_tokenId]][_oldOwner];
        }
        if (_tokenOwner != address(0)) {
            itemOf[_itemOfOwnerByToken[_tokenId]][_tokenOwner] = true;
        }
    }

    function presentNft(uint256 _id, address to) external onlyOwner {
        uint256 id = getItemId(_id);
        require(itemOf[items[id].id][to] != true, "Item Purchased");
        require(items[id].isSold, "Item not sold");

        _beforeMintItem(currentTokenId(), id);
        _mintItem(to, items[id].tokenURI);
        itemOf[items[id].id][to] = true;

        emit PresentNft(to, items[id].id);
    }
}

