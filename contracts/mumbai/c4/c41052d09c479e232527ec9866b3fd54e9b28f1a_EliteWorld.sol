pragma solidity ^0.8.2;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./LevelELW.sol";

contract EliteWorld is LevelELW {
    using SafeMath for uint256;

    event BuyItem(
        address indexed account,
        uint256 tokenId,
        uint256 price,
        uint256 point
    );
    event BuyFreeItems(
        address indexed account,
        uint256 id0,
        uint256 id1,
        uint256 id2
    );
    event ExchangeItemPoint(address indexed account, uint256 id, uint256 point);

    event PrizeAuction(address indexed account, uint256 id, uint256 idLocal);

    // ============ Transaction ============

    // Allow buy 3 free item body, shirt, pants at the same time
    // Buy only once
    // Free items cannot be resold or auctioned
    function buyFreeItems(
        uint256 _id0,
        uint256 _id1,
        uint256 _id2
    ) external {
        require(!isBuyFree[msg.sender], "EW.E01: Free item purchased");
        require(freeItems[_id0], "EW.E02: Invalid item body");
        require(freeItems[_id1], "EW.E03: Invalid item shirt");
        require(freeItems[_id2], "EW.E04: Invalid item pants");

        uint256 id0 = getItemId(_id0);
        uint256 id1 = getItemId(_id1);
        uint256 id2 = getItemId(_id2);

        require(items[id0].idType == 2, "EW.E05: Required character item");
        require(itemOf[_id0][msg.sender] != true, "EW.E06");

        require(items[id1].idType == 3, "EW.E07");
        require(
            itemOf[_id1][msg.sender] != true,
            "EW.E07: Required shirt item"
        );

        require(items[id2].idType == 4, "EW.E09");
        require(itemOf[_id2][msg.sender] != true, "EW.E10: Pants purchased");

        uint256 currentTokenId = currentTokenId();

        _beforeMintItem(currentTokenId, id0);
        _mintItemByTokenId(msg.sender, items[id0].tokenURI, currentTokenId);
        itemBlock[_id0][msg.sender] = true;

        _beforeMintItem(currentTokenId + 1, id1);
        _mintItemByTokenId(msg.sender, items[id1].tokenURI, currentTokenId + 1);
        itemBlock[_id1][msg.sender] = true;

        _beforeMintItem(currentTokenId + 2, id2);
        _mintItemByTokenId(msg.sender, items[id2].tokenURI, currentTokenId + 2);
        itemBlock[_id2][msg.sender] = true;

        incrementTokenId(3);

        isBuyFree[msg.sender] = true;

        emit BuyFreeItems(
            msg.sender,
            currentTokenId,
            currentTokenId + 1,
            currentTokenId + 2
        );
    }

    // Only buy items that you don't own
    function buyItem(uint256 _id) external {
        uint256 id = getItemId(_id);
        require(itemOf[items[id].id][msg.sender] != true, "EW.E11: Purchased");
        require(items[id].isSold, "EW.E12: Item not sold");

        IEliteWorldVault(tokenVault).deposit(
            address(msg.sender),
            items[id].price
        );

        uint256 currentTokenId = currentTokenId();
        _beforeMintItem(currentTokenId, id);
        _mintItem(msg.sender, items[id].tokenURI);
        itemOf[items[id].id][msg.sender] = true;

        emit BuyItem(
            msg.sender,
            currentTokenId,
            items[id].price,
            items[id].point
        );
    }

    // ============ Exchage item point ============

    // mint token by point from farmer
    // function exchangeItemPoint(uint256 _id) external {
    //     uint256 id = getItemId(_id);
    //     require(items[id].point != 0, "Invalid item");

    //     IEliteWorldVault(tokenVault).exchangePoint(
    //         address(msg.sender),
    //         items[id].point
    //     );

    //     _beforeMintItem(currentTokenId(), id);
    //     _mintItem(msg.sender, items[id].tokenURI);
    //     ownedItemQuantity[msg.sender][items[id].id] = ownedItemQuantity[
    //         msg.sender
    //     ][items[id].id].add(1);

    //     emit ExchangeItemPoint(msg.sender, items[id].id, items[id].point);
    // }

    // ============ Prize auction ============
    function prizeAuction(uint256 _id, address owner) external {
        require(approvers[msg.sender], "Caller is not approver");

        _beforeMintItem(currentTokenId(), _id);
        _mintItem(owner, items[_id].tokenURI);
        itemOf[items[_id].id][owner] = true;

        emit PrizeAuction(owner, _id, items[_id].id);
    }
}

