    function calcRatioAfterBuy(uint256 investmentAmount, uint256 outcomeIndex)
        public
        view
        returns (uint256)
    {
        require(outcomeIndex < positionIds.length, "invalid outcome index");

        uint256[] memory poolBalances = getPoolBalances();

        uint256 buyTokenPoolBalance = poolBalances[outcomeIndex];
        uint256 endingOutcomeBalance = buyTokenPoolBalance.mul(ONE);
        uint256[] memory poolBalancesAfterBuy = new uint256[](
            poolBalances.length
        );
        uint256 calcRatioDenominator; // Ratio excluding
        for (uint256 i = 0; i < poolBalances.length; i++) {
            if (i != outcomeIndex) {
                uint256 poolBalance = poolBalances[i];
                console.log("pool balance : ", poolBalance);
                poolBalancesAfterBuy[i] = poolBalance.add(investmentAmount);
                console.log("poolBalancesAfterBuy : ", poolBalancesAfterBuy[i]);
                console.log("calcRatioDenominator ", calcRatioDenominator);
                calcRatioDenominator = calcRatioDenominator.add(
                    poolBalancesAfterBuy[i]
                );
                console.log("calcRatioDenominator ", calcRatioDenominator);

                endingOutcomeBalance = endingOutcomeBalance
                    .mul(poolBalance)
                    .ceildiv(poolBalancesAfterBuy[i]);
                console.log("endingOutcomeBalance ", endingOutcomeBalance);
            }
        }

        console.log(
            "final endingOutcomeBalance ",
            endingOutcomeBalance.ceildiv(ONE)
        );

        console.log(
            "calcRatioDenominator.add(endingOutcomeBalance) ",
            calcRatioDenominator.add(endingOutcomeBalance.ceildiv(ONE))
        );
        console.log(
            "(endingOutcomeBalance.mul(ONE)).ceildiv(calcRatioDenominator.add(endingOutcomeBalance)) ",
            (endingOutcomeBalance).ceildiv(
                calcRatioDenominator.mul(ONE).add(endingOutcomeBalance)
            )
        );

        // newARatio =  aNew/(aNew + bNew + cNew)
        return
            (endingOutcomeBalance).div(
                calcRatioDenominator.add(endingOutcomeBalance.ceildiv(ONE))
            );
    }

    function calcRatioAfterSell(uint256 investmentAmount, uint256 outcomeIndex)
        public
        view
        returns (uint256)
    {
        uint256[] memory poolBalances = getPoolBalances();
        uint256 sellTokenPoolBalance = poolBalances[outcomeIndex];
        uint256 endingOutcomeBalance = sellTokenPoolBalance.mul(ONE);
        uint256 ratioDenominator;
        for (uint256 i = 0; i < poolBalances.length; i++) {
            if (i != outcomeIndex) {
                uint256 poolBalance = poolBalances[i];
                ratioDenominator = ratioDenominator.add(
                    poolBalance.sub(investmentAmount)
                );
                endingOutcomeBalance = endingOutcomeBalance
                    .mul(poolBalance)
                    .ceildiv(poolBalance.sub(investmentAmount));
            }
        }
        require(endingOutcomeBalance > 0, "must have non-zero balances");

        return
            endingOutcomeBalance.ceildiv(
                (endingOutcomeBalance.ceildiv(ONE)).add(ratioDenominator)
            );
    }

    function calcBuyAmount(
        uint256 investmentAmount,
        uint256 outcomeIndex,
        uint256 fee
    ) public view returns (uint256) {
        require(outcomeIndex < positionIds.length, "invalid outcome index");

        uint256[] memory poolBalances = getPoolBalances();

        uint256 investmentAmountMinusFees = investmentAmount.sub(
            investmentAmount.mul(fee) / ONE
        );

        console.log("investmentAmount.mul(fee) ", investmentAmount.mul(fee));
        console.log("investmentAmountMinusFees ", investmentAmountMinusFees);
        uint256 buyTokenPoolBalance = poolBalances[outcomeIndex];
        uint256 endingOutcomeBalance = buyTokenPoolBalance.mul(ONE);
        for (uint256 i = 0; i < poolBalances.length; i++) {
            if (i != outcomeIndex) {
                uint256 poolBalance = poolBalances[i];
                endingOutcomeBalance = endingOutcomeBalance
                    .mul(poolBalance)
                    .ceildiv(poolBalance.add(investmentAmountMinusFees));
            }
        }
        require(endingOutcomeBalance > 0, "must have non-zero balances");

        return
            buyTokenPoolBalance.add(investmentAmountMinusFees).sub(
                endingOutcomeBalance.ceildiv(ONE)
            );
    }

    function calcSellAmountWithoutFees(
        uint256 returnAmount,
        uint256 outcomeIndex
    ) public view returns (uint256 outcomeTokensForSell) {
        require(outcomeIndex < positionIds.length, "invalid outcome index");

        uint256[] memory poolBalances = getPoolBalances();
        // uint256 returnAmountPlusFees = returnAmount.mul(ONE) / ONE.sub(fee);
        uint256 sellTokenPoolBalance = poolBalances[outcomeIndex];
        uint256 endingOutcomeBalance = sellTokenPoolBalance.mul(ONE);
        for (uint256 i = 0; i < poolBalances.length; i++) {
            if (i != outcomeIndex) {
                uint256 poolBalance = poolBalances[i];
                endingOutcomeBalance = endingOutcomeBalance
                    .mul(poolBalance)
                    .ceildiv(poolBalance.sub(returnAmount));
            }
        }
        require(endingOutcomeBalance > 0, "must have non-zero balances");

        return endingOutcomeBalance;
    }

    function calcSellAmount(
        uint256 returnAmount,
        uint256 outcomeIndex,
        uint256 fee
    ) public view returns (uint256 outcomeTokenSellAmount) {
        require(outcomeIndex < positionIds.length, "invalid outcome index");

        uint256[] memory poolBalances = getPoolBalances();
        uint256 returnAmountPlusFees = returnAmount.add(fee);
        uint256 sellTokenPoolBalance = poolBalances[outcomeIndex];
        uint256 endingOutcomeBalance = sellTokenPoolBalance.mul(ONE);
        for (uint256 i = 0; i < poolBalances.length; i++) {
            if (i != outcomeIndex) {
                uint256 poolBalance = poolBalances[i];
                endingOutcomeBalance = endingOutcomeBalance
                    .mul(poolBalance)
                    .ceildiv(poolBalance.sub(returnAmountPlusFees));
            }
        }
        require(endingOutcomeBalance > 0, "must have non-zero balances");

        return
            returnAmountPlusFees.add(endingOutcomeBalance.ceildiv(ONE)).sub(
                sellTokenPoolBalance
            );
    }

    function getProtocolFees(uint256 amount) public view returns (uint256) {
        return amount.mul(protocolFees).ceildiv(ONE);
    }

    function buy(
        uint256 investmentAmount,
        uint256 outcomeIndex,
        uint256 minOutcomeTokensToBuy
    ) external {
        console.log("fpmm owner ", fpmmOwner);
        console.log("getProtocolFees ", getProtocolFees(investmentAmount));
        uint256 protocolFees = getProtocolFees(investmentAmount);
        totalProtocolFeesCollected = totalProtocolFeesCollected.add(
            protocolFees
        );
        uint256 investmentAmountMinusProtocolFees = investmentAmount.sub(
            protocolFees
        );

        console.log("buy ");
        console.log(
            "user balance before buy ",
            conditionalTokens.balanceOf(msg.sender, positionIds[outcomeIndex])
        );
        uint256 currentRatio = calcRatioAfterBuy(
            investmentAmountMinusProtocolFees,
            outcomeIndex
        );
        console.log("yahan 1");
        (uint256 dynamicFees, uint256 applicableFeeFactor) = getEffectiveFees(
            investmentAmountMinusProtocolFees,
            outcomeIndex,
            currentRatio
        );

        uint256 outcomeTokensToBuy = calcBuyAmount(
            investmentAmountMinusProtocolFees,
            outcomeIndex,
            applicableFeeFactor
        );

        console.log("outcomeTokensToBuy ", outcomeTokensToBuy);
        feePoolWeight = feePoolWeight.add(dynamicFees);
        require(
            outcomeTokensToBuy >= minOutcomeTokensToBuy,
            "minimum buy amount not reached"
        );
        require(
            collateralToken.transferFrom(
                _msgSender(),
                address(this),
                investmentAmount
            ),
            "cost transfer failed"
        );

        uint256 investmentAmountMinusFees = investmentAmountMinusProtocolFees
            .sub(dynamicFees);
        require(
            collateralToken.approve(
                address(conditionalTokens),
                investmentAmountMinusFees
            ),
            "approval for splits failed"
        );
        splitPositionThroughAllConditions(investmentAmountMinusFees);

        console.log("outcomeTokensToBuy ", outcomeTokensToBuy);
        console.log("position id 0 ", positionIds[0]);
        console.log("position id 1 ", positionIds[1]);
        console.log("dynamiceFee ", dynamicFees);
        console.log(
            "before conditionalTokens.balanceOf(msg.sender, positionIds[outcomeIndex]) ",
            conditionalTokens.balanceOf(msg.sender, positionIds[outcomeIndex])
        );
        // conditionalTokens.bala
        conditionalTokens.safeTransferFrom(
            address(this),
            _msgSender(),
            positionIds[outcomeIndex],
            outcomeTokensToBuy,
            ""
        );

        uint256 outcomeUserBalance = conditionalTokens.balanceOf(
            _msgSender(),
            positionIds[outcomeIndex]
        );
        console.log("after outcomeUserBalance ", outcomeUserBalance);
        uint256[] memory poolBalances = getPoolBalances();
        console.log("pool 0 ", poolBalances[0]);
        console.log("pool 1 ", poolBalances[1]);
        emit FPMMBuy(
            _msgSender(),
            investmentAmount,
            dynamicFees,
            outcomeIndex,
            outcomeTokensToBuy
        );
    }

    function sell(
        uint256 returnAmount,
        uint256 outcomeIndex,
        uint256 maxOutcomeTokensToSell
    ) external {
        uint256 protocolFees = getProtocolFees(returnAmount);
        totalProtocolFeesCollected = totalProtocolFeesCollected.add(
            protocolFees
        );
        uint256 returnAmountPlusProtocolFees = returnAmount.add(protocolFees);
        uint256 currentRatio = calcRatioAfterSell(
            returnAmountPlusProtocolFees,
            outcomeIndex
        );

        (uint256 dynamicFees, uint256 applicableFeeFactor) = getEffectiveFees(
            returnAmountPlusProtocolFees,
            outcomeIndex,
            currentRatio
        );

        uint256 outcomeTokensToSell = calcSellAmount(
            returnAmountPlusProtocolFees,
            outcomeIndex,
            dynamicFees
        );
        console.log("outcomeTokensToSell ", outcomeTokensToSell);
        require(
            outcomeTokensToSell <= maxOutcomeTokensToSell,
            "maximum sell amount exceeded"
        );
        console.log(
            "balance of ",
            conditionalTokens.balanceOf(msg.sender, positionIds[outcomeIndex])
        );
        conditionalTokens.safeTransferFrom(
            _msgSender(),
            address(this),
            positionIds[outcomeIndex],
            outcomeTokensToSell,
            ""
        );

        // uint256 feeAmount = returnAmount.mul(dynamicFees) / (ONE.sub(dynamicFees));
        feePoolWeight = feePoolWeight.add(dynamicFees);
        uint256 returnAmountPlusFees = returnAmount.add(dynamicFees);
        mergePositionsThroughAllConditions(returnAmountPlusFees);

        // protocol fees
        require(
            collateralToken.transfer(_msgSender(), returnAmount),
            "return transfer failed"
        );

        uint256[] memory poolBalances = getPoolBalances();
        console.log("pool 0 ", poolBalances[0]);
        console.log("pool 1 ", poolBalances[1]);

        emit FPMMSell(
            _msgSender(),
            returnAmount,
            dynamicFees,
            outcomeIndex,
            outcomeTokensToSell
        );
    }
}

