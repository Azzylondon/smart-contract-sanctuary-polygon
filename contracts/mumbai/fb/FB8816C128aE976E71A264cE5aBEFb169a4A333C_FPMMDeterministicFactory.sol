        );
        uint256 applicableFeeFactor = 0;
        if (actualFeeFactor > 0) {
            applicableFeeFactor = actualFeeFactor.ceildiv(ONE);
        } else {
            applicableFeeFactor = feeFactor;
        }
        console.log("applicableFeeFactor ", applicableFeeFactor);
        uint256 dynamicFees = (investmentAmount.mul(applicableFeeFactor))
            .ceildiv(ONE);

        console.log("dynamicFees ", dynamicFees);
        console.log(
            "investment - dynamic fees ",
