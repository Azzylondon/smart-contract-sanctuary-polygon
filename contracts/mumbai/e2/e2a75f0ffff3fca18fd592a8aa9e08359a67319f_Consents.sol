pragma solidity ^0.8.0;

import {ArraysUint32, ArraysBytes32} from "./lib/ArrayUtils.sol";
import "./access/Ownable.sol";
import "hardhat/console.sol";


contract Points is Ownable {


    event PointsAdded(
        bytes32 id,
        uint256 amount
        );

    event PointsRemoved(
        bytes32 id,
        uint256 amount
    );

    event PointsAdjusted(
        bytes32 id,
        uint256 oldAmount,
        uint256 newAmount
    );  

    event PersonAdded(
        bytes32 id
    );

    struct PersonsPoints {
        bytes32 id;
        uint256 points;
    }

    using ArraysUint32 for uint32[];
    using ArraysBytes32 for bytes32[];

    mapping(bytes32 => PersonsPoints ) private personPoints;


    function getPersonPoints(bytes32 _id) public view returns (uint256){
        require(checkIfPersonExists(_id), "Points: Person doesn't exist");
        return personPoints[_id].points;
    }


    function addPerson(bytes32 _id) public onlyOwner {
        personPoints[_id] = PersonsPoints({
            id: _id,
            points: 0
        });

        emit PersonAdded(_id);

    }

    
    function addPersonPoints(bytes32 _id, uint256 _amount) external onlyOwner {
         require(_amount > 0, "Points: invalid amount");

         if (!checkIfPersonExists(_id)){
            addPerson(_id);
         }
        // add points

        personPoints[_id].points += _amount;

        emit PointsAdded(
            _id,
            _amount
        );
    }


    function removePersonPoints(bytes32 _id, uint256 _amount) external onlyOwner {
        require(_amount > 0, "Points: invalid amount");
        require(_amount < getPersonPoints(_id),"Points: removing more than total");
        require(checkIfPersonExists(_id), "Points: person doesn't exist");

        // remove points
        personPoints[_id].points -= _amount;

        emit PointsRemoved(
            _id,
            _amount
        );
    }

    function adjustPersonPoints(bytes32 _id, uint256 _amount) external onlyOwner {
        require(_amount != getPersonPoints(_id), "Points: amount matches current balance");
        uint256 _oldAmount = getPersonPoints(_id);

        //set new amount of points

        personPoints[_id].points = _amount;

        emit PointsAdjusted(
            _id,
            _oldAmount,
            _amount
        );

    }


    function checkIfPersonExists(bytes32 _id) public view returns (bool) {

        if(personPoints[_id].id != 0){
            return true;
        }
        else {
            return false;
        }

    
    }

}

