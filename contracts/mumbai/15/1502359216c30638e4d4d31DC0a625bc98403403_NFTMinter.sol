                IERC721Receiver(destination).onERC721Received(
                    address(this),
                    destination,
                    0,
                    ""
                )
            returns (bytes4 retval) {
                return retval == IERC721Receiver.onERC721Received.selector;
            } catch {
                return false;
            }
        } else {
            return true;
        }
    }
}

