     */
    function setUser(
        uint256 tokenId,
        address user,
        uint64 expires
    ) public override onlyOwner {
        _labels[tokenId] = Label(user, expires, Status.Active);
    }

    function mintLabel(address recipient, string memory metadataURI)
        public
        override
        onlyOwner
    {
        _labelId.increment();
        uint256 labelId = _labelId.current();

        setUser(labelId, recipient, uint64(block.timestamp + LABEL_EXPIRATION));

        _mint(labelsVault, labelId);
        _setTokenURI(labelId, metadataURI);
    }

    function burnLabel(uint256 labelId) external override onlyOwner {
        require(
            labelStatus(labelId) == Status.Invalid,
            "TBLabel: label is still valid"
        );

        delete _labels[labelId];
        _burn(labelId);
