// Inherits
contract NFTMinter is ERC721Enumerable, Ownable, ReentrancyGuard {
    using Strings for uint256;

    uint256 public cost = 0.001 ether;
    uint256 public feesPercent = 5;
    address public royaltieAdresseCreator = 0x4cfd844F847eD4FC2d7755116CE436299836E987;
    address public fundTresoryWithdraw = 0x2950FB062f9d882493935Ef71BDD98F38aC7B11c;

    bool public paused = false;
    mapping(address => uint256) public addressMintedBalance;

    event Sale(address from, address to, uint256 value);

    constructor() ERC721("Nft1AnTerraBioDao", "TBIO1AN") {
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://gateway.pinata.cloud/ipfs/QmVHEGwgnXb4d6bfBrxZERbgZYsHnZp54pHU8B3y97fCNn";
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        _requireMinted(tokenId);
        return "https://gateway.pinata.cloud/ipfs/QmVHEGwgnXb4d6bfBrxZERbgZYsHnZp54pHU8B3y97fCNn";
    }

    // public
    function mint() public payable nonReentrant {
        require(!paused, "the contract is paused");
        require(msg.value >= cost, "insufficient funds");

        uint256 supply = totalSupply();
        address from = msg.sender;

        addressMintedBalance[from]++;
        uint256 royality = (msg.value * feesPercent) / 100;
        payable(royaltieAdresseCreator).transfer(royality);

        _safeMint(from, supply + 1);
    }

    //only owner
    function pause(bool _state) public onlyOwner {
        paused = _state;
    }

    function withdraw() public payable onlyOwner {
        payable(fundTresoryWithdraw).transfer(address(this).balance);
    }
}

