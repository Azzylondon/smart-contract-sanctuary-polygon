pragma solidity 0.8.16;

import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/utils/Address.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";

contract TerraBioFaucets is Ownable {
    using Address for address payable;

    uint256 public sbyRequestAmount;
    uint256 public tbioRequestAmount;
    uint256 public waitingTime;

    IERC20 private immutable _token;

    struct LastDrip {
        uint64 sby;
        uint64 tbio;
    }

    mapping(address => LastDrip) private _lastDrip;

    /**
     * @dev if some ERC20 are sent directly to this contract, this event
     * will not be emitted.
     */
    event Deposit(
        address indexed account,
        bool indexed isERC20,
        uint256 amount
    );
    event Drip(address indexed recipient, bool indexed isERC20, uint256 amount);

    constructor(address tbioAddress) {
        sbyRequestAmount = 10**17; // 0.1 SBY
        tbioRequestAmount = 10**20; // 100 TBIO
        waitingTime = 60 * 60 * 6; // 6 hours
        _token = IERC20(tbioAddress);
    }

    receive() external payable {
        emit Deposit(msg.sender, false, msg.value);
    }

    function deposit(uint256 amount) external {
        _token.transferFrom(msg.sender, address(this), amount);
        emit Deposit(msg.sender, true, amount);
    }

    function dripSby() external {
        uint256 amount = sbyRequestAmount;
        require(amount > 0, "Faucets: faucet paused");
        require(address(this).balance >= amount, "Faucets: empty faucet");

        uint64 lastDrip = _lastDrip[msg.sender].sby;
        require(
            block.timestamp > lastDrip + waitingTime,
            "Faucets: to close from last drip"
        );

        _lastDrip[msg.sender].sby = uint64(block.timestamp);
        payable(msg.sender).sendValue(amount);
        emit Drip(msg.sender, false, amount);
    }

    function dripTbio() external {
        uint256 amount = tbioRequestAmount;
        require(amount > 0, "Faucets: faucet paused");
        require(
            _token.balanceOf(address(this)) >= amount,
            "Faucets: empty faucet"
        );

        uint64 lastDrip = _lastDrip[msg.sender].tbio;
        require(
            block.timestamp > lastDrip + waitingTime,
            "Faucets: to close from last drip"
        );

        _lastDrip[msg.sender].tbio = uint64(block.timestamp);
        _token.transfer(msg.sender, amount);
        emit Drip(msg.sender, true, amount);
    }

    function setSbyRequestAmount(uint256 amount) external onlyOwner {
        sbyRequestAmount = amount;
    }

    function setTbioRequestAmount(uint256 amount) external onlyOwner {
        tbioRequestAmount = amount;
    }

    function pauseFaucets() external onlyOwner {
        delete sbyRequestAmount;
        delete tbioRequestAmount;
    }

    function setWaitingTime(uint256 duration) external onlyOwner {
        waitingTime = duration;
    }

    function withdrawFunds(bool sbyBalance, bool tbioBalance)
        external
        onlyOwner
    {
        if (sbyBalance) {
            payable(msg.sender).sendValue(address(this).balance);
        }

        if (tbioBalance) {
            _token.transfer(msg.sender, _token.balanceOf(address(this)));
        }
    }
}

