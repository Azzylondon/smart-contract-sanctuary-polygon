pragma solidity 0.8.16;

contract Calculator {
    mapping(uint16 => uint16) private _powerDemul;

    constructor() {
        // powerDemul = Coefficient multiplicateu*10
        _powerDemul[1] = 1;
        _powerDemul[7] = 10;
        _powerDemul[15] = 20;
        _powerDemul[30] = 40;
        _powerDemul[120] = 250;
        _powerDemul[365] = 500;
    }

    function calcVotePower(uint256 tbioAmount, uint16 lockDelay)
        public
        view
        returns (uint256)
    {
        return tbioAmount * _powerDemul[lockDelay];
    }
}

