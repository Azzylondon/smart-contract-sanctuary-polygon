    function renewLabel(
        uint256 labelId,
        address user,
        string memory updatedURI
    ) external override onlyOwner {
        require(
            labelStatus(labelId) == Status.Outpassed,
            "TBLabel: label cannot be renewed"
        );

        bytes32 lastURI = keccak256(bytes(tokenURI(labelId)));
        require(
            lastURI != keccak256(bytes(updatedURI)),
            "TBLabel: URI should be updated"
        );

        _setTokenURI(labelId, updatedURI);
        setUser(labelId, user, uint64(block.timestamp + LABEL_EXPIRATION));
    }

    function migrateLabel(uint256 labelId, address newUser)
        external
        override
        onlyOwner
    {
        Label memory label = _labels[labelId];
        require(
            labelStatus(labelId) == Status.Active,
            "TBLabel: label is not migrable"
        );
        setUser(labelId, newUser, label.expiration);
    }

    function userOf(uint256 tokenId) public view returns (address) {
        return _labels[tokenId].user;
    }

    function userExpires(uint256 tokenId) external view returns (uint256) {
        return _labels[tokenId].expiration;
    }

    /**
     * @dev Return the status of the label depends on the expiration
     *
     * TODO return the status if this latter is arbitrary setted
     */
    function labelStatus(uint256 labelId)
        public
        view
        override
        returns (Status)
    {
        Label memory label = _labels[labelId];
        uint256 timestamp = block.timestamp;

        if (label.labelStatus == Status.NotAttributed) {
            return Status.NotAttributed;
        }

        // here return others status (Paused, Suspended, ...)

        if (timestamp >= label.expiration * 2) {
            return Status.Invalid;
        } else if (timestamp >= label.expiration) {
            return Status.Outpassed;
        } else {
            return Status.Active;
        }
    }

    /**
     * @dev Rewrite this check as {_checkOnERC721Received} is private on the ERC721
     */
    function _isERC721Received(address destination) internal returns (bool) {
