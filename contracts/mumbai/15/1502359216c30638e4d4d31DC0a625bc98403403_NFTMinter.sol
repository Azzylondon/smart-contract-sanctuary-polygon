    mapping(uint256 => Label) private _labels;

    constructor(
        string memory name_,
        string memory symbol_,
        address labelsVault_
    ) ERC721(name_, symbol_) {
        require(
            _isERC721Received(labelsVault_),
            "TBLabel: cannot handle ERC721"
