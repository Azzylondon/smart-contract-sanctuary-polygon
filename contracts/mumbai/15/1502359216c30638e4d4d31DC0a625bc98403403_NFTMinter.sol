/// @dev ......

pragma solidity 0.8.16;

import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC20/IERC20.sol";

contract Empowering is Ownable {
    uint256 private constant TOKEN_AMOUNT = 20 * 10**18;
    uint256 private constant DELAY = 60; // 1 mins
    IERC20 private immutable _token;

    /**
     * @dev as {TOKEN_AMOUNT} will, at some point, change, this state (whitelist) should be stored in
     * a authentification contract which manage user's access in the system.
     */
    struct Empowerment {
        bool active;
        uint64 quitCommit;
    }

    mapping(address => Empowerment) private _userEmpowerment;

    event EmpowermentStarted(
        address indexed account,
        bool indexed quitCancelled
    );
    event EmpowermentFinished(
        address indexed account,
        bool indexed quitCommitment
    );

    constructor(address tbioAddress_) {
        _token = IERC20(tbioAddress_);
    }

    function empower() external {
        Empowerment memory em = _userEmpowerment[msg.sender];

        require(!em.active, "Empowering: already whitelisted");
        require(em.quitCommit == 0, "Empowering: unbound active");

        em.active = true;
        _userEmpowerment[msg.sender] = em;
        _token.transferFrom(msg.sender, address(this), TOKEN_AMOUNT);
        emit EmpowermentStarted(msg.sender, false);
    }

    function quit() external {
        Empowerment memory em = _userEmpowerment[msg.sender];
        require(em.active, "Empowering: not whitelisted");

        em.quitCommit = uint64(block.timestamp);
        em.active = false;
        _userEmpowerment[msg.sender] = em;

        emit EmpowermentFinished(msg.sender, true);
    }

    function cancel() external {
        Empowerment memory em = _userEmpowerment[msg.sender];

        require(em.quitCommit > 0, "Empowering: no commitment");

        em.active = true;
        em.quitCommit = 0;
        _userEmpowerment[msg.sender] = em;

        emit EmpowermentStarted(msg.sender, true);
    }

    function withdraw() external {
        Empowerment memory em = _userEmpowerment[msg.sender];

        require(em.quitCommit > 0, "Empowering: no quit commitment");
        require(
            block.timestamp > em.quitCommit + DELAY,
            "Empowering: unbounding active"
        );

        em.quitCommit = 0;
        _userEmpowerment[msg.sender] = em;

        _token.transfer(msg.sender, TOKEN_AMOUNT);
        emit EmpowermentFinished(msg.sender, false);
    }

    function isEmpowered(address account) public view returns (bool) {
        return _userEmpowerment[account].active;
    }

    function isQuittingEmpowerment(address account) public view returns (bool) {
        return _userEmpowerment[account].quitCommit > 0 ? true : false;
    }

    function timeBeforeWithdraw(address account) public view returns (uint256) {
        Empowerment memory em = _userEmpowerment[account];
        require(em.quitCommit > 0, "Empowering: no quit commit");

        if (block.timestamp > em.quitCommit + DELAY) {
            return 0;
        } else {
            return (em.quitCommit + DELAY) - block.timestamp;
        }
    }
}

