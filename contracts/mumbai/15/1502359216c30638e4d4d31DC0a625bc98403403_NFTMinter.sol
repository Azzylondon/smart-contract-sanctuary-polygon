    uint256 public constant override LABEL_EXPIRATION = 60 * 60 * 24 * 366;
    address public override labelsVault;

    /**
     * @dev {labelStatus} always on {Active} when the token is minted
     * otherwise the status is set to {NotAttributed}
     */
    struct Label {
        address user;
        uint64 expiration;
        Status labelStatus;
