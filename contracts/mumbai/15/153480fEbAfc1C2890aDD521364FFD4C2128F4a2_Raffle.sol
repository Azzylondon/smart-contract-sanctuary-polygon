//Contract Parameters
// Create three lottery categories
// Basic category -- $1
// Investor category -- $10
// Whale category -- $100

// Duration -- Every 2 days
// For testnet every 3 hour
// For testnet payout happens every 1 hour

// Payouts -- Automatically
    // Contract gets -- 50%
    // 1 Winner -- 25%
    // 2 Winner -- 15%
    // 3 Winner -- 10%
    
contract Raffle is KeeperCompatibleInterface, VRFConsumerBase, ConfirmedOwner{

    //Raffle Address = 0x153480fEbAfc1C2890aDD521364FFD4C2128F4a2

    uint256 public raffleStartTime;
    uint256 internal raffleID;
    uint256 internal immutable raffleInterval = 1 * 1 hours;
    uint256 internal immutable resetInterval = 30 * 1 minutes; 
    bytes32 internal keyHash;
    uint256 internal fee;
    uint256 internal rafflePool;
    uint256 internal rebootChecker;

    uint256 noOfWinners = 3;

    //created a test Usdt
    address internal USDC =  0xe75613bc32e3ec430aDbD46D8dDf44C2b7F82071;

    enum RaffleState {
        WAITING_FOR_REBOOT,
        LOADING_IN_GAME,
        PAYOUT,
        NULL
    }

    enum RaffleCategory {
        BASIC,
        INVESTOR,
        WHALE
    }

    struct RaffleStruct{
        uint256 ID;                             //Lottery ID
        address payable [] winners;               // Winner address
        uint256[] tickets;           // tickets id (tickets)
        uint256 noOfTicketsSold;                   // Tickets sold
        uint256[] winnersPayout;       // Contains the % payouts of the winners
        uint256[] winningTickets;       // Contains array of winning Tickets
        uint256 noOfPlayers;
        mapping(uint256 => address) ticketOwner; //a mapping that maps the tickets to their owners
        mapping(address => uint256) userTickets; // a mapping that stores the number of tickets bought by a user 
    }


    struct RaffleData{
        uint256 ticketPrice;
        uint256 rafflePool;
        RaffleState raffleState;
    }

    //Maps lottery category to each lottery indexes of each lottery, for record keeping.
    mapping (RaffleCategory => mapping (uint => RaffleStruct)) internal raffles;
    mapping (RaffleCategory => RaffleData) internal rafflesData;
    mapping (bytes32 => RaffleCategory) internal bytesCategoryMapping;
    //Mapping for users that qualify for r
    mapping (RaffleCategory => mapping (address => uint256)) internal rollovers;
    

    // values set are for the mumbai testnet
    constructor(address _vrfCoordinator, address _link, bytes32 _keyHash, uint256 _fee)
        VRFConsumerBase(_vrfCoordinator, _link) ConfirmedOwner(msg.sender)
    {
        keyHash = _keyHash;
        fee = _fee;
        rebootChecker = 3;
        setRaffleData();
   }

    // Function to be called by the chainlink keepers that start the raffle
    function startRaffle()
        internal
        stateCheck()
    {
        raffleID++;

        //initiating raffle
        raffleStartTime = block.timestamp;

        // creating raffle sessions
        RaffleCategory[3] memory categoryArray = [RaffleCategory.BASIC, RaffleCategory.INVESTOR, RaffleCategory.WHALE];
        for (uint256 i = 0; i < categoryArray.length; i++){
            RaffleStruct storage _raffle = raffles[RaffleCategory.BASIC][raffleID];
            _raffle.ID = raffleID;
            setRaffleState(categoryArray[i], RaffleState.LOADING_IN_GAME);
        }
        rebootChecker = 0;
    }

    // This function sets the raffle initial data 
    function setRaffleData() internal {
        RaffleData storage _basicRaffleData = rafflesData[RaffleCategory.BASIC];
        _basicRaffleData.ticketPrice =  1 * 10 ** 18;

        RaffleData storage _investorRaffleData = rafflesData[RaffleCategory.INVESTOR];
        _investorRaffleData.ticketPrice =  10 * 10 ** 18;

        RaffleData storage _whaleRaffleData = rafflesData[RaffleCategory.WHALE];
        _whaleRaffleData.ticketPrice =  100 * 10 ** 18;
    
    }

    // To help monitor the flow of the contract, this function allows the contract to change the state of each raffle
    function setRaffleState(RaffleCategory _category, RaffleState _state) internal {
        RaffleData storage _raffleData = rafflesData[_category];
        _raffleData.raffleState = _state;
    }

    function getRaffleState(RaffleCategory _category) public view returns(RaffleState){
        RaffleData storage _raffleData = rafflesData[_category];
        return(_raffleData.raffleState);
    }

    function viewRaffle(RaffleCategory _category, uint256 _raffleID) public view returns(
        uint256 ID,                      
        address payable []  memory winners,              
        uint256 noOfTicketsSold,                 
        uint256[] memory winningTickets,
        uint256
    ){
        RaffleStruct storage _raffle = raffles[_category][_raffleID];
        RaffleData storage _raffleData = rafflesData[_category];
        return(
            _raffle.ID,
            _raffle.winners,
            _raffle.noOfTicketsSold,
            _raffle.winningTickets,
            _raffleData.rafflePool
        );
    }


    function buyTicket(RaffleCategory _category, uint256 _noOfTickets, uint256[] memory _tickets, uint256 _amount) public estimateAmountToBeSpent(rafflesData[_category].ticketPrice, _noOfTickets, _amount) payable {
        require(_tickets.length == _noOfTickets, "Error in amount of tickets generated");
        require(IERC20(USDC).transferFrom(msg.sender, address(this), _amount), "Transfer failed");
        RaffleStruct storage _raffle = raffles[_category][raffleID];
        if(_raffle.userTickets[msg.sender] == 0){
            _raffle.noOfPlayers++;
        }
        rafflesData[_category].rafflePool += _amount;
        assignTickets(_category, _noOfTickets, _tickets);
    }

    function assignTickets(RaffleCategory _category, uint256 _noOfTickets, uint256[] memory _tickets) internal {
        RaffleStruct storage _raffle = raffles[_category][raffleID];
        for(uint n = 0; n < _noOfTickets; n++){
            _raffle.tickets.push(_tickets[n]);
            _raffle.ticketOwner[_tickets[n]] = msg.sender;
        }
        _raffle.noOfTicketsSold += _noOfTickets;
        updateWinnersPayouts(_category);
    }

    function updateWinnersPayouts(RaffleCategory _category) internal {
        RaffleStruct storage _raffle = raffles[_category][raffleID];
        uint256 _rafflePool = rafflesData[_category].rafflePool;
        uint256 _25percent = _rafflePool * 25 / 100;
        uint256 _15percent = _rafflePool * 15 / 100;
        uint256 _10percent = _rafflePool * 10 / 100;
        _raffle.winnersPayout = [_25percent, _15percent, _10percent]; 
    }

    function getWinningTickets(RaffleCategory _category) public onlyOwner returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        requestId = requestRandomness(keyHash, fee);
        bytesCategoryMapping[requestId] = _category; 
    }

    function fulfillRandomness(bytes32 requestId , uint256 randomness) internal override {
        RaffleCategory _category  = bytesCategoryMapping[requestId];
        uint256[] memory winningTickets = expand(_category, randomness);
        getWinners(_category, winningTickets);
    }

    function expand(RaffleCategory _category, uint256 randomValue) internal view returns (uint256[] memory winningTickets) {
        RaffleStruct storage _raffle = raffles[_category][raffleID];

        winningTickets = new uint256[](noOfWinners);

        for (uint256 i = 0; i < noOfWinners; i++) {
            winningTickets[i] = uint256(keccak256(abi.encode(randomValue, i))) % _raffle.noOfTicketsSold ;
        }
        return winningTickets;
    }

    function getWinners(RaffleCategory _category, uint256[] memory _winningTickets) internal {
        RaffleStruct storage _raffle = raffles[_category][raffleID];
        for (uint256 i = 0; i < noOfWinners; i++){
            uint256 ticketNos = _raffle.tickets[_winningTickets[i]];
            _raffle.winners[i] = payable(_raffle.ticketOwner[ticketNos]);
        }
        setRaffleState(_category, RaffleState.PAYOUT);
    } 

    function payoutWinners(RaffleCategory _category) onlyOwner() public {
        RaffleStruct storage _raffle = raffles[_category][raffleID];
        
        for (uint256 i = 0; i < noOfWinners; i++){
            require(IERC20(USDC).transferFrom(address(this), _raffle.winners[i], _raffle.winnersPayout[i]), "Transfer failed");         
        }        
        setRaffleState(_category, RaffleState.WAITING_FOR_REBOOT);
        rebootChecker++;
    }

    function checkUpkeep(bytes calldata /* checkData */) external view override returns (bool upkeepNeeded, bytes memory performData) {
        RaffleCategory[3] memory categoryArray = [RaffleCategory.BASIC, RaffleCategory.INVESTOR, RaffleCategory.WHALE];
        bool restart = false;
        
        if(rebootChecker == 3) {
            restart = true;
        }

        for (uint256 i = 0; i < categoryArray.length; i++){
            RaffleCategory _category = categoryArray[i];

            RaffleStruct storage _raffle = raffles[_category][raffleID];

            RaffleData storage _raffleData = rafflesData[_category];

            if(((block.timestamp - raffleStartTime) > raffleInterval) && (_raffleData.raffleState == RaffleState.LOADING_IN_GAME)){
                    if(_raffle.noOfTicketsSold < 10 && _raffle.noOfPlayers < 5){
                        upkeepNeeded = true;
                        performData = abi.encode(4, _category);
                        continue;
                    }
                    upkeepNeeded = true;
                    performData = abi.encode(1, _category);

            }else if(_raffleData.raffleState == RaffleState.PAYOUT){
                    upkeepNeeded = true;
                    performData = abi.encode(2, _category);
                    
            }else if((block.timestamp - raffleStartTime) > (raffleInterval + resetInterval) && (restart)){
                upkeepNeeded = true;
                performData = abi.encode(3, _category);
            }
        }
    }

    function performUpkeep(bytes calldata performData) external override {
        (int comment, RaffleCategory _category) = abi.decode(performData, (int, RaffleCategory));
        if (comment == 1){
               getWinningTickets(_category);
        }else if (comment == 2){
               payoutWinners(_category);
        }else if (comment == 3){
            startRaffle();
        }else if (comment == 4){
            rollover(_category);
        }
    }

    function rollover(RaffleCategory _category) internal {
        RaffleStruct storage _raffle = raffles[_category][raffleID];

        for(uint256 i; i < _raffle.noOfTicketsSold; i++){
            address player = _raffle.ticketOwner[i];
            rollovers[_category][player] = _raffle.userTickets[player];
        }
        setRaffleState(_category, RaffleState.WAITING_FOR_REBOOT);
        rebootChecker++;
    }

    function checkRollover(RaffleCategory _category) public {
        uint256 ticketsToRollover = rollovers[_category][msg.sender];
        uint256[] memory _tickets = getRandom(ticketsToRollover);
        uint256 _amount = ticketsToRollover * rafflesData[_category].ticketPrice;
        buyTicket(_category, ticketsToRollover, _tickets, _amount);
        rollovers[_category][msg.sender] = 0;
    }

    function getRandom(uint256 _noOfTickets) internal  view returns (uint256[] memory tickets){
        uint256 modulo = 99999;
        uint256[] memory _tickets;
        for(uint256 i = 0; i < _noOfTickets; i++){
            _tickets[i] = uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty))) % modulo + uint256(9999);
        }
        return(_tickets);
    }

    function checkLinkBalance() public view returns(uint256){
        return(LINK.balanceOf(address(this)));
    }

    modifier stateCheck() {
        require(rebootChecker == 3, "Reboot check not complete");
        _;
    }

    modifier estimateAmountToBeSpent(uint256 _ticketPrice, uint256 _noOfTickets, uint256 _amount) {
        require((_noOfTickets * _ticketPrice) == _amount, "Insufficient Funds");
        _;
    }


    //Odds of Winning is increased by the number of tickets a person buys, but it does not guarantee winning,
        // as the randomness is generated randomly using the chainlink vrf and not with any existing variable in the contract

    // Users are given indexes for each ticket bought, a mapping to store the each user to a ticket id.
    // So after the raffle is drawn lucky index number for raffle is chosen and the winners are awarded
    // Since there are 3 winners raffles will be drawn thrice.
        // first winner is the first person 25%
        // second is the 2nd person 15%
        // third is the 3rd person 10%
        // This is done by using the keccak function to alter the random value gotten from the vrf request.
    
    //The random values gotten for each lottery are just basically indexes which are then looked up in the user-ticket mapping 
    
    //PHASE TWO, Using keepers to automate the payout
}

