pragma solidity >=0.8.0 <0.9.0;

import "../openzeppelin/Safemath.sol";
import "./User.sol";
import "./DealStorage.sol";
import "../openzeppelin/IERC20.sol";
import "hardhat/console.sol";
import "../opengsn/BaseRelayRecipient.sol";
import "../openzeppelin/Ownable.sol";

contract Deal is DealStorage, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event ListBounty(address indexed client, bytes32 indexed dealId, uint256 acceptanceDeadline,
        uint256 reward);
    event DelistBounty(address indexed client, bytes32 indexed dealId);
    event DealFirstConfirmation(address indexed client, address indexed freelancer, bytes32 indexed dealId, uint deadline, uint lockedStake, uint reward, string bountyCid);
    event DealSecondConfirmation(address indexed freelancer, bytes32 indexed dealId);
    event BountyCompleted(
        address indexed freelancer,
        bytes32 indexed dealId,
        string submission
    );

    event BountyAccepted(address indexed client, address indexed freelancer,
        bytes32 indexed dealId,
        uint reward);
    event DeadlineViolation(address indexed client, address indexed freelancer, bytes32 dealId, string deadlineType, uint amount);

    User userMetadata;
    address tokenAddress;
    uint platformFeePercentage = 2;

    constructor(
        address _userContract,
        address _tokenAddress
    ) {
        userMetadata = User(_userContract);
        tokenAddress = _tokenAddress;
    }

    /**
     * @dev Check if msg sender is the client of the job
     * @param dealId The unique identity of the job
     **/
    modifier onlyCreator(bytes32 dealId) {
        require(dealIdToDeal[dealId].creator == msg.sender, "Not data owner");
        _;
    }

    /**
     * @dev Check if msg sender is the contributor of the job
     * @param dealId The unique identity of the job
     **/
    modifier onlyFreelancer(bytes32 dealId) {
        require(
            dealIdToDeal[dealId].freelancer == msg.sender,
            "Not freelancer"
        );
        _;
    }

    /**
     * @dev Check if job is incomplete
     * @param dealId The unique identity of the job
     **/
    modifier onlyWhenIncomplete(bytes32 dealId) {
        require(dealIdToDeal[dealId].completed == false, "Already completed");
        _;
    }

    /**
     * @dev Check if job is not accepted yet
     * @param dealId The unique identity of the job
     **/
    modifier onlyWhenUnaccepted(bytes32 dealId) {
        require(dealIdToDeal[dealId].accepted == false, "Already accepted");
        _;
    }

    function listBounty(
        uint256 acceptanceDeadline,
        uint256 reward
        ) external returns (bytes32) {
            userMetadata.initializeUserIfFirstTime(msg.sender);

            uint confirmationDeadline =
                block.timestamp +
                1 days;

            Deadline memory deadline = Deadline(
                0, acceptanceDeadline, confirmationDeadline
            );

            Deal memory deal = Deal(
                true,
                false,
                false,
                false,
                msg.sender,
                address(0),
                0,
                "",
                deadline,
                ""
            );

            bytes32 dealId = keccak256(
            abi.encodePacked(address(this), dealIds.length)
            );
            dealIds.push(dealId);
            dealIdToDeal[dealId] = deal;

            dealIdToEscrow[dealId] = new Escrow(tokenAddress);
            dealIdToEscrow[dealId].deposit(
                reward,
                msg.sender
            );

            emit ListBounty(msg.sender, dealId, reward, acceptanceDeadline);
            return dealId;
        }

    function delistBounty(bytes32 dealId) external onlyCreator(dealId) {
        require(dealIdToDeal[dealId].created = true, "Bounty is already inactive");
        require(dealIdToDeal[dealId].confirmed = false, "Deal confirmed, bounty cannot be delisted");
        dealIdToDeal[dealId].created  = false;
        dealIdToEscrow[dealId].refund(msg.sender);

        emit DelistBounty(msg.sender, dealId);

    }

    /**
     * @dev This confirmation is sent by the client upon picking a contributor and his proposal. This is the start of a new deal.
     * @param freelancer Address of freelancer whose proposal has been accepted by job creator
     * @param lockedStake Amount that freelancer needs to lockup as collateral to work on the job. This collateral will be used in case of a dispute
     **/
    function firstConfirmation(
        uint256 deadline,
        address freelancer,
        uint256 lockedStake,
        bytes32 dealId,
        string memory bountyCid
    ) external onlyCreator(dealId){
        dealIdToDeal[dealId].deadline.completionDeadline = deadline;
        dealIdToDeal[dealId].freelancer = freelancer;
        dealIdToDeal[dealId].lockedStake = lockedStake;
        dealIdToDeal[dealId].bountyCid = bountyCid;
        emit DealFirstConfirmation(msg.sender, freelancer, dealId, deadline, lockedStake, dealIdToEscrow[dealId].balanceOfEscrow(), bountyCid);
    }

    /**
     * @dev This confirmation is sent by freelancer upon being picked to work on a certain job. This is to make sure the deal is confirmed by both parties
     *      before work can begin.
     * @param dealId The unique identity of the job
     **/
    function secondConfirmation(bytes32 dealId)
        external
        onlyFreelancer(dealId)
    {
        dealIdToDeal[dealId].confirmed = true;
        userMetadata.initializeUserIfFirstTime(msg.sender);

        userMetadata.lockUserStake(
            dealIdToDeal[dealId].freelancer,
            dealIdToDeal[dealId].lockedStake
        );

        emit DealSecondConfirmation(
            msg.sender, dealId
        );
    }

    /**
     * @dev This is called by freelancer when work is completed or revised.
     * @param dealId The unique identity of the job
     * @param submission The content identifier of the submission so the work can be verified in case of dispute.
     * TODO: How can we make sure the submission is private and can only be verified by concerned parties?
     **/
    function complete(bytes32 dealId, string memory submission)
        external
        onlyFreelancer(dealId)
    {
        dealIdToDeal[dealId].submission = submission;

        dealIdToDeal[dealId].completed = true;

        emit BountyCompleted(
            msg.sender, dealId, submission
        );
    }

    /**
     * @dev This is called by job creator when he accepts the work submitted
     * @param dealId The unique identity of the job
     **/
    function accept(bytes32 dealId) external onlyCreator(dealId) {
        dealIdToDeal[dealId].accepted = true;

        userMetadata.unlockUserStake(
            dealIdToDeal[dealId].freelancer,
            dealIdToDeal[dealId].lockedStake
        );

        uint256 total = dealIdToEscrow[dealId].balanceOfEscrow();

        uint256 fees = total.mul(platformFeePercentage).div(100);

        uint256 stakeAmount = total
            .mul(userMetadata.getUserAllocation(dealIdToDeal[dealId].freelancer))
            .div(100);


        IERC20(tokenAddress).safeTransferFrom(msg.sender, address(this), fees);
        dealIdToEscrow[dealId].withdraw(total, dealIdToDeal[dealId].freelancer);
        userMetadata.addToUserStake(dealIdToDeal[dealId].freelancer, stakeAmount);

        emit BountyAccepted(msg.sender, dealIdToDeal[dealId].freelancer, dealId, total);
    }

    /**
     * @dev This can be called by job creator if the deadline to submit work has ended. It will refund the collateralized amount to job creator
     *      and add to forgone fees of the freelancer (which is a reputation score)
     * @param dealId The unique identity of the job
     **/
    function callCompletionDeadlineViolation(bytes32 dealId)
        external
        onlyCreator(dealId)
        onlyWhenIncomplete(dealId)
    {
        require(block.timestamp > dealIdToDeal[dealId].deadline.completionDeadline, "Completion deadline hasnt been violated yet");

        address freelancer = dealIdToDeal[dealId].freelancer;
        dealIdToDeal[dealId].freelancer = address(0);
        dealIdToEscrow[dealId].refund(msg.sender);
        userMetadata.slashLockedStake(
            freelancer,
            dealIdToDeal[dealId].creator,
            dealIdToDeal[dealId].lockedStake
        );

        emit DeadlineViolation(msg.sender, dealIdToDeal[dealId].freelancer, dealId, "Completion", dealIdToEscrow[dealId].balanceOfEscrow());
    }

    /**
     * @dev This can be called by job creator if the deadline to confirm has expired. It will just be used by frontend to set notify job creator
     * to pick another proposal
     * @param dealId The unique identity of the job
     **/
    function callConfirmationDeadlineViolationAndRefund(bytes32 dealId)
        external
        onlyCreator(dealId)
    {
        require(getIfConfirmationDeadlinePassed(dealId) == true, "Confirmation deadline hasnt been violated yet");

        dealIdToDeal[dealId].freelancer = address(0);
        dealIdToEscrow[dealId].refund(msg.sender);
        emit DeadlineViolation(msg.sender, dealIdToDeal[dealId].freelancer, dealId, "Completion", dealIdToEscrow[dealId].balanceOfEscrow());
    }

    /**
     * @dev This can be called by job creator if the deadline to confirm has expired. It will just be used by frontend to set notify job creator
     * to pick another proposal
     * @param dealId The unique identity of the job
     **/
    function getIfConfirmationDeadlinePassed(bytes32 dealId)
        public
        view
        returns (bool)
    {
        if (block.timestamp > dealIdToDeal[dealId].deadline.confirmationDeadline) {
            return true;
        } else {
            return false;
        }
    }

    /**
     * @dev This can be called by freelancer if the deadline to accept his submitted work has ended. It will pay the advance fees in job creater's
     * escrow to him and prevent the job creator from using his work.
     * @param dealId The unique identity of the job
     **/
    function callAcceptanceDeadlineViolation(bytes32 dealId)
        external
        onlyFreelancer(dealId)
        onlyWhenUnaccepted(dealId)
    {
        require(block.timestamp > dealIdToDeal[dealId].deadline.acceptanceDeadline, "Acceptance deadline hasnt been violated yet"); 
            
        dealIdToDeal[dealId].accepted = true;

        
        uint256 amount = dealIdToEscrow[dealId].balanceOfEscrow();

        uint256 stakeAmount = amount
            .mul(userMetadata.getUserAllocation(msg.sender))
            .div(100);

        dealIdToEscrow[dealId].withdraw(amount, msg.sender);
        userMetadata.addToUserStake(msg.sender, stakeAmount);
        
        emit DeadlineViolation(dealIdToDeal[dealId].creator, msg.sender, dealId, "Acceptance", amount);
    }


    /**
     * @dev This can be called by owner wallet to retrieve fees and add to treasury
     **/
    function withdrawFee()
        external
        onlyOwner
    {
        IERC20(tokenAddress).safeTransferFrom(address(this), msg.sender, IERC20(tokenAddress).balanceOf(address(this)));
    }

    /**
     * @dev This can be called by owner wallet to retrieve fees and add to treasury
     **/
    function setPlatformFee(uint percentage)
        external
        onlyOwner
    {
        platformFeePercentage = percentage;
    }
}

