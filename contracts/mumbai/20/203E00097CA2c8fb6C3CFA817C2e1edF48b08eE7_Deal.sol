import "../openzeppelin/Ownable.sol";
import "../openzeppelin/Address.sol";
import "../openzeppelin/SafeERC20.sol";
import "../openzeppelin/Safemath.sol";
import "../protocol/FriendRegistry.sol";

/**
 * @title Escrow
 * @dev Base escrow contract, holds funds designated for a payee until they
 * withdraw them.
 *
 * Intended usage: This contract (and derived staking contracts) should be a
 * standalone contract, that only interacts with the contract that instantiated
 * it. That way, it is guaranteed that all tokens will be handled according to
 * the staking rules. The contract that uses the stake pool should be its owner, 
 * and provide public methods redirecting to the stake pool's deposit and withdraw.
 */
contract StakePool is Ownable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    uint private ownerStake;
    uint private lockedStake;
    address stakeOwner;
    address token;
    address deployer;

    constructor(address _owner, address _token, address _deployer){
        stakeOwner = _owner;
        token = _token;
        deployer = _deployer;
    }   


    modifier onlyDeployer() {
        require(
            deployer == msg.sender,
            "Caller is not the deployer"
        );
        _;
    }

    function getOwnerStake() public view returns(uint){
        return ownerStake;
    }

    function getLockedStake() public view returns(uint){
        return lockedStake;
    }

    function getUnlockedStake() public view returns(uint){
        return ownerStake - lockedStake;
    }


    function stake(uint amount) external onlyDeployer{
        ownerStake += amount;

        IERC20(token).safeTransferFrom(stakeOwner, address(this), amount);
    }

    function lock(uint amount) external onlyDeployer{
        require(amount <= ownerStake, "Lock amount is more than staked amount");

        lockedStake += amount;
    }

    function unstake(uint amount) external onlyDeployer{
        require(amount <= ownerStake.sub(lockedStake), "Unstaking amount is more than staked amount");

        ownerStake -= amount;

        IERC20(token).safeTransferFrom(address(this), stakeOwner, amount);
    }


    function unlock(uint amount) external onlyDeployer {
        require(amount <= lockedStake, "Unlocking amount is more than locked amount");

        lockedStake -= amount;
    }


    function slash(uint amount, address to) external onlyDeployer{
        require(amount <= lockedStake, "Slash amount is more than locked amount");

        IERC20(token).safeTransferFrom(address(this), to, amount);
    }
}

