pragma solidity >=0.8.0 <0.9.0;
import "../utility/Escrow.sol";

contract DealStorage {
    /**
     * @dev Deal storage. This is only created if deal is successfully agreed on by both parties
     **/
    struct Deadline {
        uint completionDeadline;
        uint acceptanceDeadline;
        uint confirmationDeadline;
    }

    struct Deal {
        bool created;
        bool confirmed;
        bool completed;
        bool accepted;
        address creator;
        address freelancer;
        uint lockedStake;
        string submission;
        Deadline deadline;
        string bountyCid;
    }


    bytes32[] dealIds;
    mapping(bytes32 => Deal) internal dealIdToDeal;
    mapping(bytes32 => Escrow) internal dealIdToEscrow;

    uint256 constant decimalsInConversion = 8;


    function getFreelancer(bytes32 dealId) external view returns (address){
        return dealIdToDeal[dealId].freelancer;
    }

    function getCreator(bytes32 dealId) external view returns (address){
        return dealIdToDeal[dealId].creator;
    }

    function getDealIds() external view returns (bytes32[] memory) {
        return dealIds;
    }

    function getDeal(bytes32 dealId) external view returns (Deal memory) {
        return dealIdToDeal[dealId];
    }

    function getReward(bytes32 dealId) external view returns (uint256) {
        return dealIdToEscrow[dealId].balanceOfEscrow();
    }
}

