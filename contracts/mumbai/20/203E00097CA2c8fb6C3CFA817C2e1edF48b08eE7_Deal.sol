pragma solidity >=0.8.0 <0.9.0;

import "../openzeppelin/Safemath.sol";
import "./FriendRegistry.sol";
import "../utility/StakePool.sol";
import "hardhat/console.sol";

contract User {
    using SafeMath for uint256;
    using SafeMath for uint32;

    // Use MAX_UINT to signify null uint value
    uint256 constant MAX_UINT = 2**256 - 1;

    //Default stake allocation = 5% of earned amount
    uint256 constant DEFAULT_STAKE_ALLOCATION = 5; 

    // Null bytes32 values
    bytes32 constant NULL = "";
    address stakingTokenAddress;

    /**
     * @dev user financials
     **/
    struct UserFinancials {
        uint256 stakeAllocation;        
    }


    mapping(address => bool) public userInitialized;
    mapping(address => UserFinancials) public userToFinancials;
    mapping(address => StakePool) public userToStakePool;

    event UserInititialized(address indexed user);
    event UserStakeAllocationChanged(address indexed user, uint newAllocationAmount);
    event UserStaked(address indexed user, uint amount);
    event UserStakeLocked(address indexed user, uint amount);
    event UserStakeUnlocked(address indexed user, uint amount);
    event UserUnstaked(address indexed user, uint amount);
    event UserSlashed(address indexed user, address indexed to, uint amount);

    FriendRegistry friendRegistry;

    constructor(address _friendRegistry, address _stakingTokenAddress) {
        friendRegistry = FriendRegistry(_friendRegistry);
        stakingTokenAddress = _stakingTokenAddress;
    }


    modifier onlyFriend(address contractAddress) {
        require(
            friendRegistry.isFriend(contractAddress) == true,
            "Contract is not a friend contract"
        );
        _;
    }

    modifier onlyIfUserExists(address userAddress) {
        require(userInitialized[userAddress] == true, "user doesnt exist");
        _;
    }

    /**
     * @dev Sets a user's stake allocation called by user.
     **/
    function setMyAllocation(uint allocation)
        external
    {     
        userToFinancials[msg.sender].stakeAllocation = allocation;
        emit UserStakeAllocationChanged(msg.sender, allocation);
    }


    /**
     * @dev Adds to a user's stake pool called by user.
     **/
    function addToUserStake(address user, uint amount)
        onlyIfUserExists(user)
        onlyFriend(msg.sender)
        external
    {
        userToStakePool[user].stake(amount);
        emit UserStaked(user, amount);
    }


    /**
     * @dev Adds to a user's stake
     * - Called by user
     **/
    function addToMyStake(uint amount) external {
        initializeUserIfFirstTime();
        userToStakePool[msg.sender].stake(amount);
        emit UserStaked(msg.sender, amount);
    }

    /**
     * @dev Reduce a user's stake pool called by user.
     **/
    function reduceMyStake(uint amount)
        onlyIfUserExists(msg.sender)
        external
    {
        userToStakePool[msg.sender].unstake(amount);
        emit UserUnstaked(msg.sender, amount);
    }


    /**
     * @dev Adds to a user's stake pool called by user.
     **/
    function lockUserStake(address user, uint amount)
        onlyIfUserExists(user)
        onlyFriend(msg.sender)
        external
    {
        userToStakePool[user].lock(amount);
        emit UserStakeLocked(user, amount);
    }

     /**
     * @dev Adds to a user's stake pool called by user.
     **/
    function addToUserLockedStake(address user, uint amount)
        onlyIfUserExists(user)
        onlyFriend(msg.sender)
        external
    {
        userToStakePool[user].stake(amount);
        userToStakePool[user].lock(amount);

        emit UserStaked(user, amount);
        emit UserStakeLocked(user, amount);
    }


    /**
     * @dev Adds to a user's stake pool called by user.
     **/
    function unlockUserStake(address user, uint amount)
        onlyIfUserExists(user)
        onlyFriend(msg.sender)
        external
    {
        userToStakePool[user].unlock(amount);
        emit UserStakeUnlocked(user, amount);
    }


    /**
     * @dev Slash a user's locked stake when he failed to complete job. This is only applicable for the contributor. 
     *  For clients, the escrow advance amount is used as collateral
     *  - Can only be called by a friend contract
     * @param user The address of the new user
     * @param to The address to which the slashed amount goes. Currently, it would be the client's address but for some use cases it may also
     *          be burned or sent to the treasury
     * @param amount The amount to slash from the locked amount. This must always be less than locked amount.
     **/
    function slashLockedStake(address user, address to, uint amount) external onlyFriend(msg.sender) {
        userToStakePool[user].unlock(amount);
        userToStakePool[user].unstake(amount);
        userToStakePool[user].slash(amount, to);

        emit UserUnstaked(user, amount);
        emit UserStakeUnlocked(user, amount);
        emit UserSlashed(user, to, amount);
    }


    /**
     * @dev Initialize a user's metadata.
     * - Called by createuserIfFirstTime when a user picks up his first job or stakes his tokens for the first time
     * @param user The address of the new user
     **/
    function initializeUserIfFirstTime(address user) external onlyFriend(msg.sender) {
        if (userInitialized[user] == false){
            userInitialized[user] = true;
            userToFinancials[user].stakeAllocation = DEFAULT_STAKE_ALLOCATION;

            userToStakePool[user] = new StakePool(msg.sender, stakingTokenAddress, address(this));

            emit UserInititialized(user);
        }

    }

    /**
     * @dev Initialize a user's metadata.
     * - Called by internal methods that user directly calls upon sign up such as addToMyStake
     **/
    function initializeUserIfFirstTime() private {
        if (userInitialized[msg.sender] == false){
            userInitialized[msg.sender] = true;
            userToFinancials[msg.sender].stakeAllocation = DEFAULT_STAKE_ALLOCATION;

            userToStakePool[msg.sender] = new StakePool(msg.sender, stakingTokenAddress, address(this));

            emit UserInititialized(msg.sender);
        }
    }

    /**
     * @dev Gets a user by address.
     **/
    function getUserAllocation(address user)
        external
        view
        returns (
            uint
        )
    {
        return userToFinancials[user].stakeAllocation;
    }


    /**
     * @dev Gets a user by address.
     **/
    function getUserStake(address user)
        external
        view
        returns (
            uint
        )
    {
        return userToStakePool[user].getOwnerStake();
    }


    /**
     * @dev Gets a user by address.
     **/
    function getUserLockedStake(address user)
        external
        view
        returns (
            uint
        )
    {
        return userToStakePool[user].getLockedStake();
    }

    /**
     * @dev Gets my user stake by address.
     **/
    function getMyStake()
        external
        view
        returns (
            uint
        )
    {
        return userToStakePool[msg.sender].getOwnerStake();
    }

    /**
     * @dev Gets my user locked stake by address.
     **/
    function getMyLockedStake()
        external
        view
        returns (
            uint
        )
    {
        return userToStakePool[msg.sender].getLockedStake();
    }
}

