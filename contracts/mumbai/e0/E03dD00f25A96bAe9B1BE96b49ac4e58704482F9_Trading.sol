pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./interfaces/IAdmin.sol";
import "../Periphery/BasicMetaTransaction.sol";
import "./interfaces/IWhitelisting.sol";
import "contracts/Periphery/APIConsumer.sol";
import "hardhat/console.sol";

contract Trading is Initializable, AccessControl, BasicMetaTransaction {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    bytes32 public constant OPERATOR = keccak256("OPERATOR");
    event buyEvent(uint amount,address user,address Trading,address token);

    IAdmin public admin;
    IERC20 public usdc;
    IERC20 public token;
    IWhitelisting public whitelisting;
    APIConsumer internal priceFeed;
    uint public price;

    function initialize(
        address _admin,
        address _usdc,
        address _token,
        address _whitelisting,
        address _priceFeed
    ) external initializer {
        admin = IAdmin(_admin);
        usdc = IERC20(_usdc);
        token = IERC20(_token);
        whitelisting = IWhitelisting(_whitelisting);
        priceFeed = APIConsumer(_priceFeed);
    }

    function setUSDCAddress(address _usdc) external {
        require(admin.hasRole(OPERATOR, _msgSender()), "Trading: OnlyOperator");
        usdc = IERC20(_usdc);
    }

    function setPriceFeed(address _priceFeed) external {
        // require(admin.hasRole(OPERATOR, _msgSender()), "Trading: OnlyOperator");
        priceFeed = APIConsumer(_priceFeed);
    }

    function buy(uint _amount) external {
        // require(
        //     whitelisting.isWhitelisted(_msgSender()) ||
        //         whitelisting.isKYC(_msgSender())
        // );
        // require(usdc.balanceOf(_msgSender()) > 0 ,"Not enough USDC");
        require(token.balanceOf(address(this)) >= _amount, "Not enough token");
        uint tokenValue = _oraclePrice(_amount);
        if (tokenValue > 0) {
            usdc.transferFrom(_msgSender(), address(this), uint(tokenValue));
            token.transfer(_msgSender(), _amount);
        }
        emit buyEvent(_amount,_msgSender(),address(this),address(token));
    }

    function _oraclePrice(uint _amount) internal returns (uint) {
        priceFeed.requestPrice();
        uint priceOfOne = priceFeed.price();
        price = ((priceOfOne * _amount) * 352)/10**13;
        return price;
    }

    function _msgSender()
        internal
        view
        override(BasicMetaTransaction, Context)
        returns (address sender)
    {
        if (msg.sender == address(this)) {
            bytes memory array = msg.data;
            uint256 index = msg.data.length;
            assembly {
                // Load the 32 bytes word from memory with the address on the lower 20 bytes, and mask those.
                sender := and(
                    mload(add(array, index)),
                    0xffffffffffffffffffffffffffffffffffffffff
                )
            }
        } else {
            return msg.sender;
        }
    }
}

