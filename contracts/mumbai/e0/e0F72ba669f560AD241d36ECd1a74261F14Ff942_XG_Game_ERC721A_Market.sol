/// @title Xternity Games - ERC721A Game Contract Template (for EVM chains)
/// @author Xternity Games
/// @notice ERC721A Game contract template
/// @dev ERC721A Game contract template
///      - Default price will be displayed in USD
///      - Price will be calculated in TOKEN : USD rate
contract XG_Game_ERC721A_Market is
    Ownable,
    ERC2981,
    ERC721A,
    Pausable,
    ReentrancyGuard
{
    using Strings for uint256;

    struct PaymentToken {
        address token;
        address aggregator;
        uint8 decimals;
        bool isNative;
        bool enabled;
    }
    XG private _treasury; // Treasury contract address (including royalty, all payments will be sent to treasury contract - XG)
    bool private _useDefaultGameTokenPrice;
    uint256 private _defaultGameTokenPrice;
    string[] private _paymentTokenSymbols; // Payment token symbols
    mapping(uint256 => uint256) private _totalSupplyPerGameToken; // Total supply per GameToken
    mapping(uint256 => uint256) private _tokenIdPerGameToken; // Token ID per GameToken
    mapping(string => PaymentToken) private _paymentTokenPerSymbol; // PaymentToken per symbol
    mapping(uint256 => uint256) private _pricePerGameTokenId; // Price (in USD) per Token ID

    uint256 public gameId; // Game ID
    string public baseURI; // Game baseURI

    error InvalidArraySize();
    error InvalidPaymentAmount();
    error SetPriceFailed();
    error TokenNotEnabled();
    error TokenNotExist();
    error ZeroAddress();

    event Paused();
    event UnPaused();
    event GameIDChanged(uint256 gameId);
    event TreasuryChanged(address newTreasury);

    /// @notice Contract constructor
    /// @dev Contract constructor
    constructor(
        string memory name_,
        string memory symbol_,
        uint256 gameId_,
        string memory baseURI_,
        address treasury_,
        address maticAggregator_, // 0x0715A7794a1dc8e42615F059dD6e406A6594651A (MATIC/USD on Mumbai)
        address usdcAggregator_, // 0x572dDec9087154dC5dfBB1546Bb62713147e0Ab0 (USDC/USD on Mumbai)
        address usdtAggregator_, // 0x92C09849638959196E976289418e5973CC96d645 (USDT/USD on Mumbai)
        bool useDefaultGameTokenPrice_,
        uint256 defaultGameTokenPrice_
    ) ERC721A(name_, symbol_) {
        if (treasury_ == address(0)) {
            revert ZeroAddress();
        }
        gameId = gameId_;
        baseURI = baseURI_;
        _treasury = XG(payable(treasury_));
        // Add MATIC, USDC, USDT as default payment tokens (All token addresses are on Mumbai)
        _paymentTokenPerSymbol["MATIC"] = PaymentToken(
            address(0),
            maticAggregator_,
            18,
            true,
            true
        );
        _paymentTokenSymbols.push("MATIC");
        _paymentTokenPerSymbol["USDC"] = PaymentToken(
            address(0xe6b8a5CF854791412c1f6EFC7CAf629f5Df1c747),
            usdcAggregator_,
            ERC20(address(0xe6b8a5CF854791412c1f6EFC7CAf629f5Df1c747))
                .decimals(),
            false,
            true
        );
        _paymentTokenSymbols.push("USDC");
        _paymentTokenPerSymbol["USDT"] = PaymentToken(
            address(0xA02f6adc7926efeBBd59Fd43A84f4E0c0c91e832),
            usdtAggregator_,
            ERC20(address(0xe6b8a5CF854791412c1f6EFC7CAf629f5Df1c747))
                .decimals(),
            false,
            true
        );
        _paymentTokenSymbols.push("USDT");
        _useDefaultGameTokenPrice = useDefaultGameTokenPrice_;
        _defaultGameTokenPrice = defaultGameTokenPrice_;
    }

    /// @notice Hook that is called before any token transfer
    /// @dev Hook that is called before any token transfer
    function _beforeTokenTransfers(
        address from_,
        address to_,
        uint256 startTokenId_,
        uint256 quantity_
    ) internal override whenNotPaused {
        super._beforeTokenTransfers(from_, to_, startTokenId_, quantity_);
    }

    function _checkPaymentAmount(
        uint256 gameTokenId_,
        uint256 amount_,
        string memory symbol_,
        uint256 paymentAmount_
    ) internal view {
        uint256 tokenPrice;
        if (_useDefaultGameTokenPrice) {
            tokenPrice = _defaultGameTokenPrice;
        } else {
            tokenPrice = getRate(getPriceForGameTokenID(gameTokenId_), symbol_);
        }
        if (tokenPrice * amount_ != paymentAmount_) {
            revert InvalidPaymentAmount();
        }
    }

    /**
     * @dev The denominator with which to interpret the fee set in {_setTokenRoyalty} and {_setDefaultRoyalty} as a
     * fraction of the sale price. Defaults to 1000 (10%) so fees are expressed in basis points.
     */
    function _feeDenominator() internal pure override returns (uint96) {
        return 1000;
    }

    /// @notice Returns baseURI part for GameToken
    /// @dev Returns baseURI part for GameToken
    /// @param gameTokenId_ (uint256) GameToken ID
    /// @param tokenId_ (uint256) Token ID
    /// @return gameTokenURI (string memory) Returns GameToken baseURI part
    function _gameTokenURI(uint256 gameTokenId_, uint256 tokenId_)
        internal
        view
        returns (string memory)
    {
        return
            bytes(baseURI).length > 0
                ? string(
                    abi.encodePacked(
                        baseURI,
                        "games/",
                        gameId.toString(),
                        "/"
                        "gameTokens/",
                        gameTokenId_.toString(),
                        "/",
                        tokenId_.toString()
                    )
                )
                : "";
    }

    /// @notice Return start token ID
    /// @dev Return start token ID
    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    function _transferPaymentToTreasury(
        string memory symbol_,
        uint256 paymentAmount_,
        bool isNative_
    ) internal {
        if (isNative_) {
            (bool success, ) = address(_treasury).call{
                value: paymentAmount_ * (1 ether)
            }("");
            if (!success) {
                revert();
            }
        } else {
            address token = _paymentTokenPerSymbol[symbol_].token;
            ERC20(token).increaseAllowance(address(this), paymentAmount_);
            ERC20(token).transferFrom(
                address(this),
                address(_treasury),
                paymentAmount_
            );
        }
    }

    /// @notice Pause contract
    /// @dev Pause contract
    function pause() external onlyOwner {
        _pause();
        emit Paused();
    }

    /// @notice Unpause contract
    /// @dev Unpause contract
    function unpause() external onlyOwner {
        _unpause();
        emit UnPaused();
    }

    function addPaymentToken(
        address token_,
        address aggregator_,
        bool isNative
    ) external onlyOwner {
        string memory symbol;
        if (isNative) {
            symbol = "MATIC";
            _paymentTokenPerSymbol[symbol].enabled = true;
        } else {
            symbol = ERC20(token_).symbol();
            _paymentTokenPerSymbol[symbol] = PaymentToken(
                token_,
                aggregator_,
                ERC20(token_).decimals(),
                false,
                true
            );
        }
        for (uint256 i; i < _paymentTokenSymbols.length; i++) {
            if (
                keccak256(bytes(_paymentTokenSymbols[i])) ==
                keccak256(bytes(symbol))
            ) {
                return;
            }
        }
        _paymentTokenSymbols.push(symbol);
    }

    function removePaymentToken(string calldata symbol)
        external
        onlyOwner
        whenPaused
    {
        _paymentTokenPerSymbol[symbol].enabled = false;
        uint256 id;
        for (uint256 i; i < _paymentTokenSymbols.length; i++) {
            if (
                keccak256(bytes(_paymentTokenSymbols[i])) ==
                keccak256(bytes(symbol))
            ) {
                id = i;
                break;
            }
        }
        delete _paymentTokenSymbols[id];
    }

    function setPriceForGameTokenID(uint256 gameTokenId_, uint256 price_)
        external
        onlyOwner
    {
        if (_useDefaultGameTokenPrice) {
            revert SetPriceFailed();
        }
        _pricePerGameTokenId[gameTokenId_] = price_;
    }

    function setPriceForGameTokenIDs(
        uint256[] calldata gameTokenIds_,
        uint256 price_
    ) external onlyOwner {
        if (_useDefaultGameTokenPrice) {
            revert SetPriceFailed();
        }
        for (uint256 i; i < gameTokenIds_.length; i++) {
            _pricePerGameTokenId[gameTokenIds_[i]] = price_;
        }
    }

    function setDefaultGameTokenPrice(uint256 price_) external onlyOwner {
        // Setting default token price will not reset price per tokenId
        if (!_useDefaultGameTokenPrice) {
            revert SetPriceFailed();
        }
        _defaultGameTokenPrice = price_;
    }

    function removeDefaultGameTokenPrice() external onlyOwner {
        // Removing default token price will not reset price per tokenId
        if (!_useDefaultGameTokenPrice) {
            revert SetPriceFailed();
        }
        _useDefaultGameTokenPrice = false;
        _defaultGameTokenPrice = 0;
    }

    function setTreasury(address payable newTreasury_)
        external
        onlyOwner
        whenPaused
    {
        if (newTreasury_ == address(0)) {
            revert ZeroAddress();
        }
        _treasury = XG(payable(newTreasury_));
        emit TreasuryChanged(newTreasury_);
    }

    /// @notice Update Game information
    /// @dev Update Game information (gameId)
    /// @param gameId_ (uint256) Game contract gameId
    function updateGame(uint256 gameId_) external onlyOwner whenPaused {
        gameId = gameId_;
        emit GameIDChanged(gameId_);
    }

    function airdropTokens(
        address[] calldata accounts_,
        uint256[] calldata gameTokenIds_,
        uint256[] calldata amounts_,
        uint96[] calldata feeNumerators_
    ) external onlyOwner {
        if (
            accounts_.length != gameTokenIds_.length ||
            accounts_.length != amounts_.length ||
            accounts_.length != feeNumerators_.length
        ) {
            revert InvalidArraySize();
        }
        for (uint256 i; i < accounts_.length; i++) {
            unchecked {
                uint256 currentId = _nextTokenId();
                for (uint256 j; j < amounts_[i]; j++) {
                    _tokenIdPerGameToken[currentId] = gameTokenIds_[i];
                    _setTokenRoyalty(
                        currentId,
                        address(_treasury),
                        feeNumerators_[i]
                    );
                    currentId++;
                }
                _totalSupplyPerGameToken[gameTokenIds_[i]] += amounts_[i];
            }
            _mint(accounts_[i], amounts_[i]);
        }
    }

    function buyTokenWithNativeToken(
        uint256 gameTokenId_,
        uint256 amount_,
        uint96 feeNumerator_
    ) external payable nonReentrant {
        _checkPaymentAmount(gameTokenId_, amount_, "MATIC", msg.value);
        unchecked {
            uint256 currentId = _nextTokenId();
            for (uint256 i; i < amount_; i++) {
                _tokenIdPerGameToken[currentId] = gameTokenId_;
                _setTokenRoyalty(currentId, address(_treasury), feeNumerator_);
                currentId++;
            }
            _totalSupplyPerGameToken[gameTokenId_] += amount_;
        }
        _transferPaymentToTreasury("MATIC", msg.value, true);
        _mint(msg.sender, amount_);
    }

    function buyTokenWithERC20Token(
        uint256 gameTokenId_,
        uint256 amount_,
        uint96 feeNumerator_,
        string memory symbol_,
        uint256 paymentAmount_
    ) external nonReentrant {
        _checkPaymentAmount(gameTokenId_, amount_, symbol_, paymentAmount_);
        unchecked {
            uint256 currentId = _nextTokenId();
            for (uint256 i; i < amount_; i++) {
                _tokenIdPerGameToken[currentId] = gameTokenId_;
                _setTokenRoyalty(currentId, address(_treasury), feeNumerator_);
                currentId++;
            }
            _totalSupplyPerGameToken[gameTokenId_] += amount_;
        }
        _transferPaymentToTreasury(symbol_, paymentAmount_, false);
        _mint(msg.sender, amount_);
    }

    /// @notice Mint `amount_` tokens for `gameTokenId_`
    /// @dev Mint `amount_` tokens
    /// @param to_ (address) Mint address
    /// @param gameTokenId_ (uint256) GameToken ID
    /// @param amount_ (uint256) Token amount to mint
    /// @param feeNumerator_ (uint96) Fee numerator
    function mintToken(
        address to_,
        uint256 gameTokenId_,
        uint256 amount_,
        uint96 feeNumerator_
    ) external onlyOwner {
        unchecked {
            uint256 currentId = _nextTokenId();
            for (uint256 i; i < amount_; i++) {
                _tokenIdPerGameToken[currentId] = gameTokenId_;
                _setTokenRoyalty(currentId, address(_treasury), feeNumerator_);
                currentId++;
            }
            _totalSupplyPerGameToken[gameTokenId_] += amount_;
        }
        _mint(to_, amount_);
    }

    /// @notice Bulk mint `amounts_` tokens for `gameTokenIds_`
    /// @dev Mint `amount_` tokens
    /// @param to_ (address) Mint address
    /// @param gameTokenIds_ (uint256[] calldata) GameToken IDs
    /// @param amounts_ (uint256[] calldata) Token amounts to mint
    /// @param feeNumerators_ (uint96[] calldata) Fee numerators
    function bulkMintToken(
        address to_,
        uint256[] calldata gameTokenIds_,
        uint256[] calldata amounts_,
        uint96[] calldata feeNumerators_
    ) external onlyOwner {
        if (
            gameTokenIds_.length != amounts_.length ||
            gameTokenIds_.length != feeNumerators_.length
        ) {
            revert InvalidArraySize();
        }
        uint256 totalAmount;
        unchecked {
            uint256 currentId = _nextTokenId();
            for (uint256 i; i < gameTokenIds_.length; i++) {
                for (uint256 j; j < amounts_[i]; j++) {
                    _tokenIdPerGameToken[currentId] = gameTokenIds_[i];
                    _setTokenRoyalty(
                        currentId,
                        address(_treasury),
                        feeNumerators_[i]
                    );
                    currentId++;
                }
                _totalSupplyPerGameToken[gameTokenIds_[i]] += amounts_[i];
                totalAmount += amounts_[i];
            }
        }
        _mint(to_, totalAmount);
    }

    /// @notice Burn token `id_`
    /// @dev Burn token `id_`
    /// @param id_ (uint256) Token ID
    function burnToken(uint256 id_) external onlyOwner {
        _burn(id_);
        _resetTokenRoyalty(id_);
    }

    /// @notice Burn tokens `ids_`
    /// @dev Burn token `id_`
    /// @param ids_ (uint256[] calldata) Token ID
    function burnBatchToken(uint256[] calldata ids_) external onlyOwner {
        unchecked {
            for (uint256 i; i < ids_.length; i++) {
                _burn(ids_[i]);
                _resetTokenRoyalty(ids_[i]);
            }
        }
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721A, ERC2981)
        returns (bool)
    {
        return
            ERC721A.supportsInterface(interfaceId) ||
            ERC2981.supportsInterface(interfaceId);
    }

    /// @notice Opensea contract-level metadata
    /// @dev Return contract URI for Opensea
    /// @return contractURI (string memory) Returns contract URI
    function contractURI() public view returns (string memory) {
        return
            bytes(baseURI).length > 0
                ? string(abi.encodePacked(baseURI, "games/", gameId.toString()))
                : "";
    }

    function contractVersion() external pure returns (string memory) {
        return "1.0.1";
    }

    function getPaymentToken(string memory symbol_)
        public
        view
        returns (PaymentToken memory)
    {
        if (!_paymentTokenPerSymbol[symbol_].enabled) {
            revert TokenNotEnabled();
        }
        return _paymentTokenPerSymbol[symbol_];
    }

    function getPaymentTokens() public view returns (string[] memory symbols) {
        uint256 index;
        for (uint256 i; i < _paymentTokenSymbols.length; i++) {
            if (_paymentTokenPerSymbol[_paymentTokenSymbols[i]].enabled) {
                symbols[index] = _paymentTokenSymbols[i];
                index++;
            }
        }
        return symbols;
    }

    function getPaymentTokensLength() public view returns (uint256 length) {
        for (uint256 i; i < _paymentTokenSymbols.length; i++) {
            if (_paymentTokenPerSymbol[_paymentTokenSymbols[i]].enabled) {
                length++;
            }
        }
    }

    function getDefaultGameTokenPrice() public view returns (uint256) {
        return _defaultGameTokenPrice;
    }

    function getPriceForGameTokenID(uint256 gameTokenId_)
        public
        view
        returns (uint256)
    {
        if (_useDefaultGameTokenPrice) {
            return _defaultGameTokenPrice;
        }
        return _pricePerGameTokenId[gameTokenId_];
    }

    function getRate(uint256 usdAmount_, string memory symbol_)
        public
        view
        returns (uint256 rate)
    {
        if (!_paymentTokenPerSymbol[symbol_].enabled) {
            revert TokenNotEnabled();
        }
        PaymentToken memory paymentToken = _paymentTokenPerSymbol[symbol_];
        AggregatorV3Interface priceFeed = AggregatorV3Interface(
            paymentToken.aggregator
        );
        (, int256 price, , , ) = priceFeed.latestRoundData();
        rate = usdAmount_ * uint256(price);
    }

    // @notice Returns total supply per GameToken
    /// @dev Returns total supply per GameToken
    /// @param gameTokenId_ (uint256) GameToken ID
    /// @return _totalSupplyPerGameToken (uint256) Returns total supply per GameToken
    function totalSupplyGameToken(uint256 gameTokenId_)
        public
        view
        returns (uint256)
    {
        return _totalSupplyPerGameToken[gameTokenId_];
    }

    /// @notice Return tokenURI for `tokenId_`
    /// @dev Return tokenURI for `tokenId_`
    /// @param tokenId_ (uint256) Token ID
    /// @return tokenURI (string memory) Returns tokenURI
    function tokenURI(uint256 tokenId_)
        public
        view
        override
        returns (string memory)
    {
        if (!_exists(tokenId_)) {
            revert TokenNotExist();
        }
        uint256 gameTokenId = _tokenIdPerGameToken[tokenId_];
        return _gameTokenURI(gameTokenId, tokenId_);
    }
}

