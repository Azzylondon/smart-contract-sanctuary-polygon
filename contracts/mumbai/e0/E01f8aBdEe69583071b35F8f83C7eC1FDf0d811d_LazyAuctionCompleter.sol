pragma solidity ^0.8.4;

library Shared {
    struct Spend {
        address asset;
        uint256 amount;
        address to;
        address from;
        uint256 deadline;
        uint256 nonce;
    }

    struct LazyAuction {
        uint256 id;
        string tokenURI;
        Spend spendData;
    }

    struct Auction {
        uint256 id;
        uint256 tokenId;
        Spend spendData;
    }

    string constant SPEND_TYPE =
        "Spend(address asset,uint256 amount,address to,address from,uint256 deadline,uint256 nonce)";
    bytes32 constant SPEND_TYPEHASH = keccak256(abi.encodePacked(SPEND_TYPE));

    string constant LAZY_AUCTION_TYPE =
        "LazyAuction(uint256 id,string tokenURI,Spend spendData)";
    bytes32 constant LAZY_AUCTION_TYPEHASH =
        keccak256(abi.encodePacked(LAZY_AUCTION_TYPE, SPEND_TYPE));

    string constant AUCTION_TYPE =
        "Auction(uint256 id,uint256 tokenId,Spend spendData)";
    bytes32 constant AUCTION_TYPEHASH =
        keccak256(abi.encodePacked(AUCTION_TYPE, SPEND_TYPE));

    function hashSpend(Shared.Spend memory _spend)
        internal
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encode(
                    SPEND_TYPEHASH,
                    _spend.asset,
                    _spend.amount,
                    _spend.to,
                    _spend.from,
                    _spend.deadline,
                    _spend.nonce
                )
            );
    }

    function hashLazyAuction(Shared.LazyAuction memory _lazyAuction)
        internal
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encode(
                    LAZY_AUCTION_TYPEHASH,
                    _lazyAuction.id,
                    keccak256(abi.encodePacked(_lazyAuction.tokenURI)),
                    hashSpend(_lazyAuction.spendData)
                )
            );
    }

    function hashAuction(Shared.Auction memory _auction)
        internal
        pure
        returns (bytes32)
    {
        return
            keccak256(
                abi.encode(
                    AUCTION_TYPEHASH,
                    _auction.id,
                    _auction.tokenId,
                    hashSpend(_auction.spendData)
                )
            );
    }

    event Complete(
        address indexed seller,
        address indexed buyer,
        uint256 indexed id,
        address asset,
        uint256 amount,
        uint256 nftId
    );
}

pragma solidity ^0.8.4;

import "@gnosis.pm/zodiac/contracts/core/Module.sol";

/// @title A Module that enables minting ERC721 tokens via the owner
abstract contract Minter is Module {
  address public nft;

  constructor(address _nft){
    nft = _nft;
  }

  /// @notice mint an ERC721 token
  /// @dev Call safeMint on the ERC721 contract on behalf of the owner
  /// @dev Must revert if the mint call failed
  /// @param _to The address to mint the token to
  /// @param _tokenURI The token metaddata URI
  function _mint(
    address _to,
    string memory _tokenURI
  )
  internal
  returns (uint256)
  {
    (bool success, bytes memory returnData) = execAndReturnData(
      nft,
      0,
      abi.encodeWithSignature('safeMint(address,string)', _to, _tokenURI),
      Enum.Operation.Call
    );

    require(success, 'NFT Mint failed');

    uint256 id;
    assembly {
      id := mload(add(returnData, 0x20))
    }

    return id;
  }
}

