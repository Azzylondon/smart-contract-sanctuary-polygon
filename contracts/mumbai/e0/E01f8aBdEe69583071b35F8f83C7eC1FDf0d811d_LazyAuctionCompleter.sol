import "../Shared.sol";

/// @title A Spender Module that enables spending from a vault  
abstract contract Spender is Module {
  // the address of the vault this module will spend from
  address public vault;

  constructor(address _vault){
    vault = _vault;
  }

  /// @notice Call the spend function on the vault on behalf of the vault owner
  /// @dev This just forwards the call to the vault - the responsibiltiy is on the 
  // vault for verifing the signed spend message 
  /// @dev Must revert if the forwarded function call was unsuccessful 
  /// @param _spendData The data for the signed Spend message 
  /// @param _signature An EIP-712 signature of the Spend message signed by the spender
  function _spend(
    Shared.Spend memory _spendData,
    bytes memory _signature
  ) 
  internal
  {
   (bool success) = exec(
        vault,
        0,
        abi.encodeWithSignature(
          'spend((address,uint256,address,address,uint256,uint256),bytes)',
          _spendData,
          _signature
        ),
        Enum.Operation.Call
      );
    require(success, 'Spend failed');
  }
}

