/// @title A Module implementing Minter and Spender that enables anyone to complete an auction
// and transfer the fungible + non-fungible tokens
contract LazyAuctionCompleter is Spender, Minter, EIP712 {
    mapping(uint256 => bool) public isAuctionComplete;

    /// @param _owner The owner of the ERC721 contract
    /// @param _vault The address of the vault the assets will be transferred from
    /// @param _nft The address of the ERC721 contract
    constructor(
        address _owner,
        address _vault,
        address _nft
    ) EIP712("LazyAuction", "1") Minter(_nft) Spender(_vault) {
        bytes memory initializeParams = abi.encode(_owner, _vault, _nft);
        setUp(initializeParams);
    }

    // - the sender
    /// @dev Mints a NFT and spends some staked asset from the vault
    /// @param _spendSignature An EIP-712 signature of the spend message signed by the spender
    /// @param _auctionSignature An EIP-712 signature of the LazyAuction message signed by either the seller or the owner
    function complete(
        Shared.LazyAuction calldata _lazyAuction,
        bytes calldata _spendSignature,
        bytes calldata _auctionSignature
    ) external {
        require(!isAuctionComplete[_lazyAuction.id], "Auction is complete");

        bool isValidSignature = 
            SignatureChecker.isValidSignatureNow(
                owner(),
                _hashTypedDataV4(Shared.hashLazyAuction(_lazyAuction)),
                _auctionSignature
            );
            // ||
            // // TODO: maybe remove this for now
            // SignatureChecker.isValidSignatureNow(
            //     _lazyAuction.spendData.to,
            //     _hashTypedDataV4(Shared.hashLazyAuction(_lazyAuction)),
            //     _auctionSignature
            // );

        // TODO: also check that the auction signature was signed by the spender
        // otherwise NOIZD / the seller could use the spend signature to complete any auction with the same price

        require(isValidSignature, "invalid auction signature");

        isAuctionComplete[_lazyAuction.id] = true;

        // do transfer
        _spend(_lazyAuction.spendData, _spendSignature);

        // do mint
        uint256 nftId = _mint(_lazyAuction.spendData.to, _lazyAuction.tokenURI);

        emit Shared.Complete(
            _lazyAuction.spendData.from,
            _lazyAuction.spendData.to,
            _lazyAuction.id,
            _lazyAuction.spendData.asset,
            _lazyAuction.spendData.amount,
            nftId
        );
    }

    /// @dev Initialize function, will be triggered when a new proxy is deployed
    /// @param initializeParams Parameters of initialization encoded
    function setUp(bytes memory initializeParams) public override initializer {
        __Ownable_init();
        (address _owner, , ) = abi.decode(
            initializeParams,
            (address, address, address)
        );

        // Avatar: is the Safe. The Address that will ultimately execute the transaction that is passed by the module.
        setAvatar(_owner);
        // Target: is the address that this module will call execTransactionFromModule() on.
        // In most cases this will be the Safe, but in some cases it could be a special kind of module called a modifier,
        // that sits between a module and an avatar and... well... modifies the transactions passed to it in some way.
        setTarget(_owner);
        transferOwnership(_owner);
    }

}

