interface Paths {
    function getPaths(string memory, string memory)
        external
        pure
        returns (string memory);
}

contract Banner is ERC721Enumerable {
    uint256 public tokenCounter;
    uint256 public priceUSD;

    struct svgParams {
        string text;
        string bodyColor;
        string backgroundColor;
        string textColor;
        string bannerColor;
    }
    mapping(string => bool) check;
    mapping(uint256 => svgParams) tokenParams;

    address oracleAddress;
    address pathsAddress;

    address public owner1;
    address public owner2;
    address public owner3;
    address public owner1SetOwner2;
    address public owner1SetOwner3;
    address public owner2SetOwner1;
    address public owner2SetOwner3;
    address public owner3SetOwner1;
    address public owner3SetOwner2;

    constructor() ERC721("BANNER ON CHAIN TEST V1", "BANNER") {
        tokenCounter = 1;
        priceUSD = 1;

        owner1 = msg.sender;
        owner2 = address(0x2f1751F413F371D85A9497DeC859E92fBD44ec67);
        owner3 = address(0x5Aaa2662D5BD4f3e16b4Feee444d0Cfc08e970B7);
        owner1SetOwner2 = owner2;
        owner1SetOwner3 = owner3;
        owner2SetOwner1 = owner1;
        owner2SetOwner3 = owner3;
        owner3SetOwner1 = owner1;
        owner3SetOwner2 = owner2;
    }

    function setPathAddress(address _pathsAddress) public onlyOwner1 {
        pathsAddress = _pathsAddress;
    }

    function setOracleAddress(address _oracleAddress) public onlyOwner1 {
        oracleAddress = _oracleAddress;
    }

    function getPathAddress() public view returns (address) {
        return pathsAddress;
    }

    function getOracleAddress() public view returns (address) {
        return oracleAddress;
    }

    function getPrice()
        public
        view
        returns (
            uint256,
            uint256,
            uint256
        )
    {
        (, int256 answer, , , ) = AggregatorV3Interface(oracleAddress)
            .latestRoundData();
        uint256 price = 1000000000000000000 / uint256(answer);
        price = price * 100000000;
        uint256 priceMin = price - (price / 10);
        uint256 priceMax = price + (price / 10);
        return (price, priceMin, priceMax);
    }

    function mint(
        string memory _text,
        string memory _bodyColor,
        string memory _backgroundColor,
        string memory _textColor,
        string memory _bannerColor
    ) public payable {
        (, uint256 priceMin, uint256 priceMax) = getPrice();
        require(
            (msg.value > priceMin) && (msg.value < priceMax),
            "wrong payment"
        );
        require((bytes(_text).length > 0), "minumum 1 character");
        require((bytes(_text).length < 16), "maximum 15 character");
        require(check_char(_text) == true, "illegal character");
        require(check[_text] == false, "already minted");

        _safeMint(msg.sender, tokenCounter);
        check[_text] = true;
        tokenParams[tokenCounter].text = _text;
        tokenParams[tokenCounter].bodyColor = _bodyColor;
        tokenParams[tokenCounter].backgroundColor = _backgroundColor;
        tokenParams[tokenCounter].bannerColor = _bannerColor;
        tokenParams[tokenCounter].textColor = _textColor;
        tokenCounter++;
    }

    function ownedBanners(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++)
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        return tokenIds;
    }

    function bannerParams(uint256 _tokenId)
        public
        view
        returns (svgParams memory)
    {
        return tokenParams[_tokenId];
    }

    function frontend_check(string memory _word) public view returns (bool) {
        return check[_word];
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721)
        returns (string memory)
    {
        require(
            (tokenId < tokenCounter) && (tokenId > 0),
            "None Existing Token"
        );
        return banner_metadata(tokenId);
    }

    function banner_metadata(uint256 _tokenId)
        public
        view
        returns (string memory)
    {
        string memory base = "data:application/json;base64,";
        return
            string(
                abi.encodePacked(
                    base,
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name": "',
                                "Banner #",
                                c_t(_tokenId),
                                '", ',
                                '"description": "Banner On Chain',
                                '", ',
                                '"attributes": "", ',
                                '"image": "',
                                get(_tokenId),
                                '"}'
                            )
                        )
                    )
                )
            );
    }

    function get(uint256 _tokenId) public view returns (string memory _title) {
        _title = string(
            abi.encodePacked(
                "data:image/svg+xml;base64,",
                Base64.encode(
                    bytes(
                        string(
                            abi.encodePacked(
                                title(_tokenId),
                                path(_tokenId),
                                text(_tokenId)
                            )
                        )
                    )
                )
            )
        );
    }

    function text(uint256 _tokenId) public view returns (string memory _title) {
        uint256 x = 180;
        uint256 y = 190;
        uint256 px = 250;

        if (bytes(tokenParams[_tokenId].text).length == 1) {
            x = 180;
            if (
                ((bytes(string(abi.encodePacked(tokenParams[_tokenId].text)))[
                    0
                ] >= 0x30) &&
                    (bytes(
                        string(abi.encodePacked(tokenParams[_tokenId].text))
                    )[0] <= 0x39))
            ) {
                y = 200;
            } else {
                y = 190;
            }
            if (
                bytes(string(abi.encodePacked(tokenParams[_tokenId].text)))[
                    0
                ] == 0x51
            ) {
                y = 180;
                px = 230;
            } else {
                px = 250;
            }
        }

        if (bytes(tokenParams[_tokenId].text).length == 2) {
            if (
                ((bytes(string(abi.encodePacked(tokenParams[_tokenId].text)))[
                    0
                ] == 0x51) ||
                    (bytes(
                        string(abi.encodePacked(tokenParams[_tokenId].text))
                    )[1] == 0x51))
            ) {
                x = 130;
                y = 180;
                px = 200;
            } else {
                x = 120;
                y = 190;
                px = 220;
            }
        }
        if (bytes(tokenParams[_tokenId].text).length == 3) {
            x = 120;
            y = 170;
            px = 145;
        }
        if (bytes(tokenParams[_tokenId].text).length == 4) {
            x = 120;
            y = 150;
            px = 110;
        }
        if (bytes(tokenParams[_tokenId].text).length == 5) {
            x = 120;
            y = 150;
            px = 90;
        }
        if (bytes(tokenParams[_tokenId].text).length == 6) {
            x = 120;
            y = 140;
            px = 73;
        }
        if (bytes(tokenParams[_tokenId].text).length == 7) {
            x = 120;
            y = 140;
            px = 63;
        }
        if (bytes(tokenParams[_tokenId].text).length == 8) {
            x = 120;
            y = 140;
            px = 55;
        }
        if (bytes(tokenParams[_tokenId].text).length == 9) {
            x = 120;
            y = 140;
            px = 50;
        }
        if (bytes(tokenParams[_tokenId].text).length == 10) {
            x = 118;
            y = 140;
            px = 45;
        }
        if (bytes(tokenParams[_tokenId].text).length == 11) {
            x = 120;
            y = 140;
            px = 40;
        }
        if (bytes(tokenParams[_tokenId].text).length == 12) {
            x = 120;
            y = 135;
            px = 37;
        }
        if (bytes(tokenParams[_tokenId].text).length == 13) {
            x = 120;
            y = 130;
            px = 34;
        }
        if (bytes(tokenParams[_tokenId].text).length == 14) {
            x = 118;
            y = 130;
            px = 32;
        }
        if (bytes(tokenParams[_tokenId].text).length == 15) {
            x = 118;
            y = 130;
            px = 30;
        }
        _title = string(
            abi.encodePacked(
                '<text x="',
                c_t(x),
                '" y="',
                c_t(y),
                '" font-size="',
                c_t(px),
                'px" fill="',
                tokenParams[_tokenId].textColor,
                '" font-family= "Courier New" transform="rotate(1 50,40)" >',
                tokenParams[_tokenId].text,
                "</text></svg>"
            )
        );
    }

    function path(uint256 _tokenId) public view returns (string memory _paths) {
        _paths = Paths(pathsAddress).getPaths(
            tokenParams[_tokenId].bodyColor,
            tokenParams[_tokenId].bannerColor
        );
    }

    function title(uint256 _tokenId)
        public
        view
        returns (string memory _title)
    {
        _title = string(
            abi.encodePacked(
                '<svg version="1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 453.5 453.5" enable-background="new 0 0 453.5 453.5"><style type="text/css" >.st0{fill:#EF4136;} .st1{fill:#E8E5D1;} </style><path fill="',
                tokenParams[_tokenId].backgroundColor,
                '" d="M0,0h453.5v453.5H0V0z"/>'
            )
        );
    }

    modifier onlyOwners() {
        require(
            (msg.sender == owner1) ||
                (msg.sender == owner2) ||
                (msg.sender == owner3),
            "only owners"
        );
        _;
    }

    modifier onlyOwner1() {
        require(msg.sender == owner1, "only owner1");
        _;
    }

    modifier onlyOwner2() {
        require(msg.sender == owner2, "only owner2");
        _;
    }
    modifier onlyOwner3() {
        require(msg.sender == owner3, "only owner3");
        _;
    }

    function setPriceUSD(uint256 _newPrice) public onlyOwners {
        priceUSD = _newPrice;
    }

    function withdraw() public payable onlyOwners {
        uint256 contractBalance = address(this).balance;
        uint256 share = contractBalance / 3;
        payable(owner1).transfer(share);
        payable(owner2).transfer(share);
        payable(owner3).transfer(share);
    }

    function owner1NewSetOwner2(address _newOwner2) public onlyOwner1 {
        owner1SetOwner2 = _newOwner2;
    }

    function owner1NewSetOwner3(address _newOwner3) public onlyOwner1 {
        owner1SetOwner3 = _newOwner3;
    }

    function owner2NewSetOwner1(address _newOwner1) public onlyOwner2 {
        owner2SetOwner1 = _newOwner1;
    }

    function owner2NewSetOwner3(address _newOwner3) public onlyOwner2 {
        owner2SetOwner3 = _newOwner3;
    }

    function owner3NewSetOwner1(address _newOwner1) public onlyOwner3 {
        owner3SetOwner1 = _newOwner1;
    }

    function owner3NewSetOwner2(address _newOwner2) public onlyOwner3 {
        owner3SetOwner2 = _newOwner2;
    }

    function changeOwner() public onlyOwners {
        if (owner2SetOwner1 == owner3SetOwner1) {
            owner1 = owner2SetOwner1;
        }
        if (owner1SetOwner2 == owner3SetOwner2) {
            owner2 = owner1SetOwner2;
        }
        if (owner1SetOwner3 == owner2SetOwner3) {
            owner3 = owner1SetOwner3;
        }
    }

    function check_char(string memory _text) public pure returns (bool) {
        for (uint256 i = 0; i < bytes(_text).length; i++) {
            if (
                ((bytes(string(abi.encodePacked(_text)))[i] >= 0x30) &&
                    (bytes(string(abi.encodePacked(_text)))[i] <= 0x39)) ||
                ((bytes(string(abi.encodePacked(_text)))[i] >= 0x41) &&
                    (bytes(string(abi.encodePacked(_text)))[i] <= 0x5A))
            ) {
                //pass
            } else {
                return false;
            }
        }
        return true;
    }

    function c_t(uint256 _i)
        internal
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }
}

