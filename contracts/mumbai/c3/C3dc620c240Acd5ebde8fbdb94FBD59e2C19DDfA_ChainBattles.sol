    function getInfoOfCharacter(uint tokenId)public view returns(string memory,string memory){
        Character memory character = tokenToCharacter[tokenId];
        return (character.name, character.level.toString());
    }
    function getTokenURI(uint256 tokenId) public view returns (string memory){
    bytes memory dataURI = abi.encodePacked(
        '{',
            '"name": "Chain Battles #', tokenId.toString(), '",',
            '"description": "Battles on chain game",',
            '"image": "', generateCharacter(tokenId), '",',
            '"clan":"Saint Lou"'
        '}'
    );
    return string(
        abi.encodePacked(
            "data:application/json;base64,",
            Base64.encode(dataURI)
        )
    );
    }
    function mint(string memory name) public{
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        _safeMint(msg.sender,newItemId);
        Character memory character = Character(name,0);
        tokenToCharacter[newItemId] = character;
        _setTokenURI(newItemId,getTokenURI(newItemId));
    }
    function train(uint256 tokenId) public {
        require(_exists(tokenId),"Please use an existing Token");
        require(msg.sender == ownerOf(tokenId), "You must own this Token to train it");
        Character memory ch = tokenToCharacter[tokenId];
        uint256 newLevel = ch.level + 1;
        tokenToCharacter[tokenId] = Character(ch.name,newLevel);
        _setTokenURI(tokenId,getTokenURI(tokenId));
    }
    function updateName(string memory _name, uint256 tokenId) public{
		require(_exists(tokenId),"Please use an existing Token");
        require(msg.sender == ownerOf(tokenId), "You must own this Token to train it");
		Character memory ch = tokenToCharacter[tokenId];
		tokenToCharacter[tokenId] = Character(_name,ch.level);
		_setTokenURI(tokenId,getTokenURI(tokenId));
	}
    function setNewOffer(uint256 tokenId) public payable{
        require(msg.value > 0,"Inssuficent amount");
        uint256 oldAmount = tokenToAddrestoOffert[tokenId][msg.sender].amount;
        Offert memory _offert = Offert(msg.value + oldAmount,block.timestamp);
        if(oldAmount == 0){
            ofertsOfToken[tokenId].push(msg.sender);
        }
        tokenToAddrestoOffert[tokenId][msg.sender] = _offert;
    }
    function transferToken(uint256 tokenId, address buyer)external{
        require(msg.sender == ownerOf(tokenId));
        uint256 _amount = tokenToAddrestoOffert[tokenId][buyer].amount;
        (bool itOk,) = payable(ownerOf(tokenId)).call{value:_amount}("");
        require(itOk, "There were problems making the transaction");
        safeTransferFrom(msg.sender, buyer, tokenId);
    }
    function withrowMyOffert(uint tokenId)external {
        uint256 _amount = tokenToAddrestoOffert[tokenId][msg.sender].amount;
        require(_amount > 0 , "Insufficient amount");
        (bool itOk,) = payable(msg.sender).call{value:_amount}("");
        require(itOk, "There were problems making the transaction");
        tokenToAddrestoOffert[tokenId][msg.sender].amount = 0;
        address[] memory _address = ofertsOfToken[tokenId];
        for(uint256 i = 0; i < _address.length;i++){
            if(_address[i] == msg.sender){
                delete _address[i];
            }
        }
        ofertsOfToken[tokenId] = _address;
    }
    function getListOfertsByToken(uint256 tokenId)external view returns(address[] memory){
        return ofertsOfToken[tokenId];
    }
}

