pragma solidity ^0.8.4;

import './token/IERC1155.sol';
import './security/Pausable.sol';
import './token/IERC1155TokenReceiver.sol';
import 'hardhat/console.sol';

contract NFTCoreAuction is Pausable, IERC1155TokenReceiver {
    bytes4 constant MAGIC_ON_ERC1155_RECEIVED = 0xf23a6e61;
    bytes4 constant MAGIC_ON_ERC1155_BATCH_RECEIVED = 0xbc197c81;

    // Cut owner takes on each auction, measured in basis points (1/100 of a percent).
    // Values 0-10,000 map to 0%-100%
    uint256 public constant ownerCut = 9475;

    struct Auction {
        // Index of auction
        uint128 id;
        // Contract address
        address nftAddress;
        // Token id
        uint256 tokenId;
        // Current owner of NFT
        address owner;
        // Price (in wei) at beginning of auction
        uint128 price;
        // Selling or not
        bool status;
        // sell order or buy order : true means sell , false means buy
        bool isSell;
    }

    Auction[] auctions;
    /// index of the auction
    uint256 auctionCount;
    /// Mapping from id to index of Oreders.
    mapping(uint256 => mapping(address => uint256)) idToIndex;

    //nft address -> token id -> seller
    mapping(address => mapping(uint256 => mapping(address => Auction))) public auction;
    mapping(address => bool) public permission;

    modifier isPermitted(address addr) {
        require(permission[addr], "Contract hasn't permitted");
        _;
    }

    event AuctionCreated(
        uint256 indexed _index,
        address indexed _nftAddress,
        uint256 indexed _tokenId,
        address _owner,
        uint256 _price
    );

    event AuctionCancelled(address indexed _nftAddress, uint256 indexed _tokenId, address indexed _owner);
    event AuctionSuccessful(
        address indexed _nftAddress,
        uint256 indexed _tokenId,
        uint256 _totalPrice,
        address _bidder,
        bool isSell
    );

    function setPermission(address addr, bool permitted) external onlySuperAdmin {
        permission[addr] = permitted;
    }

    ///@dev get all exit auctions
    function getAllExistAuctions() external view returns (Auction[] memory) {
        return auctions;
    }

    ///@dev get auction details
    ///@param _tokenId token id
    ///@param _owner seller address
    function getAuction(
        address _nftAddress,
        uint256 _tokenId,
        address _owner
    ) public view returns (Auction memory) {
        return auction[_nftAddress][_tokenId][_owner];
    }

    ///@dev create auction , notice that one just one nft can be placed on auction, even seller have more than one
    ///@param _nftAddress nft contract address
    ///@param _tokenId token id
    ///@param _price the price seller want to sell
    function createAuction(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _price
    ) external whenNotPaused isPermitted(_nftAddress) {
        address _seller = msg.sender;
        require(_owns(_nftAddress, _seller, _tokenId), 'Not Own The Token');
        Auction memory _beforeAuction = auction[_nftAddress][_tokenId][msg.sender];
        require(_beforeAuction.status == false, 'Is On Auction');
        Auction memory _auction = Auction(
            uint128(auctionCount),
            _nftAddress,
            _tokenId,
            _seller,
            uint128(_price),
            true,
            true
        );

        _addAuction(_auction, auctionCount);
        _escrow(_nftAddress, _tokenId, _seller);
    }

    function bidAuction(
        address _nftAddress,
        uint256 _tokenId,
        address _seller
    ) external payable whenNotPaused isPermitted(_nftAddress) {
        Auction memory _auction = auction[_nftAddress][_tokenId][_seller];
        require(msg.sender != _auction.owner, 'Bidder and seller should not be the same');

        _bid(_auction, msg.value);
        _transfer(_nftAddress, _tokenId, address(this), msg.sender);
    }

    ///@dev create buy auction, notice that user should send the Matic when call this function
    ///@param _nftAddress nft contract address
    ///@param _tokenId token id
    ///@param _price the price buyer want to buy
    function createBuyAuction(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _price
    ) external payable whenNotPaused isPermitted(_nftAddress) {
        Auction memory _beforeAuction = auction[_nftAddress][_tokenId][msg.sender];
        require(_beforeAuction.status == false, 'Is On Auction');
        require(_price > 0, 'Price Should Not Be 0 ');
        require(msg.value >= _price, 'Not Enough Money');
        Auction memory _auction = Auction(
            uint128(auctionCount),
            _nftAddress,
            _tokenId,
            msg.sender,
            uint128(_price),
            true,
            false
        );

        _addAuction(_auction, auctionCount);
    }

    function bidBuyAuction(
        address _nftAddress,
        uint256 _tokenId,
        address _buyer
    ) external payable whenNotPaused isPermitted(_nftAddress) {
        require(_owns(_nftAddress, msg.sender, _tokenId), 'Not Own The Token');
        Auction memory _auction = auction[_nftAddress][_tokenId][_buyer];
        require(msg.sender != _auction.owner, 'Token seller and buyer should not be the same');

        _bidBuy(_auction);
        _transfer(_nftAddress, _tokenId, msg.sender, _auction.owner);
    }

    function cancelAuction(address _nftAddress, uint256 _tokenId) external whenNotPaused isPermitted(_nftAddress) {
        Auction memory _auction = auction[_nftAddress][_tokenId][msg.sender];
        require(_isOnAuction(_auction), 'Not On Auction');
        require(msg.sender == _auction.owner);
        _cancelAuction(_auction);
    }

    function _owns(
        address _nftAddress,
        address _claimant,
        uint256 _tokenId
    ) private view returns (bool) {
        IERC1155 _nftContract = IERC1155(_nftAddress);
        return (_nftContract.balanceOf(_claimant, _tokenId) >= 1);
    }

    function _addAuction(Auction memory _auction, uint256 _id) internal {
        uint256 indexToAuctions = auctions.length;
        idToIndex[_id][_auction.owner] = indexToAuctions;

        auction[_auction.nftAddress][_auction.tokenId][_auction.owner] = _auction;
        auctions.push(_auction);
        auctionCount++;
        emit AuctionCreated(_id, _auction.nftAddress, _auction.tokenId, _auction.owner, _auction.price);
    }

    function _escrow(
        address _nftAddress,
        uint256 _tokenId,
        address _owner
    ) private {
        IERC1155 _nftContract = IERC1155(_nftAddress);
        _nftContract.safeTransferFrom(_owner, address(this), _tokenId, 1, '0x');
    }

    function _isOnAuction(Auction memory _auction) internal pure returns (bool) {
        return (_auction.status == true);
    }

    function _cancelAuction(Auction memory _auction) internal {
        _removeAuction(_auction);
        _transfer(_auction.nftAddress, _auction.tokenId, address(this), _auction.owner);
        emit AuctionCancelled(_auction.nftAddress, _auction.tokenId, _auction.owner);
    }

    function _transfer(
        address _nftAddress,
        uint256 _tokenId,
        address _from,
        address _to
    ) internal {
        IERC1155 _nftContract = IERC1155(_nftAddress);
        _nftContract.safeTransferFrom(_from, _to, _tokenId, 1, '0x');
    }

    function _bid(Auction memory _auction, uint256 _bidAmount) internal {
        require(_isOnAuction(_auction), 'Not On Auction');
        uint256 _price = _auction.price;
        require(_bidAmount >= _price, 'Bid is lower than the price');

        address payable _seller = payable(_auction.owner);
        _removeAuction(_auction);
        if (_price > 0) {
            uint256 _sellerProceeds = _computeCut(_price);
            _seller.transfer(_sellerProceeds);
        }
        if (_bidAmount > _price) {
            uint256 _bidExcess = _bidAmount - _price;
            payable(msg.sender).transfer(_bidExcess);
        }
        emit AuctionSuccessful(_auction.nftAddress, _auction.tokenId, _price, msg.sender, true);
    }

    function _bidBuy(Auction memory _auction) internal {
        require(_isOnAuction(_auction), 'Not On Auction');
        uint256 _price = _auction.price;

        address payable _seller = payable(msg.sender);
        _removeAuction(_auction);
        if (_price > 0) {
            uint256 _sellerProceeds = _computeCut(_price);
            _seller.transfer(_sellerProceeds);
        }

        emit AuctionSuccessful(_auction.nftAddress, _auction.tokenId, _price, msg.sender, false);
    }

    function _computeCut(uint256 _price) internal pure returns (uint256) {
        return (_price * ownerCut) / 10000;
    }

    function _removeAuction(Auction memory _auction) internal {
        delete auction[_auction.nftAddress][_auction.tokenId][_auction.owner];

        uint256 lastIndex = auctions.length - 1;
        uint256 targetIndex = idToIndex[_auction.id][_auction.owner];
        Auction memory lastAuction = auctions[lastIndex];
        // delete in auctions
        auctions[targetIndex] = lastAuction;
        auctions.pop();
        //change idToIndex
        uint256 lastAuctionId = lastAuction.id;
        address lastOwner = lastAuction.owner;
        idToIndex[lastAuctionId][lastOwner] = targetIndex;
        delete idToIndex[_auction.id][_auction.owner];
    }

    function onERC1155Received(
        address _operator,
        address _from,
        uint256 _id,
        uint256 _value,
        bytes calldata _data
    ) external override returns (bytes4) {
        return MAGIC_ON_ERC1155_RECEIVED;
    }

    function onERC1155BatchReceived(
        address _operator,
        address _from,
        uint256[] calldata _ids,
        uint256[] calldata _values,
        bytes calldata _data
    ) external override returns (bytes4) {
        return MAGIC_ON_ERC1155_BATCH_RECEIVED;
    }
}

