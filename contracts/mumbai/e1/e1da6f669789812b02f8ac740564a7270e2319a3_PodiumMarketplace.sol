pragma solidity >=0.8.4;

interface IPodiumMarketplace {

	// ------------------------------------------------------------------------
	// Events emitted

	// Emitted when a new order is created
	event NewOrder(
		OrderType orderType,
		bool initialOffering,
		address indexed seller, 
		uint256 indexed tokenId,
		uint256 unitPrice,
		uint256 amount,
		uint256 listDuration
	);

	// Emitted when a order is deleted
	event OrderCanceled(
		OrderType orderType,
		address indexed owner, 
		uint256 indexed tokenId
	);

	// Emitted when a token is sold
	event TokenSale(
		address indexed seller, 
		address indexed buyer, 
		uint256 indexed tokenId, 
		uint256 amount, 
		uint256 totalPrice,
		bool isListing
	);

	// Emitted when balance is withdrawn from contract
	event WithdrawBalance(address indexed caller, uint256 amount);

	// Emitted when balance changes
	event BalanceChange(
		address indexed creator,
		address indexed feeTaker,
		address indexed seller,
		uint256 creatorAmount,
		uint256 feeTakerAmount,
		uint256 sellerAmount
	);

	// Emitted when a new escrow action is created (put into escrow)
	event NewEscrow(
		address indexed previousOwner,
		address indexed currentOwner,
		uint256 amount,
		uint256 indexed tokenId
	);

	// Emitted when multiple escrows are released
	event ReleaseEscrows(
		uint256 indexed tokenId
	);

	// Emitted when escrow is released
	event ReleaseEscrow(
		address indexed previousOwner,
		address indexed currentOwner,
		uint256 amount,
		uint256 indexed tokenId
	);

	// Emitted privileged function is called 
	// New Contract is 0 (credits or payable token)
	// Fee change is 1 (primary or secondary)
	// Fee taker is 2
	event PrivilegedFunction(uint256 indexed functionCalled);


	// Throw error when trying to list at price 0
	error PriceCannotBeZero(uint256 tokenId, uint256 unitPrice);

	// Throw error if list duration is 0
	error DurationCannotBeZero(uint256 tokenId, uint256 listDuration);

	// Throw error if listing is not active
	error OrderNotActive(address owner, uint256 tokenId);

	// Throw error when does account does not own enough Tokens
	error InsufficientBalanceAllowance(address account, uint256 tokenId, uint256 amount);

	// Throw error when initial offering
	error InitialOffering(uint256 tokenId);

	// Throw error when initial offering
	error TokenDNE(uint256 tokenId);

	// Throw error when initial offering
	error OrderDNE(uint256 tokenId);

	// Throw error when initial offering
	error OrderExists(uint256 tokenId);

	// Throw error when wrong amounts input
	error IncorrectAmounts();

	struct EscrowHolding {
		address previousOwner;
		address currentOwner;
		uint256 amount;
	}

	/* @notice Listing information including seller, amount and unit price
	 * @dev listing may sell any amount <= amount
	 *
	 * unitPrice is the price in USD (* some multiple)
	 * amount is in bps. See {CreditsContract-maxTokenSupply}. 
	 * expiry is when the order expires (in epochs)
	 * sold indicated if a token was sold
	*/
	struct Order {
		uint256 unitPrice;
		uint256 amount;
		uint256 expiry;
		OrderStatus status;
	}

	// Status of listing or bids
	enum OrderStatus{SOLD, ACTIVE, INITIAL, HOLD}

	// Type of order, sell or buy
	enum OrderType{SELL, BUY}

}

