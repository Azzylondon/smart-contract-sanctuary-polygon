pragma solidity >=0.8.4;
import "hardhat/console.sol";

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./IPodiumMarketplace.sol";
import "./MarketplaceAdministrative.sol";

/*
 * @title: Marketplace for Podium Labs Creato credits
 */
contract PodiumMarketplace is 
	Initializable,
	UUPSUpgradeable,
	ReentrancyGuardUpgradeable, 
	IPodiumMarketplace,
	MarketplaceAdministrative
{

	// Mapping from token ID and owner address to Listings
	mapping(uint256 => mapping(address => Order)) public listings;

	// Mapping from token ID and owner address to Listings
	mapping(uint256 => mapping(address => Order)) public bids;

	// Mapping from token ID to owner and prev owner, amount for escrow
	mapping(uint256 => EscrowHolding []) public escrow;

	// Mapping from owner to sales balance
	mapping(address => uint256) public salesBalance;

	// Mapping from owner to bid escrow balance
	mapping(address => uint256) public bidsBalance;
	
	// constructor()  {
	// 	_disableInitializers();
	// }


	/*
	 * @dev: Initializing the contract once with UUPS standard
	 * @param _Credits: Address of the ERC1155 Creator Credits
	 * @param _PayableToken: Address of the payable token (USDC?)
	 * @param _marketSaleFee: bps of the market fee on sales
	 * @param _marketPrimaryFee: bps of the primary sale fees
	 * @param _rolesOwner:  Initial address containing all role access
	 */
	function initialize(
		address _Credits, 
		address _PayableToken, 
		uint256 _marketSaleFee, 
		uint256 _marketPrimaryFee,
		address _feeTaker,
		address _rolesOwner
	) initializer public{
		__UUPSUpgradeable_init();
		__ReentrancyGuard_init();
		__MarketplaceAdministrative_init(
			_Credits, 
			_PayableToken, 
			_marketSaleFee, 
			_marketPrimaryFee,
			_feeTaker,
			_rolesOwner
		);

	}

	/*
	 * @dev: Upgrade the implementation of this contract. UUPS standard
	 * @param newImplementation: address of new contract implementation
	 */
	function _authorizeUpgrade(address newImplementation)
		internal
		onlyRole(UPGRADER_ROLE)
		override(MarketplaceAdministrative, UUPSUpgradeable)
	{}


	// ------------------------------------------------------------------------
	// Contrzct main function logic

	/*
	 * @dev: List specified token and amounts at specific price
	 * @notice: if listing exsits, it will update the listing
	 * @param _tokenId: ID of token to list
	 * @param _unitPrice: price per token
	 * @param _amount: amount of tokens to list
	 * @param _expiry: expiry of listing
	 * 
	 * Emits {NewOrder} Event of Type SELL
	 */
	function list(
		uint256 _tokenId,
		uint256 _unitPrice,
		uint256 _amount, 
		uint256 _expiry
	) external virtual nonReentrant lockable
	{
		notInitOffering(msg.sender, _tokenId);

		_listTo(msg.sender,_tokenId, Order(_unitPrice,_amount, _expiry, OrderStatus.ACTIVE));
	}

	/*
	 * @dev: bid on specified token and amounts at specific price
	 * @param _tokenId: ID of token to list
	 * @param _unitPrice: price per token
	 * @param _amount: amount of tokens to list
	 * @param _expiry: expiry of listing
	 * 
	 * Emits {NewOrder} Event of Type BUY
 	 */
	function bid(
		uint256 _tokenId,
		uint256 _unitPrice,
		uint256 _amount
	) external virtual nonReentrant lockable
	{

		if(_amount <= 0){
			revert IncorrectAmounts();
		}

		if(Credits.getTokenInfo(_tokenId).maxSupply <= 0 ||
			listings[_tokenId][Credits.getTokenInfo(_tokenId).creator].status == OrderStatus.INITIAL
		){
			revert TokenDNE(_tokenId);
		}

		if(bids[_tokenId][msg.sender].status != OrderStatus.SOLD){
			revert OrderExists(_tokenId);
		}

		orderPriceNotZero(_tokenId, _unitPrice);

		Order memory newBid = Order(_unitPrice,_amount, 0, OrderStatus.ACTIVE);

		_checkAllowanceAndTransfer(newBid.amount * newBid.unitPrice);
		bidsBalance[msg.sender] += newBid.amount * newBid.unitPrice;
		bids[_tokenId][msg.sender] = newBid;

		emit NewOrder(OrderType.BUY,false,msg.sender, _tokenId, _unitPrice,_amount,0);

	}

	/*
 	 * @dev: De-list specified token
 	 * @notice If owned or has been transfered out, listings
	 * are associated by token and owner. So may be canceled
	 * even without explicit current ownership
	 * 
 	 * @param _tokenId: Id of token to cancel listing for
 	 * 
 	 * Emits {OrderCanceled} Event of type SELL
 	 */
	function delist(uint256 _tokenId) external virtual nonReentrant lockable
	{
		notInitOffering(msg.sender, _tokenId);

		if(listings[_tokenId][msg.sender].amount <= 0){
			revert OrderDNE(_tokenId);
		}

		delete listings[_tokenId][msg.sender];
		emit OrderCanceled(OrderType.SELL ,msg.sender, _tokenId);
	}

	/*
	 * @dev: Cancel bids on specific token
	 * @param _tokenId: Id of token to cancel listing for
	 * 
	 * Emits {OrderCanceled} event of type BUY
	 */
	function cancelBid(uint256 _tokenId) external virtual nonReentrant lockable
	{

		if(bids[_tokenId][msg.sender].status != OrderStatus.ACTIVE){
			revert OrderDNE(_tokenId);
		}

		uint256 refundAmount = (bids[_tokenId][msg.sender].amount * 
			bids[_tokenId][msg.sender].unitPrice);
		delete bids[_tokenId][msg.sender];

		bidsBalance[msg.sender] -= refundAmount;
		bool refundSuccess = PayableToken.transfer(
				msg.sender, 
				refundAmount
		);

		require(refundSuccess, "Withdrawal failed.");

		emit OrderCanceled(OrderType.BUY, msg.sender, _tokenId);
	}

	/*
	 * @dev: Buy listed token
	 * @notice: the events emitted may potentially be capped if purchase bulk 
	 * from more than 9 users
	 * @param _tokenId: ID of token to buy
	 * @param _seller: seller of the token
	 * @param _amount: amount of that token to purchase
	 * 
	 * Emits {TokenSale}, {BalanceChange}, {NewEscrow} events
	 */
	function buyListed(
		uint256[] memory _tokenId, 
		address[] memory _seller, 
		uint256[] memory _amount
	) external virtual  whenNotPaused lockable nonReentrant
	{

		if (_tokenId.length!=_seller.length || _seller.length!=_amount.length)
		{
			revert IncorrectAmounts();
		}

		// Purchase every amount from token
		for(uint256 i = 0; i < _tokenId.length; i++){

			Order memory currentListing = listings[_tokenId[i]][_seller[i]];

			if((currentListing.amount < _amount[i]) || (_amount[i] == 0)){
				revert IncorrectAmounts();
			}

			sufficientCreditBalance(_seller[i],_tokenId[i],_amount[i]);

			if(
				block.timestamp > currentListing.expiry ||
				(currentListing.status != OrderStatus.ACTIVE && 
				currentListing.status != OrderStatus.INITIAL)
			){
				revert OrderNotActive(_seller[i], _tokenId[i]);
			}

			uint256 totalPrice = _amount[i] * currentListing.unitPrice;
			
			_checkAllowanceAndTransfer(totalPrice);

			// Primary sale: Escrow tokens
			if(currentListing.status == OrderStatus.INITIAL){
				_transferAndEscrow(msg.sender, _tokenId[i],_amount[i]);
			}

			// Secondary sale: Direct transfer
			else{
				Credits.safeTransferFrom(_seller[i], msg.sender, _tokenId[i], _amount[i], "");
				listings[_tokenId[i]][_seller[i]].amount -= _amount[i];

				if(listings[_tokenId[i]][_seller[i]].amount <= 0){
					delete listings[_tokenId[i]][_seller[i]];
				}
			}

			_balanceSalesUpdate(_tokenId[i],totalPrice,_seller[i],msg.sender,_amount[i], true, (currentListing.status == OrderStatus.INITIAL));
		}
	}

	/*
	 * @dev: Sell token to existing bid
	 * @param _tokenId: ID of token to sell
	 * @param _buyer: address of the buyer
	 * @param _amount: amount of _tokenId to sell
	 * 
	 * Emits {TokenSale} and {BalanceChange} events
	 */
	function acceptBid(
		uint256[] memory _tokenId, 
		address[] memory _buyer, 
		uint256[] memory _amount
	) external virtual  nonReentrant whenNotPaused lockable
	{

		if (_tokenId.length!=_buyer.length || _buyer.length!=_amount.length)
		{
			revert IncorrectAmounts();
		}

		// Purchase every amount from token
		for(uint256 i = 0; i < _tokenId.length; i++){

			Order memory currentBid = bids[_tokenId[i]][_buyer[i]];

			if((currentBid.amount < _amount[i]) || (_amount[i] == 0)){
				revert IncorrectAmounts();
			}

			sufficientCreditBalance(msg.sender,_tokenId[i],_amount[i]);

			// if(currentBid.status != OrderStatus.ACTIVE){
			// 	revert OrderNotActive(_buyer, _tokenId);
			// }

			Credits.safeTransferFrom(msg.sender, _buyer[i], _tokenId[i], _amount[i], "");

			bids[_tokenId[i]][_buyer[i]].amount -= _amount[i];
			if(bids[_tokenId[i]][_buyer[i]].amount <= 0){
				delete bids[_tokenId[i]][_buyer[i]];
			}

			uint256 totalPrice = _amount[i] * currentBid.unitPrice;

			// This should never occur but here as safety
			if(bidsBalance[_buyer[i]] < totalPrice){
				revert InsufficientBalanceAllowance(_buyer[i], _tokenId[i], _amount[i]);
			}

			bidsBalance[_buyer[i]] -= totalPrice;

			_balanceSalesUpdate(_tokenId[i], totalPrice, msg.sender, _buyer[i], _amount[i], false, false);
		}
	}

	/*
	 * @dev: Send Payable token to owner based on salesBalances_ book
	 * Making a withdrawal is safer than transfering funds automatically
	 * See https://fravoll.github.io/solidity-patterns/pull_over_push.html
	 *
	 * Emits a {WithdrawBalance} and {BalanceChange} events.
	 */
	function withdrawSalesBalance() 
		external virtual nonReentrant whenNotPaused lockable
	{
		require(
			salesBalance[msg.sender] > 0, 
			"PodiumMarketplace: Balance is 0"
		);

		uint256 withdrawAmount = salesBalance[msg.sender];

		salesBalance[msg.sender] = 0;

		bool success = PayableToken.transfer(
			msg.sender, 
			withdrawAmount
		);
		require(success, "Withdrawal failed.");

		emit WithdrawBalance(msg.sender, withdrawAmount);
	}

	/*
	 * @dev: Triggered when a user is "exercising" the credits
	 * tokens are escrowed. Escrow tokens are from caller to creator
	 * @param _tokenId: ID of token to escorw
	 * @param _amount: amount of _tokenId to escrow
	 * 
	 * @return: the amount of successfully escrowed tokens
	 * 
	 * Emits {Transfer} event from Credits contract. To escrow account
	 * Emits {NewEscrow} event
	 */
	function exerciseCredits(
		uint256 _tokenId,
		uint256 _amount
	) external virtual nonReentrant whenNotPaused lockable
	{
		sufficientCreditBalance(msg.sender, _tokenId, _amount);
		ICreatorCredits.TokenInfo memory tokenInfo = 
			Credits.getTokenInfo(_tokenId);	 

		Credits.safeTransferFrom(msg.sender, address(this), _tokenId, _amount, "");
		escrow[_tokenId].push(EscrowHolding(msg.sender,tokenInfo.creator,_amount));

		emit NewEscrow(msg.sender, tokenInfo.creator, _amount, _tokenId);
	}

	/*
	 * @dev: Mint token for creator and list specified amount of tokens
	 * @notice: Mint function is restricted to Podium (MINTER_ROLE)
	 * @param _tokenInfo: metadata for token see {ICreatorCredits}
	 * @param _unitPrice: the listing price for each token
	 * @param _listamount: amount of tokens to list
	 * @param _expiry: expiry of the listing (init offering)
	 * 
	 * @return: token ID of the mint and list
	 * 
	 * Emits {Transfer} and {Mint} events through the Credits Contract
	 * Emits {NewOrder} event of Type SELL
	 */
	function mintAndList(
		ICreatorCredits.TokenInfo memory _tokenInfo,
		uint256 _unitPrice,
		uint256 _listamount,
		uint256 _expiry
	) external virtual onlyRole(MINTER_ROLE) nonReentrant whenNotPaused lockable returns(uint256)
	{
		if(_tokenInfo.maxSupply <= 0){
			revert InitialOffering(_tokenInfo.maxSupply);
		}
		uint256 tokenId = Credits.mint(_tokenInfo);

		if(_listamount > 0){
			_listTo(
				_tokenInfo.creator, 
				tokenId, 
				Order(_unitPrice,_listamount,_expiry,OrderStatus.INITIAL)
			);
		}

		return tokenId;
	}

	/*
	 * @dev: Mint additional tokens for creator and list specified amount of 
	 * @notice: will update overall listing if exists. Will list otherwise
	 * Mint function is restricted to Podium
	 * @param _tokenId: the token ID to mint more from
	 * @param _quantity: the amount of tokens to mint
	 * @param _to: address where the tokens will go
	 * @param _unitPrice: the listing price for each token
	 * @param _listamount: amount of tokens to list
	 * @param _expiry: expiry of the listing (init offering)
	 * @return: token ID of the mint and list
	 * 
	 * Emits {Transfer} and {Mint} events through the Credits Contract
	 * Emits {NewOrder} event of Type SELL
	 */
	function mintAdditionalAndList(
		uint256 _tokenId,
		uint256 _quantity,
		address _to,
		uint256 _unitPrice,
		uint256 _listamount,
		uint256 _expiry
	) external virtual onlyRole(MINTER_ROLE) nonReentrant whenNotPaused lockable returns(uint256)
	{

		Credits.mintAdditional(_tokenId,_quantity,_to);
		ICreatorCredits.TokenInfo memory tokenInfo = 
			Credits.getTokenInfo(_tokenId);

		if(_listamount > 0){
			_listTo(
				tokenInfo.creator, 
				_tokenId, 
				Order(_unitPrice,_listamount,_expiry,OrderStatus.INITIAL)
			);
		}

		return _tokenId;
	}


	/*
	 * @dev: fiatPurchase of the tokens. Only Podium can call this function
	 * If initial offering, escrow the tokens, otherwise transfer them 
	 * directly.
	 * @notice: there must be an active listing to complete purchase
	 * @param _buyer: address of the buyer
	 * @param _seller: address of the seller
	 * @param _tokenId: ID of token to be purchase
	 * @param _amount: amount of _tokenId to be purchase
	 * 
	 * Emits {Transfer} event from Credits contract
	 * May Emits {NewEscrow} event if on initial offering
	 */
	function fiatPurchase(
		address _seller,
		address _buyer,
		uint256 _tokenId,
		uint256 _amount
	) external virtual onlyRole(MINTER_ROLE) nonReentrant whenNotPaused lockable
	{

		sufficientCreditBalance(_seller,_tokenId,_amount);
	
		Order memory currentListing = listings[_tokenId][_seller];

		// Ensure correct amounts
		if(currentListing.amount < _amount){
			revert IncorrectAmounts();
		}

		// Make sure listing has not expired and active
		if(
			block.timestamp > currentListing.expiry ||
			(currentListing.status != OrderStatus.ACTIVE && 
			currentListing.status != OrderStatus.INITIAL)
		){
			revert OrderNotActive(_seller, _tokenId);
		}

		if(currentListing.status != OrderStatus.INITIAL){

			Credits.safeTransferFrom(_seller, _buyer, _tokenId, _amount, "");
			listings[_tokenId][_seller].amount -= _amount;
			if(listings[_tokenId][_seller].amount <= 0){
				listings[_tokenId][_seller].status = OrderStatus.SOLD;
			}
		}
		else{
			_transferAndEscrow(_buyer, _tokenId, _amount);
		}

		emit TokenSale(_seller,_buyer,_tokenId,_amount,0, true);

	}

	/*
	 * @dev: Complete initial sale, transfer escrowed tokens and 
	 * burn the remaining supply.
	 * Can only be called once for each token Mint (except additional)
	 * @param _tokenId: ID of token to compelete the offering for
	 * 
	 * Emits {Transfer} event from Credits contract. From Escrow holder
	 * Emits {ReleaseEscrow} event from releaseEscrow function
	 */
	function completeOffering(uint256 _tokenId)
		external virtual nonReentrant onlyRole(MINTER_ROLE) whenNotPaused lockable
	{
		ICreatorCredits.TokenInfo memory tokenInfo = Credits.getTokenInfo(_tokenId);		

		Order memory currentListing = listings[_tokenId][tokenInfo.creator];
		
		if
		(
			(currentListing.expiry > block.timestamp && 
				currentListing.status != OrderStatus.HOLD) || 
			currentListing.expiry == 0
		)
		{
			revert InitialOffering(_tokenId);
		}
		listings[_tokenId][tokenInfo.creator].status = OrderStatus.SOLD;
		listings[_tokenId][tokenInfo.creator].expiry = 0;

		Credits.burn(tokenInfo.creator, _tokenId, currentListing.amount);
		_releaseEscrow(false, true, _tokenId, address(0), address(0), 0);
	}


	/*
	 * @dev: Transfer escrowed tokens to new owner or back to
	 * original owner
	 * This is restricted to podium.
	 * @notice: when _initialSaleRelease, params other than {_revertEscrow}
	 * and {_tokenId} are ignored
	 * @param _revertEscrow: true means return tokens to original owner
	 * false means transfer to new owner
	 * @param _initialSaleRelease: if true, all escrowed tokens of ID
	 * will be released. This can only called once and by the complete offering
	 * function.
	 * @param _tokenId: ID of token to be released from escrow
	 * @param _previousOwner: previous owner in escrow
	 * @param _currentOwner: current owner in escrow
	 * @param _amount: amount of tokens to be released
	 * 
	 * Emits {Transfer} event from Credits 
	 * Emits {ReleaseEscrows} or {ReleaseEscrow} event
	 */
	function releaseEscrow(
		bool _revertEscrow,
		bool _initialSaleRelease,
		uint256 _tokenId,
		address _previousOwner,
		address _currentOwner,
		uint256 _amount
	) public virtual nonReentrant onlyRole(MINTER_ROLE) whenNotPaused lockable
	{
		_releaseEscrow(
			_revertEscrow,
			_initialSaleRelease,
			_tokenId,
			_previousOwner,
			_currentOwner,
			_amount
		);
	}


	/*
	 * @dev: Internal list specified token and amounts
	 * at specific price for specific Owner address
	 * @param _owner: address of owner listing
	 * @param _tokenId: ID of token to be listed
	 * @param _tokenListing: token listing information See {IPodiumMarketplace}
	 * 
	 * Emits a {NewOrder} event.
	 */
	function _listTo(
		address _owner,
		uint256 _tokenId,
		Order memory _order
	)   private
	{

		sufficientCreditBalance(_owner, _tokenId, _order.amount);
		orderPriceNotZero(_tokenId, _order.unitPrice);

		if(_order.expiry < block.timestamp) {
			revert DurationCannotBeZero(_tokenId, _order.expiry);
		}

		listings[_tokenId][_owner] = _order;

		bool initialOffering = (_order.status == OrderStatus.INITIAL);

		emit NewOrder(OrderType.SELL,initialOffering,_owner, _tokenId, _order.unitPrice,_order.amount,_order.expiry);
	}

	/*
	 * @dev: Checks that allowance for payable token and amounts are correct
	 * throws if marketplace allowance is too low for payable token
	 * @param _amount: amount of token to be transfered
	 * 
	 */
	function _checkAllowanceAndTransfer(uint256 _amount) private
	{
		// Make sure Marketplace is allowed to spend $PayableToken
		if(
			PayableToken.allowance(msg.sender, address(this)) < _amount ||
			PayableToken.balanceOf(msg.sender) < _amount
		){
			revert InsufficientBalanceAllowance(msg.sender, 0, _amount);
		}
		bool success = PayableToken.transferFrom(
			msg.sender, 
			address(this), 
			_amount
		);

		require(success, "PayableToken transfer failed.");
	}

	/*
	 * @dev: Internal function to transfer tokens into escrow and update tracking
	 * @notice: will only be called with initial offering purchase (fiat and crypto)
	 * @param _buyer: address of the buyer to which tokens will go
	 * @param _tokenId: ID of token to be escrow
	 * @param _amount: amount of _tokenId to be escrowed
	 * 
	 * Emits {NewEscrow} event
	 */
	function _transferAndEscrow(		
		address _buyer,
		uint256 _tokenId,
		uint256 _amount
	) private
	{

		ICreatorCredits.TokenInfo memory tokenInfo = Credits.getTokenInfo(_tokenId);		

		Credits.safeTransferFrom(tokenInfo.creator, address(this), _tokenId, _amount, "");
		escrow[_tokenId].push(EscrowHolding(tokenInfo.creator,_buyer,_amount));

		listings[_tokenId][tokenInfo.creator].amount -= _amount;
		if(listings[_tokenId][tokenInfo.creator].amount <= 0){
			listings[_tokenId][tokenInfo.creator].status = OrderStatus.HOLD;
		}

		emit NewEscrow(tokenInfo.creator,_buyer,_amount,_tokenId);
	}

	/*
	 * @dev: Update balances after sale
	 * @param _tokenId: ID of token that was sold
	 * @param _totalPrice: price paid for tokens, total
	 * @param _seller: token seller
	 * @param _buyer: token buyer
	 * @param _amount: amount of tokens sold 
	 * 
	 * Emits {TokenSale} and {BalanceChange} events
	 */
	function _balanceSalesUpdate(
		uint256 _tokenId, 
		uint256 _totalPrice,
		address _seller, 
		address _buyer,
		uint256 _amount,
		bool _list,
		bool _primary
	) private
	{

		ICreatorCredits.TokenInfo memory tokenInfo = 
			Credits.getTokenInfo(_tokenId);

		uint256 fee;
		if(_primary){
			fee = (_totalPrice * marketPrimaryFee) / 10000;
		}else{
			fee = (_totalPrice * marketSaleFee) / 10000;
		}
		
		uint256 creatorRoyalties = (_totalPrice * tokenInfo.royalties)/10000;
		
		salesBalance[tokenInfo.creator] += creatorRoyalties;
		salesBalance[feeTaker] += fee;
		uint256 sellerIncrease = (_totalPrice - creatorRoyalties - fee);
		salesBalance[_seller] += sellerIncrease;

		emit TokenSale(_seller,_buyer,_tokenId,_amount,_totalPrice, _list);

		emit BalanceChange(
			tokenInfo.creator, feeTaker, _seller,
			creatorRoyalties, fee, sellerIncrease
		);
	}

	/*
	 * @dev: Internall releaseEscrow
	 * With no reentrancy guard
	 * See {releaseEscrow}
	 * 
	 * Emits {Transfer} event from Credits 
	 * Emits {ReleaseEscrows} or {ReleaseEscrow} event
	 */
	function _releaseEscrow(
		bool _revertEscrow,
		bool _initialSaleRelease,
		uint256 _tokenId,
		address _previousOwner,
		address _currentOwner,
		uint256 _amount
	) internal virtual whenNotPaused lockable
	{

		uint256 i = 0;
		while(i < escrow[_tokenId].length){
			EscrowHolding memory currentEscrow = escrow[_tokenId][i];

			if(
				((currentEscrow.previousOwner == _previousOwner) &&
				(currentEscrow.currentOwner == _currentOwner) &&
				(currentEscrow.amount == _amount) ) ||
				_initialSaleRelease
			)
			{
				// Return to sender
				if(_revertEscrow){
					Credits.safeTransferFrom(address(this),currentEscrow.previousOwner, _tokenId,currentEscrow.amount, "");
				}
				// New owner
				else{
					Credits.safeTransferFrom(address(this), currentEscrow.currentOwner, _tokenId, currentEscrow.amount, "");
				}

				// Fix indexing
				EscrowHolding memory popped = escrow[_tokenId][escrow[_tokenId].length - 1];
				if(i < (escrow[_tokenId].length -1 )){
					escrow[_tokenId][i] = popped;
				}
				escrow[_tokenId].pop();
				
				if(!_initialSaleRelease){
					emit ReleaseEscrow(currentEscrow.previousOwner, currentEscrow.currentOwner, currentEscrow.amount, _tokenId);
					break;
				}
			}
			// Increment for search
			else{i++;}
		}

		if(_initialSaleRelease){
			emit ReleaseEscrows(_tokenId);
		}
	}

	// ------------------------------------------------------------------------
	// Modifiers used 

	/*
	 * @dev: throws if owner does not have enough tokens to list requested
	 * amount. Also inherently checks if _owner is an actual owner
	 * @param _owner: address of owner to check
	 * @param _tokenId: ID of token to check ownership
	 * @param _amount: amount to check balance
	 */
	function sufficientCreditBalance(
		address _owner, 
		uint256 _tokenId,
		uint256 _amount
		) private view 
	{
		if(Credits.balanceOf(_owner, _tokenId) < _amount || _amount <= 0){
			revert InsufficientBalanceAllowance(_owner, _tokenId, _amount);
		}
	}

	/*
	 * @dev: throws if token is not in initial offering phase
	 * @param _owner: owner to check listing for
	 * @param _tokenId: ID of token to check
	 */
	function notInitOffering (address _owner, uint256 _tokenId) private view {
		Order memory currentListing = listings[_tokenId][_owner];
		if(
			currentListing.status == OrderStatus.INITIAL ||
			currentListing.status == OrderStatus.HOLD
		){
			revert InitialOffering(_tokenId);
		}
	}

	/*
	 * @dev: throws if order price is zero
	 * @param _owner: caller address
	 * @param _tokenId: ID of token for the order
	 * @param _unitPrice: unitPrice of the order
	 */
	function orderPriceNotZero (
		uint256 _tokenId, 
		uint256 _unitPrice
		) private pure 
	{
		if(_unitPrice <= 0) {
			revert PriceCannotBeZero(_tokenId, _unitPrice);
		}
	}


}

