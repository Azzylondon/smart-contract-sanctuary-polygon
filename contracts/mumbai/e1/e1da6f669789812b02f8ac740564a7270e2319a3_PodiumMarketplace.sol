 * @notice Admin and operational functions and data for marketplace
/ -------------------------------------------------------------------------- */
pragma solidity >=0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "../creatorCredits/ICreatorCredits.sol";
import "./IPodiumMarketplace.sol";

abstract contract MarketplaceAdministrative is 
	Initializable,
	PausableUpgradeable, 
	AccessControlUpgradeable,
	UUPSUpgradeable,
	IPodiumMarketplace,
	ERC1155HolderUpgradeable
{

	// Podium Creator Credits Token contract
	ICreatorCredits public Credits;
	IERC20Upgradeable public PayableToken;

	// Marketplace fee (2.5% = 250)
	uint256 public marketSaleFee;

	// Marketplace payout fee on primary sales, in bps
	uint256 public marketPrimaryFee;

	// Marketplace fee address
	address public feeTaker;

	// Wallet authorized to mint tokens
	address public minterAddress;

	// Locked functions
	mapping(bytes4 => bool) public functionLocked;

	bytes32 public constant CONTRACT_SETTER_ROLE = keccak256("CONTRACT_SETTER_ROLE");
	bytes32 public constant FEE_SETTER_ROLE = keccak256("FEE_SETTER_ROLE");
	bytes32 public constant FEETAKER_SETTER_ROLE = keccak256("FEETAKER_SETTER_ROLE");
	bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
	bytes32 public constant LOCKER_ROLE = keccak256("LOCKER_ROLE");
	bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
	bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

	// constructor()  {
	// 	_disableInitializers();
	// }

	function __MarketplaceAdministrative_init(
		address _Credits, 
		address _PayableToken, 
		uint256 _marketSaleFee, 
		uint256 _marketPrimaryFee,
		address _feeTaker,
		address _rolesOwner
	) initializer internal{
		__Pausable_init();
		__UUPSUpgradeable_init();
		__AccessControl_init();
		__ERC1155Holder_init();

		_grantRole(DEFAULT_ADMIN_ROLE, _rolesOwner);
		_grantRole(CONTRACT_SETTER_ROLE,_rolesOwner);
		_grantRole(FEE_SETTER_ROLE,_rolesOwner);
		_grantRole(FEETAKER_SETTER_ROLE,_rolesOwner);
		_grantRole(PAUSER_ROLE,_rolesOwner);
		_grantRole(LOCKER_ROLE,_rolesOwner);
		_grantRole(MINTER_ROLE,_rolesOwner);
		_grantRole(UPGRADER_ROLE, _rolesOwner);

		Credits = ICreatorCredits(_Credits);
		PayableToken = IERC20Upgradeable(_PayableToken);
		marketSaleFee = _marketSaleFee;
		marketPrimaryFee = _marketPrimaryFee;
		feeTaker = _feeTaker;


	}

	function _authorizeUpgrade(address newImplementation)
		internal
		onlyRole(UPGRADER_ROLE)
		override
		virtual
	{}

	// ------------------------------------------------------------------------
	// Administrative Setters

	/** 
	 * @notice attach new mint contract to mint from
	 *
	 * Emits a {PrivilegedFunction} event.
	 */
	function newContract(address _newContract, bool _mintContract) 
		public virtual onlyRole(CONTRACT_SETTER_ROLE)
	{
		if(_mintContract){
			Credits = ICreatorCredits(_newContract);
		}else{
			PayableToken = IERC20Upgradeable(_newContract);
		}

		emit PrivilegedFunction(0);
	}

	/**
	 * @notice Setter that changes marketplace sales fee
	 *
	 * Emits a {PrivilegedFunction} event.
	 */
	function setMarketSaleFee(uint256 _newFee, bool _marketSaleFee) 
		external virtual onlyRole(FEE_SETTER_ROLE)
	{
		if(_marketSaleFee){
			marketSaleFee = _newFee;
		}else{
			marketPrimaryFee = _newFee;	
		}

		emit PrivilegedFunction(1);
	}

	/**
	 * @notice Setter that changes marketplace fee receiver
	 * that can withdraw fees
	 *
	 * Emits a {PrivilegedFunction} event.
	 */
	function setFeeTaker(address _newFeeTaker) 
		external virtual onlyRole(FEETAKER_SETTER_ROLE)
	{
		feeTaker = _newFeeTaker;
		emit PrivilegedFunction(2);
	}

	/*
	 * @notice Pause used to pause functions as needed
	*/
	function pause() external virtual onlyRole(PAUSER_ROLE) {
		_pause();
	}

	/*
	 * @notice Unpause used to unpause functions as needed
	*/
	function unpause() external virtual onlyRole(PAUSER_ROLE) {
		_unpause();
	}

	 /**
	 * @notice Modifier applied to functions that will be disabled when they're no longer needed
	 */
	modifier lockable() {
		require(!functionLocked[msg.sig], "Function has been locked");
		_;
	}

	/**
	 * @notice Lock individual functions 
	 * @dev Only affects functions with the lockable modifier
	 * @param id First 4 bytes of the calldata (i.e. function identifier)
	 */
	function lockFunction(bytes4 id) public virtual onlyRole(LOCKER_ROLE) {
		functionLocked[id] = true;
	}

	/**
	 * @notice unLock individual functions 
	 * @dev Only affects functions with the lockable modifier
	 * @param id First 4 bytes of the calldata (i.e. function identifier)
	 */
	function unlockFunction(bytes4 id) public virtual onlyRole(LOCKER_ROLE) {
		functionLocked[id] = false;
	}

	// The following functions are overrides required by Solidity.

	function supportsInterface(bytes4 interfaceId)
		public
		view
		virtual
		override(AccessControlUpgradeable, ERC1155ReceiverUpgradeable)
		returns (bool)
	{
		return super.supportsInterface(interfaceId);
	}

}

