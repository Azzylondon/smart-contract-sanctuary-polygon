pragma solidity ^0.6.6;
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@uniswap/v2-core/contracts/interfaces/IERC20.sol";

contract Swap {

    IUniswapV2Router02 public uniRouter = IUniswapV2Router02(0x8954AfA98594b838bda56FE4C12a09D7739D179b);
    
    
    function swapper(address token1, address token2) public  {
        address[] memory path = new address[](2);
        path[0] = token1;
        path[1] = token2;
        uint amountOut = IERC20(token1).balanceOf(address(this));
        uint amountIn = uniRouter.getAmountsIn(
            amountOut,
            path
        )[0];
                
        IERC20(token1).approve(address(uniRouter), amountIn);
        
        // uint allowed = IERC20(token1).allowance(msg.sender, address(uniRouter));        
        
        uniRouter.swapExactTokensForTokens(
            amountIn, 
            amountOut,
            path, 
            msg.sender, 
            block.timestamp + 120
        );
    }


}

