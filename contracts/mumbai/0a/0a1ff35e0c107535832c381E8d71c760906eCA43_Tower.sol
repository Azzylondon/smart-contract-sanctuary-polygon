pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./ITower.sol";
import "./TowerState.sol";
import "../shared/Adminable.sol";
import "../shared/randomizer/IRandomizer.sol";
import "../tokens/sacrificialalter/ISacrificialAlter.sol";
import "../tokens/gp/IGP.sol";
import "../tokens/wnd/IWnD.sol";

abstract contract TowerContracts is
    Initializable,
    ITower,
    TowerState
{
    function __TowerContracts_init() internal initializer {
        TowerState.__TowerState_init();
    }

    modifier requireContractsSet() {
        require(
            address(wndNFT) != address(0) &&
                address(gpToken) != address(0) &&
                address(wndGame) != address(0) &&
                address(randomizer) != address(0),
            "Contracts not set"
        );
        _;
    }

    function setContracts(
        address _wndNFT,
        address _gp,
        address _wndGame,
        address _rand
    ) external onlyOwner {
        require(_wndNFT != address(0)
            && _gp != address(0)
            && _wndGame != address(0)
            && _rand != address(0), "Invalid contract address");
        wndNFT = IWnD(_wndNFT);
        gpToken = IGP(_gp);
        wndGame = IWnDGame(_wndGame);
        randomizer = IRandomizer(_rand);
    }
}

