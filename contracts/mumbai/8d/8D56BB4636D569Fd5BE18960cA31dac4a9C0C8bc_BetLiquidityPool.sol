/**
 * @title Liquidity Box
 * @author Michael Larsen
 * @notice Abstract contract for storing tokens in a locked state.
 * @notice This is a sub contract for the BookieMain app.
 * @notice Its keeps a list of locked tokens and owns the locked tokens.
 * @notice TokenValidator is Accesshandler, Accesshandler is Initializable.
 */
abstract contract LiquidityBox is ILiquidityBox, TokenValidator {
    using SafeMath for uint256;

    ITokenTransferProxy private tokenTransferProxy;
    ILockBox internal extLockBox;
    IRewardHandler private feeRewards1;
    IRewardHandler private feeRewards2;

    uint8 internal feePermille;
    uint8 internal feeSplitPercent1;
    uint8 internal feeSplitPercent2;

    /**
     * Event that fires when tokens are deposited.
     * @param owner is the address that deposited tokens.
     * @param tokenAdd is the token contract address.
     * @param amount is the amount deposited, after fee deduction.
     * @param fee is the fee paid.
     */
    event TokensDeposited(
        address indexed owner,
        address tokenAdd,
        uint256 amount,
        uint256 fee
    );

    /**
     * Event that fires when tokens are withdrawn.
     * @param owner is the address that withdrew tokens.
     * @param tokenAdd is the token contract address.
     * @param amount is the amount withdrawn, including the fee.
     * @param fee is the fee paid.
     */
    event TokensWithdrawn(
        address indexed owner,
        address tokenAdd,
        uint256 amount,
        uint256 fee
    );

    /**
     * @notice Event fires when deposit for 0 tokens is attempted.
     * @param owner is the owner of the tokens.
     * @param tokenAdd is the token contract address.
     */
    event DepositZero(address indexed owner, address tokenAdd);

    /**
     * @notice Event fires when withdraw for 0 lpTokens is attempted.
     * @param owner is the owner of the lpTokens.
     * @param lpToken is the lpToken contract address.
     */
    event WithdrawZero(address indexed owner, address lpToken);

    /**
     * @notice Event fires when fees are set.
     * @param feePermille is the fee permille to charge.
     * @param feePercent1 is the percent of the fee that goes to handler 1.
     * @param feePercent2 is the percent of the fee that goes to handler 2.
     */
    event FeesSet(
        uint8 feePermille,
        uint8 feePercent1,
        uint8 feePercent2
    );

    /**
     * @notice Event fires when invalid fees are set.
     * @param feePermille is the fee permille to charge.
     * @param feePercent1 is the percent of the fee that goes to handler 1.
     * @param feePercent2 is the percent of the fee that goes to handler 2.
     */
    error InvalidFees(
        uint8 feePermille,
        uint8 feePercent1,
        uint8 feePercent2
    );

    /**
     * Error for Insufficient liquidity balancefor withdrawel.
     * Needed `required` but only `available` available.
     * @param available balance available.
     * @param required requested amount to transfer.
     */
    error InsufficientBalance(uint256 available, uint256 required);

    /**
     * Error for Insufficient LP Token balance for withdrawel.
     * Needed `required` but only `available` available.
     * @param available balance available.
     * @param required requested amount to transfer.
     */
    error InsufficientLPTokenBalance(uint256 available, uint256 required);

    /**
     * Error for Insufficient LPToken allowance for withdrawal.
     * Needed `required` but only `available` available.
     * @param available allowance available.
     * @param required requested amount to transfer.
     */
    error InsufficientAllowance(uint256 available, uint256 required);

    /**
     * Error for token transfer failure, when trying to deposit.
     * @param user is the address that tried to deposit.
     * @param tokenAdd is the token contract address
     * @param amount is the requested amount to transfer, including the fee.
     */
    error TokenDepositFailed(address user, address tokenAdd, uint256 amount);

    /**
     * Error for token transfer failure during a withdraw request.
     * @param receiver is the address to receive the tokens.
     * @param tokenAdd is the token contract address.
     * @param amount is the requested amount to transfer, including the fee.
     */
    error TokenWithdrawFailed(
        address receiver,
        address tokenAdd,
        uint256 amount
    );

    /**
     * Error for account address 0.
     */
    error BadAccountZero();

    /**
     * @notice Default constructor.
     */
    constructor() TokenValidator() {}

    /**
     * @notice Initializes this contract with reference to other contracts.
     * @param inLPToken The Token used to keep the liquidity balance.
     * @param inTokenTransferProxy The TokenTransferProxy contract  address.
     * @param inExtLockBox Is a contract that holds externally locked tokens.
     * @param inFeeRewards1 Is 1st contract that handles dep/wtd fees.
     * @param inFeeRewards2 Is 2nd contract that handles dep/wtd fees.
     * @param inFeePermille is the fee permille to charge.
     * @param inFeePercent1 Is percent of fees that goes to handler 1.
     * @param inFeePercent2 Is percent of fees that goes to handler 2.
     */
    function initBox(
        ILPToken inLPToken,
        ITokenTransferProxy inTokenTransferProxy,
        ILockBox inExtLockBox,
        IRewardHandler inFeeRewards1,
        IRewardHandler inFeeRewards2,
        uint8 inFeePermille,
        uint8 inFeePercent1,
        uint8 inFeePercent2
    )
        external
        notInitialized
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _initFees(
            inFeeRewards1,
            inFeeRewards2,
            inFeePermille,
            inFeePercent1,
            inFeePercent2);

        _initBox(
            inLPToken,
            inTokenTransferProxy,
            inExtLockBox);
    }

    /**
     * @notice Transfer own tokens to external Box. Doesn't affect LPTokens.
     * @param tokenAdd The token type to transfer.
     * @param amount The amount to transfer.
     */
    function moveToExternalBox(
        address tokenAdd,
        uint256 amount
    )
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _moveToExt(tokenAdd, amount);
    }

    /**
     * @notice Sets the amount of fees to charge and distribute.
     * @param inFeePermille is the fee permille to charge.
     * @param inFeePercent1 is the percent of the fee that goes to handler 1.
     * @param inFeePercent2 is the percent of the fee that goes to handler 2.
     */
    function setFees(
        uint8 inFeePermille,
        uint8 inFeePercent1,
        uint8 inFeePercent2
    )
        external
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _setFees(inFeePermille, inFeePercent1, inFeePercent2);
    }

    /**
     * @notice Gets a users available balance of an LPToken.
     * @param owner Is the user that owns the LPTokens.
     * @param tokenAdd The token type that matches the LPToken.
     * @return uint256 The amount owned by the user.
     */
    function getUserLPTokenBalanceFromToken(address owner, address tokenAdd)
        external
        override
        view
        onlyAllowedToken(tokenAdd)
        returns (uint256)
    {
        return IERC20(allowedTokens[tokenAdd]).balanceOf(address(owner));
    }

    /**
     * @notice Gets the LPs available balance for a token.
     * @param tokenAdd The address of the token type.
     * @return uint256 The amount currently locked.
     */
    function getTokenBalance(address tokenAdd) public view returns (uint256) {
        return IERC20(tokenAdd).balanceOf(address(this));
    }

    /**
     * @notice Gets the LPs full balance for a token, including the amount
     *         currently locked in external box.
     * @param tokenAdd The token type.
     * @return uint256 The combined LP balance.
     */
    function getFullBalance(address tokenAdd) public view returns (uint256) {
        console.log("getFullBalance(", tokenAdd, ")");
        uint256 fullTokenBalance = IERC20(tokenAdd).balanceOf(address(this));
        console.log("getFullBalance() extBox addr:", address(extLockBox));
        if (address(extLockBox) != address(0)) {
            fullTokenBalance = fullTokenBalance
                .add(extLockBox.getLockedAmount(address(this), tokenAdd));
        }
        return fullTokenBalance;
    }

    /**
     * @notice Gets the matching token valueof an amount of LPTokens.
     * @param lpTokenAdd The address of lpToken type.
     * @param amount The amount of LPTokens.
     * @return tokenAmount The matching amount.
     */
    function getMatchingTokenAmount(address lpTokenAdd, uint256 amount)
        public
        override
        view
        onlyAllowedLPToken(lpTokenAdd)
        returns (uint256 tokenAmount)
    {
        address tokenAdd = ILPToken(lpTokenAdd).tokenAdd();
        uint256 totalSupplyLPT = ILPToken(lpTokenAdd).totalSupply();
        if (amount == 0 || totalSupplyLPT == 0) {
            return 0;
        }
        tokenAmount = getFullBalance(tokenAdd).mul(amount).div(totalSupplyLPT);
    }

    /**
     * @notice Gets the matching LPToken value of an amount of ERC20 Tokens.
     * @param tokenAdd The ERC20 token type.
     * @param amount The amount of tokens.
     * @return lpTokenAmount The matching amount.
     */
    function getMatchingLPTokenAmount(address tokenAdd, uint256 amount)
        public
        override
        view
        onlyAllowedToken(tokenAdd)
        returns (uint256 lpTokenAmount)
    {
        address lpToken = allowedTokens[tokenAdd];
        uint256 totalSupplyLPT = ILPToken(lpToken).totalSupply();
        if (amount == 0 || totalSupplyLPT == 0) {
            return 0;
        }
        lpTokenAmount = totalSupplyLPT.mul(amount).div(getFullBalance(tokenAdd));
    }

    /**
     * @notice Initializes this contract with reference to other contracts.
     * @param inLPToken The Token used to keep the liquidity balance.
     * @param inTokenTransferProxy The TokenTransferProxy contract  address.
     * @param inExtLockBox Is a contract that holds externally locked tokens.
     */
    function _initBox(
        ILPToken inLPToken,
        ITokenTransferProxy inTokenTransferProxy,
        ILockBox inExtLockBox
    )
        internal
        notInitialized
    {
        extLockBox = inExtLockBox;
        tokenTransferProxy = inTokenTransferProxy;
        // Add the token to the list of accepted input tokens.
        grantRole(AccessHandler.TOKEN_ROLE, inLPToken.tokenAdd());
        // Pair the new input token, with the matching lpToken
        allowedTokens[inLPToken.tokenAdd()] = address(inLPToken);
        AccessHandler.initialize();
    }

    /**
     * @notice Initializes this contract with fees details.
     * @param inFeeRewards1 Is 1st contract that handles dep/wtd fees.
     * @param inFeeRewards2 Is 2nd contract that handles dep/wtd fees.
     * @param inFeePermille is the fee permille to charge.
     * @param inFeePercent1 Is percent of fees that goes to handler 1.
     * @param inFeePercent2 Is percent of fees that goes to handler 2.
     */
    function _initFees(
        IRewardHandler inFeeRewards1,
        IRewardHandler inFeeRewards2,
        uint8 inFeePermille,
        uint8 inFeePercent1,
        uint8 inFeePercent2
    )
        internal
        notInitialized
    {
        feeRewards1 = inFeeRewards1;
        feeRewards2 = inFeeRewards2;
        _setFees(inFeePermille, inFeePercent1, inFeePercent2);
    }

    /**
     * @notice Increases the users deposited amount for a token.
     * @param owner The owner to update.
     * @param tokenAdd The address of the token type to deposit.
     * @param amount The amount to deposit, including the fee.
     */
    function deposit(
        address owner,
        address tokenAdd,
        uint256 amount
    ) internal {
        ILPToken lpToken = ILPToken(allowedTokens[tokenAdd]);
        uint256 totalSupply = lpToken.totalSupply();
        uint256 addAmount = amount.mul(uint256(1000).sub(feePermille)).div(1000);
        uint256 lpAmount = addAmount;
        if (totalSupply > 0) {
            // This is not the first deposit
            // Calc the rel increase of LP balance, and mint the same rel
            // amount of LPTokens
            uint256 fullTokenBalance = getFullBalance(tokenAdd);
            lpAmount = addAmount.mul(totalSupply).div(fullTokenBalance);
        }

        bool success = transferViaProxy(owner, tokenAdd, address(this), amount);
        if (!success) {
            revert TokenDepositFailed({
                user: owner,
                tokenAdd: tokenAdd,
                amount: amount
            });
        }

        uint256 fee;
        if (feePermille > 0) {
            fee = amount.sub(addAmount);

            // Update the pools rewards, before minting lpTokens
            if (feeSplitPercent1 > 0) {
                // Add the fee to the common reward pools
                uint256 fee1 = fee.mul(feeSplitPercent1).div(100);
                feeRewards1.addRewards(owner, fee1);
                // Transfer the fee to the reward handler
                IERC20(tokenAdd).transfer(address(feeRewards1), fee1);
            }
            if (feeSplitPercent2 > 0) {
                // Add the fee to the common reward pools
                uint256 fee2 = fee.mul(feeSplitPercent2).div(100);
                feeRewards2.addRewards(owner, fee2);
                // Transfer the fee to the reward handler
                IERC20(tokenAdd).transfer(address(feeRewards2), fee2);
            }
        }
        // Update depositors assigned rewards, using old distribution
        feeRewards1.updateRewards(owner);
        feeRewards2.updateRewards(owner);

        // Now mint depositors new tokens.
        console.log("Will mint:", lpAmount / 1 ether);
        ILPToken(lpToken).mint(owner, lpAmount);
        emit TokensDeposited(owner, tokenAdd, addAmount, fee);
    }

    /**
     * @notice Return deposited tokens to user and burn their LPTokens.
     * @param owner The user to update.
     * @param lpTokenAdd The address of lpToken type to return.
     * @param amount The amount of LPTokens to return.
     * @return bool True if the withdraw succeeded, false if not.
     */
    function withdraw(
        address owner,
        address lpTokenAdd,
        uint256 amount
    ) internal returns (bool) {
        if (amount == 0) {
            emit WithdrawZero(owner, lpTokenAdd);
            return false;
        }
        ILPToken lpToken = ILPToken(lpTokenAdd);
        uint256 lpTokenBalance = lpToken.balanceOf(owner);
        if (amount > lpTokenBalance) {
            revert InsufficientLPTokenBalance({
                available: lpTokenBalance,
                required: amount
            });
        }
        uint256 allowance = lpToken.allowance(owner, address(this));
        if (amount > allowance) {
            revert InsufficientAllowance({
                available: allowance,
                required: amount
            });
        }
        address tokenAdd = lpToken.tokenAdd();
        uint256 tokenAmount = getMatchingTokenAmount(lpTokenAdd, amount);
        uint256 availableBalance = getTokenBalance(tokenAdd);
        if (tokenAmount > availableBalance) {
            revert InsufficientBalance({
                available: availableBalance,
                required: tokenAmount
            });
        }
        uint256 returnAmount = tokenAmount;
        uint256 fee;
        if (feePermille > 0) {
            returnAmount = returnAmount.mul(uint256(1000).sub(feePermille)).div(1000);
            fee = tokenAmount.sub(returnAmount);

            // Update withdrawers assigned rewards, before before burning tokens
            feeRewards1.updateRewards(owner);
            feeRewards2.updateRewards(owner);
            // Burn the holders tokens before adding new rewards
            lpToken.burnFrom(owner, amount);
            if (feeSplitPercent1 > 0) {
                uint256 fee1 = fee.mul(feeSplitPercent1).div(100);
                // Add the fee to the common reward pool
                feeRewards1.addRewards(owner, fee1);
                // Transfer the fee to the reward handler
                IERC20(tokenAdd).transfer(address(feeRewards1), fee1);
            }
            if (feeSplitPercent2 > 0) {
                uint256 fee2 = fee.mul(feeSplitPercent2).div(100);
                // Add the fee to the common reward pool
                feeRewards2.addRewards(owner, fee2);
                // Transfer the fee to the reward handler
                IERC20(tokenAdd).transfer(address(feeRewards2), fee2);
            }
        }
        // Return the withdrawn tokens, after fee deduction
        bool success = IERC20(tokenAdd).transfer(owner, returnAmount);
        if (!success) {
            revert TokenWithdrawFailed({
                receiver: owner,
                tokenAdd: tokenAdd,
                amount: tokenAmount
            });
        }

        emit TokensWithdrawn(owner, tokenAdd, tokenAmount, fee);
        return true;
    }

    /**
     * @notice Transfer own tokens to external Box. Doesn't affect LPTokens.
     * @param tokenAdd The token type to transfer.
     * @param amount The amount to transfer.
     */
    function _moveToExt(
        address tokenAdd,
        uint256 amount
    )
        internal
        onlyAllowedToken(tokenAdd)
    {
        if (amount == 0)
            return;
        if (address(extLockBox) == address(0))
            revert BadAccountZero();

        uint256 bal = getTokenBalance(tokenAdd);
        if (amount > bal) {
            revert InsufficientBalance({
                available: bal,
                required: amount
            });
        }
        IERC20(tokenAdd).transfer(address(extLockBox), amount);
    }

    /**
     * @notice Sets the amount of fees to charge and distribute.
     * @param inFeePermille is the fee permille to charge.
     * @param inFeePercent1 is the percent of the fee that goes to handler 1.
     * @param inFeePercent2 is the percent of the fee that goes to handler 2.
     */
    function _setFees(
        uint8 inFeePermille,
        uint8 inFeePercent1,
        uint8 inFeePercent2
    ) private {
        if (inFeePermille > 0 && inFeePercent1 + inFeePercent2 != 100) {
            revert InvalidFees({
                feePermille: inFeePermille,
                feePercent1: inFeePercent1,
                feePercent2: inFeePercent2
            });
        }
        feePermille = inFeePermille;
        feeSplitPercent1 = inFeePercent1;
        feeSplitPercent2 = inFeePercent2;
        emit FeesSet(feePermille, feeSplitPercent1, feeSplitPercent2);
    }

    /**
     * @notice Transfers tokens using TokenTransferProxy.
     * @param owner Address transfering tokens from.
     * @param tokenAdd Address of token type to transfer.
     * @param to Address receiving tokens.
     * @param value Amount of tokens to transfer.
     * @return bool Success of tokens transfer.
     */
    function transferViaProxy(
        address owner,
        address tokenAdd,
        address to,
        uint256 value
    ) private returns (bool) {
        return tokenTransferProxy.transferFrom(tokenAdd, owner, to, value);
    }
}

