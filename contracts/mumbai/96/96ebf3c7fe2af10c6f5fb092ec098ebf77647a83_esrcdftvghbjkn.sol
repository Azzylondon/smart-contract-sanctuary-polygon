pragma solidity >=0.7.0 <0.9.0;

/**
 * @title Storage
 * @dev Store & retrieve value in a variable
 */
contract esrcdftvghbjkn {

    uint256 number;

    /**
     * @dev Store value in variable
     * @param num value to store
     */
    function storeesrcdftvghbjkn(uint256 num) public {
        number = num;
    }

    /**
     * @dev Return value 
     * @return value of 'number'
     */
    function retrieveesrcdftvghbjkn() public view returns (uint256){
        return number;
    }
}