pragma solidity 0.8.16;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./interface/IPoolBetting.sol";
import "./utils/Math.sol";

contract PoolBetting is Ownable, AccessControl, IPoolBetting {

    bytes32 public constant FINANCIAL_ROLE = keccak256("FINANCIAL_ROLE");
    bytes32 public constant ORACLE_ROLE = keccak256("ORACLE_ROLE");

    using EnumerableSet for EnumerableSet.Bytes32Set;

    IERC20 public token;

    // Commission for all the bets, 5% by default
    uint256 commissionToApply = 5;

    // Commission for all the bets available for withdrawal
    uint256 public commissionAvailable;

    // Commission withdrawn already
    uint256 public commissionWithdrawn;

    // Minimal bet amount
    uint256 public minBetAmount = 10**16;

    mapping(bytes32 => Event) public eventDetails;

    // List of active events
    EnumerableSet.Bytes32Set private activeEvents;

    // Total staked amount by the account. Limiting by the maxLimitAmount;
    //eventId => user => player => Bet
    mapping(bytes32 => mapping(address => mapping(uint16 => Bet))) public bets;

    modifier activeEvent(bytes32 _eventId) {
        require(eventDetails[_eventId].status == EventStatus.OPEN, "Event is not active");
        _;
    }

    modifier onlyOracle() {
        require(hasRole(ORACLE_ROLE, msg.sender), "ORACLE_ROLE is required");
        _;
    }

    modifier onlyFinance() {
        require(hasRole(FINANCIAL_ROLE, msg.sender), "FINANCIAL_ROLE is required");
        _;
    }

    constructor(address _token) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        token = IERC20(_token);
        minBetAmount = 10**(IERC20Metadata(_token).decimals()-2);
    }

    function createNewEvent(
        bytes32 _eventId,
        uint256 _bettingStarts,
        uint256 _bettingStops,
        uint256 _eventStarts,
        uint256 _eventStops,
        uint16 _totalPlayers
    )
        external
        override
        onlyOracle
    {
        require(
            _bettingStarts < _bettingStops,
            "Incorrect date for betting start"
        );
        require(_eventStarts < _eventStops, "Incorrect date for event start");
        require(_totalPlayers <= 500, "Too many players");
        require(eventDetails[_eventId].totalPlayers == 0, "Event ID already exists");

        uint256[] memory playersBetAmount = new uint[](_totalPlayers);

        eventDetails[_eventId] = Event(
            _eventStarts,
            _eventStops,
            _bettingStarts,
            _bettingStops,
            0,
            0,
            _totalPlayers,
            0,
            EventStatus.OPEN,
            playersBetAmount
        );

        activeEvents.add(_eventId);

        emit EventCreatedNotification(
            _eventId,
            _bettingStarts,
            _bettingStops,
            _eventStarts,
            _eventStops,
            _totalPlayers,
            block.timestamp
        );
    }

    function closeEvent(bytes32 _eventId, uint16 _winnerPlayerID)
        external
        override
        onlyOracle
        activeEvent(_eventId)
    {
        eventDetails[_eventId].winner = _winnerPlayerID;
        commissionAvailable += eventDetails[_eventId].totalCommission;
        eventDetails[_eventId].status = EventStatus.CLOSED;

        activeEvents.remove(_eventId);

        emit EventClosedNotification(
            _eventId,
            _winnerPlayerID,
            block.timestamp
        );
    }

    function cancelEvent(bytes32 _eventId)
        external
        override
        onlyOracle
        activeEvent(_eventId)
    {

        eventDetails[_eventId].status = EventStatus.CANCELLED;

        activeEvents.remove(_eventId);

        emit EventCancelledNotification(
            _eventId,
            block.timestamp
        );
    }

    function placeBet(bytes32 _eventId, uint16 _playerId, uint256 _amount)
        external
        override
        activeEvent(_eventId)
    {
        require(eventDetails[_eventId].bettingStarts <= block.timestamp, "Betting is not started yet");
        require(eventDetails[_eventId].bettingStops >= block.timestamp, "Betting is finished already");
        require(_playerId < eventDetails[_eventId].totalPlayers, "Player number is invalid");

        require(_amount >= minBetAmount, "Bet is too small");
        require(_amount <= token.balanceOf(msg.sender), "Insufficient funds");
        require(_amount <= token.allowance(msg.sender, address(this)), "Allowance is required");

        uint256 commission = (_amount / 100) * commissionToApply;
        uint256 betAmount = _amount - commission;

        bets[_eventId][msg.sender][_playerId].betAmount += betAmount;
        bets[_eventId][msg.sender][_playerId].commission += commission;

        eventDetails[_eventId].totalBetAmount += betAmount;
        eventDetails[_eventId].totalCommission += commission;
        eventDetails[_eventId].playersBetAmount[_playerId] += betAmount;

        (bool success) = token.transferFrom(msg.sender, address(this), _amount);
        require(success, "Transfer Failed");

        emit BetIsPlacedNotification(
            _eventId,
            _playerId,
            betAmount,
            commission,
            msg.sender,
            block.timestamp);
    }


    function withdraw(bytes32 _eventId)
        external
        override
    {
        require(
            eventDetails[_eventId].status == EventStatus.CLOSED ||
            eventDetails[_eventId].status == EventStatus.CANCELLED,
            "Event is not closed"
        );
        uint16 winner = eventDetails[_eventId].winner;

        uint256 toWithdraw = this.availableForWithdraw(_eventId, msg.sender);

        bets[_eventId][msg.sender][winner].withdrawn = true;

        (bool success) = token.transfer(msg.sender, toWithdraw);
        require(success, "Transfer Failed");

        emit WithdrawNotification(
            _eventId,
            msg.sender,
            toWithdraw,
            block.timestamp
        );
    }

    function eventDetailsView(bytes32 _eventId)
        external
        override
        view
        returns (Event memory)
    {
        return eventDetails[_eventId];
    }

    function betView(bytes32 _eventId, address user, uint16 player)
    external
    override
    view
    returns (Bet memory)
    {
        return bets[_eventId][user][player];
    }


    function availableForWithdraw(bytes32 _eventId, address _user)
        external
        override
        view
        returns (uint256)
    {
        uint16 winner = eventDetails[_eventId].winner;

        // Bet / Profit has been withdrawn already
        if (bets[_eventId][_user][winner].withdrawn) {
            return 0;
        }

        if (eventDetails[_eventId].status == EventStatus.OPEN) {
            return 0;
        }

        // Entire amount is available for withdrawal if event has been cancelled
        if (eventDetails[_eventId].status == EventStatus.CANCELLED) {
            return bets[_eventId][_user][winner].betAmount + bets[_eventId][_user][winner].commission;
        }

        uint256 userBet = bets[_eventId][_user][winner].betAmount;

        if (userBet == 0) {
            return 0;
        }

        uint256 prizePool = eventDetails[_eventId].totalBetAmount - eventDetails[_eventId].playersBetAmount[winner];

        uint256 shares = Math.mulDiv(userBet, prizePool, eventDetails[_eventId].playersBetAmount[winner]);

        return shares + userBet;
    }


    function withdrawCommission()
        external
        override
        onlyFinance
    {
        require(commissionAvailable <= token.balanceOf(address(this)), "Insufficient funds");

        uint256 totalCommission = commissionAvailable;
        commissionAvailable = 0;

        commissionWithdrawn += totalCommission;

        (bool success) = token.transfer(msg.sender, totalCommission);
        require(success, "Transfer Failed");

        emit WithdrawCommissionNotification(
            totalCommission,
            block.timestamp
        );
    }

    function getActiveEvents(uint128 page, uint128 limit) external view returns (bytes32[] memory){
        uint256 start = page * limit;
        uint256 end = (page + 1) * limit;
        if (start >= activeEvents.length()) {
            bytes32[] memory empty;
            return empty;
        }
        if (end > activeEvents.length()) {
            end = activeEvents.length();
        }
        bytes32[] memory result = new bytes32[](end-start);
        for (uint256 i = start; i < end; i++) {
            result[i-start] = activeEvents.values()[i];
        }
        return result;
    }

    // Owner Only

    function setMinBetAmount(uint256 _minBetAmount)
        external
        onlyOwner
    {
        minBetAmount = _minBetAmount;
    }

    function setCommissionToApply(uint256 _commissionToApply)
        external
        onlyOwner
    {
        commissionToApply = _commissionToApply;
    }
}

