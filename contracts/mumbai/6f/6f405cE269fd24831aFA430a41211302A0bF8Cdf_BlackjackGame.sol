pragma solidity ^0.8.9;

library Card {
    function getCardRank(uint256 card) internal pure returns (uint256) {
        return (card / 4) + 1;
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.9;

library CardDeck {
    struct State {
        bool x;
        mapping(uint256 => bool) used;
        uint256 usedCount;
    }

    using CardDeck for CardDeck.State;

    function drawCard(State storage state, uint256 index) internal returns (uint256) {
        require(index >= 0 && index < state.cardsLeft(), "Index out of bounds");
        uint256 currentCard = 0;
        for (uint256 i = 0; i <= index; i++) {
            currentCard++;
            while (state.used[currentCard]) {
                currentCard++;
            }
        }
        state.used[currentCard] = true;
        state.usedCount++;
        return currentCard - 1;
    }

    function cardsLeft(State storage state) internal view returns (uint256) {
        return 52 - state.usedCount;
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.9;

import "./Card.sol";

library Hand {
    struct State {
        uint256 sum;
        bool hasAce;
        uint256[] cards;
    }

    function addCard(State storage state, uint256 card) internal {
        state.cards.push(card);
        uint256 rank = Card.getCardRank(card);
        if (rank == 1) {
            state.sum += 1;
            state.hasAce = true;
        } else if (rank < 10) {
            state.sum += rank;
        } else {
            state.sum += 10;
        }
    }

    function getSum(State storage state) internal view returns (uint256) {
        if (state.sum < 12 && state.hasAce) {
            return state.sum + 10;
        }
        return state.sum;
    }
}

