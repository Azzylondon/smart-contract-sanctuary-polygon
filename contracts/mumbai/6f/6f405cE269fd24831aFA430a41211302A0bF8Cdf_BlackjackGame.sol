pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "./libs/IRandomizer.sol";
import "./libs/CardDeck.sol";
import "./libs/Hand.sol";

import "hardhat/console.sol";

enum State {
    EMPTY,
    CREATED,
    PLAYERS_PLAY,
    HIT,
    DOUBLE,
    STAND,
    DEALERS_PLAY,
    ENDED,
    PLAYER_LOST,
    DRAW,
    PLAYER_WON,
    REDEEMED
}

enum WinState {
    NONE,
    PLAYER_LOST,
    DRAW,
    PLAYER_WON,
    PLAYER_SURRENDER,
    PLAYER_BLACKJACK
}

struct Game {
    State state;
    WinState winState;
    uint256 value;
    CardDeck.State deck;
    address payable player;

    uint256 round;

    Hand.State playerHand;
    Hand.State dealerHand;
}

struct GameInfo {
    State state;
    WinState winState;
    uint256 value;
    
    address payable player;

    uint256 round;

    Hand.State playerHand;
    Hand.State dealerHand;
}

struct PlayerInfo {
    bytes32 currentGame;
    bytes32[] games;
}

contract BlackjackGame is Ownable {
    using Address for address payable;
    using CardDeck for CardDeck.State;
    using Hand for Hand.State;

    IRandomizer randomizer;
    bool mockRandomizer;
    mapping(bytes32 => Game) games;
    mapping(address => bytes32) public latestGameId;
    mapping(address => PlayerInfo) players;

    event PlayerBust(bytes32 gameId);
    event DealerBust(bytes32 gameId);
    event Blackjack(bytes32 gameId);
    event GameEnd(bytes32 gameId);
    event PlayerWon(bytes32 gameId);
    event PlayerDraw(bytes32 gameId);
    event PlayerLost(bytes32 gameId);
    event Payout(bytes32 gameId, uint256 amount);

    constructor(IRandomizer _randomizer, bool _mockRandomizer) {
        randomizer = _randomizer;
        mockRandomizer = _mockRandomizer;
    }

    function createGame() public payable {
        require(msg.value >= 2, "You need to send at least 2 wei");
        PlayerInfo storage player = players[msg.sender];
        State currentGameState = games[player.currentGame].state;
        require(currentGameState == State.EMPTY || currentGameState == State.REDEEMED, "You already have a game in progress");

        bytes32 gameId = keccak256(abi.encodePacked(msg.sender, block.timestamp));

        Game storage game = games[gameId];
        require(game.state == State.EMPTY, "Game already exists");
        transitionTo(gameId, State.CREATED);
        game.value = msg.value;
        game.player = payable(msg.sender);

        player.currentGame = gameId;
        player.games.push(gameId);

        seed(gameId);
        latestGameId[msg.sender] = gameId;
    }

    function setup(bytes32 gameId) public onlyPlayer(gameId) onlyState(gameId, State.CREATED) {
        Game storage game = games[gameId];

        game.playerHand.addCard(drawCard(gameId));
        game.dealerHand.addCard(drawCard(gameId));
        game.playerHand.addCard(drawCard(gameId));

        transitionTo(gameId, State.PLAYERS_PLAY);
    }

    function hit(bytes32 gameId) public onlyPlayer(gameId) onlyState(gameId, State.PLAYERS_PLAY) {
        seed(gameId);
        transitionTo(gameId, State.HIT);
    }

    function double(bytes32 gameId) public payable onlyPlayer(gameId) onlyState(gameId, State.PLAYERS_PLAY) {
        Game storage game = games[gameId];
        require(msg.value == game.value, "Must pay the same amount as the initial bet");
        require(game.round == 1, "Can only double on the first round");

        game.value += msg.value;
        seed(gameId);
        transitionTo(gameId, State.DOUBLE);
    }

    function stand(bytes32 gameId) public onlyPlayer(gameId) onlyState(gameId, State.PLAYERS_PLAY) {
        seed(gameId);
        transitionTo(gameId, State.STAND);
    }

    function continueGame(bytes32 gameId) public {
        State state = games[gameId].state;
        if (state == State.HIT) {
            continueHit(gameId);
        } else if (state == State.DOUBLE) {
            continueDouble(gameId);
        } else if (state == State.STAND) {
            continueStand(gameId);
        }
    }

    function continueHit(bytes32 gameId) internal onlyPlayer(gameId) onlyState(gameId, State.HIT) {
        Game storage game = games[gameId];
        game.playerHand.addCard(drawCard(gameId));
        transitionTo(gameId, State.PLAYERS_PLAY);
    }

    function continueDouble(bytes32 gameId) internal onlyPlayer(gameId) onlyState(gameId, State.DOUBLE) {
        Game storage game = games[gameId];
        game.playerHand.addCard(drawCard(gameId));
        transitionTo(gameId, State.DEALERS_PLAY);
    }

    function continueStand(bytes32 gameId) internal onlyPlayer(gameId) onlyState(gameId, State.STAND) {
        transitionTo(gameId, State.DEALERS_PLAY);
    }

    function surrender(bytes32 gameId) public onlyPlayer(gameId) onlyState(gameId, State.PLAYERS_PLAY) {
        Game storage game = games[gameId];
        require(game.round == 1, "Can only surrender on the first round");
        game.winState = WinState.PLAYER_SURRENDER;
        
        uint256 payAmount = game.value / 2;
        transitionTo(gameId, State.REDEEMED);

        payout(gameId, payAmount);
    }

    function transitionTo(bytes32 gameId, State toState) private {
        // console.log("transition to", uint256(toState));

        Game storage game = games[gameId];
        game.state = toState;

        if (toState == State.PLAYERS_PLAY) {
            onPlayersPlay(gameId);
        } else if (toState == State.DEALERS_PLAY) {
            onDealersPlay(gameId);
        } else if (toState == State.ENDED) {
            onEnd(gameId);
        } else if (toState == State.PLAYER_LOST) {
            onPlayerLost(gameId);
        } else if (toState == State.DRAW) {
            onDraw(gameId);
        } else if (toState == State.PLAYER_WON) {
            onPlayerWon(gameId);
        }
    }

    function onPlayersPlay(bytes32 gameId) private {
        Game storage game = games[gameId];
        game.round += 1;
        if (game.round == 1 && game.playerHand.getSum() == 21) {
            emit Blackjack(gameId);
            game.dealerHand.addCard(drawCard(gameId));
            transitionTo(gameId, State.ENDED);
            return;
        }
        if (game.playerHand.getSum() > 21) {
            emit PlayerBust(gameId);
            game.dealerHand.addCard(drawCard(gameId));
            transitionTo(gameId, State.PLAYER_LOST);
            return;
        }
    }

    function onDealersPlay(bytes32 gameId) private {
        Game storage game = games[gameId];
        game.dealerHand.addCard(drawCard(gameId));
        if (game.playerHand.getSum() > 21) {
            emit PlayerBust(gameId);
            transitionTo(gameId, State.PLAYER_LOST);
            return;
        }
        if (game.dealerHand.getSum() < 17) {
            transitionTo(gameId, State.DEALERS_PLAY);
            return;
        }
        if (game.dealerHand.getSum() > 21) {
            emit DealerBust(gameId);
            transitionTo(gameId, State.PLAYER_WON);
            return;
        }
        transitionTo(gameId, State.ENDED);
    }

    function onEnd(bytes32 gameId) private {
        emit GameEnd(gameId);

        uint256 playerSum = games[gameId].playerHand.getSum();
        uint256 dealerSum = games[gameId].dealerHand.getSum();
        
        if (playerSum == dealerSum) {
            transitionTo(gameId, State.DRAW);
        } else if (playerSum > dealerSum) {
            transitionTo(gameId, State.PLAYER_WON);
        } else {
            transitionTo(gameId, State.PLAYER_LOST);
        }
    }

    function onPlayerLost(bytes32 gameId) private {
        emit PlayerLost(gameId);
        games[gameId].winState = WinState.PLAYER_LOST;
        transitionTo(gameId, State.REDEEMED);
    }

    function onDraw(bytes32 gameId) private {
        emit PlayerDraw(gameId);

        Game storage game = games[gameId];
        game.winState = WinState.DRAW;
        uint256 payAmount = game.value;
        transitionTo(gameId, State.REDEEMED);

        payout(gameId, payAmount);
    }

    function onPlayerWon(bytes32 gameId) private {
        emit PlayerWon(gameId);

        Game storage game = games[gameId];
        if (game.round == 1 && game.playerHand.getSum() == 21) {
            game.winState = WinState.PLAYER_BLACKJACK;
        } else {
            game.winState = WinState.PLAYER_WON;
        }
        uint256 payAmount = game.value * 2;
        transitionTo(gameId, State.REDEEMED);

        payout(gameId, payAmount);
    }

    function seed(bytes32 gameId) private {
        randomizer.seed(gameId);
    }

    function drawCard(bytes32 gameId) private returns (uint256) {
        Game storage game = games[gameId];
        uint256 index = randomizer.random(gameId, game.deck.cardsLeft());
        if (mockRandomizer) {
            return index;
        }
        return game.deck.drawCard(index);
    }

    function payout(bytes32 gameId, uint256 payAmount) private {
        Game storage game = games[gameId];
        game.player.sendValue(payAmount);
        emit Payout(gameId, payAmount);
    }

    function getCurrentGame(address player) public view returns (bytes32) {
        return players[player].currentGame;
    }

    function getGameState(bytes32 gameId) public view returns (State) {
        return games[gameId].state;
    }

    function getGame(bytes32 gameId) public view returns (GameInfo memory) {
        Game storage game = games[gameId];
        GameInfo memory gameInfo;
        gameInfo.state = game.state;
        gameInfo.winState = game.winState;
        gameInfo.player = game.player;
        gameInfo.value = game.value;
        gameInfo.round = game.round;
        gameInfo.playerHand = game.playerHand;
        gameInfo.dealerHand = game.dealerHand;
        return gameInfo;
    }

    function getPlayerSum(bytes32 gameId) public view returns (uint256) {
        return games[gameId].playerHand.getSum();
    }

    function getDealerSum(bytes32 gameId) public view returns (uint256) {
        return games[gameId].dealerHand.getSum();
    }

    function resetPlayer() external {
        players[msg.sender].currentGame = 0;
    }

    function withdraw(uint256 amount) external onlyOwner {
        require(address(this).balance >= amount, "Not enough balance");
        payable(msg.sender).sendValue(amount);
    }

    // Functions related to previewing game states
    function previewContinue(bytes32 gameId) public {
        Game storage game = games[gameId];
        if (game.state == State.CREATED) {
            setup(gameId);
        } else {
            continueGame(gameId);
        }
        bytes memory gameInfo = abi.encode(getGame(gameId));
        revert(string.concat("gameInfo:", string(gameInfo)));
    }

    function setupOrContinue(bytes32 gameId) internal {
        Game storage game = games[gameId];
        if (game.state == State.CREATED) {
            setup(gameId);
        } else {
            continueGame(gameId);
        }
    }

    function continueAndHit(bytes32 gameId) public {
        setupOrContinue(gameId);
        hit(gameId);
    }

    function continueAndDouble(bytes32 gameId) public payable {
        setupOrContinue(gameId);
        double(gameId);
    }

    function continueAndStand(bytes32 gameId) public {
        setupOrContinue(gameId);
        stand(gameId);
    }

    function continueAndSurrender(bytes32 gameId) public {
        setupOrContinue(gameId);
        surrender(gameId);
    }

    receive() external payable onlyOwner {

    }

    modifier onlyPlayer(bytes32 gameId) {
        Game storage game = games[gameId];
        require(game.player == msg.sender, "You are not the owner of this game");
        _;
    }

    modifier onlyState(bytes32 gameId, State state) {
        Game storage game = games[gameId];
        require(game.state == state, "Game is not in the correct state");
        _;
    }
}

