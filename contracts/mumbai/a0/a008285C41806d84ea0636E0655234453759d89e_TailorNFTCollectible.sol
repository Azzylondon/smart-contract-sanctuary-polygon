        0xba1aa5bbc6b8041418eeb66d935650729704cb07d45789621b0f966fb6decb10;
    bytes32 internal constant TRUSTED_DEPLOYER =
        0xd0e126342bd1588770ee6698792a354eb154e4d8e09a2fc30cd7284f45e8f4b0;

    ITailorControlAccess internal TAILOR_ACCESS_CONTROL;

    function __TailorFoundation_init_unchained(address configuration_address)
        internal
        onlyInitializing
    {
        __setRoles_init(configuration_address);
        __Ownable_init();
    }

    function __TailorFoundation_init(address configuration_address)
        internal
        initializer
    {
        __TailorFoundation_init_unchained(configuration_address);
    }

