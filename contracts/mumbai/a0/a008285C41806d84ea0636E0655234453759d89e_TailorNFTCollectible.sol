    }

    function _onlyAdmin() internal view returns (bool) {
        return TAILOR_ACCESS_CONTROL.hasRole(TAILOR_ADMIN, _msgSender());
    }

    function _isTrustedDeployer(address caller) internal view returns (bool) {
        return TAILOR_ACCESS_CONTROL.hasRole(TRUSTED_DEPLOYER, caller);
    }

    function _isTrustedForwarder(address forwarder)
