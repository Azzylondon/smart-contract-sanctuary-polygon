        _;
    }

    modifier onlyCollectionCreator() {
        require(
            TAILOR_ACCESS_CONTROL.hasRole(TAILOR_ADMIN, _msgSender()) ||
                TAILOR_ACCESS_CONTROL.hasRole(COLLECTION_CREATOR, _msgSender()),
            "Not Collection Creator"
        );
        _;
