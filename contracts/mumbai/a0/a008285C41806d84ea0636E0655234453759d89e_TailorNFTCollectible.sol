TAILORTAILORTAILORTAILORTAILORTAILOR                 TAILORTAILORTAILORT                 TAILORTAILORTAILORTAILORTAILORTAILOR           
TAILORTAILORTAILORTAILORTAILORTAIL                     TAILORTAILORTAI                     TAILORTAILORTAILORTAILORTAILORTAIL           
TAILORTAILORTAILORTAILORTAIL                               TAILO                               TAILORTAILORTAILORTAILORTAILO            
TAILORTAILORTAILORTAILORTAIL                               TAILO                               TAILORTAILORTAILORTAILORTAIL            
 TAILORTAILORTAILORTAILORTAI                               TAILO                               TAILORTAILORTAILORTAILORTAI             
  TAILORTAILORTAILORTAILORTA                               TAILO                               TAILORTAILORTAILORTAILORTA              
	TAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAI               
		TAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAIL                 
		TAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILO                    
			TAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAI                        
					TAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAILORTAIL                           
 */

contract TailorNFTCollectible is
    Initializable,
    ERC721URIStorageUpgradeable,
    TailorFoundation,
    TailorRoyalties
{
    /**
     * @dev
     * We add this placeholder to be able to add inherited contract for a future upgrade
     * and keep the correct slots for our state variables
     */
    uint256[500] private __placeholder;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using Collections for Collections.Collection;

    bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;
    bytes4 private constant _INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 private constant _INTERFACE_ID_ERC721_METADATA = 0x5b5e139f;

    bytes32 private constant _TAILOR_ADMIN =
        0x3f6ce2d5571ab8f3fb1cf068a92ec99edc01aa4a757338b1f082acee66c84cb2;
    bytes32 private constant _COLLECTION_CREATOR =
        0x605f3e359b5224b78fd55afc82a0086dba41cff5d3cb8e860e84b9e27d7c4dfa;
    bytes32 private constant _TRUSTED_FORWARDER =
        0xba1aa5bbc6b8041418eeb66d935650729704cb07d45789621b0f966fb6decb10;

    // mapping(COLLECTIONID_128 => Collections.Collection) internal collections;
    mapping(uint128 => Collections.Collection) internal collections;
    // mapping(NFTCATEGORYID_128 => COLLECTIONID_128) internal nftCategories;
    mapping(uint128 => uint128) internal nftCategories;

    event CollectionCreated(
        address creator,
        uint128 collectionId,
        string baseURI,
        uint128[] nftCategoryIds,
        uint32[] collectionScarcitiesSupply
    );
    event NFTCategoryBurnt(
        address creator,
        uint128 collectionId,
        uint128 nftCategoryId
    );
    event CollectionBurnt(address creator, uint128 collectionId);
    event NFTCategoryAdded(
        address creator,
        uint128 collectionId,
        uint128 nftCategoryId,
        uint32 collectionScarcitySupply
    );
    event CollectionURIChanged(uint128 collectionId, string baseURI);

    event NFTCategoryURIChanged(
        uint128 collectionId,
        uint128 nftCategoryId,
        string metadataURI
    );

    /**
     * @dev Function to get collection Creator
     * @param collectionId id of the collection
     */
    function _getCollectionCreator(uint128 collectionId)
        internal
        view
        returns (address)
    {
        return collections[collectionId].collectionCreator;
    }

    function _hasAccessRight(uint128 collectionId)
        internal
        view
        returns (bool)
    {
        require(
            _onlyAdmin() || _msgSender() == _getCollectionCreator(collectionId),
            "Caller Not Authorized"
        );
        return true;
    }

    modifier hasAccessRight(uint128 collectionId) {
        _hasAccessRight(collectionId);
        _;
    }

    /**
     * @dev Function to create a collection | Emits CollectionCreated event
     * @param collectionId id of the collection
     * @param baseURI baseURI of the collection
     * @param collectionScarcitiesSupply array of collection scarcities
     * @param nftCategoryMetaData array of nftcatgeory metadata the collection
     * TO DO : Include Royalty Settings (TailorRoyalties)
     */
    function createCollection(
        uint128 collectionId,
        string calldata baseURI,
        uint128[] calldata nftCategoryIds,
        string[] calldata nftCategoryMetaData,
        uint32[] calldata collectionScarcitiesSupply
    ) external onlyCollectionCreator {
        require(collectionId > 0, "collectionId should be > 0");
        Collections.Collection storage newCollection = collections[
            collectionId
        ];
        require(
            nftCategoryIds.length == collectionScarcitiesSupply.length,
            string(
                abi.encodePacked(
                    "CategoryIds and Supply not equal : ",
                    StringsUpgradeable.toString(nftCategoryIds.length),
                    " ; ",
                    StringsUpgradeable.toString(
                        collectionScarcitiesSupply.length
                    )
                )
            )
        );
        require(
            bytes(newCollection.baseURI).length == 0,
            "Collection already exists"
        );
        require(bytes(baseURI).length > 0, "baseURI params is required");
        newCollection.collectionCreator = _msgSender();
        newCollection.baseURI = baseURI;
        newCollection.nbCategories = uint16(collectionScarcitiesSupply.length);
        for (uint16 i = 0; i < collectionScarcitiesSupply.length; i++) {
            require(
                nftCategories[nftCategoryIds[i]] == 0,
                "nftCategory already exists"
            );
            nftCategories[nftCategoryIds[i]] = collectionId;
            newCollection.maxSupplies[
                nftCategoryIds[i]
            ] = collectionScarcitiesSupply[i];
            newCollection._nftCategoryIdx[i] = nftCategoryIds[i];
            if (bytes(nftCategoryMetaData[i]).length > 0) {
                newCollection.nftCategoryMetadata[
                    nftCategoryIds[i]
                ] = nftCategoryMetaData[i];
            }
        }
        emit CollectionCreated(
            _msgSender(),
            collectionId,
            baseURI,
            nftCategoryIds,
            collectionScarcitiesSupply
        );
    }

    /**
     * @dev External Function to buy a ticket | Emits Transfer event
     * @param to address of the buyer
     * @param collectionId id of the collection
     * @param nftCategoryId id of NFT category
     * @param urimetadata URI metadata
     */
    function buyNFT(
        address to,
        uint128 collectionId,
        uint128 nftCategoryId,
        uint128 nftId,
        string calldata urimetadata
    ) external hasAccessRight(collectionId) {
        _buyNFT(to, collectionId, nftCategoryId, nftId, urimetadata);
    }

    /**
     * @dev Internal function to buy a ticket
     */
    function _buyNFT(
        address to,
        uint128 collectionId,
        uint128 nftCategoryId,
        uint128 nftId,
        string calldata urimetadata
    ) internal {
        require(_msgSender() != address(0), "Impossible to buy from 0 address");
        Collections.Collection storage collection = collections[collectionId];

        require(bytes(collection.baseURI).length > 0, "Collection not found");
        require(
            nftCategories[nftCategoryId] == collectionId,
            "nftCategoryId out-of-bound"
        );
        require(
            (bytes(collection.nftCategoryMetadata[nftCategoryId]).length > 0) ||
                (bytes(urimetadata).length > 0),
            "Metadata is null for token"
        );

        CountersUpgradeable.Counter
            storage nftCategoryAlreadyMinted = collection.alreadyMinted[
                nftCategoryId
            ];

        nftCategoryAlreadyMinted.increment();

        if (!collection.soldItem) {
            collection.soldItem = true;
        }

        require(
            uint32(nftCategoryAlreadyMinted.current()) <=
                collection.maxSupplies[nftCategoryId],
            string(
                abi.encodePacked(
                    "Max supply : ",
                    StringsUpgradeable.toString(
                        collection.maxSupplies[nftCategoryId]
                    )
                )
            )
        );

        uint256 newId = TokenIdStructure._computeTokenId(
            TokenIdStructure.TokenIdStruct(nftCategoryId, nftId)
        );

        _safeMint(to, newId);
        if (bytes(urimetadata).length > 0) {
            _setTokenURI(newId, urimetadata);
        }
    }

    /**
     * @dev External Function to buy a batch of ticket | Emits Transfer event
     * @param to address of the buyer
     * @param collectionId id of the collection
     * @param nftCategoryId id of NFT category
     * @param urimetadata URI metadata
     */
    function batchBuyNFT(
        address[] calldata to,
        uint128 collectionId,
        uint128[] calldata nftCategoryId,
        uint128[] calldata nftId,
        string[] calldata urimetadata
    ) external onlyAdmin {
        Collections.Collection storage collection = collections[collectionId];
        require(bytes(collection.baseURI).length > 0, "Collection not found");

        for (uint256 i; i < to.length; i++) {
            require(
                nftCategories[nftCategoryId[i]] == collectionId,
                "nftCategoryId out-of-bound"
            );
            _buyNFTChecked(
                to[i],
                collection,
                nftCategoryId[i],
                nftId[i],
                urimetadata[i]
            );
        }
        if (!collection.soldItem) {
            collection.soldItem = true;
        }
    }

    /**
     * @dev Internal function to buy a ticket
     */
    function _buyNFTChecked(
        address to,
        Collections.Collection storage collection,
        uint128 nftCategoryId,
        uint128 nftId,
        string calldata urimetadata
    ) internal {
        require(_msgSender() != address(0), "Impossible to buy from 0 address");
        require(
            (bytes(collection.nftCategoryMetadata[nftCategoryId]).length > 0) ||
                (bytes(urimetadata).length > 0),
            "Metadata is null for token"
        );
        CountersUpgradeable.Counter
            storage nftCategoryAlreadyMinted = collection.alreadyMinted[
                nftCategoryId
            ];
        nftCategoryAlreadyMinted.increment();

        require(
            uint32(nftCategoryAlreadyMinted.current()) <=
                collection.maxSupplies[nftCategoryId],
            string(
                abi.encodePacked(
                    "Max supply : ",
                    StringsUpgradeable.toString(
                        collection.maxSupplies[nftCategoryId]
                    )
                )
            )
        );

        uint256 newId = TokenIdStructure._computeTokenId(
            TokenIdStructure.TokenIdStruct(nftCategoryId, nftId)
        );

        _safeMint(to, newId);
        if (bytes(urimetadata).length > 0) {
            _setTokenURI(newId, urimetadata);
        }
    }

    /**
     * @dev See {BaseRelayRecipient-_msgSender}.
     * Returns caller address
     */
    function _msgSender()
        internal
        view
        override(TailorFoundation, ContextUpgradeable)
        returns (address)
    {
        return TailorFoundation._msgSender();
    }

    /**
     * @dev Initializes the contract by setting a `name` and a `symbol` to the token collection.
     * @param ERC721name name of the contract
     * @param ERC721symbol symbol of the contract
     */
    function initialize(
        string memory ERC721name,
        string memory ERC721symbol,
        address controlAccessAddress
    ) public initializer {
        __ERC721_init(ERC721name, ERC721symbol);
        __TailorFoundation_init(controlAccessAddress);
    }

    modifier hasBurnRight(uint256 tokenId) {
        uint128 nftCategoryId = TokenIdStructure.getNftCategoryId(tokenId);
        uint128 collectionId = nftCategories[nftCategoryId];
        _hasAccessRight(collectionId);
        _;
    }

    /**
     * @dev Function to add nftCategoryId inside collection | Emits AddCategoryCollection event
     * @param collectionId id of the collection
     * @param collectionScarcitiesSupply supply of nftCategory
     */
    function addNFTCategory(
        uint128 collectionId,
        uint128 nftCategoryId,
        uint32 collectionScarcitiesSupply
    ) external hasAccessRight(collectionId) {
        require(
            nftCategories[nftCategoryId] == 0,
            "nftCategory already exists"
        );
        Collections.Collection storage c = collections[collectionId];
        require(bytes(c.baseURI).length > 0, "Collection not found");
        nftCategories[nftCategoryId] = collectionId;
        c.maxSupplies[nftCategoryId] = collectionScarcitiesSupply;
        c._nftCategoryIdx[c.nbCategories] = nftCategoryId;
        c.nbCategories += 1;
        emit NFTCategoryAdded(
            _msgSender(),
            collectionId,
            nftCategoryId,
            collectionScarcitiesSupply
        );
    }

    /**
     * @dev Function to burn entire collection | Emits CollectionBurnt event
     * @param collectionId id of the collection
     */
    function burnCollection(uint128 collectionId)
        external
        hasAccessRight(collectionId)
    {
        Collections.Collection storage c = collections[collectionId];
        require(bytes(c.baseURI).length > 0, "Collection not found");
        require(c.soldItem == false, "NFT has already been sold");
        for (uint16 i = 0; i < c.nbCategories; i++) {
            nftCategories[c._nftCategoryIdx[i]] = 0;
        }
        delete collections[collectionId];

        emit CollectionBurnt(_msgSender(), collectionId);
    }

    /**
     * @dev Function to burn nftCategoryId inside collection | Emits NFTCategoryBurnt event
     * @param collectionId id of the collection
     * @param nftCategoryId id of the nftCategory
     */
    function burnNFTCategory(uint128 collectionId, uint128 nftCategoryId)
        external
        hasAccessRight(collectionId)
    {
        Collections.Collection storage c = collections[collectionId];
        require(bytes(c.baseURI).length > 0, "Collection not found");
        require(
            nftCategories[nftCategoryId] == collectionId,
            "NFTCategory not in collection"
        );
        CountersUpgradeable.Counter storage nftCategoryAlreadyMinted = c
            .alreadyMinted[nftCategoryId];

        require(
            nftCategoryAlreadyMinted.current() == 0,
            "Collection has a minted ticket"
        );
        delete c.maxSupplies[nftCategoryId];
        nftCategories[nftCategoryId] = 0;
        emit NFTCategoryBurnt(_msgSender(), collectionId, nftCategoryId);
    }

    /**
     * @dev Function to burn individual nft | Emits Transfer event
     * @param tokenId id of the token
     */
    function burnNFT(uint256 tokenId) external hasBurnRight(tokenId) {
        require(
            _exists(tokenId),
            "ERC721: operator query for nonexistent token"
        );
        uint128 nftCategoryId = TokenIdStructure.getNftCategoryId(tokenId);
        uint128 collectionId = nftCategories[nftCategoryId];
        Collections.Collection storage collection = collections[collectionId];
        CountersUpgradeable.Counter
            storage nftCategoryAlreadyMinted = collection.alreadyMinted[
                nftCategoryId
            ];

        nftCategoryAlreadyMinted.decrement();
        super._burn(tokenId);
    }

    /**
     * @dev Function to get a collection
     * @param collectionId id of collection
     * Returns string
     */
    function collectionURI(uint128 collectionId)
        external
        view
        returns (string memory uri)
    {
        Collections.Collection storage c = collections[collectionId];
        require(bytes(c.baseURI).length > 0, "Collection not found");
        return string(c.baseURI);
    }

    /**
     * @dev Function to get a collection
     * @param collectionId id of collection
     * Returns string
     */
    function setNFTCategoryURI(
        uint128 collectionId,
        uint128 nftCategoryId,
        string calldata _urimetadata
    ) external hasAccessRight(collectionId) {
        Collections.Collection storage c = collections[collectionId];
        require(bytes(c.baseURI).length > 0, "Collection not found");
        require(
            nftCategories[nftCategoryId] == collectionId,
            "nftCategoryId out-of-bound"
        );
        c.nftCategoryMetadata[nftCategoryId] = _urimetadata;
        emit NFTCategoryURIChanged(
            collectionId,
            nftCategoryId,
            string(abi.encodePacked(c.baseURI, _urimetadata))
        );
    }

    /**
     * @dev Function to modify a collection URI
     * @param collectionId id of collection
     * @param _collectionBaseURI baseURI
     * Returns string
     */
    function setCollectionURI(
        uint128 collectionId,
        string calldata _collectionBaseURI
    ) external hasAccessRight(collectionId) {
        Collections.Collection storage c = collections[collectionId];
        require(bytes(c.baseURI).length > 0, "Collection not found");
        require((bytes(_collectionBaseURI).length > 0), "InvalidURIError");
        c.baseURI = _collectionBaseURI;
        emit CollectionURIChanged(collectionId, _collectionBaseURI);
    }

    /**
     * @dev Sets the royalty information for a specific collection id.
     * @param collectionId collection id
     * @param receiver address of receiver
     * @param feeNumerator fee numerator
     */
    function setCollectionRoyalty(
        uint128 collectionId,
        address receiver,
        uint96 feeNumerator
    ) external hasAccessRight(collectionId) {
        require(
            bytes(collections[collectionId].baseURI).length > 0,
            "Collection not found"
        );
        super._setCollectionRoyalty(collectionId, receiver, feeNumerator);
    }

    function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
        external
        view
        override(TailorRoyalties)
        returns (address, uint256)
    {
        uint128 collectionId = nftCategories[
            TokenIdStructure.getNftCategoryId(_tokenId)
        ];
        RoyaltyInfo memory collectionRoyalty = _collectionRoyaltyInfo[
            collectionId
        ];

        uint256 royaltyAmount = (_salePrice *
            collectionRoyalty.royaltyFraction) / _feeDenominator();
        return (collectionRoyalty.receiver, royaltyAmount);
    }

    /**
     * @dev Function to get structure of tokenId
     * @param tokenId tokenId
     */
    function getNFTinfo(uint256 tokenId)
        external
        view
        returns (
            uint128 collectionId,
            uint128 nftCategoryId,
            uint128 nftId
        )
    {
        nftCategoryId = TokenIdStructure.getNftCategoryId(tokenId);
        collectionId = nftCategories[nftCategoryId];
        nftId = TokenIdStructure.getNftId(tokenId);
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     * @param tokenId id of the token
     * Returns string
     */
    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI query for nonexistent token"
        );
        string memory _tokenURI = super.tokenURI(tokenId);
        uint128 nftCatgoryId = TokenIdStructure.getNftCategoryId(tokenId);
        uint128 collectionId = nftCategories[nftCatgoryId];
        string memory metadata = collections[collectionId].nftCategoryMetadata[
            nftCatgoryId
        ];

        return
            string(
                abi.encodePacked(
                    collections[collectionId].baseURI,
                    bytes(_tokenURI).length > 0 ? _tokenURI : metadata
                )
            );
    }

    /**
     * @dev See {RelayRecipient-_msgData}.
     * Returns bytes calldata of caller
     */
    function _msgData()
        internal
        view
        override(TailorFoundation, ContextUpgradeable)
        returns (bytes calldata)
    {
        return TailorFoundation._msgData();
    }

    /**
     * @dev Function to supportInterfaces
     * @param interfaceId bytes4 of interfaceID
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, TailorRoyalties)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

