pragma solidity ^0.8.3;



contract BloopMaketPlace_ERC720 is ReentrancyGuard {
  using SafeMath for uint256;
  using Counters for Counters.Counter;
  Counters.Counter private _itemIds;
  Counters.Counter private _itemsSold;

  address adminAddress;
  uint256 listingTax;
  address payable marketplaceTaxAddress;

  struct NFTExternalContractToken {
    uint256 tokenId;
    address approved;
  }

 struct MarketItem {
    uint itemId;
    address nftContract;
    uint256 tokenId;
    address payable seller;
    uint256 sellerTax;
    address payable owner;    
    uint256 minPrice;
    uint256 listingTax;
    uint256 efectiveListingTax;
    bool forSale;
    bool hidden;
    bool inCustody;
  }

  event MarketItemCreated (
    uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    address seller,
    uint256 sellerTax,
    address owner,
    uint256 minPrice,
    uint256 listingTax,
    uint256 efectiveListingTax,    
    bool forSale,
    bool hidden,
    bool inCustody   
  );

  mapping(uint256 => MarketItem) private idToMarketItem;

  constructor() {
    marketplaceTaxAddress = payable(msg.sender);
    listingTax = 250;
    adminAddress = msg.sender;
  }

  function getMarketItem(uint256 marketItemId) public view returns (MarketItem memory) {
    return idToMarketItem[marketItemId];
  }

  function _createMarketItem(address nftContract, uint256 tokenId, uint256 minPrice, address seller, uint256 sellerTax) private returns (MarketItem memory) {
    _itemIds.increment();
    uint256 itemId = _itemIds.current();
  
    BloopNFT nft = BloopNFT(nftContract);
    BloopNFT.mintedMarketItem memory mintedItem = nft.getMarketplaceItemData(tokenId);

    require((minPrice > 0 ? minPrice : mintedItem.minPrice) > 0, "Price must be at least 1");
    require((sellerTax > 0 ? sellerTax : mintedItem.sellerTax) < 10000, "The percentage value must be less than 10000 where it is equivalent to 100%.");

    idToMarketItem[itemId] = MarketItem(
      itemId,
      nftContract,
      tokenId,
      payable(mintedItem.seller != address(0) ? mintedItem.seller : seller),
      mintedItem.sellerTax > 0 ? mintedItem.sellerTax : sellerTax,
      payable(mintedItem.creator != address(0) ? mintedItem.creator : msg.sender),
      mintedItem.minPrice > 0 ? mintedItem.minPrice : minPrice,
      listingTax,
      0,
      true,
      false,
      true
    );

    return idToMarketItem[itemId];
  }

  function createMarketItem(address nftContract, uint256 tokenId, uint256 minPrice, address seller, uint256 sellerTax) public nonReentrant payable {
    MarketItem memory item = _createMarketItem(nftContract, tokenId, minPrice, seller, sellerTax);
    BloopNFT nft = BloopNFT(nftContract);
    nft.transferFrom(payable(msg.sender), address(this), tokenId);
    emit MarketItemCreated(
      item.itemId,
      item.nftContract,
      item.tokenId,  
      payable(item.seller),
      item.sellerTax,
      payable(item.owner),
      item.minPrice,
      item.listingTax,
      0,
      true,
      false,
      true
    );
  }

  function purchMarketItem(address nftContract, uint256 itemId) public payable returns (MarketItem memory) {
    MarketItem storage item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    require(item.forSale == true, "Item not available for sale on this moment.");
    uint256 minPrice = item.minPrice;
    uint256 tokenId = item.tokenId;
    uint256 sellerTaxValue = 0;
    uint256 receivedValue = msg.value;
    require(receivedValue >= minPrice, "Please submit the asking minimun price in order to complete the purchase.");

    uint256 taxValue = receivedValue.mul(listingTax).div(10000);
    require(taxValue > 0, "Error on calulate value from payment split to marketplace.");
    payable(marketplaceTaxAddress).transfer(taxValue);

    uint ownerTotal = receivedValue.sub(taxValue);

    if(item.seller != address(0) && item.sellerTax != 0){
      sellerTaxValue = ownerTotal.mul(item.sellerTax).div(10000);
      require(sellerTaxValue > 0, "Error on calulate value from payment split to seller.");
      payable(item.seller).transfer(sellerTaxValue);
      ownerTotal = ownerTotal.sub(sellerTaxValue);
    }

    require(ownerTotal > 0 && ownerTotal < receivedValue, "Error on calulate value from payment split to owner.");
    require((ownerTotal + taxValue + sellerTaxValue) == receivedValue, "Error on calulate value from split payments.");
    payable(item.owner).transfer(ownerTotal);

    IERC721(nftContract).transferFrom(address(this), msg.sender, tokenId);

    item.owner = payable(msg.sender);
    item.seller = payable(item.seller);
    item.efectiveListingTax = listingTax;
    item.forSale = false;
    item.inCustody = false;
    _itemsSold.increment();

    return item;
  }

  function fullPurchMarketItem(address nftContract, uint256 tokenId) public payable returns (MarketItem memory) {
    MarketItem memory item;
    BloopNFT nft = BloopNFT(nftContract);
    BloopNFT.mintedMarketItem memory mintedItem = nft.getMarketplaceItemData(tokenId);
    if(mintedItem.creator != address(0))
    {
      item = _createMarketItem(nftContract, tokenId, mintedItem.minPrice, mintedItem.seller, mintedItem.sellerTax);
      emit MarketItemCreated(
      item.itemId,
      item.nftContract,
      item.tokenId,   
      payable(item.seller),
      item.sellerTax,
      payable(item.owner),
      item.minPrice,
      item.listingTax,
      0,
      true,
      false,
      true
      );
    }

    return purchMarketItem(nftContract, item.itemId);
  }

  function getMarketplaceItemMinPrice(uint itemId) public view returns (uint256) {
    MarketItem memory item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    return item.minPrice;
  }

  function getSellerTaxValue(uint itemId) public view returns (uint256) {
    MarketItem memory item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    require(item.seller != address(0) && item.sellerTax != 0, "Error on calulate value from payment split to seller.");
    return item.minPrice.mul(item.sellerTax).div(10000);
  }

  function getMarketplaceTaxValue(uint itemId) public view returns (uint256) {
    MarketItem memory item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    return item.minPrice.mul(listingTax).div(10000);
  }

  function fetchMarketItem(uint itemId) public view returns (MarketItem memory) {
    MarketItem memory item = idToMarketItem[itemId];
    return item;
  }

  function hiddenMarketItem(uint itemId, bool hidden) public returns (MarketItem memory){
    MarketItem storage item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == item.owner || msg.sender == item.seller, "Only owner, seller or admin.");
    item.hidden = hidden;
    return item;
  }

  function saleMarketItem(uint itemId, bool forSale) public returns (MarketItem memory){
    MarketItem storage item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == item.owner || msg.sender == item.seller, "Only owner, seller or admin.");
    item.forSale = forSale;

    return item;
  }

  function updatePreMarketItem(address nftContract, uint256 tokenId, uint128 minPrice, address seller, uint128 sellerTax, bool forSale, bool hidden) public payable returns (MarketItem memory){
    MarketItem memory item;
    BloopNFT nft = BloopNFT(nftContract);
    BloopNFT.mintedMarketItem memory mintedItem = nft.getMarketplaceItemData(tokenId);
    require(mintedItem.creator != address(0) || (mintedItem.originalCreator == msg.sender && nft.ownerOf(tokenId) == address(this)), "Not listed on marktplace.");
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == mintedItem.creator || msg.sender == mintedItem.seller, "Only owner, seller or admin.");

    item = _createMarketItem(nftContract, tokenId, minPrice > 0 ? minPrice : mintedItem.minPrice, seller, sellerTax);

    emit MarketItemCreated(
    item.itemId,
    item.nftContract,
    item.tokenId,   
    payable(item.seller),
    item.sellerTax,
    payable(item.owner),
    item.minPrice,
    item.listingTax,
    0,
    forSale,
    hidden,
    true
    );
   
    return item;
  }

   function updateMarketItem(uint itemId, uint128 minPrice, address seller, uint128 sellerTax, bool forSale, bool hidden) public returns (MarketItem memory){
    MarketItem storage item = idToMarketItem[itemId];

    require(item.inCustody == true, "Item not found in custody.");
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == item.owner || msg.sender == item.seller, "Only owner, seller or admin.");
    require(minPrice > 0, "Price must be at least 1");
    require(sellerTax < 10000, "The percentage value must be less than 10000 where it is equivalent to 100%.");
    
    item.minPrice = minPrice;
    item.seller = payable(seller);
    item.sellerTax = sellerTax;
    item.forSale = forSale;
    item.hidden = hidden;
    
    return item;
  }

  function removeCustodyPreMarketItem(address nftContract, uint256 tokenId) public returns (MarketItem memory){
    MarketItem memory item;
    BloopNFT nft = BloopNFT(nftContract);
    BloopNFT.mintedMarketItem memory mintedItem = nft.getMarketplaceItemData(tokenId);
    require(mintedItem.creator != address(0) || (mintedItem.originalCreator == msg.sender && nft.ownerOf(tokenId) == address(this)), "Not listed on marktplace.");
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == mintedItem.creator || msg.sender == mintedItem.seller, "Only owner, seller or admin.");
    
    item.forSale = false;
    item.inCustody = false;
    item.hidden = true;

    nft.transferFrom(address(this), mintedItem.creator, tokenId);

    item.forSale = false;
    item.inCustody = false;
    _itemsSold.increment();

    return item;
  }

  function removeCustodyMarketItem(uint itemId) public returns (MarketItem memory){
    MarketItem storage item = idToMarketItem[itemId];
    require(item.inCustody == true, "Item not found in custody.");
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == item.owner || msg.sender == item.seller, "Only owner, seller or admin.");
    
    item.forSale = false;
    item.inCustody = false;
    item.hidden = true;

    IERC721(idToMarketItem[itemId].nftContract).transferFrom(address(this), idToMarketItem[itemId].owner, idToMarketItem[itemId].tokenId);

    item.forSale = false;
    item.inCustody = false;
    _itemsSold.increment();

    return item;
  }

  function updateAdminAddress(address newAddress) public {
    require(msg.sender == adminAddress || msg.sender == address(this), "Not admin.");
    adminAddress = newAddress;
  }

  function transferByAdmin(uint itemId, address toAddress) public returns (MarketItem memory) {
    require(msg.sender == adminAddress || msg.sender == address(this), "Not admin.");

    IERC721(idToMarketItem[itemId].nftContract).transferFrom(address(this), toAddress, idToMarketItem[itemId].tokenId);
    idToMarketItem[itemId].owner = payable(toAddress);
    idToMarketItem[itemId].inCustody = false;
    MarketItem memory item = idToMarketItem[itemId];
    return item;
  }

  function updateMarketplaceTaxAddress(address newAddress) public {
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == marketplaceTaxAddress , "Not admin.");
    marketplaceTaxAddress = payable(newAddress);
  }

  function getMarketplaceTaxAddress() public view returns (address){
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == marketplaceTaxAddress, "Not admin.");
    return marketplaceTaxAddress;
