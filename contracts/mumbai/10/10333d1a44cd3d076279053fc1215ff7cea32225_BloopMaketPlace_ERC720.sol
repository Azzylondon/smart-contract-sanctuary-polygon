  function updateMarketplaceListingTax(uint128 newListingTax) public {
    require(msg.sender == adminAddress || msg.sender == address(this) || msg.sender == marketplaceTaxAddress, "Not admin.");
    require(newListingTax < 10000, "The percentage value must be less than 10000 where it is equivalent to 100%.");
