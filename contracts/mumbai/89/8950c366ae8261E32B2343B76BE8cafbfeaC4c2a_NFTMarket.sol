  function getUnsoldItems() public view returns (MarketItem[] memory) {

    uint currentIndex = 0;

    MarketItem[] memory items = new MarketItem[](mapsize);
    for (uint i = 0; i < mapsize; i++) {
        uint currentId = i + 1;
        MarketItem memory currentItem = nftOnSale[currentId];

        if(currentItem.qty > 0){
           items[currentIndex] = currentItem;
           currentIndex += 1;
        }

  
    }
   
    return items;
  }

  function getItemsByOwner() public view returns (MarketItem[] memory) {
    uint itemCount = 0;
    uint currentIndex = 0;

    for (uint i = 0; i < mapsize; i++) {
      if (nftOnSale[i + 1].owner == msg.sender) {
        itemCount += 1;
      }
    }

    MarketItem[] memory items = new MarketItem[](itemCount);
    for (uint i = 0; i < mapsize; i++) {
      if (nftOnSale[i + 1].owner == msg.sender) {
        uint currentId = i + 1;
        MarketItem memory currentItem = nftOnSale[currentId];
        items[currentIndex] = currentItem;
        currentIndex += 1;
      }
    }
   
    return items;
  }

