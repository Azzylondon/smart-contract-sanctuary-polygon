contract NFTMarket is ReentrancyGuard, PriceConsumerV3,ERC165, IERC721Receiver,IERC1155Receiver {


  using Counters for Counters.Counter;
  using ERC165Checker for address;

  Counters.Counter private _itemIds;
  Counters.Counter private _itemsSold;

  address payable marketPlaceOwner;
  uint256 marketplaceFee = 2;
  IERC20 eliteToken;



  constructor(address eliteTokenAddress) PriceConsumerV3() {
    marketPlaceOwner = payable(msg.sender);
    eliteToken = IERC20(eliteTokenAddress);
  }

  struct MarketItem {
    uint itemId;//id sul marketplace
    address nftContract; // indirizzo del contratto della collection
    uint256 tokenId; // id del token
    uint256 qty;//quante sono in vendita
    address payable seller;//chi l'ha messo in vendita
    address payable owner;//chi riceve il pagamento della vendita
    address payable creator;//chi ha creato la prima volta
    uint256 price;//prezzo di vendita in matic
    uint256 eliteprice;//prezzo di vendita in elite
    uint256 creatorFee;//percentuale da dare al creator
  }

  mapping(uint256 => MarketItem) private nftOnSale;
  uint mapsize;

  event MarketItemCreated (
    uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    address seller,
    address owner,
    uint256 price,
    uint256 eliteprice,
    uint256 qty
  );

  event feeDistributed(
      address to,
      uint256 amount,
      address token
  );

  event ItemSold (
        uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    address buyer,
    uint256 price,
    uint256 qty

  );

  function getMarketItemById(uint256 marketItemId) public view returns (MarketItem memory) {
    MarketItem memory item = nftOnSale[marketItemId];
    return item;
  }

  function addItemToMarket(
    address nftContract,
    uint256 tokenId,
    uint256 price,
    uint256 eliteprice,
    uint256 qty,
    address creator,
    uint256 creatorFee
  ) public nonReentrant  returns (uint256) {

    _itemIds.increment();
    uint256 itemId = _itemIds.current();
  
    nftOnSale[itemId] =  MarketItem(
      itemId,
      nftContract,
      tokenId,
      qty,
      payable(msg.sender),
      payable(address(0)),
      payable(creator),
      price,
      eliteprice,
      creatorFee
    );

    mapsize++;


    if(nftContract.supportsInterface(type(IERC1155).interfaceId)){

        IERC1155(nftContract).safeTransferFrom(msg.sender, address(this), tokenId,qty,"");

    }else if(nftContract.supportsInterface(type(IERC721).interfaceId)){

        IERC721(nftContract).safeTransferFrom(msg.sender, address(this), tokenId);
    }else{

        revert("The nft contract must conform to ERC721 or ERC1155 standard");
    }


    emit MarketItemCreated(
      itemId,
      nftContract,
      tokenId,
      msg.sender,
      address(0),
      price,
      eliteprice,
      qty
    );


     return itemId;
  }




