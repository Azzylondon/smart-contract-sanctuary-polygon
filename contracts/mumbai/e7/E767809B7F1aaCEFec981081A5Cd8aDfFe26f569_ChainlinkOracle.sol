pragma solidity ^0.8.13;

import "./interfaces/HERC20I.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract PriceOracle is Ownable {
  /// @notice Indicator that this is a PriceOracle contract (for inspection)
  bool public constant IS_PRICE_ORACLE = true;

  uint256 public pricePerNft = 1e18;

  constructor() {}

  /**
   * @notice Get the underlying price of a hToken asset
   * @param hToken The hToken to get the underlying price of
   * @return The underlying asset price mantissa (scaled by 1e18).
   *  Zero means the price is unavailable.
   */
  function getUnderlyingPrice(HERC20I hToken) external view returns (uint256) {
    hToken;
    return pricePerNft;
  }

  /**
   * @notice Get the underlying price of an nft asset
   * @param _tokenId The NFT id to get the underlying price of
   * @return The underlying asset price mantissa (scaled by hToken decimals).
   *  Zero means the price is unavailable.
   */
  function getUnderlyingNFTPrice(uint256 _tokenId, uint256 _decimals) external view returns (uint256) {
    _tokenId;
    if (_decimals < 18) return (pricePerNft * 10**_decimals) / 1e18;
    return pricePerNft;
  }

  function setPricePerNft(uint256 _newPrice) external onlyOwner {
    pricePerNft = _newPrice;
  }
}

