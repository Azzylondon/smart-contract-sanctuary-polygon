		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,uint)", p0, p1, p2, p3));
	}

	function log(address p0, address p1, address p2, string memory p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,string)", p0, p1, p2, p3));
	}

	function log(address p0, address p1, address p2, bool p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,bool)", p0, p1, p2, p3));
	}

	function log(address p0, address p1, address p2, address p3) internal view {
		_sendLogPayload(abi.encodeWithSignature("log(address,address,address,address)", p0, p1, p2, p3));
	}

}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "./interfaces/HivemindI.sol";
import "./interfaces/InterestRateModelI.sol";
import "./ErrorReporter.sol";

contract HToken is ERC1155, IERC721Receiver, ErrorReporter, AccessControlEnumerable {
  using Counters for Counters.Counter;

  // For public inspection
  bool public constant isHToken = true;

  /**
   * @notice Maximum borrow rate that can ever be applied (.0005% / block)
   */

  uint256 public constant borrowRateMaxMantissa = 0.0005e16;

  /**
   * @notice Maximum fraction of interest that can be set aside for reserves
   */
  uint256 public constant reserveFactorMaxMantissa = 1e18;

  /**
   * @notice Contract which oversees inter-hToken operations
   */
  HivemindI public hivemind;

  /**
   * @notice Model which tells what the current interest rate should be
   */
  InterestRateModelI public interestRateModel;

  /**
   * @notice Initial exchange rate used when minting the first HToken (used when totalSupply = 0)
   */
  uint256 internal initialExchangeRateMantissa;

  /**
   * @notice Fraction of interest currently set aside for reserves
   */
  uint256 public reserveFactorMantissa;

  /**
   * @notice Block number that interest was last accrued at
   */
  uint256 public accrualBlockNumber;

  /**
   * @notice Accumulator of the total earned interest rate since the opening of the market
   */
  uint256 public borrowIndex;

  /**
   * @notice decimals of underlying
   */
  uint256 public immutable decimals;

  /*** Market Events ***/

  /**
   * @notice Event emitted when interest is accrued
   */
  event AccrueInterest(uint256 cashPrior, uint256 interestAccumulated, uint256 borrowIndex, uint256 totalBorrows);

  /**
   * @notice Event emitted when interest is accrued
   */
  event AccrueCouponInterest(
    Coupon activeCoupon,
    uint256 debtPrior,
    uint256 interestAccumulated,
    uint256 borrowIndexPrior,
    uint256 borrowIndexCurrent
  );

  /**
   * @notice Event emitted when tokens are minted
   */
  event Mint(address minter, uint256 mintAmount, uint256 mintTokens);

  /**
   * @notice Event emitted when tokens are redeemed
   */
  event Redeem(address redeemer, uint256 redeemAmount, uint256 redeemTokens);

  /**
   * @notice Event emitted when underlying is borrowed
   */
  event Borrow(address borrower, uint256 borrowAmount, uint256 tokenId, uint256 totalBorrows);

  /**
   * @notice Event emitted when a borrow is repaid
   */
  event RepayBorrow(address indexed payer, uint256 repayAmount, uint256 accountBorrows, uint256 totalBorrows, uint256 _collateralId);

  /**
   * @notice Event emitted when a borrow is liquidated
   */
  event LiquidateBorrow(
    address indexed liquidator,
    address borrower,
    uint256 repayAmount,
    address indexed hTokenCollateral,
    uint256 seizeTokens
  );

  /*** Admin Events ***/

  /**
   * @notice Event emitted when hivemind is changed
   */
  event NewHivemindAddr(HivemindI oldHivemind, HivemindI newHivemind);

  /**
   * @notice Event emitted when liquidator is changed
   */
  event NewLiquidator(address oldLiquidator, address newLiquidator);

  /**
   * @notice Event emitted when interestRateModel is changed
   */
  event NewMarketInterestRateModel(InterestRateModelI oldInterestRateModel, InterestRateModelI newInterestRateModel);

  /**
   * @notice Event emitted when the reserve factor is changed
   */
  event NewReserveFactor(uint256 oldReserveFactorMantissa, uint256 newReserveFactorMantissa);

  /**
   * @notice Event emitted when the reserves are added
   */
  event ReservesAdded(address benefactor, uint256 addAmount, uint256 newTotalReserves);

  /**
   * @notice Event emitted when the reserves are reduced
   */
  event ReservesReduced(address admin, uint256 reduceAmount, uint256 newTotalReserves);

  // Fee model branch events
  event NewFuseFee(uint256 oldFuseFeeMantissa, uint256 newFuseFeeMantissa);
  event NewAdminFee(uint256 oldAdminFeeMantissa, uint256 newAdminFeeMantissa);

  string internal baseURI;

  // Public addresses
  address public liquidator;

  // Fee rates
  uint256 public adminFeeMantissa;
  uint256 public fuseFeeMantissa;
  uint256 public reserveFactorPlusFeesMaxMantissa = 5e17; // 50% - change later

  IERC20 public immutable underlyingToken;
  IERC721 public immutable collateralToken;

  /**
   * @notice dao address were sweeped tokens will be transmitted
   */
  address constant dao = 0x9adBd648cb2238B72B05f3e488c6A75F8F186620;

  Counters.Counter public idCounter;

  // Bookkeeping
  uint256 public totalBorrows;
  uint256 public totalReserves;
  uint256 public totalHTokenSupply;
  uint256 public totalFuseFees;
  uint256 public totalAdminFees;

  // Coupon NFT metadata
  struct Coupon {
    uint256 id; // tokenId of the collateral collection that is borrowed against
    uint256 borrowAmount; // Current borrow balance, denominated in underlying ERC20 token
    bool active; // Whether this coupon is active and should be counted in calculations etc
    uint256 index; // Mantissa formatted borrow index. Updated when interest is accrued to the coupon.
    uint256 couponId; // coupon id generated
  }

  mapping(uint256 => Coupon) public borrowCoupons;

  mapping(address => uint256[]) public userToCoupons;

  constructor(
    IERC20Metadata _underlyingToken,
    IERC721 _collateralToken,
    HivemindI _hivemindAddress,
    InterestRateModelI _interestRateModel,
    address _liquidator,
    uint256 _initialExchangeRateMantissa,
    uint256 _reserveFactorMantissa,
    uint256 _adminFeeMantissa,
    uint256 _fuseFeeMantissa
  ) ERC1155(baseURI) {
    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);

    // Setup underlying token
    underlyingToken = _underlyingToken;

    // Setup the collateral NFT collection
    collateralToken = _collateralToken;
    if (!_collateralToken.supportsInterface(type(IERC721).interfaceId)) revert WrongParams();

    // Set liquidator contract
    liquidator = _liquidator;

    // reserving id 0 for hTokens so we need to increment
    idCounter.increment();

    // Set initial exchange rate
    initialExchangeRateMantissa = _initialExchangeRateMantissa;
    if (initialExchangeRateMantissa == 0) revert HTokenError(Error.INITIAL_EXCHANGE_MANTISSA);

    // Setup hivemind
    hivemind = _hivemindAddress;

    // Initialize block number and borrow index
    accrualBlockNumber = block.number;
    borrowIndex = 1e18 / 10**(18 - _underlyingToken.decimals());

    // // Setup interest rate
    interestRateModel = _interestRateModel;

    // Set reserve factor
    _setReserveFactorFresh(_reserveFactorMantissa);

    // Set admin fee
    _setAdminFeeFresh(_adminFeeMantissa);

    // Set fuse fee
    _setFuseFeeFresh(_fuseFeeMantissa);

    // Check reserveFactorMantissa + newAdminFeeMantissa + newFuseFeeMantissa ≤ reserveFactorPlusFeesMaxMantissa
    if (adminFeeMantissa + fuseFeeMantissa + reserveFactorMantissa > reserveFactorPlusFeesMaxMantissa) revert AdminError(Error.BAD_INPUT);

    decimals = _underlyingToken.decimals();
  }

  /**
   * @notice Sets a new fuse fee for the protocol (*requires fresh interest accrual)
   * @param _newFuseFeeMantissa new fuse fee mantisa
   */
  function _setFuseFeeFresh(uint256 _newFuseFeeMantissa) internal {
    // Verify market's block number equals current block number
    if (accrualBlockNumber != getBlockNumber()) {
      revert AdminError(Error.MARKET_NOT_FRESH);
    }

    // Sanitize newFuseFeeMantissa
    if (_newFuseFeeMantissa == type(uint256).max) _newFuseFeeMantissa = fuseFeeMantissa;

    // Check reserveFactorMantissa + newAdminFeeMantissa + newFuseFeeMantissa ≤ reserveFactorPlusFeesMaxMantissa
    if (reserveFactorMantissa + adminFeeMantissa + _newFuseFeeMantissa > reserveFactorPlusFeesMaxMantissa) {
      revert AdminError(Error.BAD_INPUT);
    }

    // If setting fuse fee
    if (fuseFeeMantissa != _newFuseFeeMantissa) {
      // Set fuse fee
      uint256 oldFuseFeeMantissa = fuseFeeMantissa;
      fuseFeeMantissa = _newFuseFeeMantissa;

      // Emit event
      emit NewFuseFee(oldFuseFeeMantissa, _newFuseFeeMantissa);
    } else revert WrongParams();
  }

  /**
   * @notice Sets a new admin fee for the protocol (*requires fresh interest accrual)
   * @param _newAdminFeeMantissa new admin fee mantissa
   */
  function _setAdminFeeFresh(uint256 _newAdminFeeMantissa) internal {
    // Verify market's block number equals current block number
    if (accrualBlockNumber != getBlockNumber()) {
      revert AdminError(Error.MARKET_NOT_FRESH);
    }

    // Sanitize newAdminFeeMantissa
    if (_newAdminFeeMantissa == type(uint256).max) _newAdminFeeMantissa = adminFeeMantissa;

    // Check reserveFactorMantissa + newAdminFeeMantissa + newFuseFeeMantissa ≤ reserveFactorPlusFeesMaxMantissa
    if (reserveFactorMantissa + _newAdminFeeMantissa + fuseFeeMantissa > reserveFactorPlusFeesMaxMantissa) {
      revert AdminError(Error.BAD_INPUT);
    }

    // If setting admin fee
    if (adminFeeMantissa != _newAdminFeeMantissa) {
      // Set admin fee
      uint256 oldAdminFeeMantissa = adminFeeMantissa;
      adminFeeMantissa = _newAdminFeeMantissa;

      // Emit event
      emit NewAdminFee(oldAdminFeeMantissa, _newAdminFeeMantissa);
    } else revert WrongParams();
  }

  /**
   * @notice Sets a new reserve factor for the protocol (*requires fresh interest accrual)
   * @param _newReserveFactorMantissa new reserve factor mantissa
   */
  function _setReserveFactorFresh(uint256 _newReserveFactorMantissa) internal {
    // Verify market's block number equals current block number
    if (accrualBlockNumber != getBlockNumber()) {
      revert AdminError(Error.MARKET_NOT_FRESH);
    }

    // Check newReserveFactor ≤ maxReserveFactor
    if (_newReserveFactorMantissa + adminFeeMantissa + fuseFeeMantissa > reserveFactorPlusFeesMaxMantissa) {
      revert AdminError(Error.BAD_INPUT);
    }

    if (reserveFactorMantissa != _newReserveFactorMantissa) {
      uint256 oldReserveFactorMantissa = reserveFactorMantissa;
      reserveFactorMantissa = _newReserveFactorMantissa;

      emit NewReserveFactor(oldReserveFactorMantissa, _newReserveFactorMantissa);
    } else revert WrongParams();
  }

  /**
   * @notice Reduces reserves by transferring to admin
   * @dev Requires fresh interest accrual
   * @param _reduceAmount Amount of reduction to reserves
   */
  function _reduceReservesFresh(uint256 _reduceAmount) internal {
    // totalReserves - reduceAmount
    uint256 totalReservesNew;

    // We fail gracefully unless market's block number equals current block number
    if (accrualBlockNumber != getBlockNumber()) {
      revert AdminError(Error.MARKET_NOT_FRESH);
    }

    // Fail gracefully if protocol has insufficient underlying cash
    if (getCashPrior() < _reduceAmount) {
      revert AdminError(Error.TOKEN_INSUFFICIENT_CASH);
    }

    // Check reduceAmount ≤ reserves[n] (totalReserves)
    if (_reduceAmount > totalReserves) {
      revert AdminError(Error.BAD_INPUT);
    }

    /////////////////////////
    // EFFECTS & INTERACTIONS
    // (No safe failures beyond this point)

    // We checked reduceAmount <= totalReserves above, so this should never revert.
    // Store reserves[n+1] = reserves[n] - reduceAmount
    totalReserves -= _reduceAmount;

    // doTransferOut reverts if anything goes wrong, since we can't be sure if side effects occurred.
    doUnderlyingTransferOut(msg.sender, _reduceAmount);

    emit ReservesReduced(msg.sender, _reduceAmount, totalReservesNew);
  }

  /*
    /////////////////////////////////////////////////////////////
                    Coupon handler functions
    /////////////////////////////////////////////////////////////
  */

  /**
   * @notice get borrowed amount from a coupon
   * @param _index index of the borrwed coupon
   * @return borrwed amount of underlying
   */
  function getBorrowFromCoupon(uint256 _index) public view returns (uint256) {
    return borrowCoupons[_index].borrowAmount;
  }

  /**
   * @notice get user's coupon
   * @param _user the user to search for
   * @return user's coupons
   */
  function getUserCoupons(address _user) public view returns (Coupon[] memory) {
    uint256[] memory indices = userToCoupons[_user];
    Coupon[] memory userCoupons = new Coupon[](indices.length);

    uint256 i;
    uint256 index;
    for (i; i < indices.length; i++) {
      index = indices[i];
      userCoupons[i] = borrowCoupons[index];
    }

    return userCoupons;
  }

  /**
   * @notice search a coupon by token id
   * @param _user to serach for
   * @param _nftTokenId nft id to search for
   * @return coupon index in the borrowCoupons
   */
  function searchCouponByNftId(address _user, uint256 _nftTokenId) public view returns (uint256) {
    // Find the user's coupon
    uint256[] memory indices = userToCoupons[_user];
    uint256 activeIndex;

    uint256 i;
    for (i; i < indices.length; i++) {
      activeIndex = indices[i];
      if ((borrowCoupons[activeIndex].id == _nftTokenId) && (borrowCoupons[activeIndex].active)) {
        return activeIndex;
      }
    }

    return 0;
  }

  /**
   * @notice search a coupon by token id
   * @param _user to serach for
   * @param _couponId coupon id to search for
   * @return coupon index in the borrowCoupons
   */
  function searchCouponById(address _user, uint256 _couponId) public view returns (uint256) {
    // Find the user's coupon
    uint256[] memory indices = userToCoupons[_user];
    uint256 activeIndex;

    uint256 i;
    for (i; i < indices.length; i++) {
      activeIndex = indices[i];
      if ((borrowCoupons[activeIndex].couponId == _couponId) && (borrowCoupons[activeIndex].active)) {
        return activeIndex;
      }
    }

    return 0;
  }

  /**
   * @notice get a specific coupon for an NFT
   * @param _couponNFTId nft id
   * @return coupon
   */
  function getSpecificCoupon(uint256 _couponNFTId) external view returns (Coupon memory) {
    return borrowCoupons[_couponNFTId];
  }

  /**
   * @notice accruels interests to all coupons
   * @return all accrued interest
   */
  function accrueInterestToCoupons() public returns (uint256) {
    uint256 index;
    uint256 runningTotal;

    // We may start at position 1 as position 0 is reserved for the null coupon
    for (index = 1; index <= idCounter.current(); ) {
      if (borrowCoupons[index].active) {
        runningTotal += accrueInterestToCoupon(index);
      }
      unchecked {
        index++;
      }
    }

    return runningTotal;
  }

  /**
   * @notice accruels interest to coupon
   * @param _index borrow index
   * @return new total accrued
   */
  function accrueInterestToCoupon(uint256 _index) public returns (uint256) {
    Coupon storage activeCoupon = borrowCoupons[_index];

    uint256 initialAmount = activeCoupon.borrowAmount;
    uint256 initialIndex = activeCoupon.index;
    uint256 currentIndex = borrowIndex;

    uint256 indexDelta = currentIndex - initialIndex;
    // uint256 interestIncurred = mul_ScalarTruncate(Exp({ mantissa: indexDelta }), initialAmount);
    uint256 interestIncurred = (indexDelta * initialAmount) / 10**decimals;
    uint256 newTotal = initialAmount + interestIncurred;

    // Effects
    activeCoupon.borrowAmount = newTotal;
    activeCoupon.index = currentIndex;

    emit AccrueCouponInterest(activeCoupon, initialAmount, interestIncurred, initialIndex, currentIndex);

    return newTotal;
  }

  // TODO: Create function to view all outstanding coupons?

  // --!!-- Nasty unsafe proof of concept functions below --!!--

  /*/////////////////////////////////////////////////////////////
    Below are the functions related to the lending side of the dapp
    /////////////////////////////////////////////////////////////*/

  // Public visibility only for testing

  /**
   * @notice minting hToken to `_to`
   * @param _to minting to
   * @param _amount the amount to mint
   */
  function _mintHToken(address _to, uint256 _amount) internal {
    totalHTokenSupply += _amount;
    _mint(_to, 0, _amount, "");
  }

  /**
   * @notice Deposit underlying tokens and recieve hTokens as proof of deposit
   * @dev Must be approved to transfer amount of underlying tokens. Accrues interest and updates exchange rate prior to depositing and minting
   * @param _amount Quantity of underlying tokens to deposit
   */
  function depositUnderlyingInternal(uint256 _amount) internal {
    uint256 exchangeRate = exchangeRateCurrent();
    uint256 deposited = doUnderlyingTransferIn(msg.sender, _amount);

    uint256 tokensToMint = (deposited * 1e18) / exchangeRate;
    _mintHToken(msg.sender, tokensToMint);
  }

  /**
   * @notice does transfer out, meaning will withdraw underlying token from the contract to `_to`
   * @param _to transfer to
   * @param _amount amount to withdraw
   */
  function doUnderlyingTransferOut(address _to, uint256 _amount) internal {
    underlyingToken.transfer(_to, _amount);
  }

  /**
   * @notice does transfer in, meaning will deposit underlying token to the contract's balance
   *         `_from` needs to approve before hand
   * @param _from transfer from, it needs to have approved at least `_amount`
   * @param _amount amount to deposit
   * @return amount deposited
   */
  function doUnderlyingTransferIn(address _from, uint256 _amount) internal returns (uint256) {
    uint256 balanceBefore = underlyingToken.balanceOf(address(this));
    underlyingToken.transferFrom(_from, address(this), _amount);
    uint256 balanceAfter = underlyingToken.balanceOf(address(this));

    // TODO: Additional checking to handle false returns

    if (balanceAfter < balanceBefore) revert HTokenError(Error.TRANSFER_ERROR);
    return balanceAfter - balanceBefore;
  }

  /**
   * @notice Redeem a specified amount of hTokens for their underlying ERC-20 asset
   * It does a check against Hivemind to see if it is allowed to redeem.
   * @param _amount the amount of tokens to redeem.
   */
  function redeemInternal(uint256 _amount) internal {
    // Exchange rate is tokens of underlying per hToken
    uint256 exchangeRate = exchangeRateCurrent();

    totalHTokenSupply -= _amount;

    _burn(msg.sender, 0, _amount);

    uint256 underlyingToRedeem = (_amount * exchangeRate) / 1e18;

    //TODO for the demo this is not needed as we won t take this into consideration
    /* Fail if redeem not allowed */
    hivemind.redeemAllowed(HTokenI(address(this)), msg.sender, underlyingToRedeem);

    doUnderlyingTransferOut(msg.sender, underlyingToRedeem);
  }

  /**
   * @notice Redeem any amount of hTokens for the specified quantity of underlying ERC-20 asset
   * @param _amount to be redeemed
   */
  function redeemUnderlyingInternal(uint256 _amount) internal {
    // Exchange rate is tokens of underlying per hToken
    uint256 exchangeRate = exchangeRateCurrent();

    uint256 tokensToRedeem = (_amount * 1e18) / exchangeRate;

    totalHTokenSupply -= tokensToRedeem;

    _burn(msg.sender, 0, tokensToRedeem);

    doUnderlyingTransferOut(msg.sender, _amount);
  }

  /*///////////////////////////////////////////////////////////////
    Below are the functions related to the borrowing side of the dapp
    ///////////////////////////////////////////////////////////////*/

  /**
   * @notice used to deposit collateral
   * @param _tokenId to be deposited as collateral
   */
  function depositCollateralInternal(uint256 _tokenId) internal {
    doCollateralTransferIn(msg.sender, _tokenId);

    _mintCoupon(msg.sender, _tokenId, 0);
  }

  /**
   * @notice minting a coupon when depositing collateral, this coupon will accrue borrows
   * @param _to the receiver (who deposits the collateral)
   * @param _id the id of the token that is deposited
   * @param _borrowAmount the amount that was borrowed
   */
  function _mintCoupon(
    address _to,
    uint256 _id,
    uint256 _borrowAmount
  ) internal {
    uint256 currentId = idCounter.current();
    idCounter.increment();

    // Construct a coupon
    Coupon memory coupon;
    coupon.id = _id;
    coupon.borrowAmount = _borrowAmount;
    coupon.active = true;
    coupon.index = borrowIndex;
    coupon.couponId = currentId;

    borrowCoupons[currentId] = coupon;

    // Mint NFT
    _mint(_to, currentId, 1, "");

    // TODO: Set URI
  }

  /**
   * @notice borrows an amount and accrued it to the coupon
   * @param _borrowAmount borrowed amount
   * @param _tokenId nft that you want to use as collateral
   */
  function borrowInternal(uint256 _borrowAmount, uint256 _tokenId) internal {
    // Find the user's coupon
    uint256 activeIndex = searchCouponByNftId(msg.sender, _tokenId);

    // Can't be the null coupon
    if (activeIndex == 0) revert HTokenError(Error.COUPON_LOOKUP);

    // Sanity check to make sure the coupon was found correctly

    if (borrowCoupons[activeIndex].id != _tokenId) revert WrongParams();

    // Our contract must own the relevant tokenId
    if (collateralToken.ownerOf(_tokenId) != address(this)) revert NotOwner();

    // Check for borrow allowed
    hivemind.borrowAllowed(HTokenI(address(this)), msg.sender, _tokenId, _borrowAmount);

    // Add the borrow amount to the token and set index
    borrowCoupons[activeIndex].borrowAmount += _borrowAmount;
    borrowCoupons[activeIndex].index = borrowIndex;

    // Add the amount to total borrows
    totalBorrows += _borrowAmount;

    emit Borrow(msg.sender, _borrowAmount, _tokenId, borrowCoupons[activeIndex].borrowAmount);
    // Transfer the funds to borrower
    doUnderlyingTransferOut(msg.sender, _borrowAmount);
  }

  /**
   * @notice does collateral transfer out, transfer nft to the _to
   * @param _to where to transfer the collateral
   * @param _tokenId what to transfer, the collateral id
   */
  function doCollateralTransferOut(address _to, uint256 _tokenId) internal {
    collateralToken.transferFrom(address(this), _to, _tokenId);
  }

  /**
   * @notice does collateral transfer in, transfer nft to the hToken
   * @param _from where to get the collateral
   * @param _tokenId what to transfer, the collateral id
   */
  function doCollateralTransferIn(address _from, uint256 _tokenId) internal {
    collateralToken.transferFrom(_from, address(this), _tokenId);
  }

  /**
   * @notice repais borrow for a collateral
   * @param _repayAmount the amount to repay
   * @param _tokenId to want collateral id to pay for
   */
  function repayBorrowInternal(uint256 _repayAmount, uint256 _tokenId) internal {
    hivemind.repayBorrowAllowed(HTokenI(address(this)), msg.sender, _tokenId, _repayAmount);

    // Accrue interest
    accrueInterest();

    // Can't repay 0 amount
    if (_repayAmount == 0) revert HTokenError(Error.AMOUNT_ZERO);

    // Find the user's coupon
    uint256 activeIndex = searchCouponByNftId(msg.sender, _tokenId);

    // Sanity check to make sure the coupon was found correctly
    if (borrowCoupons[activeIndex].id != _tokenId) revert WrongParams();

    // Our contract must already own the relevant tokenId
    if (collateralToken.ownerOf(_tokenId) != address(this)) revert NotOwner();

    // Get outstanding debt
    uint256 debt = getBorrowFromCoupon(activeIndex);
    if (debt == 0) revert HTokenError(Error.NO_DEBT);

    // Set amount to repay
    uint256 payment;
    if (_repayAmount > debt) {
      payment = debt;
    } else {
      payment = _repayAmount;
    }

    // Take in repayment monies
    uint256 repayment = doUnderlyingTransferIn(msg.sender, payment);

    // Credit their account
    borrowCoupons[activeIndex].borrowAmount -= repayment;

    // TODO: Set borrow index here to ensure correct compounding

    // Decrease total borrows
    totalBorrows -= repayment;

    emit RepayBorrow(msg.sender, payment, debt, totalBorrows, _tokenId);
  }

  /**
   * @notice repais borrow for a collateral for someone else
   * @param _borrower for whom to pay
   * @param _repayAmount the amount to repay
   * @param _tokenId to want collateral id to pay for
   */
  function repayBorrowBehalfInternal(
    address _borrower,
    uint256 _repayAmount,
    uint256 _tokenId
  ) internal {
    // Accrue interest
    accrueInterest();

    // Can't repay 0 amount
    if (_repayAmount == 0) revert HTokenError(Error.AMOUNT_ZERO);

    // Find the user's coupon
    uint256 activeIndex = searchCouponByNftId(_borrower, _tokenId);

    // Sanity check to make sure the coupon was found correctly
    if (borrowCoupons[activeIndex].id != _tokenId) revert HTokenError(Error.COUPON_NOT_FOUND);

    // Our contract must already own the relevant tokenId
    // require(collateralToken.ownerOf(_tokenId)==address(this));
    // TODO: move this elsewhere if needed

    // Get outstanding debt
    uint256 debt = getBorrowFromCoupon(activeIndex);
    if (debt == 0) revert HTokenError(Error.NO_DEBT);

    // Set amount to repay
    uint256 payment;
    if (_repayAmount > debt) {
      payment = debt;
    } else {
      payment = _repayAmount;
    }

    // Take in repayment monies
    uint256 repayment = doUnderlyingTransferIn(msg.sender, payment);

    // Credit their account
    borrowCoupons[activeIndex].borrowAmount -= repayment;

    // Decrease total borrows
    totalBorrows -= repayment;
  }

