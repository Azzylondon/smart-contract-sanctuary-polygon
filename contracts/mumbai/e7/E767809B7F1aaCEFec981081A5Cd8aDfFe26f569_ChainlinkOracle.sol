   * @param _borrowAmount Amount of underlying ERC-20 to borrow
   * @param _tokenId Token ID of underlying ERC-721 to be borrowed against
   */
  function borrow(uint256 _borrowAmount, uint256 _tokenId) external {
    borrowInternal(_borrowAmount, _tokenId);
  }

  /**
   * @notice Sender repays their own borrow taken against the specified collateral asset
   * @param _repayAmount Amount of underlying ERC-20 to repay
   * @param _tokenId Token ID of underlying ERC-721 to be repaid against
   */
  function repayBorrow(uint256 _repayAmount, uint256 _tokenId) external {
    repayBorrowInternal(_repayAmount, _tokenId);
  }

  /**
   * @notice Sender repays another user's borrow taken against the specified collateral asset
   * @param _borrower User whose borrow will be repaid
   * @param _repayAmount Amount of underlying ERC-20 to repay
   * @param _tokenId Token ID of underlying ERC-721 to be repaid against
   */
  function repayBorrowBehalf(
