    // Only if NFT20 pair exists
    address pair = poolToNiftyPair[address(hToken)];
    if (pair == address(0)) revert ExternalFailure("NFT20 pair not found");

    // Only if HErc20 has been initialized
    IERC721 collateralToken = poolToCollateralToken[address(hToken)];
    if (address(collateralToken) == address(0)) revert Uninitialized();

    // Only against active deposit coupons
    HERC20I.Coupon memory activeCoupon = hToken.getSpecificCoupon(_couponNFTId);
    if (!activeCoupon.active) revert InvalidCoupon();

