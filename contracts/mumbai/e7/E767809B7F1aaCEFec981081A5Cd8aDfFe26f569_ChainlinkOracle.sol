   */
  constructor(
    IERC20Metadata _underlyingToken,
    IERC721 _collateralToken,
    HivemindI _hivemindAddress,
    InterestRateModelI _interestRateModel,
    address _liquidatorAddress,
    uint256 _initialExchangeRateMantissa,
    uint256 _reserveFactorMantissa,
    uint256 _adminFeeMantissa,
    uint256 _fuseFeeMantissa
  )
    HToken(
      _underlyingToken,
      _collateralToken,
      _hivemindAddress,
      _interestRateModel,
      _liquidatorAddress,
      _initialExchangeRateMantissa,
      _reserveFactorMantissa,
      _adminFeeMantissa,
      _fuseFeeMantissa
    )
  {
    // Work is delegated to HToken constructor
  }

  // ----- Lend side functions -----

  /**
   * @notice Deposit underlying ERC-20 asset and mint hTokens
   * @param _amount Quantity of underlying ERC-20 to transfer in
   * @dev Will revert if protocol approvals are not sufficient.
   */
  function depositUnderlying(uint256 _amount) external {
    depositUnderlyingInternal(_amount);
  }

  /**
   * @notice Redeem a specified amount of hTokens for their underlying ERC-20 asset
   * @param _redeemTokens Quantity of hTokens to redeem for underlying ERC-20
   */
  function redeem(uint256 _redeemTokens) external {
    redeemInternal(_redeemTokens);
  }

  /**
   * @notice Redeem any amount of hTokens for the specified quantity of underlying ERC-20 asset
   * @param _redeemAmount Quantity of underlying ERC-20 tokens to output
   */
  function redeemUnderlying(uint256 _redeemAmount) external {
    redeemUnderlyingInternal(_redeemAmount);
  }

