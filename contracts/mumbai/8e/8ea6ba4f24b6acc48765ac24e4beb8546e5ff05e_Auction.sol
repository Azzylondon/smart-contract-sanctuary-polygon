    function getSpecificStakeInfo(address _nft, uint256 _tokenId, address addr) public view returns (uint256) {
        return stakes[_nft][_tokenId][addr];
    }

    function getTotalStakeInfo(address addr) public view returns (uint256) {
        return totalStake[addr];
    }
    
    /**
       Seller puts the item on auction
    */
    function createTokenAuction(
        address _nft,
        uint256 _tokenId,
        uint128 _price,
        uint256 _duration
    ) external {
        require(msg.sender != address(0));
        require(_nft != address(0));
        require(_price > 0);
        require(_duration > 0);
        tokenDetails memory _auction = tokenDetails({
            seller: msg.sender,
            price: uint128(_price),
            duration: _duration,
            isActive: true
            });
        address owner = msg.sender;
        IGlipERC721Lazy(_nft).safeTransferFrom(owner, address(this), _tokenId);
        tokenToAuction[_nft][_tokenId] = _auction;
    }

    // // Simply test minting. TODO - remove this function 
