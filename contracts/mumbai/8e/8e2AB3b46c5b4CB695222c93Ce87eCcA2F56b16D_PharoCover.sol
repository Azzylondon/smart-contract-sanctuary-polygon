/// @title Pharo Cover Contract
/// @author Jaxcoder, Aphilos
/// @notice Creates a new Cover Policy for a Cover Buyer
///         PharoPhactory will call the functions from this contract
///         in the future ...
contract PharoCover is PharoEmits, Ownable, AccessControl {

    using Counters for Counters.Counter;
    Counters.Counter public _policyIds;
    Counters.Counter public _sPolicyIds;
    
    IPharoMarket public pharoMarket;
    IPharoPhinance public pharoPhinance;

    // PharoConstants.SignedPolicy[] public sPolicies;
    // PharoConstants.SignedObelisk public csObelisk;
    // PharoConstants.SignedWoC public csWoC;
    mapping( uint256 => PharoConstants.SignedPolicy[] ) public sPolicies;
    mapping( uint256 => PharoConstants.SignedObelisk ) public csObelisk;
    mapping( uint256 => PharoConstants.SignedWoC ) public csWoC;

    PharoConstants.CoverPolicy[] public policies;
    mapping( address => PharoConstants.CoverPolicy[]) public buyerWalletToPolicies;
    // mapping( address => mapping( uint256 => PharoConstants.CoverPolicy)) public buyerWalletToPolicies;
    mapping( uint256 => address) public policyIdToBuyerWallet;
    mapping( uint256 => mapping(uint256 => PharoConstants.CoverPolicy)) public pharoIdToPolicyIdToCoverPolicy;
    mapping(uint256 => PharoConstants.CoverPolicy) public policyIdToCoverPolicy;

    uint256 BLAH;

    modifier onlyKeeper(address keeperAddress) {
        require(msg.sender == keeperAddress, "You are not the keeper");
        _;
    }

    // ReentrancyGuard() 
    constructor 
    (
        address _pharoMarketAddress,
        address _pharoPhinanceAddress
    )
        public
        onlyOwner
    {
        pharoMarket   = IPharoMarket(_pharoMarketAddress);
        pharoPhinance = IPharoPhinance(_pharoPhinanceAddress);
        _setupRole(PharoConstants.OPERATOR_ROLE, msg.sender);
    }

    function grantOperatorRole(address newOperator)
        public
        onlyOwner
    {
        _setupRole( PharoConstants.OPERATOR_ROLE, newOperator );
    }

    function createPharo (
        uint256 lifetime,
        string memory name,
        string memory description,
        string memory tokenURI
    )
        external
        returns( uint256 pharoId )
    {
        pharoId = pharoMarket._createPharo( lifetime, name, description, tokenURI );
        console.log("Test Emit");
        emit TestEmit( pharoId );
        return pharoId;
    }

    /// @notice This binds a cover policy to a cover buyer
    function createCoverPolicy (
        address token,
        uint128 pharoId,
        PharoConstants.SignedPolicy memory sPolicy,
        PharoConstants.SignedObelisk memory sObelisk,
        PharoConstants.SignedWoC memory sWoC
    )
        public
        returns (uint256 pid)
    {

        // Save all the signed data locally.
        _sPolicyIds.increment();
        pid = _sPolicyIds.current();
        sPolicies[pharoId].push( sPolicy );

        console.log("Storing Signed Data.");
        _storeObelisk( pharoId, sObelisk );
        _storeWoC( pharoId, sWoC );

        // Once saved, test for bad actors.
        console.log("Testing for bad actors.");
        _testPolicy(sPolicy);

        // Once market worth, confirm valid pharoId
        console.log("Confirm Pharo exists.");
        pharoMarket._confirmPharo( pharoId );

        // Once market worthy, map the policy
        console.log(" ");
        console.log("Map & Save Policy Data.");
        PharoConstants.CoverPolicy memory policy = _mapSignedData2Policy( pharoId, sPolicy, sObelisk);
        buyerWalletToPolicies[address(msg.sender)].push(policy);
        policyIdToBuyerWallet[pid] = msg.sender;
        pharoIdToPolicyIdToCoverPolicy[pharoId][pid] = policy;

        _addCoverPolicy(_policyIds.current(), payable(msg.sender), pharoId, policy);

        console.log(" ");
        console.log("Update Pharo Market with new Policy & Signed Data");
        pharoMarket._updateMarkets( pharoId, csObelisk[pharoId], csWoC[pharoId] );

        console.log(" ");
        console.log("Execute CB Financials");
        // Financial tx's execute after all calculations to avoid reentrancy hacks.
        pharoPhinance.getCBApproval( pharoId, token, policy.stake * policy.lengthOfCover, _msgSender());
        pharoPhinance.executeCBFinancials( pharoId, token, policy.stake, _msgSender());
        console.log(" ");

        return (pid);
    }

    //+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // Data Organization
    //---------------------------------------------------------------------

    function _storeObelisk( 
        uint256 pharoId, 
        PharoConstants.SignedObelisk memory sObelisk )
    internal returns ( bool )
    {
        // delete csObelisk;
        csObelisk[pharoId] = sObelisk;

        emit ObeliskCopied();
        return true;
    }

    function updateObelisk( uint256 blah ) public returns ( uint256 )
    {
        BLAH = blah;
        // assert(BLAH==blah);
        return BLAH;
    }

    function _storeWoC( uint256 pharoId, PharoConstants.SignedWoC memory sWoC ) internal returns ( bool ) {

        csWoC[pharoId].pharo_id = sWoC.pharo_id;
        csWoC[pharoId].alpha = sWoC.alpha;
        csWoC[pharoId].beta = sWoC.beta;
        csWoC[pharoId].rateProbable = sWoC.rateProbable;
        csWoC[pharoId].min_confidence = sWoC.min_confidence;
        csWoC[pharoId].max_confidence = sWoC.max_confidence;
        csWoC[pharoId].gof = sWoC.gof;

        uint32[] memory gamma;
        gamma = new uint32[]( sWoC.gamma_x.length );
        for (uint256 sx = 0; sx < sWoC.gamma_x.length; sx++) {
            gamma[sx] = sWoC.gamma_x[sx];
        }
        csWoC[pharoId].gamma_x = gamma;

        gamma = new uint32[]( sWoC.gamma_y.length );
        for (uint256 sx = 0; sx < sWoC.gamma_y.length; sx++) {
            gamma[sx] = sWoC.gamma_y[sx];
        }
        csWoC[pharoId].gamma_y = gamma;

        return true;
    }

    function _mapSignedData2Policy( 
        uint256 pharoId, 
        PharoConstants.SignedPolicy memory sPolicy,
        PharoConstants.SignedObelisk memory sObelisk
    ) 
    internal returns( PharoConstants.CoverPolicy memory policy ) 
    {
        policy.id = _policyIds.current();
        policy.owner = msg.sender; // policy owner
        policy.status = PharoConstants.CoverPolicyStatus.ACTIVE;
        policy.pharoId = pharoId; // id of the Pharo they are buying cover under

        for (uint256 xx = sObelisk.cb_wallet_id.length; xx <= 0; xx--) {
            if (msg.sender == sObelisk.cb_wallet_id[xx]) {
                policy.coverAmount = sObelisk.cb_cover_bought[xx];
                policy.cost = sObelisk.cb_stake[xx] - sObelisk.cb_funds_avail[xx];
                policy.stake = sObelisk.cb_stake[xx];
                policy.rateEstimate = sObelisk.cb_rate_estimate[xx];
                policy.min_cover = sObelisk.cb_min_cover[xx];
                break;
            }
        }
        
        policy.lengthOfCover = sPolicy.lengthOfCover;
        policy.rewardPaid = 0;

        // policies[policy.id] = policy;
        policies.push(policy);
        console.log("Policy Stored.");
        emit CoverPolicyCreated();
        return policy;
    }

    //+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // Adding Pharo Policies
    //---------------------------------------------------------------------

    function _testPolicy( PharoConstants.SignedPolicy memory sPolicy ) 
        internal returns( bool policyApproved ) 
    {
        bool policyApproved = true;
        if ( sPolicy.min_cover > (sPolicy.rateEstimate * sPolicy.stakeAmount) ) {
            policyApproved = false;
        }

        if ( policyApproved == false ) {
            emit CoverPolicyRejected ( msg.sender, block.timestamp );
        } else {
            emit CoverPolicyAccepted ( msg.sender, block.timestamp );
        }
        require(policyApproved == true, "Gotta meet basic requirements to participate in the market!");

        return policyApproved;
    }

    /// @notice adds the cover policy to storage, and executes finances
    /// @dev designed to test Wisdom of Crowds in testnet.
    /// @param policyId the id of the policy
    /// @param coverBuyer the address of the cover buyer
    /// @param pharoId the id of the Pharo Market
    /// @param policy the cover buyers signed policy. See SignedPosition struct.
    function _addCoverPolicy
    (
        uint256 policyId,
        address payable coverBuyer,
        uint256 pharoId,
        PharoConstants.CoverPolicy memory policy
    )
        internal 
    {
        // Write Cover Policy to storage and update references
        PharoConstants.CoverPolicy storage coverPolicy = pharoIdToPolicyIdToCoverPolicy[pharoId][policyId];
        coverPolicy.status = PharoConstants.CoverPolicyStatus.PENDING;
        coverPolicy.owner = coverBuyer;
        coverPolicy.id = policyId;
        coverPolicy.pharoId = pharoId;
        coverPolicy.min_cover = policy.min_cover; 
        coverPolicy.lengthOfCover = policy.lengthOfCover;
        coverPolicy.stake = policy.stake;
        coverPolicy.rateEstimate = policy.rateEstimate;
        coverPolicy.coverAmount = 0; // final amount determined after event trigger
        coverPolicy.rewardPaid = 0; // Placeholder until the event happens. Maybe a "-1" instead?
        coverPolicy.cost = policy.stake;

        policyIdToCoverPolicy[policyId] = coverPolicy; // to retrieve later by id
        // policies.push(coverPolicy); // not sure if we even need to keep the array...

        _initializePolicy(policyId, coverBuyer);
    }
    
    /// @notice Not sure if we even need this yet, doing the same as the acceptPolicy()
    function _initializePolicy(uint256 policyId, address coverBuyerAddress) internal {
        //require(hasRole(OPERATOR_ROLE, msg.sender), "Need to be an operator");
        PharoConstants.CoverPolicy storage policy = policyIdToCoverPolicy[policyId];
        
        // if (policy.status != PharoConstants.CoverPolicyStatus.ACTIVE) revert AlreadyActive();
        if (policy.status == PharoConstants.CoverPolicyStatus.REJECTED) revert RejectedStatus();
        
        policy.status = PharoConstants.CoverPolicyStatus.ACTIVE;

        emit CoverPolicyInitialized(policyId, coverBuyerAddress, block.timestamp);
    }

    /// @dev rejects a policy and records it
    function _rejectPolicy(uint256 policyId, address coverBuyerAddress) internal {

        if (msg.sender == address(0)) revert ZeroAddressNotAllowed();

        PharoConstants.CoverPolicy storage policy = policyIdToCoverPolicy[policyId];

        if (policy.status == PharoConstants.CoverPolicyStatus.REJECTED) revert RejectedStatus();
        
        if (policy.min_cover <= 0) revert MustBeGreaterThanZero();
        if (policy.lengthOfCover <= 0) revert MustBeGreaterThanZero();
        if (policy.rateEstimate <= 0) revert MustBeGreaterThanZero();

        if (hasRole(PharoConstants.OPERATOR_ROLE, msg.sender)) revert WrongRole();

        policy.status = PharoConstants.CoverPolicyStatus.REJECTED;

        emit CoverPolicyRejected( coverBuyerAddress, block.timestamp);
    }

    /// @dev used to close a policy: 
    /// 1: after the event has been triggered, or 
    /// 2: time ran out for that Pharo, or
    /// 3: the user cancels their policy.
    function closePolicy(
        uint256 policyId, 
        address coverBuyerAddress, 
        PharoConstants.CoverPolicyStatus status
    ) 
    public {
        if (hasRole(PharoConstants.OPERATOR_ROLE, msg.sender)) revert WrongRole();

        PharoConstants.CoverPolicy storage policy = policyIdToCoverPolicy[policyId];

        if (policy.status == PharoConstants.CoverPolicyStatus.CLOSEDPAID) revert AlreadyPaid();
        if (policy.id != policyId) revert BadId();

        if (status == PharoConstants.CoverPolicyStatus.CLOSEDPAID) {
            policy.status = PharoConstants.CoverPolicyStatus.CLOSEDPAID;
        } else 
        if (status == PharoConstants.CoverPolicyStatus.CLOSEDUNPAID) {
            policy.status = PharoConstants.CoverPolicyStatus.CLOSEDUNPAID;
        }

        emit CoverPolicyClosed(policyId, coverBuyerAddress, block.timestamp);
    }

    //+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // Data about policies and Pharo
    //---------------------------------------------------------------------

    /// @dev get cover policies for a specified user
    /// @param user the address of the user
    function getCoverPolicies(address user)
        public
        view
        returns (PharoConstants.CoverPolicy[] memory)
    {
        PharoConstants.CoverPolicy[] memory Policies = new PharoConstants.CoverPolicy[](policies.length);
        for (uint256 index = 0; index < policies.length; index++) {
            if(policies[index].owner == user) {
                PharoConstants.CoverPolicy memory policy = policies[index];
                Policies[index] = policy;
            }
        }

        return Policies;
    }

    function getMarketPlayers( uint256 pharoId ) 
        public returns ( PharoConstants.SignedObelisk memory returnObelisk ) 
    {
        returnObelisk = csObelisk[pharoId];
        return returnObelisk;
    }

    //+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    // Keeper Triggered
    //---------------------------------------------------------------------

    function collectPremiums( uint128 pharoId, address token ) public {
        bool success;
        for (uint32 px = 0; px < policies.length; px++) 
        {
            success = pharoPhinance.executeCBFinancials( pharoId, token, policies[px].stake, policies[px].owner );
            if (!success) {
                
                emit InsufficientFunds( policies[px].owner, block.timestamp, pharoId );

                closePolicy( policies[px].id, policies[px].owner, policies[px].status );
            }
        }
        emit PoliciesPaid( pharoId, block.timestamp );
    }

    function pharoEventTriggered( uint256 pharoId, uint32 trueEventTime ) external {
        // Record the data before payouts

        // Update the Market

        // Pay CB's with LP money

        // Reconstitute from ReservePool
    }

    function payoutActivePoliciesForCurrentPharo( uint256 pharoId, uint32 trueEventTime ) internal {
        // todo: add access control

        address wallet_id;
        uint256 total_payout;
        bool success = pharoMarket.updateObelisk( trueEventTime );

        // loop through the policies and pay each one that is in the money
        for (uint256 xx = 0; xx < policies.length; xx++) {
        // for (uint256 xx = 0; xx < obelisk.buyerLisk.length; xx++) {

            if (policies[xx].status==PharoConstants.CoverPolicyStatus.ACTIVE) {
                
                wallet_id = policies[xx].owner;
                total_payout = pharoMarket.getObeliskPayout( policies[xx].owner );
                // totalPayout = obelisk.buyers[].totalCover + obelisk.buyers[policies[xx].owner].totalReward;

                pharoPhinance.payoutCoverPolicy( pharoId, wallet_id, total_payout );
            } 
            else {
                // closePolicy(policies[index].id, policies[index].owner, PharoConstants.CoverPolicyStatus.CLOSEDUNPAID);
            }
        }

        emit PayoutsComplete();
    }

}

