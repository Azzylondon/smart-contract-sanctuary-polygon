import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./interfaces/IFraction.sol";
import "./interfaces/ITreasury.sol";
import "./types/AccessControlled.sol";
import "hardhat/console.sol";


contract FractionalizedRights is Pausable, AccessControlled  {

  event FractionTransfer(address indexed from, address indexed to, uint256 indexed tokenId, uint256 amount); // TODO: Move to interface

  IFraction public Fraction;
  ITreasury public Treasury;

  address public owner;
  address public reserveToken;
  uint256 public fee;


  struct ReceivedRights {
    uint256 amount;
    uint256 timestamp;
  }

  mapping(bytes32 => ReceivedRights[]) public receivedRights;
  mapping(bytes32 => uint256) public receivedRightsCount;

  struct FractionNFT {
    uint256 amount;
    bytes32 typeId; // asset unique type id (ex. a specific song)
    uint256 nftId;
    uint256 pricePerFraction;
    uint256 startDate;
    uint256 endDate;
    uint256 claimed;
  }

  mapping (bytes32 => uint256) supplies; // total supplies
  mapping (bytes32 => uint256) rootIds; // root id of a specific asset/song
  mapping (uint256 => FractionNFT) fractionNfts; // nftId => FractionNFT
  mapping (address => bool) creators; // address => is creator
  mapping (uint256 => address) managedAccounts; // nftId => creator address

  /// Function called too early.
  error TooEarly();

  constructor(address _fraction, address _treasury, address _authority) AccessControlled(IAuthority(_authority)) {
      Fraction = IFraction(_fraction);
      Treasury = ITreasury(_treasury);
      owner = msg.sender;
      fee = 1000; // 10%
  }

  function getTypeId(address _creator, string memory _type, uint256 _supply) pure internal returns(bytes32) {
    return keccak256(abi.encodePacked(_creator, _type, _supply)); // composite key    
  }

  function initialize(address _creator, string memory _type, uint256 _supply, uint256 _pricePerFraction, uint256 _endDate) public onlyGovernor {
    

    bytes32 typeId = getTypeId(_creator, _type, _supply);

    bytes memory data = abi.encodeWithSignature("safeMint(address)", _creator);

    bytes memory returnData = Address.functionCall(address(Fraction), data, "Minting failed");
    uint256 nftId = abi.decode(returnData, (uint256));

    FractionNFT memory fractionnft = FractionNFT(_supply, typeId, nftId, _pricePerFraction, block.timestamp, _endDate, 0);
    fractionNfts[nftId] = fractionnft;
    supplies[typeId] = _supply;
    creators[_creator] = true;
    rootIds[typeId] = nftId;
    managedAccounts[nftId] = _creator;
  }

  function pause() public onlyGovernor {
      _pause();
  }

  function unpause() public onlyGovernor {
      _unpause();
  }

  function getFraction(uint256 _nftId) public view returns (uint256, bytes32, uint256, uint256, uint256, uint256, uint256) {
    return (fractionNfts[_nftId].amount, fractionNfts[_nftId].typeId, fractionNfts[_nftId].nftId, fractionNfts[_nftId].pricePerFraction, fractionNfts[_nftId].startDate, fractionNfts[_nftId].endDate, fractionNfts[_nftId].claimed);
  } 

  function getFractionNFT(uint256 _nftId) public view returns (FractionNFT memory) {
    return fractionNfts[_nftId];
  }

  function getFractionByKey(bytes32 _typeId) public view returns (uint256, bytes32, uint256, uint256, uint256, uint256, uint256) {
    uint256 nftId = rootIds[_typeId];
    return (fractionNfts[nftId].amount, fractionNfts[nftId].typeId, fractionNfts[nftId].nftId, fractionNfts[nftId].pricePerFraction, fractionNfts[nftId].startDate, fractionNfts[nftId].endDate, fractionNfts[nftId].claimed);
  }


  function transferFraction(uint256 _nftId, address _to, uint256 _amount) public {
    require(msg.sender == Fraction.ownerOf(_nftId), "Only owner can transfer");
    require(_amount <= fractionNfts[_nftId].amount, "Amount exceeds supply");

    fractionNfts[_nftId].amount -= _amount;
    fractionNfts[_nftId].claimed += _amount;

    bytes memory data = abi.encodeWithSignature("safeMint(address)", _to);

    bytes memory returnData = Address.functionCall(address(Fraction), data, "Minting failed");
    uint256 nftId = abi.decode(returnData, (uint256));

    fractionNfts[nftId] = FractionNFT(_amount, fractionNfts[_nftId].typeId, nftId, fractionNfts[_nftId].pricePerFraction, block.timestamp, fractionNfts[_nftId].endDate, 0);

    // skipping creator (artists) for now
    if (!creators[msg.sender]) {
      claim(_nftId);
    }    

    emit FractionTransfer(msg.sender, _to, _nftId, _amount);
  }

  function getNfts(address _user) public view returns (uint256[] memory) {
    uint256[] memory nfts = new uint256[](Fraction.balanceOf(_user));
    for (uint256 i = 0; i < nfts.length; i++) {
      nfts[i] = Fraction.tokenOfOwnerByIndex(_user, i);
    }
    return nfts;
  }

  // return balance of unclaimed royalties multiplied by price per fraction for a given user
  function pendingBalance(address _user) public view returns(uint256) {
    uint256[] memory nfts = getNfts(_user);
    uint256 balance = 0;
    for (uint256 i = 0; i < nfts.length; i++) {
      balance += pendingBalanceForNft(nfts[i]);
    }
    return balance;
  }

  // return balance of unclaimed royalties for a given nft
  function pendingBalanceForNft(uint256 _nftId) public view returns(uint256) {
    uint256 balance = 0;
    for (uint256 i = 0; i < receivedRightsCount[fractionNfts[_nftId].typeId]; i++) {
      if (receivedRights[fractionNfts[_nftId].typeId][i].timestamp > fractionNfts[_nftId].startDate) {
        balance += receivedRights[fractionNfts[_nftId].typeId][i].amount * (fractionNfts[_nftId].amount - fractionNfts[_nftId].claimed) / supplies[fractionNfts[_nftId].typeId];
      }
    }
    return balance * fractionNfts[_nftId].pricePerFraction;
  }

  function claimedBalanceForNft(uint256 _nftId) public view returns(uint256) {
    return fractionNfts[_nftId].claimed;
  }

  function setReserveToken(address _reserveToken) public onlyGovernor {
    reserveToken = _reserveToken;
  }

  // user claims their share of royalties for a given nft id by calling treasury withdraw function
  // it must be called before the end date of the nft
  // the claimer must be the owner of the nft
  function claim(uint256 _nftId) public returns (uint256) {
    require(msg.sender == Fraction.ownerOf(_nftId), "Only owner can claim");
    require(block.timestamp < fractionNfts[_nftId].endDate, "Too late");
    require(fractionNfts[_nftId].amount > fractionNfts[_nftId].claimed, "Nothing to claim");

    uint256 amount = pendingBalanceForNft(_nftId);
    require(amount > 0, "Nothing to claim");

    bytes memory data = abi.encodeWithSignature("withdraw(address,address,uint256,uint256)", reserveToken, msg.sender, amount, fee);

    bytes memory returnData = Address.functionCall(address(Treasury), data, "Withdraw failed");
    uint256 withdrawn = abi.decode(returnData, (uint256));

    // TODO: stop claiming all rights
    fractionNfts[_nftId].claimed += fractionNfts[_nftId].amount;

    return withdrawn;
  }
  

  function depositRights(bytes32 _typeId, uint256 _amount) public {
    Treasury.deposit(_amount, reserveToken);
    receivedRights[_typeId].push(ReceivedRights(_amount, block.timestamp));
    receivedRightsCount[_typeId] += 1;
  }

  function getReceivedRights(bytes32 _typeId) public view returns (uint256[] memory, uint256[] memory) {
    uint256[] memory amounts = new uint256[](receivedRightsCount[_typeId]);
    uint256[] memory timestamps = new uint256[](receivedRightsCount[_typeId]);
    for (uint256 i = 0; i < receivedRightsCount[_typeId]; i++) {
      amounts[i] = receivedRights[_typeId][i].amount;
      timestamps[i] = receivedRights[_typeId][i].timestamp;
    }
    return (amounts, timestamps);
  }

  function getReceivedRightsCount(bytes32 _typeId) public view returns (uint256) {
    return receivedRightsCount[_typeId];
  }

  function getRootId(bytes32 _typeId) public view returns (uint256) {
    return rootIds[_typeId];
  }

  // a function allowing users to buy a fraction of the root nfts using USDTMock token
  // the user must approve the contract to spend the amount of USDTMock tokens they want to spend
  // the price is set by the creator of the nft and is the same for all fractions
  // the end date is set by the creator of the nft and is the same for all fractions
  // it receives the nft id of the root nft and the amount of fractions to buy
  // it mints the fraction nft and transfers it to the caller
  // it also transfers the USDTMock tokens to the treasury
  // it also updates the amount of fractions on the root nft
  function _buyFraction(uint256 _nftId, uint256 _amount, address _to) public {
    require(rootIds[fractionNfts[_nftId].typeId] > 0, "Not a root nft"); // check if nft is a root nft
    require(_amount > 0, "Amount must be greater than 0");
    require(_amount <= fractionNfts[_nftId].amount, "Amount exceeds supply");
    require(block.timestamp < fractionNfts[_nftId].endDate, "Too late");

    uint256 price = fractionNfts[_nftId].pricePerFraction * _amount;
    require(price > 0, "Price must be greater than 0");

    Treasury.deposit(price, reserveToken);
    // TODO: send cut to creator

    bytes memory data = abi.encodeWithSignature("safeMint(address)", _to);

    bytes memory returnData = Address.functionCall(address(Fraction), data, "Minting failed");
    uint256 nftId = abi.decode(returnData, (uint256));

    fractionNfts[nftId] = FractionNFT(_amount, fractionNfts[_nftId].typeId, nftId, fractionNfts[_nftId].pricePerFraction, block.timestamp, fractionNfts[_nftId].endDate, 0);

    fractionNfts[_nftId].amount -= _amount;

    emit FractionTransfer(address(0), _to, nftId, _amount);
  }

  function buyFraction(uint256 _nftId, uint256 _amount) public {
    _buyFraction(_nftId, _amount, msg.sender);
  }

  function buyFractionBatch(uint256[] memory _nftIds, uint256[] memory _amounts) public {
    require(_nftIds.length == _amounts.length, "Arrays must be of same length");
    for (uint256 i = 0; i < _nftIds.length; i++) {
      _buyFraction(_nftIds[i], _amounts[i], msg.sender);
    }
  }

  function buyFractionTo(uint256 _nftId, uint256 _amount, address _to) public onlyGovernor {
    _buyFraction(_nftId, _amount, _to);
  }
  
  function buyFractionBatchTo(uint256[] memory _nftIds, uint256[] memory _amounts, address _to) public onlyGovernor {
    require(_nftIds.length == _amounts.length, "Arrays must be of same length");
    for (uint256 i = 0; i < _nftIds.length; i++) {
      _buyFraction(_nftIds[i], _amounts[i], _to);
    }
  }


  
}

