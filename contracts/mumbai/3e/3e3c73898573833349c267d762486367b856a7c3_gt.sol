pragma solidity ^0.8.1;
 contract gt
 { event fg (uint number, uint number1, uint number2, string ma);
 event fgg (uint number, uint number1, uint number2, string ma);
 event fgh (uint number, uint number1, uint number2, string ma);
 event fgn (uint number, uint number1, uint number2, string ma);
 function w4(uint a, uint b) public{
     uint c = a+b;
     emit fg (a , b , c , "+");
 }
function rg(uint q, uint w) public{
    uint e= q-w;
    emit fgg (q , w , e , "-");
}
function gr(uint r, uint t) public{
    uint y= r/t;
    emit fgh (r , t , y , "/");
}
function gd(uint u, uint i) public{
    uint o= u*i;
    emit fgn (u , i , o , "*");
}

 }