pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

import "cojodi/contracts/access/MerkleClaim.sol";

import "hardhat/console.sol";

contract TheSquidsTournament is MerkleClaim {
  using SafeMath for uint256;

  enum TournamentPhase {
    SUSPENDED,
    REGISTRATION,
    CLAIM,
    IN_PLAY
  }

  TournamentPhase public currentPhase;

  IERC20 public dna;
  IERC721 public theSquids;

  uint256 public registrationFee;
  uint256 public lifeFee;

  uint256 public maxUsers = 1000;
  uint256 public numRegisteredUsers = 0;
  mapping(address => bool) public registeredUsers;
  mapping(address => uint256[]) public stakes;

  event UserRegistered(address indexed user, uint256 fee, uint256 livesAmount);
  event UserBoughtLives(address indexed user, uint256 fee, uint256 livesAmount);
  event UserClaimed(address indexed user, uint256 amount);

  constructor(address theSquids_, address dna_) MerkleClaim() {
    currentPhase = TournamentPhase.SUSPENDED;

    theSquids = IERC721(theSquids_);
    dna = IERC20(dna_);
  }

  function stake(uint256[] calldata tokenIds_) external {
    for (uint256 i = 0; i < tokenIds_.length; ++i) {
      uint256 tokenId = tokenIds_[i];
      theSquids.transferFrom(msg.sender, address(this), tokenId);
      stakes[msg.sender].push(tokenId);
    }
  }

  function unstake(uint256[] calldata tokenIds_) external isNotPhase(TournamentPhase.REGISTRATION) {
    for (uint256 i = 0; i < tokenIds_.length; ++i) {
      uint256 tokenId = tokenIds_[i];
      int256 tokenIdx = stakeIndexOf(msg.sender, tokenId);

      require(tokenIdx != -1, "token not present");

      theSquids.transferFrom(address(this), msg.sender, tokenId);
      stakeIndexRemove(msg.sender, uint256(tokenIdx));
    }
  }

  function register() external isPhase(TournamentPhase.REGISTRATION) {
    require(!registeredUsers[msg.sender], "already registered");
    require(registrationFee != 0, "set registration fee");
    require(stakedBalance(msg.sender) > 0, "no stake");
    require(numRegisteredUsers < maxUsers, "max amount of users reached");

    uint256 price = userBasedRegistrationFee(msg.sender);
    dna.transferFrom(msg.sender, address(this), price);

    registeredUsers[msg.sender] = true;
    numRegisteredUsers += 1;
    amountClaimed[msg.sender] = false;

    emit UserRegistered(msg.sender, registrationFee, stakedBalance(msg.sender));
  }

  function buyLives(uint256 amount_) external {
    require(lifeFee != 0, "set life fee");

    uint256 fee = lifeFee.mul(amount_);
    dna.transferFrom(msg.sender, address(this), fee);

    emit UserBoughtLives(msg.sender, fee, amount_);
  }

  function claim(bytes32[] calldata merkleProof_, uint256 amount_)
    external
    isPhase(TournamentPhase.CLAIM)
    canClaim(merkleProof_, amount_)
  {
    registeredUsers[msg.sender] = false;
    dna.approve(address(this), amount_);
    dna.transferFrom(address(this), msg.sender, amount_);

    emit UserClaimed(msg.sender, amount_);
  }

  function reset() external isPhase(TournamentPhase.CLAIM) {
    registeredUsers[msg.sender] = false;

    emit UserClaimed(msg.sender, 0);
  }

  ////////////////////////////////////////////////////////////////////////////////
  // SETTER
  ////////////////////////////////////////////////////////////////////////////////
  function setPhase(TournamentPhase phase_) external onlyOwner {
    currentPhase = phase_;
  }

  function setRegistrationFee(uint256 amount_) external onlyOwner {
    registrationFee = amount_;
  }

  function setLifeFee(uint256 amount_) external onlyOwner {
    lifeFee = amount_;
  }

  function setDNA(address dna_) external onlyOwner {
    dna = IERC20(dna_);
  }

  function setTheSquids(address theSquids_) external onlyOwner {
    theSquids = IERC721(theSquids_);
  }

  function resetNumRegisteredUsers() external onlyOwner {
    numRegisteredUsers = 0;
  }

  function setMaxUsers(uint256 amount_) external onlyOwner {
    maxUsers = amount_;
  }

  ////////////////////////////////////////////////////////////////////////////////
  // UTILS
  ////////////////////////////////////////////////////////////////////////////////
  modifier isPhase(TournamentPhase phase_) {
    require(currentPhase == phase_, "wrong phase");
    _;
  }

  modifier isNotPhase(TournamentPhase phase_) {
    require(currentPhase != phase_, "wrong phase");
    _;
  }

  function userBasedRegistrationFee(address user_) public view returns (uint256) {
    uint256 stakedBalance_ = stakedBalance(user_);

    uint256 discount = 0;
    if (stakedBalance_ >= 2 && stakedBalance_ <= 5) {
      discount = 5;
    } else if (stakedBalance_ >= 6 && stakedBalance_ <= 10) {
      discount = 10;
    } else if (stakedBalance_ > 10) {
      discount = 15;
    }

    return (registrationFee * (100 - discount)) / 100;
  }

  function getStake(address user_) public view returns (uint256[] memory) {
    return stakes[user_];
  }

  function stakedBalance(address user_) public view returns (uint256) {
    return stakes[user_].length;
  }

  function stakeIndexOf(address user_, uint256 element_) public view returns (int256) {
    uint256[] storage _stakes = stakes[user_];
    for (uint256 i = 0; i < stakes[user_].length; ++i) {
      if (_stakes[i] == element_) {
        return int256(i);
      }
      // coverage
      continue;
    }

    return -1;
  }

  function stakeIndexRemove(address user_, uint256 index_) internal {
    // require(index_ < stakes[user_].length, "index out of bounds");
    for (uint256 i = index_; i < stakes[user_].length - 1; ++i) {
      stakes[user_][i] = stakes[user_][i + 1];
    }

    stakes[user_].pop();
  }
}

