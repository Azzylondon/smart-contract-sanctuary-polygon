pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract MerkleClaim is Ownable {
  using Strings for uint256;

  mapping(address => bool) public amountClaimed;

  bytes32 public claimMerkleRoot;

  function setClaimMerkleRoot(bytes32 merkleRoot_) external onlyOwner {
    claimMerkleRoot = merkleRoot_;
  }

  modifier canClaim(bytes32[] calldata merkleProof_, uint256 amount_) {
    require(!amountClaimed[msg.sender], "amount claimed");

    bytes32 leaf = keccak256(abi.encodePacked(msg.sender, amount_));
    require(MerkleProof.verify(merkleProof_, claimMerkleRoot, leaf), "invalid proof");

    // perform action
    _;

    amountClaimed[msg.sender] = true;
  }
}

