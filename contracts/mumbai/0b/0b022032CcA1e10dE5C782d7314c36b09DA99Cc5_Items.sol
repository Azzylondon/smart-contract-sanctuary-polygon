pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "../interfaces/IWithdrawable.sol";

contract Withdrawable is OwnableUpgradeable, UUPSUpgradeable, IWithdrawable {

  function _Withdrawable() public initializer {
    __Ownable_init();
  }

  receive() external payable {}

  function withdrawMatic() public onlyOwner override {
    require(address(this).balance > 0, "Balance must be larger than 0");
    payable(msg.sender).transfer(address(this).balance);
    emit WithdrawMatic(msg.sender, address(this).balance);
  }

  function withdrawToken(uint256 amount, address tokenAddr) public onlyOwner override {
    require(IERC20(tokenAddr).balanceOf(address(this)) >= amount, "Balance is reached limited");
    IERC20(tokenAddr).transfer(owner(), amount);
    emit WithdrawToken(owner(), amount);
  }

  function withdrawERC721Token(uint256 tokenId, address tokenAddr) public override onlyOwner {
    require(IERC721(tokenAddr).ownerOf(tokenId) == address(this), "This token does not belong to Contract");
    IERC721(tokenAddr).transferFrom(address(this), owner(), tokenId);
    emit WithdrawERC721Token(owner(), tokenId);
  }

  function withdrawERC1155Token(uint256[] memory ids, uint256[] memory amounts, address tokenAddr) public override onlyOwner {
    require(ids.length > 0 && ids.length == amounts.length, 'Params is invalid');
    for (uint256 i = 0; i < ids.length; i++) {
      require(IERC1155(tokenAddr).balanceOf(address(this), ids[i]) >= amounts[i] && amounts[i] > 0, "This token does not belong to Contract");
    }

    IERC1155(tokenAddr).safeBatchTransferFrom(address(this), owner(), ids, amounts, "");
    emit WithdrawERC1155Token(owner(), ids, amounts, tokenAddr);
  }

  function _authorizeUpgrade(address newImplementation) override internal virtual {}

}

