pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "./ItemUpgrader.sol";
import "../interfaces/IRandomizer.sol";
import "../interfaces/IItems.sol";

contract Items is ERC1155, Ownable, IItems {

  using SafeMath for uint256;
  IRandomizer private randomizer;
  IERC20 public token;
  ItemUpgrader public itemUpgrade;

  uint public constant ItemIdForBeastBox = 1;
  uint public constant ItemIdForItemBox = 2;
  uint public constant ItemIdForEKC = 9999;

  //Don't use ItemId to mint tokens
  // weapon [3, 4, 5, 6]
  // head armor [7, 8, 9, 10]
  // leg armor [11, 12, 13, 14]
  // eye [15, 16, 17, 18]
  uint256 private ItemId = 3;
  uint256 private ItemBoxRankCount = 0;
  uint256 private ItemTypeCount = 0;

  constructor(
    IERC20 _token,
    ItemUpgrader _itemUpgrade,
    address _randomizer
  ) ERC1155("https://game.example/api/item/{id}.json") {
    token = _token;
    itemUpgrade = _itemUpgrade;
    randomizer = IRandomizer(_randomizer);
    setUpItemBoxRank();
  }

  mapping(uint256 => uint256) public ItemBoxRankList;
  mapping(uint256 => uint256[]) public ItemIds;

  function setRandomizer(IRandomizer _randomizer) public onlyOwner {
    randomizer = _randomizer;
  }

  function setItemUpgrader(ItemUpgrader _itemUpgrade) public onlyOwner {
    itemUpgrade = _itemUpgrade;
  }

  function setUpItemBoxRank() private {
    setItemBoxRank(1, 8550);
    setItemBoxRank(2, 1410);
    setItemBoxRank(3, 35);
    setItemBoxRank(4, 5);
    setItemId(1);
    setItemId(2);
    setItemId(3);
    setItemId(4);
  }

  function setItemBoxRank(uint256 rank, uint256 quality) public onlyOwner {
    ItemBoxRankCount = ItemBoxRankList[rank] > 0 ? ItemBoxRankCount : ItemBoxRankCount + 1;
    ItemBoxRankList[rank] = quality;
  }

  function getItemBoxRank(uint256 rank) public view returns (uint256) {
    return ItemBoxRankList[rank];
  }

  function setItemId(uint256 TypeItem) public onlyOwner {
    for (uint256 index = 0; index < ItemBoxRankCount; index++) {
      ItemIds[TypeItem].push(ItemId);
      ++ItemId;
    }
    ++ItemTypeCount;
  }

  function getItemId(uint256 TypeItem) public view returns (uint256[] memory) {
    return ItemIds[TypeItem];
  }

  function getItemIdForBeastBox() public override pure returns (uint) {
    return ItemIdForBeastBox;
  }

  function getItemIdForItemBox() public override pure returns (uint) {
    return ItemIdForItemBox;
  }

  function getItemIdForEKC() public override pure returns (uint) {
    return ItemIdForEKC;
  }

  // remove this in production
  function __testMint(address to, uint256 id, uint256 value) public {
    _mint(to, id, value, '');
  }

  function buyBeastBox(address _to) public {
    uint256 price = 1;
    token.transferFrom(
      _to,
      address(this),
      price
    );
    _mint(_to, ItemIdForBeastBox, 1, '');
  }

  function buyItem(address _to) public {
    uint256 totalItem = getTotalBoxItem();
    require(totalItem > 0, "RUN_OUT_OF_BOX_ITEM");
    require(balanceOf(_to, ItemIdForItemBox) > 0, "USER_HAVE_NO_BOX");

    uint amount = 1;

    (uint256 randRank, uint256 randItem) = randomizer.randMod4MintItem(totalItem, ItemTypeCount, ItemBoxRankCount, ItemId);
    uint256 rankItem = setRankItem(randRank, 1, 0);
    uint256 itemId = ItemIds[randItem + 1][rankItem];

    _mint(_to, itemId, amount, '');

    burn(_to, ItemIdForItemBox, amount);
  }

  function burn(address _to, uint256 _id, uint256 _amount) public override {
    _burn(_to, _id, _amount);
  }

  function transferFrom(address _from, address _to, uint256 _id, uint256 _amount, bytes calldata _data) public {
    _safeTransferFrom(_from, _to, _id, _amount, _data);
  }

  function getTotalBoxItem() private view returns (uint256)
  {
    uint256 totalItem = 0;
    for (uint256 index = 1; index <= ItemBoxRankCount; index++) {
      totalItem += ItemBoxRankList[index];
    }
    return totalItem;
  }

  function setRankItem(uint256 randNum, uint256 index, uint256 offset) private returns (uint256)
  {
    if (index > ItemBoxRankCount) {
      return 1;
    }

    if (randNum >= offset && randNum < offset + ItemBoxRankList[index]) {
      ItemBoxRankList[index] = ItemBoxRankList[index] - 1;
      return index;
    }

    return setRankItem(randNum, index + 1, offset + ItemBoxRankList[index]);
  }

  event Upgrade(uint256 inputTokenId, UpgradeResult upgradeResult, address user);

  function upgradeItems(uint256 itemId) public {
    require(!isContract(_msgSender()), "NO_CONTRACT_CALLS");
    address account = _msgSender();
    uint256 numOfItem = this.balanceOf(account, itemId);
    uint256 numOfEKC = this.balanceOf(account, ItemIdForEKC);
    (UpgradeResult upgradeResult, uint256 nextItem, uint256 ekc4Upgrade, uint256 items4Upgrade) = itemUpgrade.upgrade(itemId, numOfItem, numOfEKC);
    uint256 price = 1;
    token.transferFrom(
      account,
      address(this),
      price
    );
    burn(account, itemId, items4Upgrade);
    burn(account, ItemIdForEKC, ekc4Upgrade);
    if (upgradeResult == UpgradeResult.INCREASE_1_LEVEL) {
      _mint(account, nextItem, 1, '');
    }
    emit Upgrade(itemId, upgradeResult, account);
  }

  function isContract(address account) internal view returns (bool) {
    bytes32 codehash;
    bytes32 accountHash = 0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470;
    assembly {codehash := extcodehash(account)}
    return (codehash != accountHash && codehash != 0x0);
  }
}

