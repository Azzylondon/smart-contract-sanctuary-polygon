pragma solidity 0.8.11;

import "./Bribe.sol";

contract BaseV1BribeFactory {
    address public last_gauge;
    address public voter;

    constructor(address _voter) {
        voter = _voter;
    }

    function createBribe(address _collection) external returns (address) {
        last_gauge = address(new Bribe(voter, _collection));
        return last_gauge;
    }
}

