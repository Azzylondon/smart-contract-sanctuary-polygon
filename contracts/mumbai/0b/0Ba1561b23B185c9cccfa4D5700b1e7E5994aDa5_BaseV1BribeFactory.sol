pragma solidity 0.8.11;

// CHECKPOINT

import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./shared/interfaces/IVe.sol";
import "./shared/interfaces/IVoter.sol";
import "hardhat/console.sol";

// Bribes pay out rewards for a given pool based on the votes that were received from the user (goes hand in hand with BaseV1Gauges.vote())
contract Bribe {
    address public immutable voter; // only voter can modify balances (since it only happens on vote())
    address public immutable _ve;

    address public immutable collection;

    uint256 public constant DURATION = 7 days; // rewards are released over 7 days
    uint256 public constant PRECISION = 10**18;

    // true for positive voter, false for negative voter
    // default snx staking contract implementation
    mapping(bool => mapping(address => uint256)) public rewardRate;
    mapping(bool => mapping(address => uint256)) public periodFinish;
    mapping(bool => mapping(address => uint256)) public lastUpdateTime;
    mapping(bool => mapping(address => uint256)) public rewardPerTokenStored;

    mapping(bool => mapping(address => mapping(uint256 => uint256)))
        public lastEarn;
    mapping(bool => mapping(address => mapping(uint256 => uint256)))
        public userRewardPerTokenStored;

    mapping(bool => address[]) public rewards;
    mapping(bool => mapping(address => bool)) public isReward;

    mapping(bool => uint256) public totalSupply;
    mapping(bool => mapping(uint256 => uint256)) public balanceOf;

    /// @notice A checkpoint for marking balance
    struct Checkpoint {
        uint256 timestamp;
        uint256 balanceOf;
    }

    /// @notice A checkpoint for marking reward rate
    struct RewardPerTokenCheckpoint {
        uint256 timestamp;
        uint256 rewardPerToken;
    }

    /// @notice A checkpoint for marking supply
    struct SupplyCheckpoint {
        uint256 timestamp;
        uint256 supply;
    }

    /// @notice A record of balance checkpoints for each account, by index
    mapping(bool => mapping(uint256 => mapping(uint256 => Checkpoint)))
        public checkpoints;
    /// @notice The number of checkpoints for each account
    mapping(bool => mapping(uint256 => uint256)) public numCheckpoints;
    /// @notice A record of balance checkpoints for each token, by index
    mapping(bool => mapping(uint256 => SupplyCheckpoint))
        public supplyCheckpoints;
    /// @notice The number of checkpoints
    mapping(bool => uint256) public supplyNumCheckpoints;
    /// @notice A record of balance checkpoints for each token, by index
    mapping(bool => mapping(address => mapping(uint256 => RewardPerTokenCheckpoint)))
        public rewardPerTokenCheckpoints;
    /// @notice The number of checkpoints for each token
    mapping(bool => mapping(address => uint256))
        public rewardPerTokenNumCheckpoints;

    event Deposit(
        bool toggle,
        address indexed from,
        uint256 tokenId,
        uint256 amount
    );
    event Withdraw(
        bool toggle,
        address indexed from,
        uint256 tokenId,
        uint256 amount
    );
    event NotifyReward(
        bool toggle,
        address indexed from,
        address indexed reward,
        uint256 amount
    );
    event ClaimRewards(
        bool toggle,
        address indexed from,
        address indexed reward,
        uint256 amount
    );

    constructor(address _voter, address _collection) {
        voter = _voter;
        _ve = IVoter(_voter)._ve();
        collection = _collection;
    }

    // simple re-entrancy check
    uint256 internal _unlocked = 1;
    modifier lock() {
        require(_unlocked == 1);
        _unlocked = 2;
        _;
        _unlocked = 1;
    }

    /**
     * @notice Determine the prior balance for an account as of a block number
     * @dev Block number must be a finalized block or else this function will revert to prevent misinformation.
     * @param tokenId The token of the NFT to check
     * @param timestamp The timestamp to get the balance at
     * @return The balance the account had as of the given block
     */
    function getPriorBalanceIndex(
        bool toggle,
        uint256 tokenId,
        uint256 timestamp
    ) public view returns (uint256) {
        uint256 nCheckpoints = numCheckpoints[toggle][tokenId];
        if (nCheckpoints == 0) {
            return 0;
        }

        // First check most recent balance
        if (
            checkpoints[toggle][tokenId][nCheckpoints - 1].timestamp <=
            timestamp
        ) {
            return (nCheckpoints - 1);
        }

        // Next check implicit zero balance
        if (checkpoints[toggle][tokenId][0].timestamp > timestamp) {
            return 0;
        }

        uint256 lower = 0;
        uint256 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint256 center = upper - (upper - lower) / 2; // ceil, avoiding overflow
            Checkpoint memory cp = checkpoints[toggle][tokenId][center];
            if (cp.timestamp == timestamp) {
                return center;
            } else if (cp.timestamp < timestamp) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return lower;
    }

    function getPriorSupplyIndex(bool toggle, uint256 timestamp)
        public
        view
        returns (uint256)
    {
        uint256 nCheckpoints = supplyNumCheckpoints[toggle];
        if (nCheckpoints == 0) {
            return 0;
        }

        // First check most recent balance
        if (
            supplyCheckpoints[toggle][nCheckpoints - 1].timestamp <= timestamp
        ) {
            return (nCheckpoints - 1);
        }

        // Next check implicit zero balance
        if (supplyCheckpoints[toggle][0].timestamp > timestamp) {
            return 0;
        }

        uint256 lower = 0;
        uint256 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint256 center = upper - (upper - lower) / 2; // ceil, avoiding overflow
            SupplyCheckpoint memory cp = supplyCheckpoints[toggle][center];
            if (cp.timestamp == timestamp) {
                return center;
            } else if (cp.timestamp < timestamp) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return lower;
    }

    function getPriorRewardPerToken(
        bool toggle,
        address token,
        uint256 timestamp
    ) public view returns (uint256, uint256) {
        uint256 nCheckpoints = rewardPerTokenNumCheckpoints[toggle][token];
        if (nCheckpoints == 0) {
            return (0, 0);
        }

        // First check most recent balance
        if (
            rewardPerTokenCheckpoints[toggle][token][nCheckpoints - 1]
                .timestamp <= timestamp
        ) {
            return (
                rewardPerTokenCheckpoints[toggle][token][nCheckpoints - 1]
                    .rewardPerToken,
                rewardPerTokenCheckpoints[toggle][token][nCheckpoints - 1]
                    .timestamp
            );
        }

        // Next check implicit zero balance
        if (rewardPerTokenCheckpoints[toggle][token][0].timestamp > timestamp) {
            return (0, 0);
        }

        uint256 lower = 0;
        uint256 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint256 center = upper - (upper - lower) / 2; // ceil, avoiding overflow
            RewardPerTokenCheckpoint memory cp = rewardPerTokenCheckpoints[
                toggle
            ][token][center];
            if (cp.timestamp == timestamp) {
                return (cp.rewardPerToken, cp.timestamp);
            } else if (cp.timestamp < timestamp) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return (
            rewardPerTokenCheckpoints[toggle][token][lower].rewardPerToken,
            rewardPerTokenCheckpoints[toggle][token][lower].timestamp
        );
    }

    function _writeCheckpoint(
        bool toggle,
        uint256 tokenId,
        uint256 balance
    ) internal {
        uint256 _timestamp = block.timestamp;
        uint256 _nCheckPoints = numCheckpoints[toggle][tokenId];

        if (
            _nCheckPoints > 0 &&
            checkpoints[toggle][tokenId][_nCheckPoints - 1].timestamp ==
            _timestamp
        ) {
            checkpoints[toggle][tokenId][_nCheckPoints - 1].balanceOf = balance;
        } else {
            checkpoints[toggle][tokenId][_nCheckPoints] = Checkpoint(
                _timestamp,
                balance
            );
            numCheckpoints[toggle][tokenId] = _nCheckPoints + 1;
        }
    }

    function _writeRewardPerTokenCheckpoint(
        bool toggle,
        address token,
        uint256 reward,
        uint256 timestamp
    ) internal {
        uint256 _nCheckPoints = rewardPerTokenNumCheckpoints[toggle][token];

        if (
            _nCheckPoints > 0 &&
            rewardPerTokenCheckpoints[toggle][token][_nCheckPoints - 1]
                .timestamp ==
            timestamp
        ) {
            rewardPerTokenCheckpoints[toggle][token][_nCheckPoints - 1]
                .rewardPerToken = reward;
        } else {
            rewardPerTokenCheckpoints[toggle][token][
                _nCheckPoints
            ] = RewardPerTokenCheckpoint(timestamp, reward);
            rewardPerTokenNumCheckpoints[toggle][token] = _nCheckPoints + 1;
        }
    }

    function _writeSupplyCheckpoint(bool toggle) internal {
        uint256 _nCheckPoints = supplyNumCheckpoints[toggle];
        uint256 _timestamp = block.timestamp;
        if (
            _nCheckPoints > 0 &&
            supplyCheckpoints[toggle][_nCheckPoints - 1].timestamp == _timestamp
        ) {
            supplyCheckpoints[toggle][_nCheckPoints - 1].supply = totalSupply[
                toggle
            ];
        } else {
            supplyCheckpoints[toggle][_nCheckPoints] = SupplyCheckpoint(
                _timestamp,
                totalSupply[toggle]
            );
            supplyNumCheckpoints[toggle] = _nCheckPoints + 1;
        }
    }

    function rewardsListLength(bool toggle) external view returns (uint256) {
        return rewards[toggle].length;
    }

    // returns the last time the reward was modified or periodFinish if the reward has ended
    function lastTimeRewardApplicable(bool toggle, address token)
        public
        view
        returns (uint256)
    {
        return Math.min(block.timestamp, periodFinish[toggle][token]);
    }

    // allows a user to claim rewards for a given token
    function getReward(
        bool toggle,
        uint256 tokenId,
        address[] memory tokens
    ) external lock {
        require(
            IVe(_ve).isApprovedOrOwner(msg.sender, tokenId),
            "Bribe: Not veNFT holder"
        );

        if (IVe(_ve).renewVote(tokenId) != 1) {
            IVoter(voter).distribute(collection);

            for (uint256 i = 0; i < tokens.length; i++) {
                (
                    rewardPerTokenStored[toggle][tokens[i]],
                    lastUpdateTime[toggle][tokens[i]]
                ) = _updateRewardPerToken(toggle, tokens[i]);

                uint256 _reward = earned(toggle, tokens[i], tokenId);

                lastEarn[toggle][tokens[i]][tokenId] = block.timestamp;
                userRewardPerTokenStored[toggle][tokens[i]][
                    tokenId
                ] = rewardPerTokenStored[toggle][tokens[i]];

                if (_reward > 0) {
                    _safeTransfer(tokens[i], msg.sender, _reward);
                }

                emit ClaimRewards(toggle, msg.sender, tokens[i], _reward);
            }
        }
    }

    // used by BaseV1Voter to allow batched reward claims
    function getRewardForOwner(
        bool toggle,
        uint256 tokenId,
        address[] memory tokens
    ) external lock {
        require(msg.sender == voter);
        address _owner = IVe(_ve).ownerOf(tokenId);
        for (uint256 i = 0; i < tokens.length; i++) {
            (
                rewardPerTokenStored[toggle][tokens[i]],
                lastUpdateTime[toggle][tokens[i]]
            ) = _updateRewardPerToken(toggle, tokens[i]);

            uint256 _reward = earned(toggle, tokens[i], tokenId);
            lastEarn[toggle][tokens[i]][tokenId] = block.timestamp;
            userRewardPerTokenStored[toggle][tokens[i]][
                tokenId
            ] = rewardPerTokenStored[toggle][tokens[i]];
            if (_reward > 0) _safeTransfer(tokens[i], _owner, _reward);

            emit ClaimRewards(toggle, _owner, tokens[i], _reward);
        }
    }

    function rewardPerToken(bool toggle, address token)
        public
        view
        returns (uint256)
    {
        if (totalSupply[toggle] == 0) {
            return rewardPerTokenStored[toggle][token];
        }

        return
            rewardPerTokenStored[toggle][token] +
            (((lastTimeRewardApplicable(toggle, token) -
                Math.min(
                    lastUpdateTime[toggle][token],
                    periodFinish[toggle][token]
                )) *
                rewardRate[toggle][token] *
                PRECISION) / totalSupply[toggle]);
    }

    function batchRewardPerToken(
        bool toggle,
        address token,
        uint256 maxRuns
    ) external {
        (
            rewardPerTokenStored[toggle][token],
            lastUpdateTime[toggle][token]
        ) = _batchRewardPerToken(toggle, token, maxRuns);
    }

    function _batchRewardPerToken(
        bool toggle,
        address token,
        uint256 maxRuns
    ) internal returns (uint256, uint256) {
        uint256 _startTimestamp = lastUpdateTime[toggle][token];
        uint256 reward = rewardPerTokenStored[toggle][token];

        if (supplyNumCheckpoints[toggle] == 0) {
            return (reward, _startTimestamp);
        }

        if (rewardRate[toggle][token] == 0) {
            return (reward, block.timestamp);
        }

        uint256 _startIndex = getPriorSupplyIndex(toggle, _startTimestamp);
        uint256 _endIndex = Math.min(supplyNumCheckpoints[toggle] - 1, maxRuns);

        for (uint256 i = _startIndex; i < _endIndex; i++) {
            SupplyCheckpoint memory sp0 = supplyCheckpoints[toggle][i];
            if (sp0.supply > 0) {
                SupplyCheckpoint memory sp1 = supplyCheckpoints[toggle][i + 1];
                (uint256 _reward, uint256 endTime) = _calcRewardPerToken(
                    toggle,
                    token,
                    sp1.timestamp,
                    sp0.timestamp,
                    sp0.supply,
                    _startTimestamp
                );
                reward += _reward;
                _writeRewardPerTokenCheckpoint(toggle, token, reward, endTime);
                _startTimestamp = endTime;
            }
        }

        return (reward, _startTimestamp);
    }

    function _calcRewardPerToken(
        bool toggle,
        address token,
        uint256 timestamp1,
        uint256 timestamp0,
        uint256 supply,
        uint256 startTimestamp
    ) internal view returns (uint256, uint256) {
        //uint endTime = Math.max(timestamp1, startTimestamp);
        return (
            (((Math.min(
                Math.max(timestamp1, startTimestamp),
                periodFinish[toggle][token]
            ) -
                Math.min(
                    Math.max(timestamp0, startTimestamp),
                    periodFinish[toggle][token]
                )) *
                rewardRate[toggle][token] *
                PRECISION) / supply),
            Math.max(timestamp1, startTimestamp)
        );
    }

    function _updateRewardPerToken(bool toggle, address token)
        internal
        returns (uint256, uint256)
    {
        uint256 _startTimestamp = lastUpdateTime[toggle][token];
        uint256 reward = rewardPerTokenStored[toggle][token];

        if (supplyNumCheckpoints[toggle] == 0) {
            return (reward, _startTimestamp);
        }

        if (rewardRate[toggle][token] == 0) {
            return (reward, block.timestamp);
        }

        uint256 _startIndex = getPriorSupplyIndex(toggle, _startTimestamp);
        uint256 _endIndex = supplyNumCheckpoints[toggle] - 1;

        if (_endIndex - _startIndex > 1) {
            for (uint256 i = _startIndex; i < _endIndex - 1; i++) {
                SupplyCheckpoint memory sp0 = supplyCheckpoints[toggle][i];
                if (sp0.supply > 0) {
                    SupplyCheckpoint memory sp1 = supplyCheckpoints[toggle][
                        i + 1
                    ];
                    (uint256 _reward, uint256 _endTime) = _calcRewardPerToken(
                        toggle,
                        token,
                        sp1.timestamp,
                        sp0.timestamp,
                        sp0.supply,
                        _startTimestamp
                    );
                    reward += _reward;
                    _writeRewardPerTokenCheckpoint(
                        toggle,
                        token,
                        reward,
                        _endTime
                    );
                    _startTimestamp = _endTime;
                }
            }
        }

        SupplyCheckpoint memory sp = supplyCheckpoints[toggle][_endIndex];
        if (sp.supply > 0) {
            (uint256 _reward, ) = _calcRewardPerToken(
                toggle,
                token,
                lastTimeRewardApplicable(toggle, token),
                Math.max(sp.timestamp, _startTimestamp),
                sp.supply,
                _startTimestamp
            );
            reward += _reward;
            _writeRewardPerTokenCheckpoint(
                toggle,
                token,
                reward,
                block.timestamp
            );
            _startTimestamp = block.timestamp;
        }

        return (reward, _startTimestamp);
    }

    function earned(
        bool toggle,
        address token,
        uint256 tokenId
    ) public view returns (uint256) {
        uint256 _startTimestamp = Math.max(
            lastEarn[toggle][token][tokenId],
            rewardPerTokenCheckpoints[toggle][token][0].timestamp
        );
        if (numCheckpoints[toggle][tokenId] == 0) {
            return 0;
        }

        uint256 _startIndex = getPriorBalanceIndex(
            toggle,
            tokenId,
            _startTimestamp
        );
        uint256 _endIndex = numCheckpoints[toggle][tokenId] - 1;

        uint256 reward = 0;

        if (_endIndex - _startIndex > 1) {
            for (uint256 i = _startIndex; i < _endIndex - 1; i++) {
                Checkpoint memory cp0 = checkpoints[toggle][tokenId][i];
                Checkpoint memory cp1 = checkpoints[toggle][tokenId][i + 1];
                (uint256 _rewardPerTokenStored0, ) = getPriorRewardPerToken(
                    toggle,
                    token,
                    cp0.timestamp
                );
                (uint256 _rewardPerTokenStored1, ) = getPriorRewardPerToken(
                    toggle,
                    token,
                    cp1.timestamp
                );
                reward +=
                    (cp0.balanceOf *
                        (_rewardPerTokenStored1 - _rewardPerTokenStored0)) /
                    PRECISION;
            }
        }

        Checkpoint memory cp = checkpoints[toggle][tokenId][_endIndex];

        (uint256 _rewardPerTokenStored, ) = getPriorRewardPerToken(
            toggle,
            token,
            cp.timestamp
        );

        reward +=
            (cp.balanceOf *
                (rewardPerToken(toggle, token) -
                    Math.max(
                        _rewardPerTokenStored,
                        userRewardPerTokenStored[toggle][token][tokenId]
                    ))) /
            PRECISION;
        return reward;
    }

    // This is an external function, but internal notation is used since it can only be called "internally" from BaseV1Gauges
    function _deposit(
        bool toggle,
        uint256 amount,
        uint256 tokenId
    ) external {
        require(msg.sender == voter);

        totalSupply[toggle] += amount;
        balanceOf[toggle][tokenId] += amount;

        _writeCheckpoint(toggle, tokenId, balanceOf[toggle][tokenId]);
        _writeSupplyCheckpoint(toggle);

        emit Deposit(toggle, msg.sender, tokenId, amount);
    }

    function _withdraw(
        bool toggle,
        uint256 amount,
        uint256 tokenId
    ) external {
        require(msg.sender == voter);

        totalSupply[toggle] -= amount;
        balanceOf[toggle][tokenId] -= amount;

        _writeCheckpoint(toggle, tokenId, balanceOf[toggle][tokenId]);
        _writeSupplyCheckpoint(toggle);

        emit Withdraw(toggle, msg.sender, tokenId, amount);
    }

    function left(bool toggle, address token) external view returns (uint256) {
        if (block.timestamp >= periodFinish[toggle][token]) return 0;
        uint256 _remaining = periodFinish[toggle][token] - block.timestamp;
        return _remaining * rewardRate[toggle][token];
    }

    // used to notify a gauge/bribe of a given reward, this can create griefing attacks by extending rewards
    function notifyRewardAmount(
        bool toggle,
        address token,
        uint256 amount
    ) external {
        require(amount > 0);
        if (rewardRate[toggle][token] == 0)
            _writeRewardPerTokenCheckpoint(toggle, token, 0, block.timestamp);
        (
            rewardPerTokenStored[toggle][token],
            lastUpdateTime[toggle][token]
        ) = _updateRewardPerToken(toggle, token);

        if (block.timestamp >= periodFinish[toggle][token]) {
            _safeTransferFrom(token, msg.sender, address(this), amount);
            rewardRate[toggle][token] = amount / DURATION;
        } else {
            uint256 _remaining = periodFinish[toggle][token] - block.timestamp;
            uint256 _left = _remaining * rewardRate[toggle][token];
            require(amount > _left);
            _safeTransferFrom(token, msg.sender, address(this), amount);
            rewardRate[toggle][token] = (amount + _left) / DURATION;
        }

        require(rewardRate[toggle][token] > 0);
        uint256 balance = IERC20(token).balanceOf(address(this));

        require(
            rewardRate[toggle][token] <= balance / DURATION,
            "Provided reward too high"
        );
        periodFinish[toggle][token] = block.timestamp + DURATION;
        if (!isReward[toggle][token]) {
            isReward[toggle][token] = true;
            rewards[toggle].push(token);
        }

        emit NotifyReward(toggle, msg.sender, token, amount);
    }

    function _safeTransfer(
        address token,
        address to,
        uint256 value
    ) internal {
        require(token.code.length > 0);
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(IERC20.transfer.selector, to, value)
        );
        require(success && (data.length == 0 || abi.decode(data, (bool))));
    }

    function _safeTransferFrom(
        address token,
        address from,
        address to,
        uint256 value
    ) internal {
        require(token.code.length > 0);
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(
                IERC20.transferFrom.selector,
                from,
                to,
                value
            )
        );
        require(success && (data.length == 0 || abi.decode(data, (bool))));
    }
}

