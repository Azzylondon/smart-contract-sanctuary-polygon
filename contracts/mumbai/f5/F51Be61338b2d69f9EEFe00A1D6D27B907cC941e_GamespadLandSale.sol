pragma solidity ^0.8.4;
pragma abicoder v2;

import "../utils/AccessProtected.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import { IERC721Receiver } from "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";

// LandSale SMART CONTRACT
contract GamespadLandSale is AccessProtected, ReentrancyGuard, IERC721Receiver {
    using Address for address;
    using SafeMath for uint256;

    // STRUCTS

    struct Sale {
        uint256 price;
        address owner;
        bool isActive;
        uint256 tokentype;
    }

    // VARIABLES

    IERC20 public TOKEN;
    IERC20 public TOKENUSD;
    IERC721 public NFT;
    bool public isEnabled;

    // MAPPINGS

    mapping(uint256 => Sale) private _nftSales; // nftId => Sale mapping

    // EVENTS

    event SaleAdded(uint256 _nftId, uint256 _price, address _owner, uint256 _timestamp, uint256 _tokentype);
    event SaleCancelled(uint256 _nftId, uint256 _price, address _owner, address _cancelledBy, uint256 _timestamp);
    event Sold(uint256 _nftId, address _seller, address _buyer, uint256 _price, uint256 _timestamp, uint256 _tokentype);
    event PriceUpdated(uint256 _nftId, uint256 _price);
    event Gift(uint256 _nftId, address _user);
    event WithdrawFunds(uint256 _balance, uint256 _tokentype);

    // CONSTRUCTOR

    constructor(
        address _nftAddress,
        address _tokenAddress,
        address _busdtokenaddress
    ) {
        require(_nftAddress.isContract(), "_nftAddress must be a contract");
        require(_tokenAddress.isContract(), "_tokenAddress must be a contract");
        NFT = IERC721(_nftAddress);
        TOKEN = IERC20(_tokenAddress);
        TOKENUSD = IERC20(_busdtokenaddress);
    }

    // MODIFIERS

    modifier onlyDuringSale() {
        require(isEnabled, "Sale is not enabled");
        _;
    }

    // PUBLIC FUNCTIONS

    /**
     * Put up Property for Sale
     *
     * @param nftId - nftId of the property
     * @param price - price to sell property for
     * @param tokenChoice -0 or 1 tokenChoice is included
     */
    function sellProperty(
        uint256 nftId,
        uint256 price,
        uint256 tokenChoice
    ) public {
        require(!_nftSales[nftId].isActive, "NFT Sale is active");
        require(tokenChoice == 0 || tokenChoice == 1, "select correct token type for sale");
        if (NFT.ownerOf(nftId) == address(this)) {
            require(isAdmin(_msgSender()) || _msgSender() == owner(), "Only owner or admin can sell property");
            _nftSales[nftId] = Sale(price, address(this), true, tokenChoice);
            emit SaleAdded(nftId, price, address(this), block.timestamp, tokenChoice);
        } else {
            NFT.transferFrom(_msgSender(), address(this), nftId);
            _nftSales[nftId] = Sale(price, _msgSender(), true, tokenChoice);
            emit SaleAdded(nftId, price, _msgSender(), block.timestamp, tokenChoice);
        }
    }

    /**
     * Put up Properties for Sale
     *
     * @param nftIds - nftId of the properties
     * @param prices - price to sell properties for
     * @param tokenchoices -choice of token 0 for saga ,1 for busd
     */
    function batchSellProperty(
        uint256[] memory nftIds,
        uint256[] memory prices,
        uint256[] memory tokenchoices
    ) public {
        require(nftIds.length == prices.length && nftIds.length == tokenchoices.length, "Length mismatch");
        for (uint256 i = 0; i < nftIds.length; i++) {
            require(!_nftSales[nftIds[i]].isActive, "NFT Sale is active");
            sellProperty(nftIds[i], prices[i], tokenchoices[i]);
        }
    }

    /**
     * update price of item on sale
     *
     * @param nftId - nftId of the property
     * @param newPrice - price to sell properties for
     */
    function updatePrice(uint256 nftId, uint256 newPrice) public {
        require(_nftSales[nftId].isActive, "NFT is not for sale");
        if (_nftSales[nftId].owner == address(this)) {
            require(isAdmin(_msgSender()) || _msgSender() == owner(), "Only owner/admin can update price");
            _nftSales[nftId].price = newPrice;
            emit PriceUpdated(nftId, newPrice);
        } else {
            require(_nftSales[nftId].owner == _msgSender(), "Only owner of NFT can Update");
            _nftSales[nftId].price = newPrice;
            emit PriceUpdated(nftId, newPrice);
        }
    }

    /**
     * withdraw deposited tokens
     *
     * @param tokenChoice - 0 or 1 token type for withdrawal
     */
    function withdrawTokens(uint256 tokenChoice) public onlyOwner {
        require(tokenChoice == 0 || tokenChoice == 1, "Select from saga or busd");
        if (tokenChoice == 0) {
            require(TOKEN.balanceOf(address(this)) > 0, "Nothing to withdraw");
            uint256 balance = TOKEN.balanceOf(address(this));
            TOKEN.transferFrom(address(this), msg.sender, balance);
            emit WithdrawFunds(balance, tokenChoice);
        }
        if (tokenChoice == 1) {
            require(TOKENUSD.balanceOf(address(this)) > 0, "Nothing to withdraw");
        }
        uint256 balance1 = TOKENUSD.balanceOf(address(this));
        TOKENUSD.transferFrom(address(this), msg.sender, balance1);
        emit WithdrawFunds(balance1, tokenChoice);
    }

    /**
     * withdraw deposited Ether
     */
    function withdrawEther() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }

    /**
     * @notice - Enable/Disable Sales
     * @dev - callable only by owner
     *
     * @param _isEnabled - enable? sales
     */
    function setEnabled(bool _isEnabled) public onlyOwner {
        isEnabled = _isEnabled;
    }

    /**
     * Airdrop nft to user
     *
     * @param nftId - nftId of the property
     * @param user - user to airdrop to
     */
    function airDrop(uint256 nftId, address user) public onlyOwner nonReentrant {
        require(!_nftSales[nftId].isActive, "NFT Sale is active");
        NFT.transferFrom(address(this), user, nftId);
        emit Gift(nftId, user);
    }

    // INTERNAL FUNCTIONS

    /**
     * Purchase Property
     *
     * @param nftId - nftId of the property
     *
     */
    function _purchaseProperty(uint256 nftId) internal {
        require(_nftSales[nftId].isActive, "NFT is not for sale");

        address seller = _nftSales[nftId].owner;
        address buyer = _msgSender();
        uint256 price = _nftSales[nftId].price;
        uint256 tokenChoice = _nftSales[nftId].tokentype;
        _nftSales[nftId].owner = buyer;
        _nftSales[nftId].isActive = false;
        if (tokenChoice == 0) {
            TOKEN.transferFrom(buyer, seller, price);
        }
        if (tokenChoice == 1) {
            TOKENUSD.transferFrom(buyer, seller, price);
        }
        NFT.transferFrom(address(this), buyer, nftId);

        emit Sold(nftId, seller, buyer, price, block.timestamp, tokenChoice);
    }

    // EXTERNAL FUNCTIONS

    /**
     * Get Property Sale
     *
     * @param nftId - nftId of the property
     */
    function getSale(uint256 nftId) external view returns (Sale memory) {
        require(_nftSales[nftId].isActive, "NFT is not up for sale");
        return _nftSales[nftId];
    }

    /**
     * Cancel Property Sale
     *
     * @param nftId - nftId of the property
     */
    function cancelSale(uint256 nftId) external {
        require(
            _nftSales[nftId].owner == _msgSender() || isAdmin(_msgSender()) || _msgSender() == owner(),
            "Only owner/admin can cancel Sale"
        );
        require(_nftSales[nftId].isActive, "NFT is not up for sale");
        _nftSales[nftId].isActive = false;
        if (_nftSales[nftId].owner != address(this)) {
            NFT.transferFrom(address(this), _nftSales[nftId].owner, nftId);
        }
        emit SaleCancelled(nftId, _nftSales[nftId].price, _nftSales[nftId].owner, _msgSender(), block.timestamp);
    }

    /**
     * Purchase Property (modifier onlyDuringSale)
     *
     * @param nftId - nftId of the property
     */
    function purchaseProperty(uint256 nftId) external onlyDuringSale nonReentrant {
        _purchaseProperty(nftId);
    }

    /**
     * Purchase Batch of properties
     *
     * @param nftIds - nftIds of the property
     */
    function batchPurchaseProperty(uint256[] memory nftIds) external onlyDuringSale nonReentrant {
        for (uint256 i = 0; i < nftIds.length; i++) {
            uint256 nftId = nftIds[i];
            _purchaseProperty(nftId);
        }
    }

    /**
     * ERC721Receiver implementer
     */
    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Context.sol";

abstract contract AccessProtected is Context, Ownable {
    mapping(address => bool) private _admins; // user address => admin? mapping

    event AdminAccessSet(address _admin, bool _enabled);

    /**
     * @notice Set Admin Access
     *
     * @param admin - Address of Minter
     * @param enabled - Enable/Disable Admin Access
     */
    function setAdmin(address admin, bool enabled) external onlyOwner {
        _admins[admin] = enabled;
        emit AdminAccessSet(admin, enabled);
    }

    /**
     * @notice Check Admin Access
     *
     * @param admin - Address of Admin
     * @return whether minter has access
     */
    function isAdmin(address admin) public view returns (bool) {
        return _admins[admin];
    }

    /**
     * Throws if called by any account other than the Admin.
     */
    modifier onlyAdmin() {
        require(_admins[_msgSender()] || _msgSender() == owner(), "Caller does not have Admin Access");
        _;
    }
}

