pragma solidity ^0.8.4;

import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {UUPSUpgradeable} from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {PausableUpgradeable} from "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";

import {IERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import {SafeERC20Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

import "hardhat/console.sol";

contract BetupV2 is Initializable, UUPSUpgradeable, OwnableUpgradeable, PausableUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    uint256 public pairCount;

    // admin address => is active
    mapping(address => bool) public isAdmin;

    // split pair property cause which is more save gas
    // pair name => chip address => pair id
    mapping(bytes32 => mapping(address => uint256)) public pairIds;

    // pair id => chip address
    mapping(uint256 => PairConfig) public pairConfig;

    // pair id => active status // 0- non-existence 1 -activated 2- unactivated
    mapping(uint256 => uint8) public pairsStatus;

    // pair id => pair counter
    mapping(uint256 => uint256) public pairsCounter;

    // pair id => counter => pairsRound
    mapping(uint256 => mapping(uint256 => PairsCRound)) public pairsCR;

    // player address => pair id => counter => PlayerRound
    mapping(address => mapping(uint256 => mapping(uint256 => PlayerRound))) public players;

    struct PairConfig {
        bytes32 pairName;
        address chip;
    }

    struct PlayerRound {
        uint256 riseAmount;
        uint256 fallAmount;
    }

    struct PairsCRound {
        uint256 oPrice;   // open price
        uint256 cPrice;   // close price
        uint256 rTotal;   // rise total amount
        uint256 fTotal;   // fall total amount
        uint256 packed1;  // 0(63) sInterval:uint32 (95) eInterval: uint32(127) oTime:uint64 (191) cTime:uint64(255) isDistribute:uint1(256)
    }

    event SetPairsEvent(
        bytes32[] pairsName,
        address[] chips
    );

    event AddPairsEvent(
        PairConfig[] pairs
    );

    event DelPairsEvent(
        uint256[] ids
    );

    event OpenEvent(
        uint256[] oPrices,
        uint256[] counters,
        uint256[] ids,
        uint64[] cTimes
    );

    event CloseEvent(
        uint256[]  ids,
        uint256[]  cPrices,
        uint256[]  counters
    );

    event StackEvent(
        address player,
        uint8 direction,
        uint256 amount
    );

    event WithdrawEvent(
        uint256[] _ids,
        uint256[] _counter,
        uint256[] refunds
    );

    event DistributeEvent(
        uint256[] _ids,
        uint256[] _counter,
        uint256[] amounts
    );


    function addPair(PairConfig[] memory _pairs) external onlyAdmin {
        for(uint256 i = 0; i < _pairs.length; i++) {
            PairConfig memory pf = _pairs[i];
            require(pf.pairName != bytes32(0), "Invalid name");
            require(pf.chip != address(0), "Invalid chip");
            require(pairIds[pf.pairName][pf.chip] == 0, "Duplicated");

            uint256 pairId = pairCount + 1;
            pairIds[pf.pairName][pf.chip] = pairId;
            pairConfig[pairId].chip = pf.chip;
            pairConfig[pairId].pairName = pf.pairName;
            pairCount++;
            pairsStatus[pairId] = 1;
        }
        emit AddPairsEvent(_pairs);
    }

    function delPair(uint256[] memory _ids) external onlyAdmin {
        for(uint256 i = 0; i < _ids.length; i++) {
            uint256 pairId = _ids[i];

            require(pairsStatus[pairId] == 1, "Invalid pair");
            pairsStatus[pairId] = 2;
        }
        emit DelPairsEvent(_ids);
    }


    function open(
        uint256[] memory _ids,
        uint256[] memory _oPrices,
        uint64[] memory _oTimes,
        uint32[] memory _periodSeconds,
        uint32[] memory _sIntervalSeconds,
        uint32[] memory _eIntervalsSeconds
    ) external onlyAdmin {
        require(
            ( (_oPrices.length ^ _ids.length)
            | (_oTimes.length ^ _oTimes.length)
            | (_ids.length ^ _oTimes.length)
            | (_sIntervalSeconds.length ^ _eIntervalsSeconds.length)
            | (_sIntervalSeconds.length ^ _periodSeconds.length) )== 0,
            "Invalid length"
          );

        uint256[] memory counters = new uint256[](_ids.length);

        for(uint256 i = 0; i < _ids.length; i++) {
            uint256 openPrice = _oPrices[i];
            uint64 openTime = _oTimes[i];
            uint64 period = _periodSeconds[i];
            uint32 sInterval = _sIntervalSeconds[i];
            uint32 eInterval = _eIntervalsSeconds[i];
            uint256 pairId = _ids[i];

            require(pairsStatus[pairId] == 1, "Invalid pair");

            require(openPrice > 0, "Invalid price");
            require(period > 0, "Invalid period");
            uint64 closeTime = openTime + period;
            require(closeTime > block.timestamp, "Invalid time");
            require(openTime + sInterval < closeTime - eInterval, "Invalid ctime");

            uint256 counter = pairsCounter[pairId];
            PairsCRound storage pcr = pairsCR[pairId][counter];
            require(pcr.oPrice == 0, "Invalid counter");

            pcr.packed1 = wrap1(sInterval, eInterval, openTime, closeTime, 0);
            pcr.oPrice = openPrice;
            counters[i] = counter;
        }

        emit OpenEvent(_oPrices, counters, _ids, _oTimes);

    }

    function close(
        uint256[] memory _ids,
        uint256[] memory _cPrices,
        uint64[] memory _nextOTimes,
        uint32[] memory _nextPeriodSeconds,
        uint32[] memory _nextSIntervalsSeconds,
        uint32[] memory _nextEIntervalsSeconds
    ) external onlyAdmin {
        require(
            (  (_cPrices.length ^ _nextOTimes.length)
            | (_nextPeriodSeconds.length ^ _nextSIntervalsSeconds.length)
            | (_nextOTimes.length ^ _nextPeriodSeconds.length)
            | (_nextEIntervalsSeconds.length ^ _nextSIntervalsSeconds.length)
            )== 0,
            "Invalid length"
        );
        uint256[] memory counters = new uint256[](_ids.length);

        for(uint256 i = 0; i < _ids.length; i++) {
            uint256 closePrice = _cPrices[i];
            uint64 nOTime = _nextOTimes[i];
            uint64 nPeriod = _nextPeriodSeconds[i];
            uint32 nSInterval = _nextSIntervalsSeconds[i];
            uint32 nEInterval = _nextEIntervalsSeconds[i];
            uint256 pairId = _ids[i];

            require(pairId > 0, "Invalid pair");
            require(pairsStatus[pairId] == 1, "Pair locked");
            require(closePrice > 0, "Invalid price");

            uint256 counter = pairsCounter[pairId];
            PairsCRound storage pcr = pairsCR[pairId][counter];
            require(pcr.cPrice == 0 && pcr.oPrice > 0, "Invalid counter");
            uint256 cTime = unbox(pcr.packed1, 191, 64);
            require(cTime < block.timestamp, "Invalid time");

            uint256 nCTime = nOTime + nPeriod;
            require(cTime + nSInterval < nCTime - nEInterval, "Invalid next time" );

            pcr.cPrice = closePrice;
            counters[i] = counter;

            uint256 nextCounter = counter + 1;
            pairsCounter[pairId] = nextCounter;
            PairsCRound storage nextPcr = pairsCR[pairId][nextCounter];
            nextPcr.oPrice = closePrice;
            nextPcr.packed1 = wrap1(nSInterval, nEInterval, nOTime, nCTime, 0);
        }

        emit CloseEvent(_ids, _cPrices, counters);
    }

    // special name for gas optimize
    // _direction 1-rise 2-fall
    function trystack(
        uint256 id,
        uint256 _amount,
        uint8 _direction
    ) external {
        require(_amount > 0, 'Invalid amount');
        require(_direction == 1 || _direction == 2, "Invalid _direction");

        uint256 pairId = id;
        uint256 counter = pairsCounter[pairId];
        PairsCRound storage pcr = pairsCR[pairId][counter];
        require(pcr.oPrice > 0 && pcr.cPrice == 0, "Invalid pair");
        uint256 oTime = unbox(pcr.packed1, 127, 64);
        uint256 cTime = unbox(pcr.packed1, 191, 64);
        uint256 sInterval = unbox(pcr.packed1, 63, 32);
        uint256 eInterval = unbox(pcr.packed1, 95, 32);

        require(
            block.timestamp > oTime + sInterval &&
            block.timestamp < cTime - eInterval ,
            "Timeout"
        );

        PlayerRound storage player =  players[msg.sender][pairId][counter];

        if(_direction == 1) {
            pcr.rTotal += _amount;
            player.riseAmount += _amount;
        }else if(_direction == 2) {
            pcr.fTotal += _amount;
            player.fallAmount += _amount;
        }


        address chip = pairConfig[pairId].chip;
        IERC20Upgradeable(chip).safeTransferFrom(msg.sender, address(this), _amount);

        emit StackEvent(msg.sender, _direction, _amount);

    }

    function withdraw(
        uint256[] memory _ids,
        uint256[] memory _counter
    ) external whenNotPaused {
        require(
            ((_ids.length ^ _counter.length)
            ) == 0,
            "Invalid length"
        );

        uint256[] memory refunds = new uint256[](_ids.length);

        uint256 accAmount = 0;
        for(uint256 i = 0; i < _ids.length; i++) {
            uint256 counter = _counter[i];
            uint256 pairId = _ids[i];

            address chip = pairConfig[pairId].chip;

            PairsCRound storage pcr = pairsCR[pairId][counter];
            PlayerRound storage player = players[msg.sender][pairId][counter];
            require(pcr.oPrice > 0 && pcr.cPrice > 0, "Not yet end");

            uint256 refund = 0;
            if(pcr.oPrice == pcr.cPrice) {
                refund = player.fallAmount + player.riseAmount;
            }else if(pcr.oPrice < pcr.cPrice) {
                uint256 riseAmount = player.riseAmount;
                refund = riseAmount + riseAmount *  pcr.fTotal * 9  / (pcr.rTotal * 10);
            }else if(pcr.oPrice > pcr.cPrice) {
                uint256 fallAmount = player.fallAmount;
                refund = fallAmount + fallAmount * pcr.rTotal * 9 / (pcr.fTotal * 10);
            }

            require(refund > 0, "No refund");

            player.fallAmount = 0;
            player.riseAmount = 0;

            refunds[i] = refund;

            if(i > 0) {
                uint256 preId = _ids[i-1];
                address preChip = pairConfig[preId].chip;
                if(preChip == chip) {
                    accAmount += refund;
                }else {
                    IERC20Upgradeable(chip).safeTransfer(msg.sender, accAmount);
                    accAmount = refund;
                }
            }else {
                if(i == 0 && _ids.length == 1) {
                    IERC20Upgradeable(chip).safeTransfer(msg.sender, refund);
                }else {
                    accAmount += refund;
                }
            }

        }

        emit WithdrawEvent(_ids, _counter, refunds);
    }

    function distribute(
        uint256[] memory _ids,
        uint256[] memory _counter
    ) external onlyAdmin {

        require(
            ((_ids.length ^ _counter.length)
            ) == 0,
            "Invalid length"
        );

        uint256[] memory amounts = new uint256[](_ids.length);

        uint256 accAmount = 0;
        for(uint i = 0; i < _ids.length; i++) {
            uint256 counter = _counter[i];
            uint256 pairId = _ids[i];

            address chip = pairConfig[pairId].chip;
            PairsCRound storage pcr = pairsCR[pairId][counter];
            uint256 base =  pcr.packed1;
            uint256 isDistribute = unbox(base, 255, 1);
            require(isDistribute == 0, "Disctributed");

            uint256 amount = 0;
            if(pcr.oPrice < pcr.cPrice) {
                amount = pcr.fTotal / 10;
            }else if(pcr.oPrice > pcr.cPrice) {
                amount = pcr.rTotal / 10;
            }

            amounts[i] = amount;
            pcr.packed1 = rewriteBox(base, 255, 1, 1);

            if(i > 0) {
                uint256 preId = _ids[i-1];
                address preChip = pairConfig[preId].chip;
                if(preChip == chip) {
                    accAmount += amount;
                }else {
                    IERC20Upgradeable(chip).safeTransfer(msg.sender, accAmount);
                    accAmount = amount;
                }
            }else {
                if(i == 0 && _ids.length == 1) {
                    IERC20Upgradeable(chip).safeTransfer(msg.sender, amount);
                }else {
                    accAmount += amount;
                }
            }
        }

        emit DistributeEvent(_ids, _counter, amounts);
    }

    modifier onlyAdmin() {
        require(isAdmin[msg.sender], "Not Admin");
        _;
    }

    function addAdmin(address _admin) external onlyOwner {
        require(!isAdmin[_admin], "Repeat admin");
        isAdmin[_admin] = true;
    }

    function delAdmin(address _admin) external onlyOwner {
        require(isAdmin[_admin], "Not admin");
        isAdmin[_admin] = false;
    }

    function validRange (uint16 _size, uint256 _data) internal pure returns(bool ifValid) {
        assembly {
            // 2^_size > _data or size ==256
            ifValid := or(eq(_size, 256), gt(shl(_size, 1), _data))
        }
    }

    function unbox (uint256 _base, uint16 _position, uint16 _size) internal pure returns (uint256 unboxed) {
        require(validRange(256, _base), "Value out of range UNBOX");
        assembly {
            // (((1 << _size) - 1) & _base >> _position)
            unboxed := and(sub(shl(_size, 1), 1), shr(_position, _base))

        }
    }

    function box (uint16 _position, uint16 _size, uint256 _data) internal pure returns (uint256 boxed) {
        require(validRange(_size, _data), "Value out of range BOX");
        assembly {
            // _data << _position
            boxed := shl(_position, _data)
        }
    }

    function rewriteBox (uint256 _box, uint16 position, uint16 size, uint256 data)
    internal pure returns (uint256 boxed) {
        assembly {
            // mask = ~((1 << size - 1) << position)
            // _box = (mask & _box) | ()data << position)
            boxed := or( and(_box, not(shl(position, sub(shl(size, 1), 1)))), shl(position, data))
        }
    }

    /**
            _sInterval:   63 + 32
            _eInterval:   95 + 32
                _oTime:   127 + 64
                _cTime:   191 + 64
         _isDistribute:   255 + 1
    **/
    function wrap1(uint256 _sInterval, uint256 _eInterval, uint256 _oTime, uint256 _cTime, uint256 _isDistribute) internal pure returns(uint256){
        uint256 _packed1 = 0;
        _packed1 |= box(63, 32, _sInterval);
        _packed1 |= box(95, 32, _eInterval);
        _packed1 |= box(127, 64, _oTime);
        _packed1 |= box(191, 64, _cTime);
        _packed1 |= box(255, 1, _isDistribute);
        return _packed1;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function initialize() external initializer {
        __UUPSUpgradeable_init();
        __Ownable_init();
        __Pausable_init();
    }
}

