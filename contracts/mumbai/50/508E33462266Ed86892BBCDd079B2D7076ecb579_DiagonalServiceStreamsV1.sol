enum ServiceState {
    STOPPED,
    ACTIVE
}

/// @dev If the package is stopped, users can no longer start subscriptions
enum PackageState {
    STOPPED,
    ACTIVE
}

/// @notice Package state information
struct Package {
    uint256 id;
    PackageState state;
    mapping(ISuperToken => int96) paymentTerms;
}

/// @notice Package return type
struct PackageReturnType {
    uint256 id;
    PackageState state;
}

/// @dev  Used for package input and output params
struct PackageData {
    ISuperToken[] tokens;
    PackageState state;
    int96[] flowRates;
}

/// @notice  The fee state per token
struct FeeState {
    int96 percent;
    address recipient;
}

struct SubscriberPackageState {
    int96 feeRate;
    bool isSubscribed;
}

/// @notice User flow state and subscription metadata per super token
struct SubscriberState {
    int96 totalInputFlowRate; // the total input flow rate
    int96 totalInputFeeRate; // the total input fee rate
    uint256 numSubscriptions; // For a service, the number of packages a user is subscribed to in a single token.
    mapping(uint256 => SubscriberPackageState) packageStates;
    uint256[] packageIds;
    bool terminated;
}

