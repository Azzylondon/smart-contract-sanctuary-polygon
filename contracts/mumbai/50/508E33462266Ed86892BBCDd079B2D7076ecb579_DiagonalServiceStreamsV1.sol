    address owner;
    ServiceState state;
    uint256 serviceId;
    uint256 packageIds;
    mapping(ISuperToken => int96) ownerFlowRates;
    mapping(ISuperToken => int96) treasuryFeeRates;
    mapping(uint256 => Package) packages;
    mapping(address => mapping(ISuperToken => SubscriberState)) subscriberStates;
    mapping(ISuperToken => bool) primaryTokens;
    ISuperToken[] primaryAcceptedTokenList;
}

