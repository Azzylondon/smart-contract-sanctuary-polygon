/**
 * @title DiagonalServiceStreamsV1
 * @author Diagonal Finance
 * @notice This library contains stream functionality for a DigaonalServiceV1 instance
 * @dev Majority of the followoing NatSpec comes from IDiagonalServiceV1
 */
library DiagonalServiceStreamsV1 {
    /*******************************
     * Constants *
     *******************************/

    /*
        E_SUPERAPP_NOT_JAILED - The app is not jailed
        E_STREAMS_NOT_CLOSED - Not all streams are closed
        E_EMERGENCY_STREAM_CLOSE_FAIL - Emergency close stream unsuccessful
        E_SUPERTOKEN_INPUT - No superTokens provided
        E_SERVICE_NOT_ACTIVE - Service not active
        E_PACKAGE_NOT_ACTIVE - Package not active
        E_UNSUPPORTED_TOKEN - SuperToken is not accepted by the package
        E_SUBSCRIPTION_EXISTS- User already subscribed
        E_SUBSCRIPTION_ALREADY_CLOSED Subscription already closed
        E_MAX_SUBSCRIPTIONS- Max subscriptions reached
        E_INVALID_PAYMENT_TERMS - Payment terms mismatch
        E_SUB_STATE_UPDATE_FAILED - Subscription state update failed
        E_FEE_UPDATE_FAILED - Fee stream failed
        E_FEE_TERMINATION_FAILED - Fee stream failed
        E_UPSERT_FAILED - Subscription upsert failed
     */

    /// @notice DiagonalRegistry reference
    /// @dev Please check chain DiagonalRegisty address before deploying
    IDiagonalRegistryV1 public constant DIAGONAL_REGISTRY =
        IDiagonalRegistryV1(0x68CA69AFCBeE6a41320e4A799b6b16bE90b84e8A);

    /// @notice Superfluid Host reference
    /// @dev Please check chain superfluid host address before deploying
    ISuperfluid public constant SUPERFLUID_HOST = ISuperfluid(0xEB796bdb90fFA0f28255275e16936D25d3418603);

    /// @notice Superfluid CFA reference
    /// @dev Please check chain superfluid cfa address before deploying
    IConstantFlowAgreementV1 public constant SUPERFLUID_CFA =
        IConstantFlowAgreementV1(0x49e565Ed1bdc17F3d220f72DF0857C26FA83F873);

    /**
     * @notice The maximum number of packages a user can be subscribed to in a given superToken
     * @dev A service can support multiple packages, all which can support multiple tokens
     * We apply a restriction in order to efficiently handle incoming stream closure in a token:
     *
     * i.e. User decides to stop streaming in a token or gets liquidated
     *        USDCx
     * (User ---X--> Service contract).
     *
     * In this case we will need to update system accounting:
     *        (i.e.`subscriberStates[subscriber][superToken]`)
     *
     * Interestingly we choose to update system state in `_resetSubscriberState`,
     * which is called on re-subscription, making bulk unsubscribe cheap!
     * @dev This constant can be updated by the protocol in the future
     */
    uint256 public constant MAX_SUBSCRIPTIONS_PER_PACKAGE_TOKEN = 3;

    /*******************************
     * Events *
     *******************************/

    /**
     * @notice Emitted when subscription is started
     * @param subscriber The address of the subscriber
     * @param packageId The id of the created package
     * @param superToken The superToken used to subscribe
     * @param flowRate The address of the initial service owner
     * @param feeRate The fee rate paid to Diagonal treasury
     */
    event SubscriptionStarted(
        address indexed subscriber,
        uint256 indexed packageId,
        ISuperToken indexed superToken,
        int96 flowRate,
        int96 feeRate
    );

    /**
     * @notice Emitted when the subscription is stopped
     * @param subscriber The address of the subscriber
     * @param packageId The id of the created package
     * @param superToken The superToken used to subscribe
     */
    event SubscriptionStopped(address indexed subscriber, uint256 indexed packageId, ISuperToken indexed superToken);

    /**
     * @notice Emitted when the subscription is terminated
     * @param subscriber The address of the subscriber
     * @param superToken The superToken used to subscribe
     */
    event SubscriptionTerminated(address indexed subscriber, ISuperToken indexed superToken);

    /**
     * @notice Emitted when service contract emergency drained
     * @param superToken The superToken drained
     */
    event EmergencyDrain(ISuperToken indexed superToken);

    /**
     * @notice Emitted when a user subscription stream is emergency closed
     * @param user The user address
     * @param superToken The superToken drained
     */
    event EmergencyCloseSubscription(address indexed user, ISuperToken indexed superToken);

    /**
     * @notice Emitted when a stream is recovered.
     * @param prevOwner The previous owner
     * @param owner The new owner
     * @param superToken The superToken
     */
    event StreamRecovered(address indexed prevOwner, address indexed owner, ISuperToken indexed superToken);

    /**
     * @notice Emitted when the service ownership is updated
     */
    event ServiceOwnershipUpdated(address indexed oldOwner, address indexed newOwner);

    /*******************************************
     * Emergency functions *
     ******************************************/

    /// @notice IDiagonalServiceV1.emergencyCloseSubscription implementation
    function emergencyCloseSubscription(
        DiagonalServiceStorage storage, /*self*/
        address user,
        ISuperToken superToken
    ) external {
        bool isJailed = SUPERFLUID_HOST.isAppJailed(ISuperApp(address(this)));
        require(isJailed, "SERV:E_SUPERAPP_NOT_JAILED");

        bytes memory encodedData = abi.encodeWithSelector(
            SUPERFLUID_CFA.deleteFlow.selector,
            superToken,
            user,
            address(this),
            new bytes(0)
        );

        bool result = _tryCallAgreement(encodedData);
        require(result, "SERV:E_EMERGENCY_STREAM_CLOSE_FAIL");
        emit EmergencyCloseSubscription(user, superToken);
    }

    /// @notice IDiagonalServiceV1.emergencyDrain implementation
    function emergencyDrain(DiagonalServiceStorage storage self, ISuperToken superToken) external {
        require(
            SUPERFLUID_CFA.getNetFlow(superToken, address(this)) == 0 && self.ownerFlowRates[superToken] == 0,
            "SERV:E_STREAMS_NOT_CLOSED"
        );
        superToken.transfer(self.owner, superToken.balanceOf(address(this)));
        emit EmergencyDrain(superToken);
    }

    /*******************************************
     * Service transfer features *
     ******************************************/

    /// @notice IDiagonalServiceV1.transferServiceOwnership implementation
    function transferServiceOwnership(DiagonalServiceStorage storage self, address to) external {
        address _owner = self.owner;
        int96 outputFlowRate;
        ISuperToken primToken;

        for (uint256 i = 0; i < self.primaryAcceptedTokenList.length; i++) {
            primToken = self.primaryAcceptedTokenList[i];
            // Check if we have outgoing flow for that subscriber
            outputFlowRate = self.ownerFlowRates[primToken];

            if (outputFlowRate > 0) {
                // We need to update the actual subscriptions (without context)
                // 1. stop the subscriptions to the old owner
                _safeUpsertSubscription(primToken, _owner, 0);

                // 2. start/update the subscriptions to the new owner
                _safeUpsertSubscription(primToken, to, outputFlowRate);
            }
        }

        emit ServiceOwnershipUpdated(_owner, to);
        self.owner = to;
    }

    /// @notice IDiagonalServiceV1.recoverStreams implementation
    function recoverStreams(
        DiagonalServiceStorage storage self,
        address prevOwner,
        ISuperToken[] calldata superTokens
    ) external {
        require(superTokens.length > 0, "SERV:E_SUPERTOKEN_INPUT");

        address _owner = self.owner;

        for (uint256 i = 0; i < superTokens.length; i++) {
            int96 outputFlowRate = self.ownerFlowRates[superTokens[i]];
            if (outputFlowRate > 0) {
                // At this point we do not know if outputFlowRate is:
                // service --> prevOwner, or, service --> owner

                // 1. Stop the subscriptions to the prev owner
                _safeUpsertSubscription(superTokens[i], prevOwner, 0);

                // 2. Start/update the subscriptions to the current owner
                _safeUpsertSubscription(superTokens[i], _owner, outputFlowRate);

                emit StreamRecovered(prevOwner, _owner, superTokens[i]);
            }
        }
    }

    /*******************************************
     * Superfluid agreement callback helpers *
     ******************************************/

    /**
     * @notice Helper function that performs necessary checks before a stream is created
     * @dev Called from the `beforeAgreementCreated` superfluid callback
     * @dev Is called on a first flow creation from the subscriber to the service
     * @param self The stored DiagonalService struct
     * @param ctx The encoded superfluid context, lets you pass metadata between hooks.
     * See https://docs.superfluid.finance/superfluid/docs/user-data for more info
     * @param superToken The super token asscoaited with the created stream
     * @return cbdata Data you want to be passed to the `afterAgreementCreated` callback
     */
    function handleBeforeCreated(
        DiagonalServiceStorage storage self,
        bytes calldata ctx,
        ISuperToken superToken
    ) external view returns (bytes memory cbdata) {
        ISuperfluid.Context memory contextDecoded = SUPERFLUID_HOST.decodeCtx(ctx);
        // the serviceId and packageId are passed as userData to the CFA agreement creation
        // If no user data is passed, we try redirecting stream to "default package" specified on service creation
        // TODO: In future allow users to specify default package
        uint256 packageId = 1;
        if (contextDecoded.userData.length > 0) {
            packageId = abi.decode(contextDecoded.userData, (uint256));
        }

        require(self.state == ServiceState.ACTIVE, "SERV:E_SERVICE_NOT_ACTIVE");
        require(self.packages[packageId].state == PackageState.ACTIVE, "SERV:E_PACKAGE_NOT_ACTIVE");
        // subscribers can only subscribe if the super token is accepted by the package
        require(self.packages[packageId].paymentTerms[superToken] > 0, "SERV:E_UNSUPPORTED_TOKEN");

        return abi.encode(packageId);
    }

    /**
     * @notice Helper function that performs the necessary checks before a stream is updated
     * @dev Called from the `beforeAgreementUpdated` superfluid callback
     * @dev Is called on any subsequent flow creation from the subscriber to the service
     * @param self The stored DiagonalService struct
     * @param ctx The encoded superfluid context, lets you pass metadata between hooks.
     * See https://docs.superfluid.finance/superfluid/docs/user-data for more info
     * @param superToken The super token asscoaited with the created stream
     * @return cbdata Data you want to be passed to the `afterAgreementUpdated` callback
     */
    function handleBeforeUpdated(
        DiagonalServiceStorage storage self,
        bytes calldata ctx,
        ISuperToken superToken
    ) external view returns (bytes memory cbdata) {
        ISuperfluid.Context memory contextDecoded = SUPERFLUID_HOST.decodeCtx(ctx);
        uint256 packageId = 1;
        if (contextDecoded.userData.length > 0) {
            packageId = abi.decode(contextDecoded.userData, (uint256));
        }

        require(self.state == ServiceState.ACTIVE, "SERV:E_SERVICE_NOT_ACTIVE");
        require(self.packages[packageId].state == PackageState.ACTIVE, "SERV:E_PACKAGE_NOT_ACTIVE");
        // subscribers can only subscribe if the super token is accepted by the package
        require(self.packages[packageId].paymentTerms[superToken] > 0, "SERV:E_UNSUPPORTED_TOKEN");

        return abi.encode(packageId);
    }

    /**
     * @notice Helper function that performs necessary checks after a stream is created (subscriber --> service)
     * @dev Called from the `afterAgreementCreated` superfluid callback
     * @dev Is called on first flow creation from the subscriber to the service
     * @param self The stored DiagonalService struct
     * @param superToken The superfluid super token
     * @param agreementId The superfluid agreement id
     * @param agreementData The agreement data, inlcuding userData
     * @param cbdata The data retruned from the `beforeAgreementCreated` hook
     * @param ctx The superfluid context
     * @return newCtx The new encoded superfluid context
     */
    function handleAfterCreated(
        DiagonalServiceStorage storage self,
        ISuperToken superToken,
        bytes32 agreementId,
        bytes calldata agreementData,
        bytes calldata cbdata,
        bytes calldata ctx
    ) external returns (bytes memory newCtx) {
        uint256 packageId = abi.decode(cbdata, (uint256));
        (address subscriber, ) = abi.decode(agreementData, (address, address));

        // Always reset the subscriber state before creation if the subscriber's previous subscription was terminated
        if (self.subscriberStates[subscriber][superToken].terminated) {
            _resetSubscriberState(self, subscriber, superToken);
        }

        // totalFlowRate is the total flowRate from subscriber to superApp.
        (, int96 totalFlowRate, , ) = SUPERFLUID_CFA.getFlowByID(superToken, agreementId);

        // code split into struct and scoping due to stack too deep error
        SubscriptionStateUpdateArgs memory subscriptionArgs;

        // Intilse struct seperatly - help optimsiser
        subscriptionArgs.subscriber = subscriber;
        subscriptionArgs.superToken = superToken;
        subscriptionArgs.packageId = packageId;
        subscriptionArgs.totalFlowRate = totalFlowRate;
        // during createFlow streamRate == totalFlowRate (sum of all streams to packages)
        subscriptionArgs.streamFlowRate = totalFlowRate;
        subscriptionArgs.ctx = ctx;

        require(_validatePaymentTerms(self, subscriptionArgs), "SERV:E_INVALID_PAYMENT_TERMS");
        // After this point we know the incoming stream corresponds to a valid package!
        // feeRate is the fee being paid to treasury is
        // packageRate is the price of the package

        // Assuming no fees, |streamFlowRate| = packagePrice
        // If fees, |streamFlowRate| = packagePrice + feeRate
        // NOTE: streamFlowRate can be negative if unsubscribing
        //
        //                        Diagonal Treasury
        //                              |
        //           streamFlowRate     |      packageRate
        // i.e. Alice -------------> superApp ---------------> Provider
        newCtx = _startSubscription(self, subscriptionArgs);

        // packageRate computed in `_validatePaymentTerms`
        newCtx = _handleAfterStateUpdate(self, superToken, subscriptionArgs.packageRate, newCtx);
    }

    /**
     * @notice Helper function that performs necessary checks after a stream is updated
     * @dev Called from the `afterAgreementUpdated` superfluid callback
     * @dev Is called if a subscriber increases or decreases the flowRate.
     *
     *      Case 1: Alice increases flowRate by y s.t. Alice ---> superApp, flowRate is y + x.
     *              Alice is trying to "subscribe", we revert unless (y, packageId, superToken) matches payment term.
     *
     *      Case 2: User decreases flowRate by y, s.t. Alice --> superApp, flowRate is x - y
     *              Alice is trying to "unsubscribe", we revert unless (|y|, packageId, superToken) matches payment term.
     *
     * @param self The stored DiagonalService struct
     * @param args A struct containing args, used to avoid stack too deep error
     * @return newCtx The new encoded superfluid context
     */
    function handleAfterUpdated(DiagonalServiceStorage storage self, SubscriptionCallbackArgs memory args)
        external
        returns (bytes memory newCtx)
    {
        ISuperToken superToken = args.superToken;

        uint256 packageId = abi.decode(args.cbdata, (uint256));
        (address subscriber, ) = abi.decode(args.agreementData, (address, address));

        newCtx = args.ctx;

        // flowRate is the total flowRate from subscriber to DiagonalApp.
        (, int96 totalFlowRate, , ) = SUPERFLUID_CFA.getFlowByID(superToken, args.agreementId);

        // We must account only for the diff because of the unified account model
        // Assuming no fees, |streamFlowRate| = packagePrice
        // If fees, |streamFlowRate| = packagePrice + feeRate
        // NOTE: streamFlowRate can be negative if unsubscribing.
        //
        //                        Diagonal Treasury
        //                              |
        //           streamFlowRate     |      packageRate
        // i.e. Alice -------------> superApp ---------------> Provider
        int96 streamFlowRate = totalFlowRate - self.subscriberStates[subscriber][superToken].totalInputFlowRate;

        // Nothing to update
        if (streamFlowRate == 0) return newCtx;

        // code split into struct and scoping due to stack too deep error
        SubscriptionStateUpdateArgs memory subscriptionArgs;

        // Intilse struct seperatly - help optimsiser
        subscriptionArgs.subscriber = subscriber;
        subscriptionArgs.superToken = superToken;
        subscriptionArgs.packageId = packageId;
        subscriptionArgs.totalFlowRate = totalFlowRate;
        subscriptionArgs.streamFlowRate = streamFlowRate;
        subscriptionArgs.ctx = args.ctx;

        require(_validatePaymentTerms(self, subscriptionArgs), "SERV:E_INVALID_PAYMENT_TERMS");
        // After this point we know the incoming stream corresponds to a valid package!
        // feeRate is the fee being paid to treasury is
        // packageRate is the price of the package

        if (streamFlowRate > 0) {
            require(self.state == ServiceState.ACTIVE, "SERV:E_SERVICE_NOT_ACTIVE");
            require(self.packages[packageId].state == PackageState.ACTIVE, "SERV:E_PACKAGE_NOT_ACTIVE");
            require(
                !self.subscriberStates[subscriber][superToken].packageStates[packageId].isSubscribed,
                "SERV:E_SUBSCRIPTION_EXISTS"
            );
            require(
                self.subscriberStates[subscriber][superToken].numSubscriptions < MAX_SUBSCRIPTIONS_PER_PACKAGE_TOKEN,
                "SERV:E_MAX_SUBSCRIPTIONS"
            );

            newCtx = _startSubscription(self, subscriptionArgs);
            // packageRate computed in `_validatePaymentTerms`
            newCtx = _handleAfterStateUpdate(self, superToken, subscriptionArgs.packageRate, newCtx);
        } else {
            newCtx = _stopSubscription(self, subscriptionArgs);
            newCtx = _handleAfterStateUpdate(self, superToken, -subscriptionArgs.packageRate, newCtx);
        }
    }

    /**
     * @notice Internal helper function which checks if the flowRate is valid, and returns terms
     * @return isValidPaymentTerms Are the payment terms valid
     */
    function _validatePaymentTerms(
        DiagonalServiceStorage storage self,
        SubscriptionStateUpdateArgs memory subscriptionArgs
    ) private view returns (bool isValidPaymentTerms) {
        ISuperToken superToken = subscriptionArgs.superToken;

        uint256 packageId = subscriptionArgs.packageId;
        int96 streamFlowRate = subscriptionArgs.streamFlowRate;

        int96 packageRate = self.packages[packageId].paymentTerms[superToken];
        int96 protocolFee = DIAGONAL_REGISTRY.protocolFees(superToken);

        int96 feeRate = 0;

        if (streamFlowRate < 0) {
            streamFlowRate = -streamFlowRate;
        }
        if (protocolFee > 0) {
            feeRate = _calculateFee(packageRate, protocolFee);
            isValidPaymentTerms = (streamFlowRate == packageRate + feeRate);
        } else {
            isValidPaymentTerms = (streamFlowRate == packageRate);
        }

        // Write feeRate and packageRate to memory
        subscriptionArgs.feeRate = feeRate;
        subscriptionArgs.packageRate = packageRate;
    }

    /**
     * @notice Helper function that terminates the flow from the subscriber to the DiagonalApp.
     * @dev This function needs to handle the following scenarios:
     * Case 1: Liquidations
     *         A stream Alice --> superApp is liquidated, as Alice runs out of tokens.
     *         In this case msg.sender will be liquidator.
     *
     * Case 2: Subscriber voluntarily closes stream.
     *         Alice voluntarily closes her stream to superApp in a given superToken.
     *         In this case we need to iterate though all packages Alice have subscribed to in the superToken,
     *         and update accointing.
     *
     * Case 3: Merchant voluntarily closes incoming stream in a superToken.
     *         In this case we use sticky streams.
     *
     */
    function handleAfterTerminated(
        DiagonalServiceStorage storage self,
        ISuperToken superToken,
        address subscriber,
        address receiver,
        bytes memory ctx
    ) external returns (bytes memory newCtx) {
        bool result = false;

        address owner = self.owner;
        int96 ownerFlowRate = self.ownerFlowRates[superToken];
        int96 subscriberFlowRate = self.subscriberStates[subscriber][superToken].totalInputFlowRate;

        // The provider closed the subscription, this is assumed to be by mistake.
        // In this case we re-start the subscription ("sticky streams")
        if (subscriber == address(this) && receiver == owner && ownerFlowRate > 0) {
            (newCtx, result) = _upsertSubscriptionWithContext(ctx, superToken, owner, ownerFlowRate);
        } else if (subscriberFlowRate > 0) {
            newCtx = _handleAfterTerminatedNonSticky(
                self,
                superToken,
                subscriber,
                owner,
                ownerFlowRate,
                subscriberFlowRate,
                ctx
            );
        }
    }

    /**
     * @notice Helper function that handles Case 1 (liquidations) and Case 2 (voluntary stream closure)
     */
    function _handleAfterTerminatedNonSticky(
        DiagonalServiceStorage storage self,
        ISuperToken superToken,
        address subscriber,
        address owner,
        int96 ownerFlowRate,
        int96 subscriberFlowRate,
        bytes memory ctx
    ) private returns (bytes memory newCtx) {
        int96 currSubTotalInputFeeRate = self.subscriberStates[subscriber][superToken].totalInputFeeRate;
        int96 totalFlowRate = ownerFlowRate - (subscriberFlowRate - currSubTotalInputFeeRate);

        self.ownerFlowRates[superToken] = totalFlowRate;

        // Flag the subscriber as terminated
        self.subscriberStates[subscriber][superToken].terminated = true;

        bool result = false;
        (newCtx, result) = _upsertSubscriptionWithContext(ctx, superToken, owner, totalFlowRate);
        if (!result) return newCtx;

        // Save SLOAD
        int96 currTreasuryFeeRate = self.treasuryFeeRates[superToken];

        if (currTreasuryFeeRate > 0 && currSubTotalInputFeeRate > 0) {
            int96 totalFeeRate = currTreasuryFeeRate - currSubTotalInputFeeRate;

            self.treasuryFeeRates[superToken] = totalFeeRate;

            (newCtx, result) = _upsertSubscriptionWithContext(
                newCtx,
                superToken,
                DIAGONAL_REGISTRY.treasury(),
                totalFeeRate
            );
        }

        emit SubscriptionTerminated(subscriber, superToken);
    }

    /*************************************************
     * Subscriber state management private functions *
     *************************************************/

    /**
     * @notice Internal function that updates subscriberState reflecting a new subscription start,
     * and emits the appropriate `SubscriptionStartedEvent`.
     */
    function _startSubscription(
        DiagonalServiceStorage storage self,
        SubscriptionStateUpdateArgs memory subscriptionArgs
    ) private returns (bytes memory newCtx) {
        // struct unpacking (stack too deep errors)
        ISuperToken superToken = subscriptionArgs.superToken;
        address subscriber = subscriptionArgs.subscriber;
        uint256 packageId = subscriptionArgs.packageId;

        int96 feeRate = subscriptionArgs.feeRate;
        int96 packageRate = subscriptionArgs.packageRate;
        newCtx = subscriptionArgs.ctx;

        // Sanity check
        assert(subscriptionArgs.streamFlowRate > 0);

        // feeRate already calculated in `_validatePaymentTerms`
        if (feeRate > 0) {
            newCtx = _handleFeeCharge(self, newCtx, subscriber, superToken, packageId, feeRate);
        }

        self.subscriberStates[subscriber][superToken].numSubscriptions += 1;
        self.subscriberStates[subscriber][superToken].packageStates[packageId].isSubscribed = true;
        self.subscriberStates[subscriber][superToken].packageIds.push(packageId);
        self.subscriberStates[subscriber][superToken].totalInputFlowRate = subscriptionArgs.totalFlowRate;
        emit SubscriptionStarted(subscriber, packageId, superToken, packageRate, feeRate);
    }

    /**
     * @notice Internal function that updates subscriberState reflecting stopping a subscription,
     * and emits the appropriate `SubscriptionStopped`.
     * @dev MUST only be called after `_validatePaymentTerms`
     * @dev streamFlowRate MUST be <= 0
     */
    function _stopSubscription(DiagonalServiceStorage storage self, SubscriptionStateUpdateArgs memory subscriptionArgs)
        private
        returns (bytes memory newCtx)
    {
        // struct unpacking (stack too deep errors)
        ISuperToken superToken = subscriptionArgs.superToken;
        address subscriber = subscriptionArgs.subscriber;
        uint256 packageId = subscriptionArgs.packageId;

        newCtx = subscriptionArgs.ctx;

        int96 userFeeRate = self.subscriberStates[subscriber][superToken].packageStates[packageId].feeRate;

        assert(subscriptionArgs.streamFlowRate < 0);

        if (userFeeRate > 0) {
            newCtx = _handleFeeTermination(self, newCtx, subscriber, superToken, packageId, userFeeRate);
        }

        self.subscriberStates[subscriber][superToken].numSubscriptions -= 1;
        self.subscriberStates[subscriber][superToken].packageStates[packageId].isSubscribed = false;
        self.subscriberStates[subscriber][superToken].totalInputFlowRate = subscriptionArgs.totalFlowRate;
        _removePackageId(self, subscriber, superToken, packageId);
        emit SubscriptionStopped(subscriber, packageId, superToken);
    }

    /**
     * @notice Helper function that updates subscriber packageIds array when unsubscribe
     */
    function _removePackageId(
        DiagonalServiceStorage storage self,
        address subscriber,
        ISuperToken superToken,
        uint256 packageId
    ) private {
        uint256 packageIdIndex = 0;
        uint256 numPackages = self.subscriberStates[subscriber][superToken].packageIds.length;

        // we should accept to max 5 subscriptions, so this should not be a concern
        for (uint256 i = 0; i < numPackages; i++) {
            uint256 packageIdTmp = self.subscriberStates[subscriber][superToken].packageIds[i]; // readability
            if (packageId == packageIdTmp) {
                packageIdIndex = i;
                break;
            }
        }

        // Case 1: If package being dropped is last element of full array, no need to write to self
        // Case 2: First element array, a) full, b) not full
        // Case 3: Middle element in array, a) full, b) not full
        // Case 4: Last element in array, b) not full
        self.subscriberStates[subscriber][superToken].packageIds[packageIdIndex] = self
        .subscriberStates[subscriber][superToken].packageIds[numPackages - 1];
        self.subscriberStates[subscriber][superToken].packageIds.pop();
    }

    /**
     * @notice Helper function that resets subscriber state
     * @dev Reset is done on re-subscribing to a package that they previously
     * terminated their subscription to.
     * @dev Re-subscribing means they deleted a stream/liquidated in a super token,
     * and then proceeded to create a new stream.
     */
    function _resetSubscriberState(
        DiagonalServiceStorage storage self,
        address subscriber,
        ISuperToken superToken
    ) private {
        // We have limited - small number of package subscriptions per service, so this should be a concern MAX_SUBSCRIPTIONS_PER_PACKAGE_TOKEN
        for (uint256 i = 0; i < self.subscriberStates[subscriber][superToken].packageIds.length; i++) {
            uint256 packageId = self.subscriberStates[subscriber][superToken].packageIds[i];
            delete self.subscriberStates[subscriber][superToken].packageStates[packageId];
        }

        delete self.subscriberStates[subscriber][superToken];
    }

    /**
     * @notice Helper function that updates service stream accounting
     */
    function _handleAfterStateUpdate(
        DiagonalServiceStorage storage self,
        ISuperToken superToken,
        int96 packageRate,
        bytes memory ctx
    ) private returns (bytes memory newCtx) {
        // Update flowRates
        int96 totalOutputFlowRate = self.ownerFlowRates[superToken] + packageRate;
        self.ownerFlowRates[superToken] = totalOutputFlowRate;

        bool result = false;
        (newCtx, result) = _upsertSubscriptionWithContext(ctx, superToken, self.owner, totalOutputFlowRate);
        require(result, "SERV:E_SUB_STATE_UPDATE_FAILED");
    }

    /**************************************************
     * Fee and flow rate management private functions *
     **************************************************/

    /**
     * @dev Helper function that handles fee charging
     * @dev Only called from `_startSubscription`
     */
    function _handleFeeCharge(
        DiagonalServiceStorage storage self,
        bytes memory ctx,
        address subscriber,
        ISuperToken superToken,
        uint256 packageId,
        int96 feeRate
    ) private returns (bytes memory newCtx) {
        /****************** Update subscriber fee state ********************** */
        self.subscriberStates[subscriber][superToken].packageStates[packageId].feeRate = feeRate;
        self.subscriberStates[subscriber][superToken].totalInputFeeRate += feeRate;

        /****************** Update subscriber fee state ********************** */
        // Subtract fees being streamed to treasury, update accounting + streams
        newCtx = _updateTreasuryFees(self, ctx, superToken, feeRate);
    }

    /**
     * @dev Helper function that handles fee termination
     * @dev Only called from `_stopSubscription`
     */
    function _handleFeeTermination(
        DiagonalServiceStorage storage self,
        bytes memory ctx,
        address subscriber,
        ISuperToken superToken,
        uint256 packageId,
        int96 userFeeRate
    ) private returns (bytes memory newCtx) {
        /****************** Update subscriber fee state ********************** */
        // Reset fees being paid on package, and subtract away fees from totalFees paid
        self.subscriberStates[subscriber][superToken].packageStates[packageId].feeRate = 0;
        self.subscriberStates[subscriber][superToken].totalInputFeeRate -= userFeeRate;

        /****************** Update treasury fee state ********************** */
        // Subtract fees being streamed to treasury, update accounting + streams
        newCtx = _updateTreasuryFees(self, ctx, superToken, -userFeeRate);
    }

    /**
     * @dev Helper function that updates treasury fee state updates.
     *      Updates accounting model and streams.
     */
    function _updateTreasuryFees(
        DiagonalServiceStorage storage self,
        bytes memory ctx,
        ISuperToken superToken,
        int96 feeRate
    ) private returns (bytes memory newCtx) {
        int96 currTreasuryFeeRate = self.treasuryFeeRates[superToken];
        self.treasuryFeeRates[superToken] = currTreasuryFeeRate + feeRate;
        int96 totalFeeRate = currTreasuryFeeRate + feeRate;

        // Update streams
        bool result = false;
        (newCtx, result) = _upsertSubscriptionWithContext(ctx, superToken, DIAGONAL_REGISTRY.treasury(), totalFeeRate);
        require(result, "SERV:E_FEE_TERMINATION_FAILED");
    }

    /**
     * @dev Helper function that calculates fee flow rate and given the price of a package
     * @param packageRate The price of the package
     * @param protocolFee The protocol fee specified
     * @return feeRate The fee rate that will be streamed to treasury
     */
    function _calculateFee(int96 packageRate, int96 protocolFee) private pure returns (int96 feeRate) {
        // intermediary multiplications casted to int256 (to prevent overflow)
        int256 packageRateTmp = int256(packageRate);
        int256 subscriptionFeePercent = int256(protocolFee);
        int256 feeRateTmp = (packageRateTmp * subscriptionFeePercent) / 100;
        feeRate = int96(feeRateTmp);
    }

    /*********************************************
     * Subscription management private functions *
     *********************************************/

    /**
     * @notice Helper function that makes superfluid stream calls
     * @dev Handles `deleteFlow`, `createFlow` and `updateFlow` superfluid calls
     * @return newCtx_ New superfluid context
     * @return result The result of the try/catch external function call
     */
    function _upsertSubscriptionWithContext(
        bytes memory ctx,
        ISuperToken superToken,
        address receiver,
        int96 newFlowRate
    ) private returns (bytes memory newCtx_, bool result) {
        (, int96 outputFlowRate, , ) = SUPERFLUID_CFA.getFlow(superToken, address(this), receiver);

        // delete flow, triggered when subscriber unsubscribes from the last subscription
        bytes memory encodedData;
        if (newFlowRate == 0) {
            encodedData = abi.encodeWithSelector(
                SUPERFLUID_CFA.deleteFlow.selector,
                superToken,
                address(this),
                receiver,
                new bytes(0)
            );
            (newCtx_, result) = _tryCallAgreementWithContext(ctx, encodedData);
            return (newCtx_, result);
        }

        bytes4 selector = SUPERFLUID_CFA.createFlow.selector;
        if (outputFlowRate != 0) {
            selector = SUPERFLUID_CFA.updateFlow.selector;
        }

        encodedData = abi.encodeWithSelector(selector, superToken, receiver, newFlowRate, new bytes(0));
        (newCtx_, result) = _tryCallAgreementWithContext(ctx, encodedData);
    }

    /**
     * @notice Helper function that try/catch external superfluid calls, and returns new context
     */
    function _safeUpsertSubscription(
        ISuperToken superToken,
        address receiver,
        int96 newFlowRate
    ) private returns (bool result) {
        (, int96 outputFlowRate, , ) = SUPERFLUID_CFA.getFlow(superToken, address(this), receiver);

        // Check if is trying to close already closed stream, used in `recoverStreams`
        require(!(outputFlowRate == 0 && newFlowRate == 0), "SERV:E_SUBSCRIPTION_ALREADY_CLOSED");

        // delete flow, triggered when the old provider flows should be stopped
        // delete flow, triggered when subscriber unsubscribes from the last subscription
        bytes memory encodedData;
        if (newFlowRate == 0) {
            encodedData = abi.encodeWithSelector(
                SUPERFLUID_CFA.deleteFlow.selector,
                superToken,
                address(this),
                receiver,
                new bytes(0)
            );
            result = _tryCallAgreement(encodedData);
        } else {
            bytes4 selector = SUPERFLUID_CFA.createFlow.selector;
            if (outputFlowRate != 0) {
                selector = SUPERFLUID_CFA.updateFlow.selector;
            }

            encodedData = abi.encodeWithSelector(selector, superToken, receiver, newFlowRate, new bytes(0));
            result = _tryCallAgreement(encodedData);
        }
        require(result, "SERV:E_UPSERT_FAILED");
    }

    /*******************************************
     * Stream management - Utils *
     ******************************************/

    /**
     * @notice Helper function that try/catch external superfluid calls, and returns new context
     */
    function _tryCallAgreementWithContext(bytes memory ctx, bytes memory encodedData)
        private
        returns (bytes memory newCtx_, bool result)
    {
        try SUPERFLUID_HOST.callAgreementWithContext(SUPERFLUID_CFA, encodedData, "0x", ctx) returns (
            bytes memory newCtx,
            bytes memory /*returnedData*/
        ) {
            newCtx_ = newCtx;
            result = true;
        } catch {
            newCtx_ = ctx;
            result = false;
        }
    }

    /**
     * @notice Helper function that try/catch external superfluid calls
     */
    function _tryCallAgreement(bytes memory encodedData) private returns (bool result) {
        try SUPERFLUID_HOST.callAgreement(SUPERFLUID_CFA, encodedData, "0x") returns (
            bytes memory /*returnedData*/
        ) {
            result = true;
        } catch {
            result = false;
        }
    }

    /*******************************************
     * Getters *
     ******************************************/

    /// @notice IDiagonalServiceV1.subscriptionPaymentTermsValid implementation
    function noFeeSubscriptionPaymentTermsValid(
        DiagonalServiceStorage storage self,
        uint256 packageId,
        ISuperToken token,
        int96 flowRate
    ) public view returns (bool valid) {
        valid = self.packages[packageId].paymentTerms[token] == flowRate;
    }
}

