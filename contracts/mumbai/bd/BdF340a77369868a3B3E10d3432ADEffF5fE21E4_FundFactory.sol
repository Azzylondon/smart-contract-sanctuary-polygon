pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "./TokenLogic.sol";

import "hardhat/console.sol";

contract FundLogic is Initializable, TokenLogic {
    using SafeMath for uint256;
    using SafeERC20 for ERC20;

    address public FUND_FACTORY;
    address private OWNER;

    address public USDC_TOKEN;

    uint256 private constant MAX_BPS = 10000; // 100%
    uint256 private constant SECONDS_IN_YEAR = 31557600; // 60*60*24*365.25
    uint256 private constant feeBpsForProtocol = 60; // 0,6%
    uint256 private constant feeDepositForProtocol = 50; // 0,5%
    uint256 private feeBpsForFund;
    uint256 private feeDepositForFund;
    uint256 private fundLastPaid;

    event FeesPaidForMinting(uint256 mintingFeeForProtocol, uint256 mintingFeeForOwner, uint256 secondsPaid);
    event FeesPaidForManagment(uint256 managmentFeeForProtocol, uint256 managmentFeeForFundOwner, uint256 secondsPaid);
    event SharesMinted(uint256 sharesAmount, uint256 investmentAmount, address owner);

    /**
     * @dev Throws error if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(owner() == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {}

    //////////////////////
    //    CONSTRUCTOR   //
    //////////////////////

    function initialize(address _owner, uint256 _fundPaymentFees, uint256 _fundManagmentFees,  address _usdcToken, string memory _name, string memory _symbol)
        public
        initializer
    {
        TokenLogic.initialize(_name, _symbol);
        
        USDC_TOKEN = _usdcToken;
        FUND_FACTORY = msg.sender;
        OWNER = _owner;

        feeBpsForFund= _fundManagmentFees;
        feeDepositForFund = _fundPaymentFees;

        __setLastPaidForFund(block.timestamp);
    }

    //////////////////////
    //      PUBLIC      //
    //////////////////////

    /**
     * @notice function to invest in the fund and mint shares of if
     * @param _investmentAmount The number of shares that the user want to buy
     */
    function investInFund(uint256 _investmentAmount) public returns (uint256) {
        require(_investmentAmount > 0, "_investmentAmount need to be >0");

        // pay fees
        bool feesPayed = __payFees(_investmentAmount);

        require(feesPayed, "error paying fee");

        ERC20 usdcToken = ERC20(USDC_TOKEN);

        // Mint share for user
        uint256 shareToMint = __sharesToMint(_investmentAmount);
        // Investment
        usdcToken.transferFrom(msg.sender, address(this), _investmentAmount);
        _mint(msg.sender, shareToMint);

        emit SharesMinted(shareToMint, _investmentAmount, msg.sender);
        
        return shareToMint;
    }

    /**
     * @notice function to redeem shares of the fund
     * @param _shares The number of shares that the user want to redeem
     */
    function redeemInvestment(uint256 _shares) public returns (bool) {
        require(_shares > 0, "_shares need to be >0");
        require(balanceOf(msg.sender) >= _shares, "user don't have all of this shares");

        // pay fees
        bool feesPayed = __payFees(0);

        require(feesPayed, "error paying fee");

        ERC20 usdcToken = ERC20(USDC_TOKEN);
        uint256 tokenToRedeem = __tokenToRedeem(_shares);
        require(getAssetBalance() >= tokenToRedeem, "not enough fund balance to redeem");

        usdcToken.safeTransfer(msg.sender, tokenToRedeem);
        _burn(msg.sender, _shares);
        return true;
    }

    //////////////////////
    //      PRIVATE     //
    //////////////////////

    /**
     * @notice pay fees for the protocol and for the fund
     * @dev use this function either for invest and for redeem but with 0 for _investmentAmount in case of redeem
     * @param _investmentAmount the investment for wich to pay fees
     */
    function __payFees(uint256 _investmentAmount) private returns(bool) {
        ERC20 usdcToken = ERC20(USDC_TOKEN);
        uint256 lastPaid = getFundLastPaid();
        uint256 secondsDue = block.timestamp.sub(lastPaid);
        uint256 assetBalance = getAssetBalance();

        // get protocole fee due for gestion from fund
        uint256 managmentFeeForProtocol = __calcFeeDueForProtocol(lastPaid, secondsDue, assetBalance);

        // get fund fee due for gestion from fund
        uint256 managmentFeeForFundOwner = __calcFeeDueForFund(lastPaid, secondsDue, assetBalance);

        // make all transfert for diffrent fee (to fund and to protocol)
        // Managment fees
        require(assetBalance >= managmentFeeForProtocol.add(managmentFeeForFundOwner), "Insufficient funds for paying fees");
        usdcToken.transfer(FUND_FACTORY, managmentFeeForProtocol);
        usdcToken.transfer(OWNER, managmentFeeForFundOwner);

        emit FeesPaidForManagment(managmentFeeForProtocol, managmentFeeForFundOwner, secondsDue);

        // Minting fees
        if (_investmentAmount > 0) {
            // get fund fee due for investment from user
            uint256 mintingFeeForOwner = _investmentAmount.div(10000).mul(getFeeDepositForFund());

            // get protocole fee due for investment from user
            uint256 mintingFeeForProtocol = _investmentAmount.div(10000).mul(getFeeDepositForProtocol());

            // check that user has enough usdc for fee + investment
            uint256 total = _investmentAmount.add(mintingFeeForOwner).add(mintingFeeForProtocol);
            uint balance = usdcToken.balanceOf(msg.sender);
            require(balance >= total, "Insufficient funds for transfer.");

            // check the allowance
            uint256 allowed = usdcToken.allowance(msg.sender, address(this));
            require(allowed >= total, "Allowance to low for transfer.");

            // Minting fees
            usdcToken.transferFrom(msg.sender, FUND_FACTORY, mintingFeeForProtocol);
            usdcToken.transferFrom(msg.sender, OWNER, mintingFeeForOwner);

            emit FeesPaidForMinting(mintingFeeForProtocol, mintingFeeForOwner, secondsDue);
        }

        // Even if feeDueForProtocol and feeDueForFund are 0, we update the lastPaid timestamp and emit the event
        __setLastPaidForFund(block.timestamp);

        return true;
    }
    
    function __calcFeeDueForFund(uint256 _lastPaid, uint256 _secondsDue, uint256 _assetBalance) private view returns (uint256 feeDue_) {
        uint256 rawTokensDue = 0;
        if (_lastPaid < block.timestamp) {
            rawTokensDue = _assetBalance
                .mul(getFeeBpsFund())
                .mul(_secondsDue)
                .div(SECONDS_IN_YEAR)
                .div(MAX_BPS);
        }

        uint256 supplyNetRawTokensDue = _assetBalance.sub(rawTokensDue);
        feeDue_ = 0;
        if (supplyNetRawTokensDue != 0) {
            feeDue_ = rawTokensDue.mul(_assetBalance).div(supplyNetRawTokensDue);
        }
        return feeDue_;
    }
    
    function __calcFeeDueForProtocol(uint256 _lastPaid, uint256 _secondsDue, uint256 _assetBalance) private view returns (uint256 feeDue_) {
        uint256 rawTokensDue  = 0;
        if (_lastPaid < block.timestamp) {
        rawTokensDue = _assetBalance
            .mul(getFeeBpsProtocol())
            .mul(_secondsDue)
            .div(SECONDS_IN_YEAR)
            .div(MAX_BPS);
        }

        uint256 supplyNetRawTokensDue = _assetBalance.sub(rawTokensDue);
        feeDue_ = 0;
        if (supplyNetRawTokensDue != 0) {
            feeDue_ = rawTokensDue.mul(_assetBalance).div(supplyNetRawTokensDue);
        }
        return feeDue_;
    }

