pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "../fund/FundLogic.sol";
import "../fund/FundProxy.sol";

contract FundFactory {
    using SafeERC20 for ERC20;

    address immutable FUND_LOGIC;
    address immutable USDC_TOKEN;
    address immutable PROTOCOL_OWNER;
    
    address[] fundsList;
    uint256 public fundCount;

    event FundProxyDeployed(address creator, address tokenProxy, string tokenName, string tokenSymbol);

    modifier onlyOwner() {
        require(msg.sender == PROTOCOL_OWNER, "Only the contract owner can call this function");
        _;
    }

    constructor(address _usdcToken) {
        FUND_LOGIC = address(new FundLogic());
        USDC_TOKEN = _usdcToken;
        PROTOCOL_OWNER = msg.sender;
        fundCount = 0;
    }

    //////////////////////
    //      PUBLIC      //
    //////////////////////

    function withdrawFees() public onlyOwner {
        ERC20 usdcToken = ERC20(USDC_TOKEN);
        uint256 balance = usdcToken.balanceOf(address(this));
        require(balance >= 0, "balance < 0");
        
        usdcToken.safeTransfer(PROTOCOL_OWNER, balance);
    }

    //////////////////////
    //      EXTERNAL    //
    //////////////////////

    function createFund(uint256 _fundPaymentFees, uint256 _fundManagmentFees, string memory _tokenName, string memory _tokenSymbol) external returns (address) {
        FundProxy proxy = new FundProxy(
            FUND_LOGIC,
            abi.encodeWithSelector(FundLogic.initialize.selector, msg.sender, _fundPaymentFees, _fundManagmentFees, USDC_TOKEN, _tokenName, _tokenSymbol)
        );

        emit FundProxyDeployed(msg.sender, address(proxy), _tokenName, _tokenSymbol);

        fundsList.push(address(proxy));
        fundCount++;

        return address(proxy);
    }

    //////////////////////
    //  STATE GETTERS   //
    //////////////////////

    /**
     * @dev Returns informations about the factory
     */
     function getFactoryInformations() public view returns (address, address, address, address, uint256, uint256) {
         return (address(this), FUND_LOGIC, USDC_TOKEN, PROTOCOL_OWNER, fundCount, ERC20(USDC_TOKEN).balanceOf(address(this)));
     }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view virtual returns (address) {
        return PROTOCOL_OWNER;
    }
    
    function getFundLogicAddress() public view returns (address) {
        return FUND_LOGIC;
    }
    
    function getUSDCAddress() public view returns (address) {
        return USDC_TOKEN;
    }
    
    function getFundsList() public view returns (address[] memory) {
        // address[] memory fundAddressList = address[](fundCount);
        // for (uint i = 0; i < fundAddressList; i++) {
        //   address storage fundAddress = fundsList[i];
        //   fundAddressList[i] = fundAddress;
        // }
        return fundsList;
    }
}

