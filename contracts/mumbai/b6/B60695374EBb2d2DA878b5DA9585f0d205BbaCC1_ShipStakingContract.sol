import "./StakingContract.sol";
import "./StakingData.sol";
import "./StakingValue.sol";
import "./StakingLoot.sol";
import "../MVPS/AvatarStatus.sol";
import "../items/PirateItem.sol";

import "../factory/IRandomNumberFactory.sol";
import "../factory/IRandomRequester.sol";

import "hardhat/console.sol";

contract ShipStakingContract is
    Ownable,
    Pausable,
    StakingContract,
    IRandomRequester
{
    using SafeMath for uint256;
    using SafeMath for uint8;

    mapping(bytes32 => address) private randomRequests;
    mapping(address => uint256) private ownerToAvatarTokenId;

    mapping(uint256 => uint256) private shipToAvatarTokenId;    
    mapping(uint256 => uint256) private avatarToShipTokenId;    
    
    event ClaimingShipReward(bytes32 indexed requestId, address indexed account);

    event ItemMinted(uint256 tokenId, string metadata);

    address internal shipContractAddress;
    IShipContract internal shipContract;
    address internal randomNumberFactoryAddress;

    IRandomNumberFactory internal randomNumberFactory;

    constructor(
        address _avatarContractAddress,
        address _shipContractAddress,
        address _randomNumberFactoryAddress,
        address _doubloonsAddress,
        address _pirateItemContractAddress,
        uint256 _rumTokenId,
        StakingLoot[] memory _stakingLoots,
        uint256[2][] memory _pirateItemRequirements)
        StakingContract(_avatarContractAddress, _doubloonsAddress, _pirateItemContractAddress, _rumTokenId) 
    {
        randomNumberFactory = IRandomNumberFactory(_randomNumberFactoryAddress);
        
        setShipContract(_shipContractAddress);

        rewardBase = uint256(1).mul(10 ** 18); // 1 doubloons

        notorietyPointsReward = 10;
        rumCreatedReward = 0;
        rumBurntReward = 0;

        minimumDaysCaptured = 10;
        bribeAmount = 10;

        setStakingLoots(_stakingLoots);
        setPirateItemRequirements(_pirateItemRequirements);
    }

    function setShipContract(address _shipContractAddress) public onlyOwner {
        shipContractAddress = _shipContractAddress;
        shipContract = IShipContract(shipContractAddress);
    }

    function stake(uint256 avatarTokenId, uint256 shipTokenId) public whenNotPaused {
        // if pirate
        if (!IStakingTaxContract(avatarContractAddress).isTaxCollector(avatarTokenId))
        {
            require(shipToAvatarTokenId[shipTokenId] == 0, "SS2");
            require(shipContract.ownerOf(shipTokenId) == msg.sender, "SS3");

            avatarToShipTokenId[avatarTokenId] = shipTokenId;
            shipToAvatarTokenId[shipTokenId] = avatarTokenId;
        }

        _stake(avatarTokenId);
    }

    function claim(uint256 avatarTokenId) public nonReentrant whenNotPaused {
        // if pirate
        if (!IStakingTaxContract(avatarContractAddress).isTaxCollector(avatarTokenId))
        {
            bytes32 requestId = randomNumberFactory.randomRequest();
            
            // storing requestId
            randomRequests[requestId] = msg.sender;
            ownerToAvatarTokenId[msg.sender] = avatarTokenId;

            emit ClaimingShipReward(requestId, msg.sender);
        }
        else
        {
            uint256 reward;
            uint256 notorietyPoints;
            uint256 rumCreated;
            uint256 rumBurnt;
            uint256 tax; 

            (reward, notorietyPoints, rumCreated, rumBurnt, tax) = calculateReward(avatarTokenId);

            super._claim(avatarTokenId);
        }
    }

    function randomnessFulfilled(uint256 randomness, bytes32 requestId) external override {
        
        address account = randomRequests[requestId];
        uint256 avatarTokenId = ownerToAvatarTokenId[account];

        uint256 notorietyPointsEncounter = 0;
        uint256 rewardEncounter = 0;

        bool pirateCaptured = false;
        for (uint256 i = 0; i < stakingLootslength; i++) {
            
            uint256[] memory randomValues = randomNumberFactory.expand(randomness, indexToStakingLoot[i].pirateItemTokenIds.length + 4);

            uint256 daysStaked = durationStaked(avatarTokenId).div(rewardDenominator);

            if (randomValues[0].mod(100).add(1) <= indexToStakingLoot[i].encounterChance.mul(daysStaked)) {
                
                if (indexToStakingLoot[i].capturedChance > 0) {
                    
                    if (randomValues[1].mod(100).add(1) <= indexToStakingLoot[i].capturedChance) {

                        pirateCaptured = true;

                        // update status metadata
                        avatarContract.setStatus(avatarTokenId, AvatarStatus.Captured);

                        break;
                    }
                }

                if (randomValues[2].mod(100).add(1) <= indexToStakingLoot[i].encounterSuccessChance)
                {
                    notorietyPointsEncounter += indexToStakingLoot[i].notorietyPoints;

                    if (indexToStakingLoot[i].maxDoubloons > 0)
                    {
                        rewardEncounter = randomValues[3].mod(indexToStakingLoot[i].maxDoubloons).add(1).mul(10 ** 18); // reward doubloons
                    }

                    // loot
                    for (uint256 j = 0; j < indexToStakingLoot[i].pirateItemTokenIds.length; j++) {
                        
                        if (randomValues[4 + j].mod(100).add(1) <= indexToStakingLoot[i].pirateItemPercentageChances[j]) {
                            
                            pirateItems.mintPirateItem(account, indexToStakingLoot[i].pirateItemTokenIds[j], 1, "");

                            emit ItemMinted(
                                indexToStakingLoot[i].pirateItemTokenIds[j], 
                                ERC1155(pirateItemContractAddress).uri(indexToStakingLoot[i].pirateItemTokenIds[j])
                            );
                        } 
                    }
                }
            }
        }

        uint256 reward;
        uint256 notorietyPoints;
        uint256 rumCreated;
        uint256 rumBurnt;
        uint256 tax; 

        if (!pirateCaptured) {
            
            (reward, notorietyPoints, rumCreated, rumBurnt, tax) = _getRewards(account, avatarTokenId);

            reward += rewardEncounter;
            notorietyPoints + notorietyPointsEncounter;

            _transferRewards(account, reward, rumCreated, rumBurnt);
        }

        // always add notoriety points, even if captured this time around
        if (notorietyPoints > 0)
        {
            avatarContract.addPirateAvatarNotorietyPoints(avatarTokenId, notorietyPoints);
        }

        emit ClaimFulfilled(reward, notorietyPoints, rumCreated, rumBurnt, tax, rewardEncounter, notorietyPointsEncounter, pirateCaptured);
    }

    function payBribe(uint256 avatarTokenId, uint256 amount) public {
        
        require(amount >= bribeAmount, "SS3");

        //require(keccak256(bytes(avatarContract.getStatus(avatarTokenId))) == keccak256(bytes(CAPTURED_STATUS)), "SS4");

        require(doubloons.transferFrom(msg.sender, address(this), amount), "SS5");

        _updateTaxValues(amount);

        _unstake(avatarTokenId);
    }

    function unstake(uint256 avatarTokenId) public {
        
        // if captured
        if (avatarContract.isCaptured(avatarTokenId)) {
            
            // must have remained in prison for the minimum duration
            require(durationStaked(avatarTokenId).div(rewardDenominator) >= minimumDaysCaptured, "SC6");
        }

        _unstake(avatarTokenId);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

import "./IStakingContract.sol";
import "./StakingData.sol";
import "./StakingValue.sol";
import "./StakingLoot.sol";
import "./IStakingTaxContract.sol";

import "../MVPS/IAvatarContract.sol";

import "../items/IPirateItemContract.sol";

import "hardhat/console.sol";

abstract contract StakingContract is
    Ownable,
    Pausable,
    ReentrancyGuard,
    IStakingContract
{
    using SafeMath for uint256;
    using SafeMath for uint8;

    IERC20 internal doubloons;

    uint8 public minimumDaysCaptured = 0;

    uint256 private constant stakeDurationCalc = 1 seconds;
    uint256 private constant notorietyPointsMultiplier = 5;

    uint256 internal constant rewardDenominator = 86400; // per day

    uint256 internal rumTokenId = 0;

    uint256 public bribeAmount = 0;
    uint256 public taxedDoubloonsBalance = 0;
    
    uint256 internal taxRecipientCount = 0;
    uint256 internal taxRecpientValue = 0;   
    
    uint256 internal rewardBase = uint256(1).mul(10 ** 18); // 1 doubloons 
    
    uint256 internal notorietyPointsReward = 0;
    uint256 internal rumCreatedReward = 0;
    uint256 internal rumBurntReward = 0;
    uint256 internal taxRate = 0; // percent

    address internal pirateItemContractAddress;
    IPirateItemContract internal pirateItems;

    address internal avatarContractAddress;
    IAvatarContract internal avatarContract;  

    // loot
    uint256 internal stakingLootslength;
    mapping(uint256 => StakingLoot) internal indexToStakingLoot;

    // staking
    mapping(uint256 => StakingData) internal stakings;
    mapping(address => uint256[]) internal stakingsToOwner;

    // entry requirements
    // dynamic array of [tokenId, amount]
    uint256[2][] internal pirateItemRequirements;
    

    // tax collectors
    mapping(uint256 => uint256) internal tokenIdToTaxCollectorsTaxValue;
    mapping(uint256 => bool) internal tokenIdIsTaxCollector;

    event ClaimFulfilled(uint256 reward, uint256 notorietyPoints, uint256 rumCreated, uint256 rumBurnt, uint256 tax, uint256 rewardEncounter, uint256 notorietyPointsEncounter, bool pirateCaptured);

    modifier isStaked(uint256 tokenId) {
        require(avatarContract.ownerOf(tokenId) == address(this), "SS1");
        _;
    }

    modifier notStaked(uint256 tokenId) {
        require(avatarContract.ownerOf(tokenId) == msg.sender, "SS2");
        _;
    }

    constructor(
        address _avatarContractAddress,
        address _doubloonsContractAddress,
        address _pirateItemContractAddress,
        uint256 _rumTokenId) 
    {
        doubloons = IERC20(_doubloonsContractAddress);

        setAvatarContract(_avatarContractAddress);
        setPirateItemContract(_pirateItemContractAddress);
        setRumTokenId(_rumTokenId);
    }

    function setPirateItemContract(address _pirateItemContractAddress) public onlyOwner {
        pirateItemContractAddress = _pirateItemContractAddress;
        pirateItems = IPirateItemContract(pirateItemContractAddress);
    }

    function setAvatarContract(address _avatarContractAddress) public onlyOwner {
        avatarContractAddress = _avatarContractAddress;
        avatarContract = IAvatarContract(avatarContractAddress);
    }

    function setPirateItemRequirements(uint256[2][] memory _pirateItemRequirements) public onlyOwner {
        pirateItemRequirements = _pirateItemRequirements;
    }

    function setStakingLoots(StakingLoot[] memory _stakingLoots) public onlyOwner {
        
        for (uint8 i = 0; i < stakingLootslength; i++) {
            StakingLoot memory stakingLoot;
            indexToStakingLoot[i] = stakingLoot;
        }

        stakingLootslength = _stakingLoots.length;

        //delete stakingLoots;
        for (uint256 i = 0; i < _stakingLoots.length; i++) {
            indexToStakingLoot[i] =
                StakingLoot(
                    _stakingLoots[i].encounterChance,
                    _stakingLoots[i].encounterSuccessChance,
                    _stakingLoots[i].captureChance,
                    _stakingLoots[i].capturedChance,
                    _stakingLoots[i].notorietyPoints,
                    _stakingLoots[i].maxDoubloons,
                    _stakingLoots[i].pirateItemTokenIds,
                    _stakingLoots[i].pirateItemPercentageChances
                );
        }
    } 

    function setRumTokenId(uint256 _rumTokenId) public onlyOwner
    {
        rumTokenId = _rumTokenId;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    // function getStakingForTokenId(uint256 tokenId) override internal view returns (StakingData memory) {
    //     return stakings[tokenId];
    // }

    function _stake(uint256 tokenId) internal virtual whenNotPaused notStaked(tokenId) {
        require(avatarContract.isApprovedForAll(msg.sender, address(this)), "S1");
        
        stakings[tokenId] = StakingData(block.timestamp, msg.sender);
        
        stakingsToOwner[msg.sender].push(tokenId);

        tokenIdIsTaxCollector[tokenId] = IStakingTaxContract(avatarContractAddress).isTaxCollector(tokenId); 

        if (tokenIdIsTaxCollector[tokenId])
        {
            tokenIdToTaxCollectorsTaxValue[tokenId] = taxRecpientValue;
            taxRecipientCount++;
        }
        else
        {
            // entry requirements for non tax collectors
            for (uint256 i = 0; i < pirateItemRequirements.length; i++) {
                if (pirateItemRequirements[i][1] > 0) {
                    require(pirateItems.balanceOf(msg.sender, pirateItemRequirements[i][0]) > 0, "S2");

                    pirateItems.burnPirateItem(msg.sender, pirateItemRequirements[i][0], pirateItemRequirements[i][1]);
                }
            }
        }

        avatarContract.transferFrom(msg.sender, address(this), tokenId);
    }

    function _stakeMultiple(uint256[] memory tokenIds) internal virtual whenNotPaused {
        for (uint8 i = 0; i < tokenIds.length; i++) {
            _stake(tokenIds[i]);
        }
    }

    function _unstake(uint256 tokenId) internal virtual isStaked(tokenId) {
        StakingData storage staking = stakings[tokenId];

        uint256[] storage ownerStakings = stakingsToOwner[msg.sender];

        uint16 index = 0;
        for (; index < ownerStakings.length; index++) {
            if (ownerStakings[index] == tokenId) {
                break;
            }
        }
        require(index < ownerStakings.length, "S4");

        ownerStakings[index] = ownerStakings[ownerStakings.length - 1];
        ownerStakings.pop();

        staking.owner = address(0);

        if (tokenIdIsTaxCollector[tokenId])
        {
            taxRecipientCount--;
        } 

        avatarContract.transferFrom(address(this), msg.sender, tokenId);
    }

    function durationStaked(uint256 tokenId) override public view returns (uint256) {
        StakingData storage staking = stakings[tokenId];

        if (staking.timestamp > 0)
        {
            return uint256(block.timestamp - staking.timestamp).div(stakeDurationCalc);
        }
        return 0;
    }

    function tokenOfOwnerByIndex(address owner, uint256 index) override public view returns (uint256) {
        return stakingsToOwner[owner][index];
    }

    function numberOfStakings(address owner) override public view returns (uint256) {
        return stakingsToOwner[owner].length;
    }

    function withdrawDoubloons(uint256 amount) public payable nonReentrant onlyOwner {
        require(doubloons.transferFrom(address(this), msg.sender, amount), "S5");
    }

    function _claim(uint256 tokenId) 
        virtual 
        internal
        whenNotPaused 
    {
        uint256 reward;
        uint256 notorietyPoints;
        uint256 rumCreated;
        uint256 rumBurnt;
        uint256 tax; 
    
        (reward, notorietyPoints, rumCreated, rumBurnt, tax) = _getRewards(msg.sender, tokenId);

        // always add notoriety points
        if (notorietyPoints > 0)
        {
            avatarContract.addPirateAvatarNotorietyPoints(tokenId, notorietyPoints);
        }

        _transferRewards(msg.sender, reward, rumCreated, rumBurnt);

        emit ClaimFulfilled(reward, notorietyPoints, rumCreated, rumBurnt, tax, 0, 0, false);
    }

    function _updateTaxValues(uint256 tax) virtual internal {
        taxedDoubloonsBalance += tax;

        if (taxRecipientCount > 0 && tax > 0) {
            // update tax individual balance    
            taxRecpientValue += tax.div(taxRecipientCount);
        }
    }

    function _getRewards(address account, uint256 tokenId) 
        internal 
        isStaked(tokenId)
        returns (uint256 reward, uint256 notorietyPoints, uint256 rumCreated, uint256 rumBurnt, uint256 tax)
    {
        StakingData storage staking = stakings[tokenId];

        require(staking.owner == account, "S6");

        (reward, notorietyPoints, rumCreated, rumBurnt, tax) = calculateReward(tokenId);

        _updateTaxValues(tax);

        if (tokenIdIsTaxCollector[tokenId]) {
            tokenIdToTaxCollectorsTaxValue[tokenId] = taxRecpientValue;
        }

        staking.timestamp = block.timestamp;
    }

    function calculateReward(uint256 tokenId) 
        virtual
        internal 
        view 
        isStaked(tokenId)
        returns (uint256 reward, uint256 notorietyPoints, uint256 rumCreated, uint256 rumBurnt, uint256 tax) 
    {
        uint256 balance = doubloons.balanceOf(address(this));

        uint256 duration = durationStaked(tokenId);

        if (!tokenIdIsTaxCollector[tokenId]) {
            
            uint8 level = avatarContract.getNotorietyLevel(tokenId, NOTORIETY_POINTS_TRAIT_NAME);
            
            if (duration < 1 || balance == 0) {
                reward = 0;
            }
            else {
                reward = duration.mul(rewardBase).div(rewardDenominator);
                reward = reward < balance ? reward : balance;
            }

            rumCreated = duration.mul(rumCreatedReward).div(rewardDenominator);
            rumBurnt = duration.mul(rumBurntReward).div(rewardDenominator);

            // increase based on notoriety level
            // 5 X level / 100
            // level 5 = 25% bonues
            // level 20 = 100% bonues
            notorietyPoints = duration.mul(notorietyPointsReward).div(rewardDenominator);

            notorietyPoints = notorietyPoints + notorietyPoints.mul(level).mul(notorietyPointsMultiplier).div(100);

            if (taxRate > 0) // tax payable by non tax collectors
            {
                tax = reward.mul(taxRate).div(100);

                reward = reward.sub(tax); 
            }
        }
        else // is a tax collector
        {
            // reward payable to tax collecters
            reward = taxRecpientValue - tokenIdToTaxCollectorsTaxValue[tokenId];
        }
    }

    function _claimAndUnstake(uint256 tokenId) virtual internal whenNotPaused {
        _claim(tokenId);
        _unstake(tokenId);
    }

    function _unstakeMultiple(uint256[] calldata tokenIds) virtual internal whenNotPaused {
        for (uint8 i = 0; i < tokenIds.length; i++) {
            _unstake(tokenIds[i]);
        }
    }

    function _claimAndUnstakeMultiple(uint256[] calldata tokenIds) virtual internal whenNotPaused {
        _claimMultiple(tokenIds);

        for (uint8 i = 0; i < tokenIds.length; i++) {
            _unstake(tokenIds[i]);
        }
    }

    function _transferRewards(
        address account, 
        uint256 reward,
        uint256 rumCreated,
        uint256 rumBurnt) 
        virtual 
        internal 
    {
        if (reward > 0) {
            require(doubloons.transfer(account, reward), "S7");
        }

        if (rumCreated > 0) {
            pirateItems.mintPirateItem(account, rumTokenId, rumCreated, "");
        }

        if (rumBurnt > 0) {
            uint256 rumOwned = pirateItems.balanceOf(account, rumTokenId);

            if (rumOwned > 0)
            {
                if (rumOwned < rumBurnt)
                {
                    rumBurnt = rumOwned;
                }
                pirateItems.burnPirateItem(account, rumTokenId, rumBurnt);
            }
        }
    } 

    function _claimMultiple(uint256[] calldata tokenIds) virtual internal whenNotPaused {
        
        uint256 rewardTotal;
        uint256 rumCreatedTotal;
        uint256 rumBurntTotal;
        uint256 taxTotal;
        uint256 notorietyPointsTotal;

        for (uint8 i = 0; i < tokenIds.length; i++) {
            
            uint256 reward;
            uint256 notorietyPoints;
            uint256 rumCreated;
            uint256 rumBurnt;
            uint256 tax; 

            (reward, notorietyPoints, rumCreated, rumBurnt, tax) = _getRewards(msg.sender, tokenIds[i]);
            
            rewardTotal += reward;
            rumCreatedTotal += rumCreated;
            rumBurntTotal += rumBurnt;
            taxTotal += tax;

            // always add notoriety points
            if (notorietyPoints > 0)
            {
                avatarContract.addPirateAvatarNotorietyPoints(tokenIds[i], notorietyPoints);

                notorietyPointsTotal += notorietyPoints;
            }
        }
        
        _transferRewards(msg.sender, rewardTotal, rumCreatedTotal, rumBurntTotal);

        emit ClaimFulfilled(rewardTotal, notorietyPointsTotal, rumCreatedTotal, rumBurntTotal, taxTotal, 0, 0, false);
    }

    function allStakingsOfOwner(address owner)
        override
        virtual
        public
        view
        returns (StakingValue[] memory)
    {
        StakingValue[] memory stakingValues = new StakingValue[](stakingsToOwner[owner].length);
        
        for (uint16 i = 0; i < stakingsToOwner[owner].length; i++) {
            
            uint256 tokenId = tokenOfOwnerByIndex(owner, i);

            uint256 reward;
            uint256 notorietyPoints;
            uint256 rumCreated;
            uint256 rumBurnt;
            uint256 tax; 

            (reward, notorietyPoints, rumCreated, rumBurnt, tax) = calculateReward(tokenId);

            stakingValues[i] = StakingValue
            (
                tokenId, 
                reward, 
                tax, 
                notorietyPoints, 
                rumCreated, 
                rumBurnt, 
                ERC721(avatarContractAddress).tokenURI(tokenId)
            );     
        }
        
        return stakingValues;
    }
}

