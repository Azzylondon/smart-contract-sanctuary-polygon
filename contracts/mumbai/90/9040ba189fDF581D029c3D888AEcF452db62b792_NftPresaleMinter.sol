    /**
     * @notice Buy the `amount_` of NFTs in public using registred erc20 contract to pay in tokens 
     * @dev Anyone can call this function
     * @param amount_ Total number of nft to buy
     * @param payTokenSymbol_ The erc20 token symbol being used to pay.
     * 
     * @dev The ERC Token symbol must match with the registered payment tokens  
     * @dev The  minter contract should be approved for the spend limit by the sender
     * 
     * @dev Error Details
     * - 0x1: Sender address in black list
     * - 0x2: There is no public sale running
     * - 0x3: User can't be buy more than max nft allowed
     * - 0x4: User can't buy more than total supply allocated for public sale
     * - 0x5: The provided erc token symbol doesn't match a registered token contract
     * - 0x6: Spender allowance is not sufficient for erc20 token transfer. Make sure that minter
     *   contract has been approved for the spend limit by the sender
     * - 0x7: No price configured for the specified payment token
     */
    function buyPublicByToken(
        uint256 amount_,
        string memory payTokenSymbol_
    ) external payable nonReentrant {
        
        address sender = _msgSender();
        require(blacklist[sender] == false, "0x1");
        require(
            block.timestamp > saleConfigs[SaleType.Public].startTime && 
            block.timestamp < saleConfigs[SaleType.Public].endTime, 
            "0x2");        
        require(
            (accountPublicSaleCount[sender].add(amount_)) <= saleConfigs[SaleType.Public].maxNFTAllowed, 
            "0x3");
        require(
            (publicSaleCount.add(amount_)) <= saleConfigs[SaleType.Public].supply, 
            "0x4");
        // the token symbol maps to registered payment token 
        require(
            (registeredPaymentTokens[payTokenSymbol_] != address(0)), 
            "0x5");
        
        // get the erc20 token address for the symbol & configured unit price (in token units)
        address paymentContractAddr = registeredPaymentTokens[payTokenSymbol_];
        IERC20Upgradeable erc20 = IERC20Upgradeable (paymentContractAddr);
        uint256 unitPrice = saleTokenPrices[SaleType.Public][paymentContractAddr];
        require(unitPrice > 0, "0x7");
        /**
         * Prerequisite: The  minter contract should be approved for the spend limit by the sender
         * we rely on approval to have happened as we check the spenderAllowance before proceeding with transfer
         * 
         */
        uint256 spenderAllowance = erc20.allowance(sender, address(this));
        require(
            spenderAllowance >= unitPrice.mul(amount_),
            "0x6"
        );
        // transfer the erc20 tokens to the minter contract
        erc20.transferFrom(sender, address(this), unitPrice.mul(amount_));
        // proceed with minting the tokens to sender
        _mintableNft.batchMint(sender, amount_);
        //_usedSignature[signature_] = true;
        // updates the counters for the NFT mint
        accountPublicSaleCount[sender] = accountPublicSaleCount[sender].add(amount_);
        publicSaleCount = publicSaleCount.add(amount_);

        // emits the event
        // event BuyPublicByToken(address indexed sender, address indexed erc20, uint256 amount);
        emit BuyPublicByToken(sender, paymentContractAddr, amount_);
    }

    /**********************************************
     * SELLER ROLE fuctions
     ***********************************************/
    /**
     * @notice Set parameters for the private sale
     * @dev Only calleable by SELLER role
     * @param startTime_ Start time of sale
     * @param endTime_ End time of sale
     * @param supply_ Total supply allocated for sale
     * @param price_ Price of minting one token
     * @param maxNFTAllowed_ max allowed NFT per user
     */
    function setPrivateSaleConfig(
        uint256 startTime_,
        uint256 endTime_,
        uint256 supply_,
        uint256 price_,
        uint256 maxNFTAllowed_
    ) external onlyRole(SELLER_ROLE) {
        //Config psConfig = saleConfigs[SaleType.Private];        
        saleConfigs[SaleType.Private].startTime = startTime_;
        saleConfigs[SaleType.Private].endTime = endTime_;
        saleConfigs[SaleType.Private].supply = supply_;
        saleConfigs[SaleType.Private].price = price_;
        saleConfigs[SaleType.Private].maxNFTAllowed = maxNFTAllowed_;
        emit SetSaleConfig(startTime_, endTime_, supply_, price_);
    }
    /**
     * @notice Set parameters for the public sale
     * @dev Only calleable by SELLER ROLE
     * @param startTime_ Start time of sale
     * @param endTime_ End time of sale
     * @param supply_ Total supply allocated for sale
     * @param price_ Price of minting one token
     * @param maxNFTAllowed_ max allowed NFT per user
     */
    function setPublicSaleConfig(
        uint256 startTime_,
        uint256 endTime_,
        uint256 supply_,
        uint256 price_,
        uint256 maxNFTAllowed_
    ) external  onlyRole(SELLER_ROLE) {
        //Config storage psConfig = saleConfigs[SaleType.Public];        
        saleConfigs[SaleType.Public].startTime = startTime_;
        saleConfigs[SaleType.Public].endTime = endTime_;
        saleConfigs[SaleType.Public].supply = supply_;
        saleConfigs[SaleType.Public].price = price_;
        saleConfigs[SaleType.Public].maxNFTAllowed = maxNFTAllowed_;
        emit SetSaleConfig(startTime_, endTime_, supply_, price_);
    }
    /**
     * @notice add erc20 token contract as payment type
     * @dev Only callable by SELLER ROLE
     * @param saleType_ applies to private or public sale
     * @param ercToken_ address of the ERC20 token, expected IERC20Upgradeable compliance
     * @param nftUnitPrice_ price of single NFT in specified ERC20 token units
     * 
     * @dev Error Details
     * - 0x1: sale type must be private(0) or public(1) only
     * - 0x2: ercToken address must be valid contract address
     */
    function addPaymentToken(
        SaleType saleType_, 
        IERC20Upgradeable ercToken_,
        string memory ercTokenSymbol_,
        uint256 nftUnitPrice_
    ) external  onlyRole(SELLER_ROLE) {
        require(address(ercToken_) != address(0), "0x1");
        saleTokenPrices[saleType_][address(ercToken_)] = nftUnitPrice_;
        registeredPaymentTokens[ercTokenSymbol_] = address(ercToken_);
        // emit the event
        emit SetPaymentToken(address(ercToken_), nftUnitPrice_, saleType_);
    }
    /**
     * @notice Update blacklist status of an account address
     * @dev Only callable by SELLER ROLE
     * @param account_ Address of user account
     * @param status_ Status of address in blacklist
     */
    function updateBlacklist(address account_, bool status_) external  onlyRole(SELLER_ROLE) {
        blacklist[account_] = status_;
        emit UpdateBlacklist(account_, status_);
    }

    /**********************************************
     * public (Owner Only functions)
     ***********************************************/
    /**
     * @notice Withdraw balance from this contract
     * @dev Only callable by owner
     */
    function withdraw() external onlyOwner {
        address payable sender = payable(_msgSender());
        uint256 balance = address(this).balance;
        sender.transfer(balance);

        emit Withdraw(sender, balance);
    }

    /**
     * @notice Withdraw all ERC tokens
     * @param payTokenSymbol_ The erc20 token symbol of the registred payment token contract
     * @dev Only callable by owner
     */
    function withdrawAllPaymentTokens(string memory payTokenSymbol_) external onlyOwner {
        address payable sender = payable(_msgSender());
        
        // get the erc20 token address for the symbol & configured unit price (in token units)
        address paymentContractAddr = registeredPaymentTokens[payTokenSymbol_];
        IERC20Upgradeable erc20 = IERC20Upgradeable (paymentContractAddr);

        // get the balance accumulated
        uint256 balance = erc20.balanceOf(address(this));
        // transfer to sender
        erc20.transfer(sender, balance);

        emit WithdrawPaymentToken(sender, payTokenSymbol_, balance);
    }
