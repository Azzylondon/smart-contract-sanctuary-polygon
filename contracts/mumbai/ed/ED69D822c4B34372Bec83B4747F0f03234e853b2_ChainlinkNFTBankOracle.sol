/**
 * @title Chainlink oracle that integrates with NFTBank
 * @author Honey Finance Labs
 * @custom:coauthor m4rio
 * @custom:contributor BowTiedPickle
 */
contract ChainlinkNFTBankOracle is PriceOracleI, ChainlinkClient, AccessControlEnumerable {
  using Chainlink for Chainlink.Request;
  using Strings for uint256;
  using Strings for uint64;

  /// @notice this corresponds to 1.0.0
  uint256 public constant version = 1_000_000;

  uint256 private constant ORACLE_PAYMENT_FLOOR = ((1 * LINK_DIVISIBILITY) / 100) * 10;
  uint256 private constant ORACLE_PAYMENT_INDIVIDUAL = ((1 * LINK_DIVISIBILITY) / 100) * 10;
  bytes32 public constant jobIdFloor = 0x3733353162396362656166313466636538623037313664306131353833383433;
  bytes32 public constant jobIdIndividual = 0x6361626236333237333435623438383038663231363339656632323839623637;

  bytes32 public constant REQUESTOR_ROLE = keccak256("REQUESTOR_ROLE");
  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

  /// @notice weth address
  IERC20 public immutable weth;

  /// @notice used for testing purposes as Chainlink oracle can not be tested locally
  bool public testing;

  /// @notice used to compute the floor price, it's ok to be private
  uint256 public TWAP_RANGE;

  /// @notice pausing the oracle, will throw error on any function that gets oracle data
  bool public paused;

  /// @notice price feeds for different ERC20s we support in the markets
  mapping(IERC20 => AggregatorV3Interface) public priceFeeds;

  /// @notice requests for floor price
  mapping(bytes32 => RequestFloor) public requestsFloor;

  /// @notice requests for individual token price
  mapping(bytes32 => RequestIndividual) public requestsIndividual;

  /// @notice maps floor price for collection
  mapping(address => ResponseFloor[]) public floorPrices;

  /// @notice we compute the mean based on TWAP_RANGE. Basically we compute the mean of the last `TWAP_RANGE` updates
  mapping(address => uint256) public lastMeanPerCollection;

  /// @notice maps a price for a certain token price
  mapping(address => mapping(uint256 => ResponseIndividual[])) public individualPrices;

  /// @notice variable used for testing real collections with our own testing collections
  mapping(address => address) public testingCollections;

  struct RequestFloor {
    uint96 time;
    address collection;
    string pricingAsset;
    bytes32 requestId;
  }

  struct RequestIndividual {
    uint96 time;
    address collection;
    uint256 tokenId;
    string pricingAsset; //because NFTBank asks in what coin you want the response to be, values `eth` or `usd`
    bytes32 requestId;
  }

  struct ResponseFloor {
    uint96 time;
    address collection;
    string pricingAsset;
    uint128 lastUpdated;
    uint128 estimate;
    bytes32 requestId;
  }

  struct ResponseIndividual {
    uint96 time;
    address collection;
    uint256 tokenId;
    string pricingAsset;
    uint256 estimate;
    bytes32 requestId;
  }

  struct DateAndPrice {
    uint128 date;
    uint128 price;
  }

  event ToggleCollectionString(address indexed _collectionAddress, string _collectionName);
  event RequestFloorFulfill(
    bytes32 _requestId,
    uint128 _date,
    uint128 _estimate,
    address indexed _collection,
    string indexed _pricingAsset
  );
  event RequestIndividualFulfill(
    bytes32 _requestId,
    uint256 _estimate,
    address indexed _collection,
    uint256 _tokenId,
    string indexed _pricingAsset
  );
  event RequestFloorSubmitted(bytes32 _requestId, uint256 _time, address indexed _collection, string indexed _pricingAsset);
  event RequestIndividualSubmitted(
    bytes32 _requestId,
    uint256 _time,
    address indexed _collection,
    string indexed _pricingAsset,
    uint256 _tokenId
  );
  event PriceFeedAdded(IERC20 indexed _erc20, address indexed _oracle);
  event TwapRangeChanged(uint256 _oldValue, uint256 _newValue);
  event OraclePaused(bool _paused);

  constructor(IERC20 _weth, uint256 _range) {
    if (address(_weth) == address(0)) revert WrongParams();

    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(REQUESTOR_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);
    weth = _weth;
    TWAP_RANGE = _range;
    setChainlinkToken(0x326C977E6efc84E512bB9C30f76E30c160eD06FB);
    setChainlinkOracle(0xd5821b900e44db9490da9b09541bbd027fBecF4E);
  }

  /**
   * @notice requesting the floor price of the entire collection
   * @dev must have REQUESTOR_ROLE
   * @param _collection collection name
   * @param _pricingAsset the returned price currency eth/usd
   */
  function requestFloor(address _collection, string calldata _pricingAsset) public override onlyRole(REQUESTOR_ROLE) {
    if (testing) _collection = testingCollections[_collection];
    Chainlink.Request memory req = buildChainlinkRequest(jobIdFloor, address(this), this.fulfillFloor.selector);

    req.addBytes("assetAddress", abi.encodePacked(_collection));
    req.add("pricingAsset", _pricingAsset);

    bytes32 reqId = sendOperatorRequest(req, ORACLE_PAYMENT_FLOOR);
    requestsFloor[reqId] = RequestFloor({
      time: uint96(block.timestamp),
      collection: _collection,
      pricingAsset: _pricingAsset,
      requestId: reqId
    });
    emit RequestFloorSubmitted(reqId, block.timestamp, _collection, _pricingAsset);
  }

  /**
   * @notice this just calls the requestFloor multiple times
   * @param _collections an array of collection names
   * @param _pricingAssets an array of the returned price currency eth/usd
   */
  function requestFloors(address[] calldata _collections, string[] calldata _pricingAssets) public override onlyRole(REQUESTOR_ROLE) {
    uint256 len = _collections.length;

    if (len != _pricingAssets.length) revert WrongParams();

    for (uint256 i; i < len; ++i) {
      requestFloor(_collections[i], _pricingAssets[i]);
    }
  }

  /**
   * @notice requesting a price for an individual token id within a collection
   * @dev must have REQUESTOR_ROLE
   * @param _collection collection name
   * @param _pricingAsset the returned price currency eth/usd
   * @param _tokenId the token id we request the price for
   */
  function requestIndividual(
    address _collection,
    string calldata _pricingAsset,
    uint256 _tokenId
  ) external override onlyRole(REQUESTOR_ROLE) {
    if (testing) _collection = testingCollections[_collection];
    Chainlink.Request memory req = buildChainlinkRequest(jobIdIndividual, address(this), this.fulfillEstimate.selector);

    req.addBytes("assetAddress", abi.encodePacked(_collection));
    req.addUint("nftId", _tokenId);
    req.add("pricingAsset", _pricingAsset);

    bytes32 reqId = sendOperatorRequest(req, ORACLE_PAYMENT_INDIVIDUAL);
    requestsIndividual[reqId] = RequestIndividual({
      time: uint96(block.timestamp),
      collection: _collection,
      tokenId: _tokenId,
      pricingAsset: _pricingAsset,
      requestId: reqId
    });
    emit RequestIndividualSubmitted(reqId, block.timestamp, _collection, _pricingAsset, _tokenId);
  }

  /**
   * @notice fulfillFloor callback function called by the Chainlink node to fulfill a floor request
   * @param _requestId The request ID for fulfillment.
   * @param _result The estimate for the given collection in the provided pricing asset.
   */
  function fulfillFloor(bytes32 _requestId, bytes32 _result) external recordChainlinkFulfillment(_requestId) {
    RequestFloor memory request = requestsFloor[_requestId];
    DateAndPrice memory dateAndPrice = getDateAndPrice(_result);
    floorPrices[request.collection].push(
      ResponseFloor({
        collection: request.collection,
        pricingAsset: request.pricingAsset,
        time: request.time,
        lastUpdated: dateAndPrice.date,
        estimate: dateAndPrice.price * 1e4,
        requestId: _requestId
      })
    );
    uint256 lastMean;
    uint256 length = floorPrices[request.collection].length;
    uint256 cachedTwapRange = TWAP_RANGE;

    for (uint256 i = 1; i <= cachedTwapRange; ) {
      if (length < i) {
        lastMean += floorPrices[request.collection][length - 1].estimate;
      } else {
        lastMean += floorPrices[request.collection][length - i].estimate;
      }
      unchecked {
        ++i;
      }
    }
    lastMeanPerCollection[request.collection] = lastMean / cachedTwapRange;
    emit RequestFloorFulfill(_requestId, dateAndPrice.date, dateAndPrice.price * 1e4, request.collection, request.pricingAsset);
  }

  /**
   * @notice fulfillFloor callback function called by the Chainlink node to fulfill an individual token id price request
   * @param _requestId The request ID for fulfillment.
   * @param _estimate The estimate for the given NFT ID in the provided pricing asset.
   */
  function fulfillEstimate(bytes32 _requestId, uint256 _estimate) external recordChainlinkFulfillment(_requestId) {
    RequestIndividual memory request = requestsIndividual[_requestId];
    individualPrices[request.collection][request.tokenId].push(
      ResponseIndividual({
        collection: request.collection,
        tokenId: request.tokenId,
        pricingAsset: request.pricingAsset,
        time: request.time,
        estimate: _estimate * 1e4,
        requestId: _requestId
      })
    );
    emit RequestIndividualFulfill(_requestId, _estimate, request.collection, request.tokenId, request.pricingAsset);
  }

  /**
   * @notice returns the underlying price for the floor of a collection
   * @param _collection address of the collection
   * @param _decimals adjust decimals of the returned price
   */
  function getUnderlyingFloorNFTPrice(address _collection, uint256 _decimals) external view override returns (uint128, uint128) {
    if (testing) _collection = testingCollections[_collection];
    if (paused) revert Paused();
    console.log("a1");
    uint256 lengthOfFloor = floorPrices[_collection].length;
    if (lengthOfFloor == 0) return (0, 0);
    ResponseFloor storage response = floorPrices[_collection][lengthOfFloor - 1];
    uint256 lastMean = lastMeanPerCollection[_collection];
    uint256 oracleDecimals = 18;
    console.log("a2");
    if (oracleDecimals < _decimals) {
      unchecked {
        _decimals -= oracleDecimals;
      }
      console.log("a3");
      return (uint128(lastMean * 10**_decimals), response.lastUpdated);
    } else {
      console.log("a4");
      unchecked {
        _decimals = oracleDecimals - _decimals;
      }
    }

    return (uint128(lastMean / 10**_decimals), response.lastUpdated);
  }

  /**
   * @notice returns the underlying price for an individual token id
   * @param _collection address of the collection
   * @param _tokenId token id within this collection
   * @param _decimals adjust decimals of the returned price
   */
  function getUnderlyingIndividualNFTPrice(
    address _collection,
    uint256 _tokenId,
    uint256 _decimals
  ) external view override returns (uint256) {
    if (testing) _collection = testingCollections[_collection];
    if (paused) revert Paused();

    ResponseIndividual[] storage prices = individualPrices[_collection][_tokenId];
    uint256 price = prices[prices.length - 1].estimate;

    uint256 oracleDecimals = 18;
    if (oracleDecimals < _decimals) {
      unchecked {
        _decimals -= oracleDecimals;
      }
      return price * 10**_decimals;
    } else {
      unchecked {
        _decimals = oracleDecimals - _decimals;
      }
    }
    return price / 10**_decimals;
  }

  /**
   * @notice returns the latest price for a given pair
   * @param _erc20 the erc20 we want to get the price for in USD
   * @param _decimals decimals to denote the result in
   */
  function getUnderlyingPriceInUSD(IERC20 _erc20, uint256 _decimals) public view override returns (uint256) {
    if (paused) revert Paused();

    uint256 oracleDecimals = priceFeeds[_erc20].decimals();
    (uint80 roundID, int256 price, , uint256 updatedAt, uint80 answeredInRound) = priceFeeds[_erc20].latestRoundData();

    if (updatedAt == 0 || price <= 0 || answeredInRound < roundID) {
      revert Unexpected("Price data is stale");
    }

    if (oracleDecimals < _decimals) {
      unchecked {
        _decimals -= oracleDecimals;
      }
      return uint256(price) * 10**_decimals;
    } else {
      unchecked {
        _decimals = oracleDecimals - _decimals;
      }
      return uint256(price) / 10**_decimals;
    }
  }

  /**
   * @notice get price of eth
   * @param _decimals adjust decimals of the returned price
   */
  function getEthPrice(uint256 _decimals) external view override returns (uint256) {
    if (paused) revert Paused();

    return getUnderlyingPriceInUSD(weth, _decimals);
  }

  function getDateAndPrice(bytes32 _data) private pure returns (DateAndPrice memory) {
    DateAndPrice memory dateAndPrice = DateAndPrice(uint128(bytes16(_data)), uint128(bytes16(_data << 128)));
    return dateAndPrice;
  }

  function _addPriceFeed(IERC20 _erc20, address _oracle) external {
    if (!hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) revert Unauthorized();
    priceFeeds[_erc20] = AggregatorV3Interface(_oracle);
    emit PriceFeedAdded(_erc20, _oracle);
  }

  function _setTwapRange(uint256 _newRange) external {
    if (!hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) revert Unauthorized();
    emit TwapRangeChanged(TWAP_RANGE, _newRange);
    TWAP_RANGE = _newRange;
  }

  /// TODO remove this from prod
  function _setTestingMode(bool _testing) external {
    if (!hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) revert Unauthorized();
    testing = _testing;
  }

  function _withdrawLink(uint256 _amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
    LinkTokenInterface linkToken = LinkTokenInterface(chainlinkTokenAddress());
    bool success = linkToken.transfer(msg.sender, _amount);
    if (!success) revert TransferError();
  }

  function _pauseOracle(bool _pausing) external onlyRole(PAUSER_ROLE) {
    emit OraclePaused(_pausing);
    paused = _pausing;
  }

  /**
   * @notice this needs to be removed in prod as we use this to map testing collection to real collections used by the oracle
   */
  /// TODO remove this from prod
  function _addTestingCollection(address _testCollection, address _prodCollection) external {
    if (!hasRole(DEFAULT_ADMIN_ROLE, msg.sender)) revert Unauthorized();

    testingCollections[_testCollection] = _prodCollection;
  }
}

