import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/ILeveler.sol";
import "hardhat/console.sol";

contract StakeNFTPool is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;

    /* ========== STATE VARIABLES ========== */
    // reward token
    IERC20 public rewardToken;
    // mining start block
    uint256 public startBlock;
    // mining end block
    uint256 public endBlock;
    // mining duration time
    uint256 public durationTime;
    // mining tokens created per block.
    uint256 public rewardPerBlock;
    // withdraw fee ratio (10 thousands), 100 = 1%
    uint256 public withdrawFeeRatio;
    // withdraw fee period, when duration time not greater than this, will be collection of charges
    uint256 public withdrawFeePeriod;
    // total staked amount
    uint256 public totalStaked;
    // total staked value
    uint256 public totalValue;
    // last reward per token
    uint256 public rewardPerTokenStored;
    // last update reward per token block
    uint256 public lastUpdateBlock;
    // The precision factor
    uint256 public constant PRECISION_FACTOR = 1e10;
    // administrator of this contract
    address public admin;
    // providing rewards address
    address public treasury;
    // NFT market address
    address public market;
    // staked records
    mapping(uint256 => StakeInfo) private _stakePool;
    // user to stake id mapping
    mapping(address => uint256[]) private _userStakeIds;
    // NFT whitelist
    mapping(address => bool) public tokenWhitelist;
    // Id of the next stake, the counter starts at 0
    Counters.Counter private _stakeCounter;

    struct StakeInfo {
        address token;
        uint256 tokenId;
        uint256 tokenLevel;
        uint256 tokenPrice;
        uint256 startTime;
        uint256 startBlock;
        uint256 endBlock;
        uint256 extractedRewards;
        uint256 waitExtractRewards;
        uint256 totalWithdrawFee;
        uint256 rewardPerTokenPaid;
        bool withdrawn;
    }


    /* ========== EVENTS ========== */
    event Staked(uint256 stakeId, address account, address token, uint256 tokenId, uint256 tokenPrice, uint256 durationTime);
    event Withdraw(uint256 stakeId, address account, address token, uint256 tokenId, uint256 fee);
    event GetReward(uint256 stakeId, address account, address token, uint256 tokenId, address rewardToken,uint256 reward);
    event NewStartAndEndBlock(uint256 oldStartBlock, uint256 oldEndBlock, uint256 newStartBlock, uint256 newEndBlock);
    event NewRewardPerBlock(uint256 oldRewardPerBlock, uint256 newRewardPerBlock);
    event NewWithdrawFeeRatioAndPeriod(uint256 oldWithdrawFeeRatio, uint256 oldWithdrawFeePeriod, uint256 newWithdrawFeeRatio, uint256 newWithdrawFeePeriod);
    event NewAdmin(address oldAdmin, address newAdmin);
    event NewMarket(address oldMarket, address newMarket);
    event NewTreasury(address oldTreasury, address newTreasury);
    event EmergencyWithdraw(uint256 stakeId, address account, uint256 tokenId);
    event TokenRecovery(address token, uint256 amount);
    event Whitelist(address token, bool isWhitelist);

    /* ========== CONSTRUCTOR ========== */
