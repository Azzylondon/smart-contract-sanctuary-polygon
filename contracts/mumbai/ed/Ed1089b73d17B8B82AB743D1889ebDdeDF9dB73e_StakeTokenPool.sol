    function getCurrentStakeId() public view returns (uint256) {
        return _stakeCounter.current();
    }

    function getTokenLevelPrice(address token, uint256 tokenId) public view returns (uint256 level,uint256 price) {
        bytes memory encodeData = abi.encodeWithSelector(bytes4(keccak256("leveler(address)")), token);
        (bool success, bytes memory returnData) = market.staticcall(encodeData);
        if(success){
            address leveler = abi.decode(returnData, (address));
            (level,price) = ILeveler(leveler).getLevelPriceById(tokenId);
        }
    }

    function lastBlockRewardApplicable() public view returns (uint256) {
        return Math.min(block.number, endBlock);
    }


    function rewardPerToken() public view returns (uint256) {
        if (totalStaked == 0) {
            return rewardPerTokenStored;
        }
        (,uint256 intervalBlock) = lastBlockRewardApplicable().trySub(lastUpdateBlock);
        return rewardPerTokenStored.add(intervalBlock.mul(rewardPerBlock).mul(PRECISION_FACTOR).div(totalValue));
    }

    function earned(uint256 stakeId) public view existStake(stakeId) returns (uint256){
        StakeInfo memory pool = _stakePool[stakeId];
        (,uint256 intervalReward) = rewardPerToken().trySub(pool.rewardPerTokenPaid);
        return pool.tokenPrice.mul(intervalReward).div(PRECISION_FACTOR).add(pool.waitExtractRewards);
    }


    /* ========== MUTATIVE FUNCTIONS ========== */

    function stake(address token, uint256 tokenId) external nonReentrant onlyWhitelist(token) checkOwner(token,tokenId) updateReward(0){
        require(block.number >= startBlock, "Stake: Not start");
        require(block.number < endBlock, "Stake: Ended");
        (uint256 level,uint256 price) = getTokenLevelPrice(token,tokenId);
        require(level > 0, "Stake: Unrecognized token");

        uint256 stakeId = _stakeCounter.current();
        _stakePool[stakeId] = StakeInfo(token,tokenId,level,price,block.timestamp,block.number,endBlock,0,0,0,rewardPerTokenStored,false);
        totalStaked += 1;
        totalValue += price;

        IERC721(token).transferFrom(msg.sender, address(this), tokenId);
        _userStakeIds[msg.sender].push(stakeId);
        emit Staked(stakeId, msg.sender, token, tokenId, price, durationTime);
        _stakeCounter.increment();
    }

    function withdraw(uint256 stakeId) public nonReentrant existUserStake(msg.sender,stakeId) updateReward(stakeId){
        require(!_stakePool[stakeId].withdrawn , "Withdraw: Already withdrawn");
        if(isFixedStake()){
            uint256 intervalTime = block.timestamp - _stakePool[stakeId].startTime;
            require(block.number >= _stakePool[stakeId].endBlock
                || intervalTime >= durationTime,
                "Withdraw: Deposit period not expired");
        }
        uint256 currentWithdrawFee = 0;
        StakeInfo storage pool = _stakePool[stakeId];
        if(withdrawFeePeriod >= block.timestamp - pool.startTime){
            uint256 feeRate = withdrawFeeRatio;
            currentWithdrawFee = (pool.tokenPrice * feeRate) / 10000;
            if (currentWithdrawFee > 0) {
                (,pool.waitExtractRewards) = pool.waitExtractRewards.trySub(currentWithdrawFee);
                pool.totalWithdrawFee += currentWithdrawFee;
            }
        }

        pool.withdrawn = true;
        totalStaked -= 1;
        totalValue -= pool.tokenPrice;

        IERC721(pool.token).transferFrom(address(this), msg.sender, pool.tokenId);
        emit Withdraw(stakeId,msg.sender,pool.token, pool.tokenId,currentWithdrawFee);
    }

    function exit(uint256 stakeId) external existUserStake(msg.sender,stakeId){
        if(!_stakePool[stakeId].withdrawn){
            withdraw(stakeId);
        }
        getReward(stakeId);
    }

    function getReward(uint256 stakeId) public nonReentrant existUserStake(msg.sender,stakeId) updateReward(stakeId){
        StakeInfo storage pool = _stakePool[stakeId];
        if(pool.waitExtractRewards > 0){
            uint256 rewards = pool.waitExtractRewards;
            pool.extractedRewards += rewards;
            pool.waitExtractRewards = 0;
            SafeERC20.safeTransferFrom(rewardToken, treasury, msg.sender, rewards);
            emit GetReward(stakeId, msg.sender, pool.token, pool.tokenId, address(rewardToken), rewards);
        }
    }

    function updateMarket(address _market) external onlyOwnerOrAdmin{
        require(_market != address(0), "UpdateMarket: Zero address");
        address oldMarket = market;
        market = _market;
        emit NewMarket(oldMarket, _market);
    }

    function updateTreasury(address _treasury) external onlyOwnerOrAdmin{
        require(_treasury != address(0), "UpdateTreasury: Zero address");
        address oldTreasury = treasury;
        treasury = _treasury;
        emit NewTreasury(oldTreasury, _treasury);
    }

    function emergencyWithdraw(uint256 stakeId) external nonReentrant existUserStake(msg.sender,stakeId) updateReward(stakeId){
        StakeInfo storage pool = _stakePool[stakeId];
        require(!pool.withdrawn , "Withdraw: Already withdrawn");
        pool.withdrawn = true;
        pool.waitExtractRewards = 0;
        totalStaked -= 1;
        totalValue -= pool.tokenPrice;
        IERC721(pool.token).transferFrom(address(this), msg.sender, pool.tokenId);
        emit EmergencyWithdraw(stakeId,msg.sender, pool.tokenId);
    }

    function recoverToken(address _token) external onlyOwnerOrAdmin {
        require(_token != address(rewardToken), "RecoverToken: Cannot recover reward token");
        uint256 balance = IERC20(_token).balanceOf(address(this));
        require(balance != 0, "RecoverToken: Cannot recover zero balance");
        IERC20(_token).safeTransfer(address(msg.sender), balance);
        emit TokenRecovery(_token, balance);
    }

    function updateStartAndEndBlock(uint256 _startBlock, uint256 _endBlock) external onlyOwnerOrAdmin {
        require(block.number < startBlock, "UpdateStartAndEndBlock: Pool has started");
        require(_startBlock < _endBlock, "UpdateStartAndEndBlock: New startBlock must be lower than new endBlock");
        require(block.number < _startBlock, "UpdateStartAndEndBlock: New startBlock must be higher than current block number");

        uint256 oldStartBlock = startBlock;
        uint256 oldEndBlock = endBlock;
        startBlock = _startBlock;
        endBlock = _endBlock;
        emit NewStartAndEndBlock(oldStartBlock,oldEndBlock,_startBlock, _endBlock);
    }

    function updateRewardPerBlock(uint256 _rewardPerBlock) external onlyOwnerOrAdmin updateReward(0){
        require(_rewardPerBlock > 0, "UpdateRewardPerBlock: Must exceed 0");
        uint256 oldRewardPerBlock = rewardPerBlock;
        rewardPerBlock = _rewardPerBlock;
        emit NewRewardPerBlock(oldRewardPerBlock, _rewardPerBlock);
    }

    function updateWithdrawFeeRatioAndPeriod(uint256 _withdrawFeeRatio,uint256 _withdrawFeePeriod) external onlyOwnerOrAdmin {
        require(_withdrawFeeRatio > 0, "UpdateWithdrawFeeRatio: Must exceed 0");
        require(_withdrawFeePeriod > 0, "UpdateWithdrawFeePeriod: Must exceed 0");
        uint256 oldWithdrawFeeRatio = withdrawFeeRatio;
        uint256 oldWithdrawFeePeriod = withdrawFeePeriod;
        withdrawFeeRatio = _withdrawFeeRatio;
        withdrawFeePeriod = _withdrawFeePeriod;
        emit NewWithdrawFeeRatioAndPeriod(oldWithdrawFeeRatio, oldWithdrawFeePeriod,_withdrawFeeRatio,_withdrawFeePeriod);
    }

    function setAdmin(address _admin) external onlyOwnerOrAdmin {
        require(_admin != address(0), "SetAdmin: Cannot be zero address");
        address oldAdmin = admin;
        admin = _admin;
        emit NewAdmin(oldAdmin,_admin);
    }

    function whitelist(address _token, bool _isWhitelist) public onlyOwnerOrAdmin {
        require(_token != address(0), "Whitelist: Zero address");
        tokenWhitelist[_token] = _isWhitelist;
        emit Whitelist(_token, _isWhitelist);
    }

    /* ========== MODIFIERS ========== */

    modifier updateReward(uint256 stakeId) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateBlock = lastBlockRewardApplicable();
        StakeInfo storage pool = _stakePool[stakeId];
        if (pool.startBlock > 0) {
            pool.waitExtractRewards = earned(stakeId);
            pool.rewardPerTokenPaid = rewardPerTokenStored;
        }
        _;
    }

    modifier existStake(uint256 stakeId) {
        require(_stakePool[stakeId].startBlock > 0,"CheckPool: No staked record");
        _;
    }

    modifier existUserStake(address account,uint256 stakeId) {
        require(account != address(0), "CheckPool: Zero address");
        require(_stakePool[stakeId].startBlock > 0,"CheckPool: No staked record");
        uint256[] memory userDepositIds  = _userStakeIds[account];
        require(userDepositIds.length > 0,"CheckPool: No staked record");
        bool existence = false;
        for (uint256 i = 0; i < userDepositIds.length; i++) {
            if(userDepositIds[i] == stakeId){
                existence = true;
                break;
            }
        }
        require(existence,"CheckPool: No staked record");
        _;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "Admin: caller is not the admin");
        _;
    }

    modifier onlyOwnerOrAdmin() {
        require(owner() == _msgSender() || admin == _msgSender(), "Ownable: caller is not the owner or admin");
        _;
    }

    modifier checkOwner(address token,uint256 tokenId) {
        require(token != address(0), "CheckOwner: Zero address");
        require(msg.sender == IERC721(token).ownerOf(tokenId), "CheckOwner: caller is not the NFT owner");
        _;
    }

    modifier onlyWhitelist(address _token) {
        require(tokenWhitelist[_token], "Whitelist: Only whitelist token");
        _;
    }
}

