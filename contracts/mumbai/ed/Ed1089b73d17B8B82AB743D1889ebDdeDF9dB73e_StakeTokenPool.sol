// SPDX-License-Identifier: MIT

pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IERC721mintById.sol";
import "./interfaces/ILeveler.sol";

contract Market is Ownable {
    using Counters for Counters.Counter;
    uint256 public listFeeRatio; //thousands
    uint256 public saleFeeRatio; //thousands
    address public listFeeCollector;
    address public saleFeeCollector;
    address public immutable purchaseToken;
    address public cashier;
    mapping(address => address) public leveler; //token to level

    //Id of the next listing
    Counters.Counter private _listingCounter;

    enum ListingStatus {Active, Sold, Canceled}

    struct Listing {
        ListingStatus status;
        address seller;
        address token;
        uint256 tokenId;
        uint256 price;
    }

    event Listed(
        uint256 listingId,
        address seller,
        address token,
        uint256 tokenId,
        uint256 price
    );

    event Sale(
        uint256 listingId,
        address seller,
        address buyer,
        address token,
        uint256 tokenId,
        uint256 price
    );

    event HomeSale(
        address seller,
        address buyer,
        address token,
        uint256 tokenId,
        uint256 price
    );

    event PriceChange(uint256 listingId, uint256 oldPrice, uint256 newPrice);

    event Cancel(uint256 listingId, address seller);

    event Whitelist(address token, bool isWhitelist);

    event CashierChange(address oldAddress, address newAddress);

    event ListFeeCollectorChange(address oldAddress, address newAddress);

    event SaleFeeCollectorChange(address oldAddress, address newAddress);

    event ListFeeChange(uint256 oldFee, uint256 newFee);

    event SaleFeeChange(uint256 oldFee, uint256 newFee);

    event TokenLevelSet(address token, address leveler);

    mapping(address => bool) public tokenWhitelist;
    mapping(uint256 => Listing) private _listings;
    mapping(address => uint256[]) private userListings; //user to listing id mapping

    modifier onlyWhitelist(address _token) {
        require(tokenWhitelist[_token], "Market: Only whitelist token");
        _;
    }

    constructor(
        address _purchaseToken,
        address _cashier,
        address _listFeeCollector,
        address _saleFeeCollector
    ) {
        require(msg.sender != address(0), "Market: Zero address");
        require(_purchaseToken != address(0), "Market: Zero address");

        purchaseToken = _purchaseToken;
        cashier = _cashier;
        listFeeCollector = _listFeeCollector;
        saleFeeCollector = _saleFeeCollector;
    }

    function setTokenLevel(address _token, address _leveler) public onlyOwner {
        require(_token != address(0), "Market: Zero address");
        require(_leveler != address(0), "Market: Zero address");
        require(leveler[_token] == address(0), "Market: already setted");
        leveler[_token] = _leveler;
        emit TokenLevelSet(_token, _leveler);
    }

    function setCashier(address _cashier) public onlyOwner {
        require(_cashier != address(0), "Market: Zero address");
        address oldCashier = cashier;
        cashier = _cashier;
        emit CashierChange(oldCashier, _cashier);
    }

    function setListFeeCollector(address _feeCollector) public onlyOwner {
        require(_feeCollector != address(0), "Market: Zero address");
        address oldFeeCollector = listFeeCollector;
        listFeeCollector = _feeCollector;
        emit ListFeeCollectorChange(oldFeeCollector, _feeCollector);
    }

    function setSaleFeeCollector(address _feeCollector) public onlyOwner {
        require(_feeCollector != address(0), "Market: Zero address");
        address oldFeeCollector = saleFeeCollector;
        saleFeeCollector = _feeCollector;
        emit SaleFeeCollectorChange(oldFeeCollector, _feeCollector);
    }

    function setListFeeRatio(uint256 _feeRatio) public onlyOwner {
        require(_feeRatio <= 1000, "Market: invalid feeRatio");
        uint256 oldFeeRatio = listFeeRatio;
        listFeeRatio = _feeRatio;
        emit ListFeeChange(oldFeeRatio, _feeRatio);
    }

    function setSaleFeeRatio(uint256 _feeRatio) public onlyOwner {
        require(_feeRatio <= 1000, "Market: invalid feeRatio");
        uint256 oldFeeRatio = saleFeeRatio;
        saleFeeRatio = _feeRatio;
        emit SaleFeeChange(oldFeeRatio, _feeRatio);
    }

    function whitelist(address _token, bool _isWhitelist) public onlyOwner {
        require(_token != address(0), "Market: Zero address");
        tokenWhitelist[_token] = _isWhitelist;
        emit Whitelist(_token, _isWhitelist);
    }

    function listToken(
        address token,
        uint256 tokenId,
        uint256 price
    ) public onlyWhitelist(token) {
        require(token != address(0), "Market: Zero address");

        IERC721(token).transferFrom(msg.sender, address(this), tokenId);
        _listings[_listingCounter.current()] = Listing(
            ListingStatus.Active,
            msg.sender,
            token,
            tokenId,
            price
        );
        userListings[msg.sender].push(_listingCounter.current());
        emit Listed(
            _listingCounter.current(),
            msg.sender,
            token,
            tokenId,
            price
        );
        _listingCounter.increment();
    }

    function changePrice(uint256 listingId, uint256 price) public {
        Listing storage listing = _listings[listingId];
        require(
            listing.status == ListingStatus.Active,
            "This listing is not active"
        );
        require(msg.sender == listing.seller, "Market: only seller can change");
        require(price > 0, "Market: zero price");
        uint256 oldPrice = listing.price;
        listing.price = price;

        emit PriceChange(listingId, oldPrice, listing.price);
    }

    function getMyListing(address _user)
        public
        view
        returns (uint256[] memory)
    {
        return (userListings[_user]);
    }

    function getNumOfListing() public view returns (uint256) {
        return (_listingCounter.current());
    }

    function getListing(uint256 listingId)
        public
        view
        returns (Listing memory)
    {
        return (_listings[listingId]);
    }

    function buyToken(uint256 listingId) public {
        Listing storage listing = _listings[listingId];
        require(
            listing.status == ListingStatus.Active,
            "This listing is not active"
        );
        require(
            msg.sender != listing.seller,
            "Market: Seller can not be buyer"
        );

        IERC721(listing.token).transferFrom(
            address(this),
            msg.sender,
            listing.tokenId
        );

        uint256 feeAmount = 0;
        if (listFeeRatio > 0) {
            SafeERC20.safeTransferFrom(
                IERC20(purchaseToken),
                msg.sender,
                listFeeCollector,
                (listing.price * listFeeRatio) / 1000
            );
            feeAmount = (listing.price * listFeeRatio) / 1000;
        }
        if (saleFeeRatio > 0) {
            SafeERC20.safeTransferFrom(
                IERC20(purchaseToken),
                msg.sender,
                saleFeeCollector,
                (listing.price * saleFeeRatio) / 1000
            );
            feeAmount = ((listing.price * saleFeeRatio) / 1000) + feeAmount;
        }

        SafeERC20.safeTransferFrom(
            IERC20(purchaseToken),
            msg.sender,
            listing.seller,
            listing.price - feeAmount
        );
        listing.status = ListingStatus.Sold;
        emit Sale(
            listingId,
            listing.seller,
            msg.sender,
            listing.token,
            listing.tokenId,
            listing.price
        );
    }

    function buyToMint(address token, uint256 tokenId) public {
        require(token != address(0), "Market: Zero address");
        IERC721mintById(token).safeMintById(msg.sender, tokenId);
        (, uint256 price) = ILeveler(leveler[token]).getLevelPriceById(tokenId);
        require(price > 0, "Market: Zero price");
        SafeERC20.safeTransferFrom(
            IERC20(purchaseToken),
            msg.sender,
            cashier,
            price
        );
        emit HomeSale(address(this), msg.sender, token, tokenId, price);
    }

    function cancelListing(uint256 listingId) public {
        Listing storage listing = _listings[listingId];
        require(
            msg.sender == listing.seller,
            "Market: only seller can cancel listing"
        );
        require(
            listing.status == ListingStatus.Active,
            "Market: Listing is not active"
        );
        IERC721(listing.token).transferFrom(
            address(this),
            msg.sender,
            listing.tokenId
        );
        listing.status = ListingStatus.Canceled;
        emit Cancel(listingId, msg.sender);
    }
}

