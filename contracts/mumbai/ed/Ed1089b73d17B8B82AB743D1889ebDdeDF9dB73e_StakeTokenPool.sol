// SPDX-License-Identifier: MIT

pragma solidity 0.8.4;

import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract StakeTokenPool is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;

    /* ========== STATE VARIABLES ========== */
    // stake token
    IERC20 public stakeToken;
    // reward token
    IERC20 public rewardToken;
    // mining start block
    uint256 public startBlock;
    // mining end block
    uint256 public endBlock;
    // mining duration time
    uint256 public durationTime;
    // mining pool supply
    uint256 public stakePoolSupply;
    // mining tokens created per block.
    uint256 public rewardPerBlock;
    // minimum stake amount
    uint256 public minStakeAmount;
    // minimum withdraw amount
    uint256 public minWithdrawAmount;
    // withdraw fee ratio (10 thousands), 100 = 1%
    uint256 public withdrawFeeRatio;
    // withdraw fee period, when duration time not greater than this, will be collection of charges
    uint256 public withdrawFeePeriod;
    // total staked amount
    uint256 public totalStaked;
    // last reward per token
    uint256 public rewardPerTokenStored;
    // last update reward per token block
    uint256 public lastUpdateBlock;
    // The precision factor
    uint256 public constant PRECISION_FACTOR = 1e10;
    // administrator of this contract
    address public admin;
    // providing rewards account
    address public treasury;
    // staked records
    mapping(uint256 => StakeInfo) private _stakePool;
    // user to stake id mapping
    mapping(address => uint256[]) private _userStakeIds;
    // Id of the next stake, the counter starts at 0
    Counters.Counter private _stakeCounter;

    struct StakeInfo {
        uint256 startTime;
        uint256 startBlock;
        uint256 endBlock;
        uint256 amount;
        uint256 extractedRewards;
        uint256 waitExtractRewards;
        uint256 totalWithdrawFee;
        uint256 rewardPerTokenPaid;
    }

    /* ========== EVENTS ========== */
    event Staked(uint256 stakeId, address stakeToken, address account, uint256 amount, uint256 durationTime);
    event Withdraw(uint256 stakeId, address stakeToken, address account, uint256 amount, uint256 fee);
    event GetReward(uint256 stakeId, address stakeToken, address rewardToken, address account, uint256 reward);
    event NewStartAndEndBlock(uint256 oldStartBlock, uint256 oldEndBlock, uint256 newStartBlock, uint256 newEndBlock);
    event NewRewardPerBlock(uint256 oldRewardPerBlock, uint256 newRewardPerBlock);
    event NewStakePoolSupply(uint256 oldStakePoolSupply, uint256 NewStakePoolSupply);
    event NewMinStakeAndWithdrawAmount(uint256 oldMinStakeAmount, uint256 oldMinWithdrawAmount, uint256 newMinStakeAmount, uint256 newMinWithdrawAmount);
    event NewWithdrawFeeRatioAndPeriod(uint256 oldWithdrawFeeRatio, uint256 oldWithdrawFeePeriod, uint256 newWithdrawFeeRatio, uint256 newWithdrawFeePeriod);
    event NewAdmin(address oldAdmin, address newAdmin);
    event NewTreasury(address oldTreasury, address newTreasury);
    event EmergencyWithdraw(uint256 stakeId, address account, uint256 amount);
    event TokenRecovery(address token, uint256 amount);

    /* ========== CONSTRUCTOR ========== */
    constructor(
        address[] memory addressParams,
        uint256[] memory uint256Params
    ) {
        stakeToken = IERC20(addressParams[0]);
        rewardToken = IERC20(addressParams[1]);
        admin = addressParams[2];
        treasury = addressParams[3];

        startBlock = uint256Params[0];
        endBlock = uint256Params[1];
        durationTime = uint256Params[2] * 1 days;

        require(startBlock < endBlock, "StakeTokenPool: Start block setting error");
        require(block.number < endBlock, "StakeTokenPool: End block setting error");

        stakePoolSupply = uint256Params[3];
        rewardPerBlock = uint256Params[4];
        minStakeAmount = uint256Params[5];
        minWithdrawAmount = uint256Params[6];
        withdrawFeeRatio = uint256Params[7];
        withdrawFeePeriod = uint256Params[8];
    }

    /* ========== VIEWS ========== */

    function isFixedStake() public view returns (bool){
        return durationTime > 0;
    }

    function getStakeIds(address account) public view returns (bool existence,uint256[] memory) {
        uint256[] memory stakeIds = _userStakeIds[account];
        return (stakeIds.length > 0,stakeIds);
    }

    function getStakeInfo(uint256 stakeId) public view returns (StakeInfo memory) {
        return _stakePool[stakeId];
    }

    function getCurrentStakeId() public view returns (uint256) {
        return _stakeCounter.current();
    }

    function lastBlockRewardApplicable() public view returns (uint256) {
        return Math.min(block.number, endBlock);
    }

    function rewardPerToken() public view returns (uint256) {
        if (totalStaked == 0) {
            return rewardPerTokenStored;
        }
        (,uint256 intervalBlock) = lastBlockRewardApplicable().trySub(lastUpdateBlock);
        return rewardPerTokenStored.add(intervalBlock.mul(rewardPerBlock).mul(PRECISION_FACTOR).div(totalStaked));
    }

    function earned(uint256 stakeId) public view existStake(stakeId) returns (uint256){
        StakeInfo memory pool = _stakePool[stakeId];
        (,uint256 intervalReward) = rewardPerToken().trySub(pool.rewardPerTokenPaid);
        return pool.amount.mul(intervalReward).div(PRECISION_FACTOR).add(pool.waitExtractRewards);
    }


    /* ========== MUTATIVE FUNCTIONS ========== */

    function stake(uint256 amount) external nonReentrant updateReward(0){
        require(block.number >= startBlock, "Stake: Not start");
        require(block.number < endBlock, "Stake: Ended");
        require(amount > 0, "Stake: Cannot stake 0");
        require(amount >= minStakeAmount, "Stake: must be higher than or equal to minStakeAmount");
        require(totalStaked.add(amount) <=  stakePoolSupply,"Stake: Cannot exceed stake pool supply");
        uint256 stakeId = _stakeCounter.current();
        _stakePool[stakeId] = StakeInfo(block.timestamp, block.number, endBlock, amount, 0, 0, 0,rewardPerTokenStored);
        totalStaked += amount;
        stakeToken.safeTransferFrom(msg.sender, address(this), amount);
        _userStakeIds[msg.sender].push(stakeId);
        _stakeCounter.increment();
        emit Staked(stakeId, address(stakeToken), msg.sender, amount, durationTime);
    }

    function withdraw(uint256 amount,uint256 stakeId) public nonReentrant existUserStake(msg.sender,stakeId) updateReward(stakeId){
        require(amount > 0, "Withdraw: Cannot withdraw 0");
        require(amount <= _stakePool[stakeId].amount , "Withdraw: Cannot exceed stake amount");
        require(amount <= totalStaked, "Withdraw: Cannot exceed total stake amount");
        if(_stakePool[stakeId].amount >= minWithdrawAmount){
            require(amount >= minWithdrawAmount, "Withdraw: must be higher than or equal to minWithdrawAmount");
        }
        if(isFixedStake()){
            uint256 intervalTime = block.timestamp - _stakePool[stakeId].startTime;
           require(block.number >= _stakePool[stakeId].endBlock
               || intervalTime >= durationTime,
               "Withdraw: Stake period not expired");
        }
        uint256 currentWithdrawFee = 0;
        StakeInfo storage pool = _stakePool[stakeId];
        if(withdrawFeePeriod >= block.timestamp - pool.startTime){
            uint256 feeRate = withdrawFeeRatio;
            currentWithdrawFee = (amount * feeRate) / 10000;
            if (currentWithdrawFee > 0) {
                (,pool.waitExtractRewards) = pool.waitExtractRewards.trySub(currentWithdrawFee);
                pool.totalWithdrawFee += currentWithdrawFee;
            }
        }
        pool.amount -= amount;
        totalStaked -= amount;
        stakeToken.safeTransfer(msg.sender, amount);
        emit Withdraw(stakeId,address(stakeToken), msg.sender,amount,currentWithdrawFee);
    }

    function exit(uint256 stakeId) external existUserStake(msg.sender,stakeId){
        withdraw(_stakePool[stakeId].amount,stakeId);
        getReward(stakeId);
    }

    function getReward(uint256 stakeId) public nonReentrant existUserStake(msg.sender,stakeId) updateReward(stakeId){
        StakeInfo storage pool = _stakePool[stakeId];
        if(pool.waitExtractRewards > 0){
            uint256 rewards = pool.waitExtractRewards;
            pool.extractedRewards += rewards;
            pool.waitExtractRewards = 0;
            SafeERC20.safeTransferFrom(rewardToken, treasury, msg.sender, rewards);
            emit GetReward(stakeId, address(stakeToken), address(rewardToken), msg.sender, rewards);
        }
    }

    function updateTreasury(address _treasury) external onlyOwnerOrAdmin{
        require(_treasury != address(0), "UpdateTreasury: Zero address");
        address oldTreasury = treasury;
        treasury = _treasury;
        emit NewTreasury(oldTreasury, _treasury);
    }

    function emergencyWithdraw(uint256 stakeId) external nonReentrant existUserStake(msg.sender,stakeId) updateReward(stakeId){
        StakeInfo storage pool = _stakePool[stakeId];
        require(pool.amount > 0 , "Withdraw: Already withdrawn");
        uint256 withdrawAmount = pool.amount;
        totalStaked -= withdrawAmount;
        pool.amount = 0;
        pool.waitExtractRewards = 0;
        if (withdrawAmount > 0) {
            stakeToken.safeTransfer(address(msg.sender), withdrawAmount);
        }
        emit EmergencyWithdraw(stakeId,msg.sender, withdrawAmount);
    }

    function recoverToken(address _token) external onlyOwner {
        require(_token != address(stakeToken), "RecoverToken: Cannot recover staked token");
        require(_token != address(rewardToken), "RecoverToken: Cannot recover reward token");
        uint256 balance = IERC20(_token).balanceOf(address(this));
        require(balance != 0, "RecoverToken: Cannot recover zero balance");
        IERC20(_token).safeTransfer(address(msg.sender), balance);
        emit TokenRecovery(_token, balance);
    }

    function updateStartAndEndBlock(uint256 _startBlock, uint256 _endBlock) external onlyOwnerOrAdmin {
        require(block.number < startBlock, "UpdateStartAndEndBlock: Pool has started");
        require(_startBlock < _endBlock, "UpdateStartAndEndBlock: New startBlock must be lower than new endBlock");
        require(block.number < _startBlock, "UpdateStartAndEndBlock: New startBlock must be higher than current block number");

        uint256 oldStartBlock = startBlock;
        uint256 oldEndBlock = endBlock;
        startBlock = _startBlock;
        endBlock = _endBlock;
        emit NewStartAndEndBlock(oldStartBlock,oldEndBlock,_startBlock, _endBlock);
    }

    function updateRewardPerBlock(uint256 _rewardPerBlock) external onlyOwnerOrAdmin updateReward(0){
        require(_rewardPerBlock > 0, "UpdateRewardPerBlock: Must exceed 0");
        uint256 oldRewardPerBlock = rewardPerBlock;
        rewardPerBlock = _rewardPerBlock;
        emit NewRewardPerBlock(oldRewardPerBlock, _rewardPerBlock);
    }

    function updateStakePoolSupply(uint256 _stakePoolSupply) external onlyOwnerOrAdmin {
        require(_stakePoolSupply > 0, "UpdateStakePoolSupply: Must exceed 0");
        uint256 oldStakePoolSupply = stakePoolSupply;
        stakePoolSupply = _stakePoolSupply;
        emit NewStakePoolSupply(oldStakePoolSupply, _stakePoolSupply);
    }

    function updateMinStakeAndWithdrawAmount(uint256 _minStakeAmount,uint256 _minWithdrawAmount) external onlyOwnerOrAdmin {
        require(_minStakeAmount > 0, "UpdateMinStakeAmount: Must exceed 0");
        require(_minWithdrawAmount > 0, "UpdateMinWithdrawAmount: Must exceed 0");
        uint256 oldMinStakeAmount = minStakeAmount;
        uint256 oldMinWithdrawAmount = minWithdrawAmount;
        minStakeAmount = _minStakeAmount;
        minWithdrawAmount = _minWithdrawAmount;
        emit NewMinStakeAndWithdrawAmount(oldMinStakeAmount, oldMinWithdrawAmount,_minStakeAmount,_minWithdrawAmount);
    }

    function updateWithdrawFeeRatioAndPeriod(uint256 _withdrawFeeRatio,uint256 _withdrawFeePeriod) external onlyOwnerOrAdmin {
        require(_withdrawFeeRatio > 0, "UpdateWithdrawFeeRatio: Must exceed 0");
        require(_withdrawFeePeriod > 0, "UpdateWithdrawFeePeriod: Must exceed 0");
        uint256 oldWithdrawFeeRatio = withdrawFeeRatio;
        uint256 oldWithdrawFeePeriod = withdrawFeePeriod;
        withdrawFeeRatio = _withdrawFeeRatio;
        withdrawFeePeriod = _withdrawFeePeriod;
        emit NewWithdrawFeeRatioAndPeriod(oldWithdrawFeeRatio, oldWithdrawFeePeriod,_withdrawFeeRatio,_withdrawFeePeriod);
    }

    function setAdmin(address _admin) external onlyOwnerOrAdmin {
        require(_admin != address(0), "SetAdmin: Cannot be zero address");
        address oldAdmin = admin;
        admin = _admin;
        emit NewAdmin(oldAdmin,_admin);
    }


    /* ========== MODIFIERS ========== */

    modifier updateReward(uint256 stakeId) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateBlock = lastBlockRewardApplicable();
        StakeInfo storage pool = _stakePool[stakeId];
        if (pool.startBlock > 0) {
            pool.waitExtractRewards = earned(stakeId);
            pool.rewardPerTokenPaid = rewardPerTokenStored;
        }
        _;
    }

    modifier existStake(uint256 stakeId) {
        require(_stakePool[stakeId].startBlock > 0,"CheckPool: No staked record");
        _;
    }

    modifier existUserStake(address account,uint256 stakeId) {
        require(account != address(0), "CheckPool: Zero address");
        require(_stakePool[stakeId].startBlock > 0,"CheckPool: No staked record");
        uint256[] memory userStakeIds  = _userStakeIds[account];
        require(userStakeIds.length > 0,"CheckPool: No staked record");
        bool existence = false;
        for (uint256 i = 0; i < userStakeIds.length; i++) {
            if(userStakeIds[i] == stakeId){
                existence = true;
                break;
            }
        }
        require(existence,"CheckPool: No staked record");
        _;
    }

    modifier onlyAdmin() {
        require(admin == _msgSender(), "Ownable: caller is not the admin");
        _;
    }

    modifier onlyOwnerOrAdmin() {
        require(owner() == _msgSender() || admin == _msgSender(), "Ownable: caller is not the owner or admin");
        _;
    }
}

