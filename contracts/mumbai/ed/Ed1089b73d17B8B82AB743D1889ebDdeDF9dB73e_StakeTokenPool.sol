        admin = addressParams[1];
        treasury = addressParams[2];
        market = addressParams[3];

        startBlock = uint256Params[0];
        endBlock = uint256Params[1];
        durationTime = uint256Params[2] * 1 days;

        require(startBlock < endBlock, "StakeNFTPool: Start block setting error");
        require(block.number < endBlock, "StakeNFTPool: End block setting error");

        rewardPerBlock = uint256Params[3];
        withdrawFeeRatio = uint256Params[4];
        withdrawFeePeriod = uint256Params[5];

        for(uint256 i=0;i < _whitelist.length;i++){
            tokenWhitelist[_whitelist[i]] = true;
        }
    }

    /* ========== VIEWS ========== */

    function isFixedStake() public view returns (bool){
