pragma solidity ^0.8.4;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "hardhat/console.sol";

/**
 * @title LootBox
 * LootBox - a randomized and openable lootbox of GameTokens
 */
contract LootBox is Ownable, Pausable, ReentrancyGuard, VRFConsumerBase, ERC1155Holder {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    EnumerableSet.UintSet internal tokenIdSet;

    address public nftAddress;
    address public nft1155Address;

    // Event for logging lootbox opens
    event LootBoxOpened(
        uint256 indexed tierId,
        address indexed buyer,
        uint256 boxesPurchased,
        uint256[] issuedNfts
    );
    event Warning(string message, address account);

    // Must be sorted by rarity
    enum Class {
        Common,
        Rare,
        Epic,
        Legendary,
        Divine,
        Hidden
    }
    uint256 constant NUM_CLASSES = 6;

    struct TierSettings {
        // Number of items to send per open.
        uint256 minQuantityPerOpen;
        // Number of items to send per open.
        // Set to 0 to disable this Tier.
        uint256 maxQuantityPerOpen;
        // Probability in basis points (out of 10,000) of receiving each class (descending)
        uint16[NUM_CLASSES] classProbabilities;
        // Number of lootbox
        uint256 remainingQuantity;
        // price of lootbox
        uint256 price;
        // start date
        uint256 startTime;
        // end date
        uint256 endTime;
        // currency
        IERC20 currency;
        // amount to discount
        uint256 amountToDiscount;
        // discount percent
        uint256 discountPercent;
        // max discount time
        uint256 maxDiscountTime;
    }

    TierSettings[] public tierToSettings;

    uint256 discountMax = 0;
    mapping(uint256 => uint256) tierDiscounts;
    mapping(uint256 => address[]) discountWhitelistArray;
    mapping(uint256 => mapping(address => bool)) clusterDiscountWhitelistArray;
    mapping(uint256 => mapping(address => uint256)) discountWhitelistMap;
    mapping(uint256 => mapping(address => uint256)) discountCountMap;

    mapping(uint256 => uint256[]) public classToTokenId1155s;
    mapping(uint256 => bool) public classIsPreminted;
    mapping(Class => EnumerableSet.UintSet) internal classToTokenIds;
    mapping(uint256 => Class) public tokenIdToClass;
    bytes32 internal keyHash;
    uint256 internal fee;
    uint256 public randomResult;
    uint256 constant INVERSE_BASIS_POINT = 10000;
    uint256 constant PERCENT_BASIS_POINT = 10000;

    /**
     * @dev Example constructor. Calls setTierSettings for you with
     *      sample settings
     * @param _nftAddress The address of the non-fungible/semi-fungible item contract
     *                    that you want to mint/transfer with each open
     */
    constructor(
        address _nftAddress,
        address _nft1155Address,
        address _vrfCoordinator,
        address _linkToken,
        bytes32 _keyHash,
        uint256 _linkFee
    ) VRFConsumerBase(_vrfCoordinator, _linkToken) {
        nftAddress = _nftAddress;
        nft1155Address = _nft1155Address;
        keyHash = _keyHash;
        fee = _linkFee;
        tierDiscounts[INVERSE_BASIS_POINT] = 0; // global discount
    }

    //////
    // GETTER FUNCTIONS
    //////
    /**
     * @dev Get number of tokenIds in a class.
     * @param _classId The class id
     */
    function getNumberOfTokenIds(Class _classId)
        external
        view
        returns (uint256)
    {
        uint256[] memory tokenIds = getTokensAtClass(_classId);

        return tokenIds.length;
    }

    /**
     * @dev Get tokenId in a class.
     * @param _classId The class id
     * @param _index The tokenId index
     */
    function getTokenId(Class _classId, uint256 _index)
        external
        view
        returns (uint256)
    {
        return classToTokenIds[_classId].at(_index);
    }

    /**
     * @dev Get add token in a class.
     * @param _class The class id
     */
    function getTokensAtClass(Class _class)
        public
        view
        returns (uint256[] memory)
    {
        return classToTokenIds[_class].values();
    }

    /**
     * @dev Get class probabilities.
     * @param _tierId The tier id
     * @param _classId The class id
     */
    function getClassProbabilities(uint256 _tierId, uint256 _classId)
        external
        view
        returns (uint256)
    {
        TierSettings memory settings = tierToSettings[_tierId];

        return settings.classProbabilities[_classId];
    }

    /**
     * @dev Get number of tier setting.
     */
    function getNumberOfTier() external view returns (uint256) {
        return tierToSettings.length;
    }

    /**
     * @dev Get number of whitelist.
     * @param _tierId The tier id
     */
    function getNumberOfWhitelist(uint256 _tierId)
        external
        view
        returns (uint256)
    {
        require(
            _tierId < tierToSettings.length || _tierId == PERCENT_BASIS_POINT,
            "LootBox#setDiscountWhiteList: INVALID_TIER_ID"
        );
        _tierId = tierDiscounts[_tierId];

        return discountWhitelistArray[_tierId].length;
    }

    /**
     * @dev Get whitelist information.
     * @param _tierId The tier id
     * @param _index The whitelist index
     */
    function getWhiteListInformation(uint256 _tierId, uint256 _index)
        external
        view
        returns (
            address,
            uint256,
            uint256
        )
    {
        require(
            _tierId < tierToSettings.length || _tierId == PERCENT_BASIS_POINT,
            "LootBox#setDiscountWhiteList: INVALID_TIER_ID"
        );
        _tierId = tierDiscounts[_tierId];

        address add = discountWhitelistArray[_tierId][_index];
        uint256 discountPercent = discountWhitelistMap[_tierId][add];
        uint256 discountCount = discountCountMap[_tierId][add];

        return (add, discountPercent, discountCount);
    }

    /**
     * @dev Get whitelist information by address.
     * @param _tierId The tier id
     * @param _add    The query address
     */
    function getWhiteListInformationByAddress(uint256 _tierId, address _add)
        external
        view
        returns (
            address,
            uint256,
            uint256
        )
    {
        require(
            _tierId < tierToSettings.length || _tierId == PERCENT_BASIS_POINT,
            "LootBox#setDiscountWhiteList: INVALID_TIER_ID"
        );
        _tierId = tierDiscounts[_tierId];

        uint256 discountPercent = discountWhitelistMap[_tierId][_add];
        uint256 discountCount = discountCountMap[_tierId][_add];
        return (_add, discountPercent, discountCount);
    }

    //////
    // INITIALIZATION FUNCTIONS FOR OWNER
    //////

    /**
     * @dev Set discount whitelist
     */
    function setDiscountWhiteList(
        uint256 _tierId,
        address[] memory _addresses,
        uint256[] memory _discountPercents
    ) external onlyOwner {
        require(
            _addresses.length == _discountPercents.length,
            "LootBox#setDiscountWhiteList: INVALID_LENGHT"
        );
        require(
            _tierId < tierToSettings.length || _tierId == PERCENT_BASIS_POINT,
            "LootBox#setDiscountWhiteList: INVALID_TIER_ID"
        );

        _tierId = tierDiscounts[_tierId];
        for (uint256 i = 0; i < _addresses.length; i++) {
            require(
                _discountPercents[i] < PERCENT_BASIS_POINT,
                "LootBox#setDiscountWhiteList: WRONG_DISCOUNT_PERCENT"
            );
            address add = _addresses[i];
            if (!clusterDiscountWhitelistArray[_tierId][add]) {
                clusterDiscountWhitelistArray[_tierId][add] = true;
                discountWhitelistArray[_tierId].push(add);
            }

            discountWhitelistMap[_tierId][add] = _discountPercents[i];
        }
    }

    /**
     * @dev If the tokens for some class are pre-minted and owned by the
     * contract owner, they can be used for a given class by setting them here
     */
    function setTokenIdForClass(uint256 _tokenId, Class _classId)
        public
        onlyOwner
    {
        require(tokenIdSet.contains(_tokenId), "token not in contract");
        Class _currentClass = tokenIdToClass[_tokenId];
        if (_currentClass == _classId) {
            return;
        }

        classToTokenIds[_currentClass].remove(_tokenId);
        classToTokenIds[_classId].add(_tokenId);
        tokenIdToClass[_tokenId] = _classId;
    }

    /**
     * @dev Alternate way to add token ids to a class
     * Note: resets the full list for the class instead of adding each token id
     */
    function setTokenId1155sForClass(Class _class, uint256[] memory _tokenIds)
        public
        onlyOwner
    {
        uint256 classId = uint256(_class);
        classIsPreminted[classId] = true;
        classToTokenId1155s[classId] = _tokenIds;
    }

    /**
     * @dev If the tokens for some class are pre-minted and owned by the
     * contract owner, they can be used for a given class by setting them here
     */
    function setClassForTokenId1155(uint256 _tokenId, uint256 _classId)
        public
        onlyOwner
    {
        _addTokenId1155ToClass(Class(_classId), _tokenId);
    }

    /**
     * @dev Alternate way to add token ids to a class
     * Note: resets the full list for the class instead of adding each token id
     */
    function setTokensForClass(Class _class, uint256[] memory _tokenIds)
        public
        onlyOwner
    {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            setTokenIdForClass(_tokenIds[i], _class);
        }
    }

    /**
     * @dev Set token IDs for each rarity class. Bulk version of `setTokenId1155sForClasses`
     * @param _tokenIds List of token IDs to set for each class, specified above in order
     */
    function setTokenId1155sForClasses(uint256[NUM_CLASSES] memory _tokenIds)
        public
        onlyOwner
    {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            Class class = Class(i);
            _addTokenId1155ToClass(class, _tokenIds[i]);
        }
    }

    /**
     * @dev Remove all token ids for a given class, causing it to fall back to
     * creating/minting into the nft address
     */
    function resetClass(Class _classId, bool _is1155) external onlyOwner {
        if (_is1155) {
            uint256 classId = uint256(_classId);
            delete classIsPreminted[classId];
            delete classToTokenId1155s[classId];
        } else {
            uint256[] memory tokenIdList = getTokensAtClass(_classId);
            for (uint256 i = 0; i < tokenIdList.length; i++) {
                classToTokenIds[_classId].remove(tokenIdList[i]);
                delete tokenIdToClass[tokenIdList[i]];
            }
        }
    }

    /**
     * @dev Set the settings for a particular lootbox tier
     * @param _tierId The Tier to set settings for
     * @param _quantityPerOpen Minimum and Maximum number of items to mint per open.
     *                            Set to 0 to disable this tier.
     * @param _classProbabilities Array of probabilities (basis points, so integers out of 10,000)
     *                            of receiving each class (the index in the array).
     *                            Should add up to 10k and be descending in value.
     * @param _remainingQuantity  Number of lootbox will be issue.
     * @param _discountSetting    Minimum amount to get the discount price. Discount percent. Max discount time.
     */
    function setTierSettings(
        uint256 _tierId,
        uint256[2] memory _quantityPerOpen,
        uint16[NUM_CLASSES] memory _classProbabilities,
        uint256 _remainingQuantity,
        uint256 _price,
        uint256[2] memory _dateTime, // prevent deep stack
        address _currency,
        uint256[3] memory _discountSetting
    ) external onlyOwner {
        uint256 totalProbability = 0;
        for (uint256 i = 0; i < NUM_CLASSES; i++) {
            totalProbability += _classProbabilities[i];
        }

        require(
            totalProbability == INVERSE_BASIS_POINT,
            "LootBox#setTierSettings: WRONG_TOTAL_PROBABILITY"
        );
        require(
            _quantityPerOpen[0] <= _quantityPerOpen[1],
            "LootBox#setTierSettings: WRONG_QUANTITY_RANGE"
        );
        require(
            _discountSetting[0] <= _quantityPerOpen[1],
            "LootBox#setTierSettings: WRONG_AMOUNT_TO_DISCOUNT"
        );
        require(
            _discountSetting[1] < PERCENT_BASIS_POINT,
            "LootBox#setTierSettings: WRONG_DISCOUNT_PERCENT"
        );

        TierSettings memory settings = TierSettings({
            minQuantityPerOpen: _quantityPerOpen[0],
            maxQuantityPerOpen: _quantityPerOpen[1],
            classProbabilities: _classProbabilities,
            remainingQuantity: _remainingQuantity,
            price: _price,
            startTime: _dateTime[0],
            endTime: _dateTime[1],
            currency: IERC20(_currency),
            amountToDiscount: _discountSetting[0],
            discountPercent: _discountSetting[1],
            maxDiscountTime: _discountSetting[2]
        });

        if (_tierId < tierToSettings.length) {
            tierToSettings[_tierId] = settings;
        } else {
            tierToSettings.push(settings);
            discountMax++;
            tierDiscounts[_tierId] = discountMax;
        }
    }

    /**
     * @dev Remove a tier.
     * @param _tierId The Tier to set remove
     */
    function removeTier(uint256 _tierId) public onlyOwner {
        require(
            _tierId < tierToSettings.length,
            "LootBox#removeTier: OUT_OF_RANGE"
        );

        for (uint256 i = _tierId; i < tierToSettings.length - 1; i++) {
            tierToSettings[i] = tierToSettings[i + 1];
            tierDiscounts[i] = tierDiscounts[i + 1];
        }
        tierToSettings.pop();
    }

    ///////
    // MAIN FUNCTIONS
    //////

    function deposit(uint256[] memory _tokenIds, Class[] memory _classIds)
        public
        onlyOwner
    {
        require(
            _tokenIds.length > 0 && _classIds.length == _tokenIds.length,
            "Invalid input"
        );
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _addTokenIdToClass(_tokenIds[i], _msgSender(), _classIds[i]);
        }
    }

    function deposit1155(
        uint256[] memory _tokenIds,
        uint256[] memory _amount,
        Class[] memory _classIds
    ) public onlyOwner {
        require(
            _tokenIds.length > 0 &&
                _amount.length == _tokenIds.length &&
                _classIds.length == _tokenIds.length,
            "Invalid input"
        );
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _addTokenId1155ToClass(_classIds[i], _tokenIds[i]);
            IERC1155(nft1155Address).safeTransferFrom(
                _msgSender(),
                address(this),
                _tokenIds[i],
                _amount[i],
                ""
            );
        }
    }

    /**
     * @dev Open a lootbox manually and send what's inside to _toAddress
     * Convenience method for contract owner.
     */
    function open(
        uint256 _tierId,
        address _toAddress,
        uint256 _amount,
        bool _is1155
    ) external payable {
        _mint(_tierId, _toAddress, _amount, _is1155);
    }

    /**
     * @dev estimate the cost of opening lootboxs
     */
    function estimateCost(
        uint256 _tierId,
        address _toAddress,
        uint256 _amount
    ) external view returns (uint256) {
        // Load settings for this box tier
        TierSettings memory settings = tierToSettings[_tierId];

        require(
            block.timestamp > settings.startTime,
            "LootBox#_mint: NOT_IN_DURATION"
        );
        require(
            block.timestamp < settings.endTime,
            "LootBox#_mint: NOT_IN_DURATION"
        );
        require(
            settings.minQuantityPerOpen <= _amount,
            "LootBox#_mint: OPEN_TOO_LESS"
        );
        require(
            settings.maxQuantityPerOpen > 0,
            "LootBox#_mint: TIER_NOT_ALLOWED"
        );
        require(
            settings.remainingQuantity >= _amount,
            "LootBox#_mint: NOT_ENOUGH_QUANTITY"
        );
        require(
            settings.maxQuantityPerOpen >= _amount,
            "LootBox#_mint: OPEN_TOO_MANY"
        );

        uint256 cost = settings.price * _amount;
        uint256 tierDiscountSettings = tierDiscounts[_tierId];
        uint256 globalDiscountsettings = tierDiscounts[INVERSE_BASIS_POINT];

        if (
            discountCountMap[tierDiscountSettings][_toAddress] <
            settings.maxDiscountTime
        ) {
            if (discountWhitelistMap[tierDiscountSettings][_toAddress] > 0) {
                // tier whitelist discount
                uint256 tierDiscount = settings.maxDiscountTime -
                    discountCountMap[tierDiscountSettings][_toAddress];
                if (tierDiscount > _amount) {
                    tierDiscount = _amount;
                }
                cost =
                    cost -
                    (tierDiscount *
                        settings.price *
                        discountWhitelistMap[tierDiscountSettings][
                            _toAddress
                        ]) /
                    PERCENT_BASIS_POINT;
            } else if (
                discountWhitelistMap[globalDiscountsettings][_toAddress] > 0
            ) {
                // global whitelist discount
                uint256 tierDiscount = settings.maxDiscountTime -
                    discountCountMap[tierDiscountSettings][_toAddress];
                if (tierDiscount > _amount) {
                    tierDiscount = _amount;
                }
                cost =
                    cost -
                    (tierDiscount *
                        settings.price *
                        discountWhitelistMap[globalDiscountsettings][
                            _toAddress
                        ]) /
                    PERCENT_BASIS_POINT;
            }
        }

        if (_amount >= settings.amountToDiscount) {
            // tier discount
            cost =
                (cost * (PERCENT_BASIS_POINT - settings.discountPercent)) /
                PERCENT_BASIS_POINT;
        }

        return cost;
    }

    /**
     * @dev Main minting logic for lootboxes
     */
    function _mint(
        uint256 _tierId,
        address _toAddress,
        uint256 _amount,
        bool _is1155
    ) internal whenNotPaused nonReentrant {
        // Load settings for this box tier
        TierSettings memory settings = tierToSettings[_tierId];

        require(
            block.timestamp > settings.startTime,
            "LootBox#_mint: NOT_IN_DURATION"
        );
        require(
            block.timestamp < settings.endTime,
            "LootBox#_mint: NOT_IN_DURATION"
        );
        require(
            settings.minQuantityPerOpen <= _amount,
            "LootBox#_mint: OPEN_TOO_LESS"
        );
        require(
            settings.maxQuantityPerOpen > 0,
            "LootBox#_mint: TIER_NOT_ALLOWED"
        );
        require(
            settings.remainingQuantity >= _amount,
            "LootBox#_mint: NOT_ENOUGH_QUANTITY"
        );
        require(
            settings.maxQuantityPerOpen >= _amount,
            "LootBox#_mint: OPEN_TOO_MANY"
        );

        uint256 cost = settings.price * _amount;
        uint256 tierDiscountSettings = tierDiscounts[_tierId];
        uint256 globalDiscountsettings = tierDiscounts[INVERSE_BASIS_POINT];

        if (
            discountCountMap[tierDiscountSettings][_toAddress] <
            settings.maxDiscountTime
        ) {
            if (discountWhitelistMap[tierDiscountSettings][_toAddress] > 0) {
                // tier whitelist discount
                uint256 tierDiscount = settings.maxDiscountTime -
                    discountCountMap[tierDiscountSettings][_toAddress];
                if (tierDiscount > _amount) {
                    tierDiscount = _amount;
                }
                cost =
                    cost -
                    (tierDiscount *
                        settings.price *
                        discountWhitelistMap[tierDiscountSettings][
                            _toAddress
                        ]) /
                    PERCENT_BASIS_POINT;
                discountCountMap[tierDiscountSettings][
                    _toAddress
                ] += tierDiscount;
            } else if (
                discountWhitelistMap[globalDiscountsettings][_toAddress] > 0
            ) {
                // global whitelist discount
                uint256 tierDiscount = settings.maxDiscountTime -
                    discountCountMap[tierDiscountSettings][_toAddress];
                if (tierDiscount > _amount) {
                    tierDiscount = _amount;
                }
                cost =
                    cost -
                    (tierDiscount *
                        settings.price *
                        discountWhitelistMap[globalDiscountsettings][
                            _toAddress
                        ]) /
                    PERCENT_BASIS_POINT;
                discountCountMap[tierDiscountSettings][
                    _toAddress
                ] += tierDiscount;
            }
        }

        if (_amount >= settings.amountToDiscount) {
            // tier discount
            cost =
                (cost * (PERCENT_BASIS_POINT - settings.discountPercent)) /
                PERCENT_BASIS_POINT;
        }

        if (address(settings.currency) == address(0)) {
            // native currency
            require(msg.value == cost, "LootBox#_mint: WRONG_FEE");
        } else {
            settings.currency.transferFrom(msg.sender, address(this), cost);
        }

        uint256 totalMinted = 0;

        getRandomNumber();

        uint256[] memory issuedNfts = new uint256[](_amount);

        while (totalMinted < _amount) {
            uint256 quantityOfRandomized = 1;
            Class class = _pickRandomClass(settings.classProbabilities);
            uint256[] memory nfts = _sendTokenWithClass(
                class,
                _toAddress,
                quantityOfRandomized,
                _is1155
            );
            for (uint256 j = 0; j < quantityOfRandomized; j++) {
                issuedNfts[totalMinted + j] = nfts[j];
            }
            totalMinted += quantityOfRandomized;
        }

        tierToSettings[_tierId].remainingQuantity -= totalMinted;

        // Event emissions
        emit LootBoxOpened(_tierId, _toAddress, _amount, issuedNfts);
    }

    function withdraw(address _tokenAddress) public onlyOwner {
        if (_tokenAddress == address(0)) {
            // native currency
            uint256 balance = address(this).balance;
            require(balance > 0, "LootBox#withdraw: NO_BALANCE");
            payable(msg.sender).transfer(balance);
        } else {
            IERC20 withDrawToken = IERC20(_tokenAddress);
            uint256 balance = withDrawToken.balanceOf(address(this));
            require(balance > 0, "LootBox#withdraw: NO_BALANCE");
            withDrawToken.transfer(
                msg.sender,
                withDrawToken.balanceOf(address(this))
            );
        }
    }

    function withdrawToken(
        uint256[] memory _tokenIds,
        uint256[] memory _tokenId1155s
    ) public onlyOwner {
        require(
            _tokenIds.length > 0 || _tokenId1155s.length > 0,
            "Invalid input"
        );
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            _removeTokenIdFromClass(_tokenIds[i], _msgSender());
        }

        for (uint256 i = 0; i < _tokenId1155s.length; i++) {
            uint256 balance = IERC1155(nft1155Address).balanceOf(
                address(this),
                _tokenId1155s[i]
            );
            if (balance > 0) {
                IERC1155(nft1155Address).safeTransferFrom(
                    address(this),
                    _msgSender(),
                    _tokenId1155s[i],
                    balance,
                    ""
                );
            }
        }
    }

    /////
    // HELPER FUNCTIONS
    /////

    // Returns the tokenId sent to _toAddress
    function _sendTokenWithClass(
        Class _class,
        address _toAddress,
        uint256 _amount,
        bool _is1155
    ) internal returns (uint256[] memory) {
        uint256[] memory result = new uint256[](_amount);
        for (uint256 i = 0; i < _amount; i++) {
            uint256 tokenId = _pickRandomIdForClass(_class, _is1155);
            // if loot box is in eth and bsc
            // no nft contract is deployed so the system will mint the token on polygon
            if (_is1155) {
                uint256 classId = uint256(_class);
                if (classIsPreminted[classId]) {
                    IERC1155(nft1155Address).safeTransferFrom(
                        address(this),
                        _toAddress,
                        tokenId,
                        _amount,
                        ""
                    );
                }
            } else {
                _removeTokenIdFromClass(tokenId, _toAddress);
            }
            result[i] = tokenId;
        }

        return result;
    }

    function _pickRandomClass(uint16[NUM_CLASSES] memory _classProbabilities)
        internal
        returns (Class)
    {
        uint16 value = uint16(_random().mod(INVERSE_BASIS_POINT));
        // Start at top class (length - 1)
        // skip common (0), we default to it
        for (uint256 i = _classProbabilities.length - 1; i > 0; i--) {
            uint16 probability = _classProbabilities[i];
            if (value < probability) {
                return Class(i);
            } else {
                value = value - probability;
            }
        }
        return Class.Common;
    }

    function _pickRandomIdForClass(Class _class, bool _is1155)
        internal
        returns (uint256)
    {
        uint256[] memory tokenIds;
        if (_is1155) {
            uint256 classId = uint256(_class);
            require(
                classIsPreminted[classId],
                "LootBox#_pickRandomIdForClass: NO_TOKEN_ON_PREMINTED_CLASS"
            );
