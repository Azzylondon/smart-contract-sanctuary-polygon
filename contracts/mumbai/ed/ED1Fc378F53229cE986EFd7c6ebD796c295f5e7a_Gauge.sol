    //     _writeCheckpoint(collection, _derivedBalance);
    //     _writeSupplyCheckpoint();
    //     //Voter(voter).emitDeposit(tokenId, user, amount);
    // }

    function left() external view returns (uint256) {
        if (block.timestamp >= periodFinish) return 0;
        uint256 _remaining = periodFinish - block.timestamp;
        return _remaining * rewardRate;
    }

    function notifyRewardAmount(
        bool toggle,
        address collection,
        address token,
        uint256 amount
    ) external lock {
        require(amount > 0);

        if (rewardRate == 0) _writeRewardPerTokenCheckpoint(0, block.timestamp);

        // (rewardPerTokenStored, lastUpdateTime) = _updateRewardPerToken(); //update the reward per voting power

        _safeTransferFrom(rewardToken, msg.sender, address(this), amount);

        rewardPerDerivedSupply += (amount * 1e18) / derivedSupply;

        // if (block.timestamp >= periodFinish) {
        //     rewardRate = amount / DURATION; //reward per sec
        // } else {
        //     uint256 _remaining = periodFinish - block.timestamp;
        //     uint256 _left = _remaining * rewardRate;
        //     require(amount > _left);
        //     _safeTransferFrom(rewardToken, msg.sender, address(this), amount);
        //     rewardRate = (amount + _left) / DURATION; //
        // }

        // require(rewardRate > 0);

        // uint256 balance = IERC20(rewardToken).balanceOf(address(this));
        // require(
        //     rewardRate <= balance / DURATION, //updating reward rate per second
        //     "Provided reward too high"
        // );
        // periodFinish = block.timestamp + DURATION;

        emit NotifyReward(msg.sender, rewardToken, amount);
    }

    function _safeTransfer(
        address token,
        address to,
        uint256 value
    ) internal {
        require(token.code.length > 0);
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(IERC20.transfer.selector, to, value)
        );
        require(success && (data.length == 0 || abi.decode(data, (bool))));
    }

    function _safeTransferFrom(
        address token,
        address from,
        address to,
        uint256 value
    ) internal {
        require(token.code.length > 0);
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(
                IERC20.transferFrom.selector,
                from,
                to,
                value
            )
        );
        require(success && (data.length == 0 || abi.decode(data, (bool))));
    }

    function _safeApprove(
        address token,
        address spender,
        uint256 value
    ) internal {
        require(token.code.length > 0);
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(IERC20.approve.selector, spender, value)
        );
        require(success && (data.length == 0 || abi.decode(data, (bool))));
    }
}

