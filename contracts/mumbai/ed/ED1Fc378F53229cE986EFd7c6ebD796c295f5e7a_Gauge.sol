import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./shared/interfaces/IVe.sol";
import "./shared/interfaces/ITradingFees.sol";
import "./shared/interfaces/IBribe.sol";
import "./shared/interfaces/IVoter.sol";
import "./shared/interfaces/IGaugeRegistry.sol";
import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
