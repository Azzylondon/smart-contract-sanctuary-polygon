// File contracts/MallTomorrow.sol

pragma solidity ^0.8.9;




contract MallTomorrowAlpha1 is Ownable, ReentrancyGuard {

    address private _APIAddress;

    struct User {
        address walletAddress;
        mapping(uint16 => StoreAlpha1) stores;
        uint16 storeCount;
        mapping(uint256 => ItemAlpha1) items;
        uint256 itemCount;
    }
    uint128 private _userCount = 0;

    mapping(address => User) private _users;

    modifier onlyAPI() {
        checkOnlyAPI();
        _;
    }

    function checkOnlyAPI() internal view virtual {
        require(owner() == _msgSender() || _APIAddress == _msgSender(), "Permission Error: You don't have permission to call this function");
    }

    event UserCreated (
        address indexed createdUserWalletAddress
    );

    event StoreCreated (
        address indexed storeOwner,
        StoreAlpha1 createdStore
    );

    event StoreApproved (
        address indexed storeAddress
    );

    event ItemCreated (
        address indexed itemOwnerAddress,
        ItemAlpha1 createdItem
    );

    event ItemSold (
        address indexed itemAddress
    );

    event ItemApproved (
        address indexed itemAddress
    );

    function createUser (address userWalletAddress_) onlyOwner public nonReentrant {
        require(checkUserExists(userWalletAddress_) == false, "CreateUserError: This wallet address already have an account");
        User storage newUser = _users[userWalletAddress_];
        newUser.walletAddress = userWalletAddress_;
        newUser.storeCount = 0;
        _userCount = _userCount + uint128(1);
        emit UserCreated(userWalletAddress_);
    }

    // TODO test this function
    function checkUserExists (address userWalletAddress_) view public returns(bool) {
        User storage userToCheck = _users[userWalletAddress_];
        if (userToCheck.walletAddress == address(0x0) && userToCheck.storeCount == 0) {
            return false;
        } else {
            return true;
        }
    }

    function createStore (address userWalletAddress_, string calldata name_, string calldata storeURI_) onlyOwner public nonReentrant {
        require(checkUserExists(userWalletAddress_) == true, "CreateStoreError: This wallet address doesn't have an account");

        StoreAlpha1 newStore = new StoreAlpha1(name_, userWalletAddress_, storeURI_);

        User storage user = _users[userWalletAddress_];

        user.storeCount = user.storeCount++;

        user.stores[user.storeCount] = newStore;

        emit StoreCreated(userWalletAddress_, newStore);
    }

    function approveStore (address storeAddress_) onlyOwner public {
        StoreAlpha1 store = StoreAlpha1(storeAddress_);
        store.approve();
        emit StoreApproved(storeAddress_);
    }

    function setStoreName (address storeAddress_, string memory name_) onlyOwner public {
        StoreAlpha1 store = StoreAlpha1(storeAddress_);
        store.setStoreName(name_);
    }

    function setStoreURI (address storeAddress_, string memory URI_) onlyOwner public {
        StoreAlpha1 store = StoreAlpha1(storeAddress_);
        store.setStoreURI(URI_);
    }

    function addItemToStore (address storeAddress_, address itemAddress_) onlyOwner public {
        StoreAlpha1 store = StoreAlpha1(storeAddress_);
        store.addItem(itemAddress_);
    }


    // TODO add this function
    // function burnStore (address storeAddress_) onlyOwner public {

    // }

    function createItem(
        address itemOwner_,
        uint32 price_,
        uint32 price_to_sell_,
        string calldata metadataURI_
    ) onlyOwner public nonReentrant {
        ItemAlpha1 newItem = new ItemAlpha1({
            itemOwner_: itemOwner_,
            price_: price_,
            price_to_sell_: price_to_sell_,
            metadataURI_: metadataURI_
        });

        User storage user = _users[itemOwner_];

        user.itemCount = user.itemCount++;

        user.items[user.itemCount] = newItem;

        emit ItemCreated(itemOwner_, newItem);
    }

    function approveItem(address itemAddress_) public onlyOwner {
        ItemAlpha1 item = ItemAlpha1(itemAddress_);
        item.approve();
        emit ItemApproved(itemAddress_);
    }

    function buyItem(address itemAddress_) public onlyOwner nonReentrant {
        ItemAlpha1 item = ItemAlpha1(itemAddress_);
        item.purchase_with_fiat();
        emit ItemSold(itemAddress_);
    }

    // TODO add change item status functions

    function setAPIAddress(address APIAddress_) onlyOwner public {
        _APIAddress = APIAddress_;
    }

}