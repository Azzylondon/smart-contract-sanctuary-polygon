        external
        view
        virtual
        returns (uint256)
    {
        return guardiansClasses[guardian][classID].maximumCoverage;
    }

    /**
     * @dev Returns guardian class URI.
     */
    function getURI(address guardian, uint256 classID)
        external
        view
        virtual
        returns (string memory)
    {
        return guardiansClasses[guardian][classID].uri;
    }

    function isFeeAboveMinimum(
        uint256 storageFeeAmount,
        uint256 numItems,
        address guardian,
        uint256 guardianClassIndex
    ) external view virtual returns (bool) {
        uint256 guardianClassFeeRate = getStorageFeeRate(
            guardian,
            guardianClassIndex
        );
        return
            minStorageTime <=
            StorageTimeMath.calculateAddedStorageTime(
                storageFeeAmount,
                guardianClassFeeRate,
                numItems
            );
    }

