    /*
     * @dev Externally called store item function by controller to update storage fees on token transfer.
     */
    function shiftStorageFeesOnTokenMove(
        address from,
        address to,
        uint256 id,
        uint256 amount
    ) external virtual {
        require(
            controller.isActiveCollection(_msgSender()) &&
                controller.isLinkedCollection(_msgSender()),
            "Not active 4k collection"
        );
        IERC11554K collection = IERC11554K(_msgSender());

        uint256 guardianClassFeeRate = getStorageFeeRateByCollectionItem(
            collection,
            id
        );

        uint256 storageFeeShiftAmount = StorageTimeMath
            .calculateRemainingFeeAmount(
                storageFeePaidUntil[from][collection][id],
                guardianClassFeeRate,
                amount
            );

        uint256 remainingFeeAmountFrom = StorageTimeMath
            .calculateRemainingFeeAmount(
                storageFeePaidUntil[from][collection][id],
                guardianClassFeeRate,
                collection.balanceOf(from, id)
            );

        uint256 remainingFeeAmountTo = StorageTimeMath
            .calculateRemainingFeeAmount(
                storageFeePaidUntil[to][collection][id],
                guardianClassFeeRate,
                collection.balanceOf(to, id)
            );

        //recalculate the remaining time with new params for FROM
        uint256 newAmountFrom = collection.balanceOf(from, id) - amount;
        if (newAmountFrom == 0) {
            storageFeePaidUntil[from][collection][id] = 0; //default
        } else {
            storageFeePaidUntil[from][collection][id] =
                block.timestamp +
                StorageTimeMath.calculateAddedStorageTime(
                    remainingFeeAmountFrom - storageFeeShiftAmount,
                    guardianClassFeeRate,
                    newAmountFrom
                );
        }

        //recalculate the remaining time with new params for TO
        uint256 newAmountTo = collection.balanceOf(to, id) + amount;
        storageFeePaidUntil[to][collection][id] =
            block.timestamp +
            StorageTimeMath.calculateAddedStorageTime(
                remainingFeeAmountTo + storageFeeShiftAmount,
                guardianClassFeeRate,
                newAmountTo
            );
    }

    /**
     * @dev Delegates whole minting/redemption process `to`.
     */
    function delegate(address to)
        external
        virtual
        onlyWhitelisted(_msgSender())
    {
        delegated[_msgSender()] = to;
    }

