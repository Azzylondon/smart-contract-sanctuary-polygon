        address guardian,
        uint256 maximumCoverage,
        uint256 mintingFee,
        uint256 redemptionFee,
        uint256 storageFeeRate
    )
        external
        virtual
        onlyWhitelisted(guardian)
        ifNotOwnerGuardianIsCaller(guardian)
        returns (uint256 classID)
    {
        classID = _addGuardianClass(
            guardian,
            maximumCoverage,
            mintingFee,
            redemptionFee,
            storageFeeRate
        );
    }

