pragma solidity ^0.8.14;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./ITroopCardsNFT.sol";
import {NFTType} from "./TroopCardsNFT.sol";

// At least one NFT needs to be staked before the reward could be added.
contract EpochRewardDistributorv2 is Ownable {
    using SafeERC20 for IERC20;
    bool public initialized;
    /// @dev An IERC20 token which would be used as a reward
    IERC20 immutable rewardsToken;

    ITroopCardsNFT public nftContract;
    /// @dev The address of the reward distributor
    address public rewardsDistributor;
    uint256 public previousEpoch = 0;
    uint256 public nextEpoch = 1;
    uint256 public previousEpochEligibleNFTCount = 0; // Previous because we distribute the rewards to the NFTs in the previous epoch
    /// @dev Count of the nfts eligible for the reward in next epoch
    uint256 public nextEpochEligibleNFTCount = 0;

    /// @dev The timestamp after which the next epoch would start
    uint256 public nextEpochTimeStamp;
    /// @dev The timestamp at which the previous epoch ended
    uint256 public previousEpochTimeStamp;
    uint256 public constant epochDuration = 7 days;

    /// @dev Reward rate of each epoch
    mapping(uint256 => uint256) public rewardRateForEpoch;
    /// @dev The epoch from which the NFT would be eligible for rewards
    mapping(uint256 => uint256) public nftEpochStart;
    /// @dev The epoch till which the NFT would be eligible for rewards
    mapping(uint256 => uint256) public nftEpochEnd;

    /// @dev Stores the total amount of rewards to be received by a NFT
    mapping(uint256 => uint256) public nftRewards;
    /// @dev Stores the total amount of rewards paid to the NFT
    mapping(uint256 => uint256) public nftRewardPaid;

    mapping(address => uint256) public leftOverRewards;

    NFTType public acceptableNFTType;

    /// @dev The amount of NFTs owned by a user
    mapping(address => uint256) public userNFTCount;

    event NFTStaked(address indexed userd, uint256 indexed id);
    event NFTUnstaked(address indexed userd, uint256 indexed id);
    event RewardAdded(
        uint256 indexed rewardNotifyAmount,
        address indexed rewardNotifier,
        uint256 epochStart,
        uint256 epochEnd
    );
    event RewardClaimed(address indexed user, uint256 indexed amount);
    event RewardDistributorSet(address indexed rewardsDistributor);
    event NFTTypeSet(NFTType indexed nftType);

    constructor(IERC20 rewardsToken_, NFTType nftType) {
        rewardsToken = rewardsToken_;
        acceptableNFTType = nftType;
    }

    /*************************************
        Utility Functions
     *************************************/

    /// @dev Check if we are in the new epoch & updates epoch related variables
    modifier epochCheck() {
        if (block.timestamp > nextEpochTimeStamp) {
            // Update the rewardRate to base of previous epoch
            rewardRateForEpoch[nextEpoch] = 0; // Setting to 0//rewardRateForEpoch[previousEpoch];

            previousEpoch = nextEpoch;
            nextEpoch += 1;

            previousEpochTimeStamp = nextEpochTimeStamp;
            nextEpochTimeStamp += epochDuration;

            // Updating the eligible NFTs. As the epoch has changed, the nfts in the nextEpochEligibleNFTCount has become eligible for rewards
            previousEpochEligibleNFTCount = nextEpochEligibleNFTCount;
        }
        _;
    }

    /// @dev Calculates the reward for the given NFT
    /// @param nftID The id of the NFT
    modifier calculateRewardsPerNFT(uint256 nftID) {
        // Calculate the rewards for the NFT:
        nftRewards[nftID] = calculateRewardOfNFT(nftID);
        _;
    }

    modifier nftOwnerCheck(uint256 nftID) {
        require(
            nftContract.balanceOf(msg.sender, nftID) > 0,
            "user doesn't own the nft"
        );
        _;
    }

    modifier nftTypeCheck(uint256 nftID) {
        require(getNFTType(nftID) == acceptableNFTType, "Invalid nft type");
        _;
    }

    /// @notice Fetches the NFTs owned by the user
    /// @param user Address of the user whose NFTs are to be fetched
    /// @return The NFTS owned by the user
    function getNFTIDsByOwner(address user)
        public
        view
        returns (uint256[] memory)
    {
        return _getUserStakedNFTs(user);
    }

    /// @notice Calculates the reward for the given NFT
    /// @param nftID ID of the nft for which rewards are to be calculated
    /// @return rewards Rewards earned by the NFT
    function calculateRewardOfNFT(uint256 nftID)
        internal
        view
        returns (uint256 rewards)
    {
        uint256 startEpoch_ = nftEpochStart[nftID];
        uint256 endEpoch_ = nftEpochEnd[nftID];

        // This check is for the case when endEpoch is not set since the NFT has not been unstaked
        if (endEpoch_ == 0 && nftContract.nftStakeStatus(nftID))
            endEpoch_ = previousEpoch;

        if (endEpoch_ < startEpoch_ || startEpoch_ == 0) return 0;
        for (uint256 index = startEpoch_; index <= endEpoch_; ) {
            rewards += rewardRateForEpoch[index];
            unchecked {
                ++index;
            }
        }

        // Deduct the rewards that have been paid out
        if (rewards >= nftRewardPaid[nftID])
            rewards = rewards - nftRewardPaid[nftID];
        // else rewards = 0;
    }

    function beforeTransferRewards(uint256 nftID, address user)
        external
        epochCheck
        calculateRewardsPerNFT(nftID)
    {
        require(msg.sender == address(nftContract));
        if (initialized) {
            uint256 reward = nftRewards[nftID];

            if (reward > 0) {
                nftRewards[nftID] = 0;
                leftOverRewards[user] += reward;
                nftRewardPaid[nftID] = 0;

                nftEpochStart[nftID] = 0;
                nftEpochEnd[nftID] = 0;
            }
        }
    }

    /// @notice Lets user get without claiming the reward available for all the NFTs owned by the user
    /// @return nftsOfUser The array of nftIds
    /// @return rewards The array of rewards;
    function getTotalAvailableRewards(uint256[] calldata nftsOfUser)
        external
        view
        returns (uint256[] memory, uint256[] memory)
    {
        uint256[] memory rewards = new uint256[](nftsOfUser.length);
        uint256 length = nftsOfUser.length;
        for (uint256 i = 0; i < length; ) {
            rewards[i] = calculateRewardOfNFT(nftsOfUser[i]);
            unchecked {
                ++i;
            }
        }
        return (nftsOfUser, rewards);
    }

    function getNFTType(uint256 nftId) internal pure returns (NFTType) {
        require(nftId != 0, "Invalid NFT ID");

        uint256 effectiveId = ((nftId - 1) % 111) + 1;
        if (effectiveId < 101) return NFTType.COMMON;
        if (effectiveId < 111) return NFTType.RARE;
        return NFTType.UNIQUE;
    }

    function getUserStakedNFTs(address user)
        external
        view
        returns (uint256[] memory nftsStaked)
    {
        return _getUserStakedNFTs(user);
    }

    function _getUserStakedNFTs(address user)
        internal
        view
        returns (uint256[] memory nftsStaked)
    {
        nftsStaked = new uint256[](userNFTCount[user]);
        uint256 counter;
        for (uint256 index = 1; index <= 1221; ) {
            if (getNFTType(index) == acceptableNFTType)
                if (nftContract.exists(index)) {
                    if (nftContract.balanceOf(user, index) > 0) {
                        if (nftContract.nftStakeStatus(index)) {
                            nftsStaked[counter] = index;
                            unchecked {
                                ++counter;
                            }
                        }
                    }
                }
            unchecked {
                ++index;
            }
        }
    }

    // USE IN DIRE SITUATIONS!
    function getUserNFTs(address user)
        external
        view
        returns (uint256[] memory userNfts)
    {
        return _getUserNFTs(user);
    }

    // NOT TO BE USED INTERNALLY!!
    function _getUserNFTs(address user)
        internal
        view
        returns (uint256[] memory userNfts)
    {
        userNfts = new uint256[](_balanceOfNFTs(user));
        uint256 counter;
        for (uint256 i = 1; i <= 1221; ) {
            if (getNFTType(i) == acceptableNFTType)
                if (nftContract.exists(i)) {
                    if (nftContract.balanceOf(user, i) > 0) {
                        userNfts[counter] = i;
                        unchecked {
                            ++counter;
                        }
                    }
                }
            unchecked {
                ++i;
            }
        }
    }

    // NOT TO BE USED INTERNALLY!!
    function _balanceOfNFTs(address user) internal view returns (uint256) {
        uint256[] memory nftIDs = new uint256[](1221);
        address[] memory userAddress = new address[](1221);
        for (uint256 i = 0; i < 1221; i++) {
            nftIDs[i] = i + 1;
            userAddress[i] = user;
        }
        uint256[] memory balances = nftContract.balanceOfBatch(
            userAddress,
            nftIDs
        );
        uint256 counter;
        for (uint256 i = 0; i < balances.length; ) {
            if (getNFTType(i + 1) == acceptableNFTType && balances[i] > 0) {
                ++counter;
            }
            unchecked {
                ++i;
            }
        }
        return counter;
    }

    /*************************************
        User Functions
     *************************************/

    /// @notice Unstake the NFT
    /// @param nftID ID of the nft which is to be unstaked
    function unstakeNFT(uint256 nftID) external epochCheck {
        nftContract.unstakeNFT(msg.sender, nftID);
        _unstakeNFT(nftID);
        // Removing it from nextEligible set so that it doesn't end up receiving the rewards
        nextEpochEligibleNFTCount -= 1;

        // Reduce the count of the NFT owned by the user
        userNFTCount[msg.sender] -= 1;
    }

    function unstakeAll(uint256[] calldata nftsOfUser) external epochCheck {
        nftContract.unstakeNFTs(msg.sender, nftsOfUser);

        uint256 nftCount = nftsOfUser.length;
        for (uint256 i = 0; i < nftCount; ) {
            _unstakeNFT(nftsOfUser[i]);
            unchecked {
                ++i;
            }
        }
        nextEpochEligibleNFTCount -= nftCount;
        userNFTCount[msg.sender] -= nftCount;
    }

    function _unstakeNFT(uint256 nftID) internal nftTypeCheck(nftID) {
        // Set the epoch till which the nft Will receive the reward
        nftEpochEnd[nftID] = previousEpoch;

        emit NFTUnstaked(msg.sender, nftID);
    }

    /// @notice Stakes the NFT
    /// @param nftID ID of nft to be staked
    function stakeNFT(uint256 nftID) external epochCheck {
        require(initialized, "Not initialized!");
        userNFTCount[msg.sender] += 1;
        // Updating the count for next set of eligible NFTs
        nextEpochEligibleNFTCount += 1;
        _stakeNFT(nftID);
    }

    function _stakeNFT(uint256 nftID) internal nftTypeCheck(nftID) calculateRewardsPerNFT(nftID){
        leftOverRewards[msg.sender] += nftRewards[nftID];
        nftContract.stakeNFT(msg.sender, nftID);

        // Recording the Epoch from which the NFT would be eligible for a reward
        nftEpochStart[nftID] = nextEpoch;
        nftEpochEnd[nftID] = 0;

        // Removing old rewards paid
        nftRewardPaid[nftID] = nftRewards[nftID];
        emit NFTStaked(msg.sender, nftID);
    }

    function stakeMultiple(uint256[] calldata nftIds) external epochCheck {
        require(initialized, "Not initialized!");
        uint256 nftCounts = nftIds.length;
        userNFTCount[msg.sender] += nftCounts;
        // Updating the count for next set of eligible NFTs
        nextEpochEligibleNFTCount += nftCounts;
        for (uint256 i = 0; i < nftCounts; ) {
            _stakeNFT(nftIds[i]);
            unchecked {
                ++i;
            }
        }
    }

    /// @notice Lets user claim a reward for an NFT
    /// @dev This is a function which can be called by the user to claim a reward for a single NFT
    /// @param nftID ID of the nft for which the reward is to be claimed
    /// @return reward The amount of reward received
    function getReward(uint256 nftID)
        external
        nftOwnerCheck(nftID)
        epochCheck
        calculateRewardsPerNFT(nftID)
        returns (uint256 reward)
    {
        reward = nftRewards[nftID] + leftOverRewards[msg.sender];
        require(reward > 0, "no reward to claim");

        nftRewards[nftID] = 0;
        nftRewardPaid[nftID] += reward;
        leftOverRewards[msg.sender] = 0;

        rewardsToken.safeTransfer(msg.sender, reward);

        emit RewardClaimed(msg.sender, reward);
    }

    /// @notice Lets user claim a reward for all the NFTs owned by the user
    /// @return reward The amount of reward received
    function getAllReward(uint256[] calldata nftsOfUser)
        external
        epochCheck
        returns (uint256 reward)
    {
        uint256 tempReward;
        uint256 length = nftsOfUser.length;
        uint256 nftid;
        for (uint256 i = 0; i < length; ) {
            nftid = nftsOfUser[i];
            require(
                nftContract.balanceOf(msg.sender, nftid) > 0,
                "user doesn't own the nft"
            );
            tempReward = calculateRewardOfNFT(nftid);

            reward += tempReward;
            // if (nftContract.nftStakeStatus(id))
            nftRewardPaid[nftid] += tempReward; // Since we need to keep record of award paid for staked NFT

            unchecked {
                ++i;
            }
        }

        reward += leftOverRewards[msg.sender];
        leftOverRewards[msg.sender] = 0;

        require(reward > 0, "no reward to claim");
        rewardsToken.safeTransfer(msg.sender, reward);

        emit RewardClaimed(msg.sender, reward);
    }

    function claimLeftOverReward() external {
        require(leftOverRewards[msg.sender] > 0, "no reward to claim");
        leftOverRewards[msg.sender] = 0;
        rewardsToken.safeTransfer(msg.sender, leftOverRewards[msg.sender]);
    }

    /*************************************
        Governance Functions
     *************************************/
    /// @dev Initializes the epoch timestamp variables
    function startEpoch() external onlyOwner {
        require(!initialized, "Initialized!");
        nextEpochTimeStamp = block.timestamp + epochDuration;
        previousEpochTimeStamp = block.timestamp;
        initialized = true;
    }

    /// @notice Sets the rewards distributor
    /// @param _rewardsDistributor Address to be set as a rewards distributor
    function setRewardsDistributor(address _rewardsDistributor)
        external
        onlyOwner
    {
        rewardsDistributor = _rewardsDistributor;
        emit RewardDistributorSet(_rewardsDistributor);
    }

    /// @notice Notifies the reward to the contract
    /// @dev The reward distributor should have approved the contract to transfer the reward.
    ///      The reward for current epoch needs to be added only after the beginning of next epoch
    /// @param rewardNotifyAmount Amount of reward
    function notifyRewardAmount(uint256 rewardNotifyAmount)
        external
    // epochCheck
    {
        require(msg.sender == rewardsDistributor, "Wrong caller");

        if (previousEpochEligibleNFTCount > 0) {
            // Decide reward rate
            rewardRateForEpoch[previousEpoch] +=
                rewardNotifyAmount /
                previousEpochEligibleNFTCount;

            // Transfer the reward amount to this contract
            rewardsToken.safeTransferFrom(
                msg.sender,
                address(this),
                rewardNotifyAmount
            );
            emit RewardAdded(
                rewardNotifyAmount,
                msg.sender,
                previousEpochTimeStamp,
                previousEpochTimeStamp - epochDuration
            );
        }

        if (block.timestamp > nextEpochTimeStamp) {
            // Update the rewardRate to base of previous epoch
            rewardRateForEpoch[nextEpoch] = 0; // Setting to 0//rewardRateForEpoch[previousEpoch];

            previousEpoch = nextEpoch;
            nextEpoch += 1;

            previousEpochTimeStamp = nextEpochTimeStamp;
            nextEpochTimeStamp += epochDuration;

            // Updating the eligible NFTs. As the epoch has changed, the nfts in the nextEpochEligibleNFTCount has become eligible for rewards
            previousEpochEligibleNFTCount = nextEpochEligibleNFTCount;
        }
    }

    /// @notice Sets the nft contract to be used
    /// @param nftContract_ Address of NFT contract to be used
    function setNFTContract(ITroopCardsNFT nftContract_) external onlyOwner {
        nftContract = nftContract_;
    }

    /// @notice Sets the type of nft that is acceptable in the distributor
    /// @param acceptableNFTType_ Type of NFT that would be acceptable
    function setAcceptableNFTType(NFTType acceptableNFTType_)
        external
        onlyOwner
    {
        acceptableNFTType = acceptableNFTType_;
        emit NFTTypeSet(acceptableNFTType_);
    }
}

