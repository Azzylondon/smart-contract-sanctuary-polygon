import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@opengsn/contracts/src/ERC2771Recipient.sol";
import "../xassets/IXAsset.sol";
import "hardhat/console.sol";
import "../utils/log.sol";

contract XAssetMacros is Ownable {

    uint256 constant MAX_INT = 2 ** 256 - 1;

    function investIntoXAsset(
        address xAsset,
        address token,
        uint256 amount
    //        int slippage
    ) external returns (uint256) {
        console.log("[Macro] XassetMacros address: %s", address(this));
        console.log("[Macro] Transfer tokens from %s to macros contract %s", _msgSender(), address(this));
        if (IERC20(token).allowance(_msgSender(), address(this)) < amount) {
            console.log("[Macro] Approving macros contract to spend invested token of sender: %s", _msgSender());
            IERC20(token).approve(address(this), MAX_INT);
            log.tokenValue("[Macro] new allowance", token, IERC20(token).allowance(_msgSender(), address(this)));
        }
        log.balance("[Macro] sender balance before transfer", token, _msgSender());
        require(IERC20(token).transferFrom(_msgSender(), address(this), amount));
        log.balance("[Macro] contract balance after transfer", token, address(this));
        if (IERC20(token).allowance(address(this), xAsset) < amount) {
            console.log("[Macro] Approving xAsset to spend invested token of macros: %s", address(this));
            IERC20(token).approve(xAsset, MAX_INT);
            log.tokenValue("[Macro] new xasset allowance", token, IERC20(token).allowance(address(this), xAsset));
        }
//        (bool success, bytes memory data) = xAsset.delegatecall(
//            abi.encodeWithSignature("invest(address, address, uint256, address)", from, token, amount, to)
//        );
//        return abi.decode(data, (uint256));
        return IXAsset(xAsset).invest(IERC20Metadata(token), amount);
    }

    function withdrawFromXAsset(
        address xAsset,
        uint256 shares
    ) external returns (uint256) {
        console.log("[Macro] withdrawFromXAsset: %s owner %s shares %s", xAsset, address(this), shares);

        uint256 baseTokenAmount = IXAsset(xAsset).withdraw(shares);
        console.log("[Macro] tokens withdrawn: %s, balance %s", baseTokenAmount, IXAsset(xAsset).getBaseToken().balanceOf(address(this)));
        require(IXAsset(xAsset).getBaseToken().transfer(msg.sender, baseTokenAmount), "ERC20: transfer failed");
        return baseTokenAmount;
    }


}

