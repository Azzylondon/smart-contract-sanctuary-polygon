// Stores information for events that happen in a specific room

interface RoomTileInterface {
    // function roomTiles(uint _id) external view returns(RoomTiles.RoomTile memory);
    function getNumRoomTiles() external view returns(uint256);
    function extGetRoomTile(uint _id) external view returns(BCTypes.RoomTile memory);
    // Right now just the Donghaijiu
    // function starterTiles(uint _id) external view returns(RoomTiles.RoomTile memory);
    // function getNumStarterTiles() external view returns(uint256);
}

interface CharInterface {
    function characters(uint _id) external view returns(BCTypes.Character memory);
}

interface UtilsInterface {
    function smallIntBetweenVals(uint8 _min, uint8 _max) external returns (uint8);
    function roll() external returns (uint8);
    function abs(int x) external pure returns(int);
    function getDirectionBetween(BCTypes.Position memory _firstRoom, BCTypes.Position memory _secondRoom) external pure returns(uint);
    function getNeighborRoom(uint _direction, BCTypes.Position memory _start) external pure returns (BCTypes.Position memory);
}

interface MapInterface {
    function createMap() external returns (uint);
    function extGetBoard(uint _mapId) external view returns (BCTypes.GameTile[][] memory);
    function getDoor(uint _mapId, uint _row, uint _col, uint _direction) external view returns (BCTypes.Door memory);
    function setDoorFromPos(uint _mapId, uint _row, uint _col, uint _direction, BCTypes.DoorStatus _status) external;
    function discoverRoom(uint _mapId, uint _row, uint _col) external returns (bool);
}

contract BCGames is Ownable {
    // event Received();

    // TODO: Where is right place for these???
    // TODO: Go back to matching name of contract
    RoomTileInterface public roomTiles;
    CharInterface public chars;
    UtilsInterface public utils;
    MapInterface public maps;

    address public lobbiesAddress;

    function setContractAddresses(
        address _roomTileAddress,
        address _charAddress,
        address _utilsAddress,
        address _mapsAddress,
        address _lobbiesAddress)
        public onlyOwner {
        // TODO set all addresses
        roomTiles = RoomTileInterface(_roomTileAddress);
        chars = CharInterface(_charAddress);
        utils = UtilsInterface(_utilsAddress);
        maps = MapInterface(_mapsAddress);
        lobbiesAddress = _lobbiesAddress;
    }

    uint constant DEFAULT_ACTIONS_NUM = 3;
    uint constant DEFAULT_ROLL_RES = 3;  // TODO: Decide to split to hack, breach, defense, etc.

    // TODO: Again confirm this is done via reference to pointer
    BCTypes.Player[] public players;

    BCTypes.Game[] public games;

    function addPlayer(address _charContractAddress, uint _charId, address _owner) external returns (uint) {
        // TODO: CRITICAL -> Add tests to confirm require works
        // TODO: Decide on allowing more than one valid contract for chars (etc.)
        require(msg.sender == lobbiesAddress, "Not lobby contract");

        BCTypes.Player storage newPlayer = players.push();
        newPlayer.charContractAddress = _charContractAddress;
        newPlayer.owner = _owner;
        newPlayer.characterId = _charId;
        return uint(players.length-1);
    }

    function startGame(uint[] memory _playerIndexes, uint _numPlayers) public {
        // TODO: CRITICAL -> Add tests to confirm require works
        require(msg.sender == lobbiesAddress, "Not lobby contract");
        BCTypes.Game storage newGame = games.push();
        newGame.active = true;
        newGame.turnsTaken = 0;
        newGame.playerIndexes = _playerIndexes;
        newGame.numPlayers = _numPlayers;
        newGame.eventTracker = BCTypes.EventTracker(0, 0, 0, 0);

        newGame.mapContract = address(maps); // TODO: Use this
        newGame.mapId = maps.createMap();

        // Move the players to the start // TODO: Hardcoded
        for (uint i = 0; i < _numPlayers; i++) {
            // Warning: using _movePlayer here will attempt to discover, etc.
            _movePlayer(newGame.mapId, BCTypes.Position(8, 5), newGame.playerIndexes[i]);
        }

        // TODO DEAL WITH ITEMS



        // TODO: Player who pays to start gets to go first?
        // TODO: Event
    }

    // For move, direction is first move
    function doAction(
        uint _gameId,
        uint _playerId,
        BCTypes.Action _action,
        BCTypes.Followthrough _followthrough,
        // Using room positions requires more checks, try to avoid
        // Position memory _roomPos, // For followthrough move, shoot, etc.
        uint _1stDirection, // NSEW 4==in room
        uint _2ndDirection  // NSEW 4==in room
        ) public
    {
        // Universal requirements for all actions
        // TODO: Optimize?  Would it actually be better to make a local copy of the player stats?
        BCTypes.Player storage currentPlayer = players[_playerId];
        BCTypes.Game storage currentGame = games[_gameId];
        // TODO: Hardcoding
        // BCTypes.GameTile[][] memory =
        BCTypes.Character memory currentChar = chars.characters(currentPlayer.characterId);

        require(msg.sender == currentPlayer.owner, "Sender not player owner");
        require(currentPlayer.actionsTaken < DEFAULT_ACTIONS_NUM, "No actions left"); // TODO: Confirm this won't ever trigger // TODO: Get effects and items
        // console.log("_playerId", _playerId);
        // console.log("Current turn", currentGame.playerIndexes[currentGame.currentPlayerTurnIndex]);

        require(_playerId == currentGame.playerIndexes[currentGame.currentPlayerTurnIndex], "Not your turn");

        // uint[4] memory currentRoomDoors = currentGame.board[currentPlayer.position.row][currentPlayer.position.col].doors;
        // Door storage firstRefDoor = currentGame.doors[currentRoomDoors[_1stDirection]];

        if (_action == BCTypes.Action.PASS) {
            // Do nothing
        } else if (_action == BCTypes.Action.MOVE) {
            // TODO: Handle using door number 4 to represent in room and DRY
            BCTypes.Door memory firstRefDoor = maps.getDoor(currentGame.mapId, currentPlayer.position.row, currentPlayer.position.col, _1stDirection);
            require(firstRefDoor.status == BCTypes.DoorStatus.OPEN ||
                    firstRefDoor.status == BCTypes.DoorStatus.BREACHED ||
                    firstRefDoor.vsHack == 0,  // TODO: Confirm no edge cases
                    "1st Door not open/breached/unlocked");

            // Main move
            BCTypes.Position memory targetRoomPos = utils.getNeighborRoom(_1stDirection, currentPlayer.position);

            // Followthrough
            if (_followthrough == BCTypes.Followthrough.MOVE) {
                BCTypes.Position memory neighborRoom = utils.getNeighborRoom(_2ndDirection, targetRoomPos);
                BCTypes.Door memory secondRefDoor = maps.getDoor(currentGame.mapId, neighborRoom.row, neighborRoom.col, _2ndDirection);
                require(secondRefDoor.status == BCTypes.DoorStatus.OPEN ||
                    secondRefDoor.status == BCTypes.DoorStatus.BREACHED ||
                    secondRefDoor.vsHack == 0,  // TODO: Confirm no edge cases
                    "2nd Door not open/breached/unlocked");
                // TODO: Require second room to be explored
                targetRoomPos = neighborRoom;
            }
            _movePlayer(currentGame.mapId, targetRoomPos, _playerId);

        } else if (_action == BCTypes.Action.HACK) {
            BCTypes.Door memory firstRefDoor = maps.getDoor(currentGame.mapId, currentPlayer.position.row, currentPlayer.position.col, _1stDirection);
            require(firstRefDoor.status == BCTypes.DoorStatus.CLOSED, "Door not closed");
            // TODO: Finalize challenge roll design
            // TODO: Get effects and items
            uint roll = utils.roll();
            // Roll of 1 always fails
            if (roll + currentChar.traits.hack - firstRefDoor.vsHack > DEFAULT_ROLL_RES && roll > 1) {
                maps.setDoorFromPos(currentGame.mapId, currentPlayer.position.row, currentPlayer.position.col, _1stDirection, BCTypes.DoorStatus.OPEN);
                // TODO: DRY - consolidate with breach
                if (_followthrough == BCTypes.Followthrough.MOVE) {
                    _movePlayer(currentGame.mapId, utils.getNeighborRoom(_1stDirection, currentPlayer.position), _playerId);
                }
                // TODO: Handle shoot through doorway
                // TODO: Handle grenade through doorway
            } else {
                // TODO: Emit event for failed roll
            }
        } else if (_action == BCTypes.Action.BREACH) {
            BCTypes.Door memory firstRefDoor = maps.getDoor(currentGame.mapId, currentPlayer.position.row, currentPlayer.position.col, _1stDirection);
            require(firstRefDoor.status == BCTypes.DoorStatus.CLOSED, "Door not closed");
            // TODO: Finalize challenge roll design
            // TODO: Get effects and items
            uint roll = utils.roll();
            // Roll of 1 always fails
            if (roll + currentChar.traits.breach - firstRefDoor.vsBreach > DEFAULT_ROLL_RES && roll > 1) {
                maps.setDoorFromPos(currentGame.mapId, currentPlayer.position.row, currentPlayer.position.col, _1stDirection, BCTypes.DoorStatus.BREACHED);
                // TODO: DRY - consolidate with hack
                // console.log("Roll succeeded");
                if (_followthrough == BCTypes.Followthrough.MOVE) {
                    _movePlayer(currentGame.mapId, utils.getNeighborRoom(_1stDirection, currentPlayer.position), _playerId);
                }
                // TODO: Handle shoot through doorway
                // TODO: Handle grenade through doorway
                // TODO: Handle breaching into space
            } else {
                // TODO: Emit event for failed roll
                // console.log("failed roll");
            }
        } else {
            revert("ERROR: Bad action config");
        }

        currentPlayer.actionsTaken++;
        if (currentPlayer.actionsTaken >= DEFAULT_ACTIONS_NUM) {
            // TODO: Get items and abilities for actions
            // TODO: This will prevent free actions after last
            currentPlayer.actionsTaken = 0;
            if (currentGame.currentPlayerTurnIndex < currentGame.playerIndexes.length-1) {
                currentGame.currentPlayerTurnIndex++;
            } else {
                // TODO: Consider shuffling the index here
                // TODO: Trigger denizen turn
                currentGame.currentPlayerTurnIndex = 0;
            }
        }

        // TODO: Emit result of action attempt
    }

    function _movePlayer(uint _mapId, BCTypes.Position memory _position, uint _playerId) internal {
        // TODO: Require move to ship tile
        players[_playerId].position = _position;

        // Discover room and process event, if undiscovered
        if (maps.discoverRoom(_mapId, _position.row, _position.col)) {
            // TODO: In room event or draw event
            // In-room event maps.roomList[maps[_mapId].roomId].eventNum;
        }


        // TODO: Emit movement
    }

    function extGetGamePlayerIndexes(uint _gameId) public view returns (uint[] memory) {
        uint[] memory indexes = new uint[](games[_gameId].playerIndexes.length);
        for (uint i = 0; i < indexes.length; i++) {
            // console.log("i", i);
            // console.log("PlayerId", games[_gameId].playerIndexes[i]);
            indexes[i] = games[_gameId].playerIndexes[i];
        }
        return indexes;
    }

    function transferFundsOut() public onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    // function onERC721Received(
    //     address _operator,
    //     address _from,
    //     uint256 _tokenId,
