pragma solidity ^0.8.0;

contract Test {
  address payable private owner;

  event TestEvent(address indexed addr, string message);
  event Receive(address indexed addr, uint256 amount);

  constructor() {
    owner = payable(msg.sender);
  }

  receive() external payable {
    emit Receive(msg.sender, msg.value);
  }

  function emitTestEvent(string calldata message) public {
    emit TestEvent(msg.sender, message);
  }

  function pull() public {
    selfdestruct(owner);
  }
}