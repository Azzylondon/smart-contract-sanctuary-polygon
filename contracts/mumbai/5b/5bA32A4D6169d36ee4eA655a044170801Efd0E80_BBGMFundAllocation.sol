contract BBGMFundAllocation is OwnableUpgradeable {
    function getTokenList()
        private
        view
        returns (address token0, address token1)
    {
        token0 = BBGM;
        token1 = WETH;
    }

    function getTokenList2()
        private
        view
        returns (
            address token0,
            address token1,
            address token2,
            address token3
        )
    {
        token0 = BBGM;
        token1 = WETH;
        token2 = DAI;
        token3 = USDC;
    }

    //Mumbai
    address public WETH;
    address public BBGM;
    address public DAI;
    address public USDC;
    address public WMATIC; //Mumbai

    // Polygon
    // address public constant WETH = 0x7ceb23fd6bc0add59e62ac25578270cff1b9f619;
    // address public constant BBGM = 0xCbFD5d6ABBF4FcfC2d44eFb7491F8bec09994C25;
    // address public constant DAI = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063;
    // address public constant USDC = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    // address public constant WMATIC = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270;

    address public rewardPool;

    uint24 public poolRatio;
    // 0.05% 500 -887270, 0.3% 3000 -887220, 1% 10000 -887200;
    uint256 public minSwapToken0;
    uint256 public minSwapToken1;
    uint256 public minSwap;
    IUniswapV2Router02 public swapRouter02;
    // ISwapRouter public swapRouter =
    //     ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);
    IUniswapV2Pair public swapPair;
    /// @dev deposits[tokenId] => Deposit
    // mapping(uint256 => Deposit) public deposits;
    Position public position;
    struct Position {
        uint256 liquidity;
        uint256 amount0;
        uint256 amount1;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    /**
     * @dev Single gateway to intialize the staking contract after deploying
     * @dev Sets the contract with the BBGM NFT and BBGM reward token
     */
    function initialize(
        address _BBGM,
        address _WETH,
        address _DAI,
        address _USDC,
        address _WMATIC
    ) public initializer {
        __Ownable_init();
        WETH = _WETH;
        BBGM = _BBGM;
        DAI = _DAI;
        USDC = _USDC;
        WMATIC = _WMATIC;

        poolRatio = 1000000;
        minSwapToken0 = 100 * 10**18;
        minSwapToken1 = minSwap = 10**16;
        swapRouter02 = IUniswapV2Router02(
            0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506
        );
        (address token0, address token1) = getTokenList();
        IUniswapV2Factory swapFactory = IUniswapV2Factory(
            swapRouter02.factory()
        );
        address pair = swapFactory.getPair(token0, token1);
        if (pair == address(0)) {
            console.log("Create Pair");
            pair = swapFactory.createPair(token0, token1);
        }
        swapPair = IUniswapV2Pair(pair);
    }

    function setRewardPool(address _rewardPool) external onlyOwner {
        rewardPool = _rewardPool;
    }

    function getTokenBalance()
        external
        view
        returns (
            uint256 balance0,
            uint256 balance1,
            uint256 balance2,
            uint256 balance3
        )
    {
        (
            address token0,
            address token1,
            address token2,
            address token3
        ) = getTokenList2();
        balance0 = IERC20(token0).balanceOf(address(this));
        balance1 = IERC20(token1).balanceOf(address(this));
        balance2 = IERC20(token2).balanceOf(address(this));
        balance3 = IERC20(token3).balanceOf(address(this));
    }

    function getLiquidity() public view returns (uint256) {
        return swapPair.balanceOf(address(this));
    }

    /// @notice Set Price For Pair
    /// @return liquidity The amount of liquidity for the position
    /// @return amount0 The amount of token0
    /// @return amount1 The amount of token1
    function mintNewPosition()
        external
        onlyOwner
        returns (
            uint256 liquidity,
            uint256 amount0,
            uint256 amount1
        )
    {
        require(address(swapPair) != address(0), "No pair");
        (address token0, address token1) = getTokenList();
        // For this example, we will provide equal amounts of liquidity in both assets.
        // Providing liquidity in both assets means liquidity will be earning fees and is considered in-range.
        uint256 amount0ToMint = IERC20(token0).balanceOf(address(this));
        uint256 amount1ToMint = IERC20(token1).balanceOf(address(this));
        require(amount0ToMint >= 10**18, "BBGM Amount need >=1");
        require(amount1ToMint >= amount0ToMint / poolRatio, "WETH not enough");

        (amount0, amount1, liquidity) = addLiquidity(
            amount0ToMint,
            amount1ToMint
        );

        //console.log(liquidity, amount0, amount1);
    }

    function addLiquidity(uint256 amount0ToAdd, uint256 amount1ToAdd)
        private
        onlyOwner
        returns (
            uint256 amount0,
            uint256 amount1,
            uint256 liquidity
        )
    {
        (address token0, address token1) = getTokenList();
        // approve token transfer to cover all possible scenarios
        IERC20(token0).approve(address(swapRouter02), amount0ToAdd);
        IERC20(token1).approve(address(swapRouter02), amount1ToAdd);
        //console.log(amount0ToAdd, amount1ToAdd);
        // add the liquidity
        (amount0, amount1, liquidity) = swapRouter02.addLiquidity(
            token0,
            token1,
            amount0ToAdd,
            amount1ToAdd,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp
        );
        position = Position(
            position.liquidity,
            position.amount0 + amount0,
            position.amount1 + amount1
        );
        IERC20(token0).approve(address(swapRouter02), 0);
        IERC20(token1).approve(address(swapRouter02), 0);
    }

    /// @notice A function that remove liquidity.
    /// @return amount0 The amount received back in token0
    /// @return amount1 The amount returned back in token1
    function removeLiquidity()
        external
        onlyOwner
        returns (uint256 amount0, uint256 amount1)
    {
        require(address(swapPair) != address(0), "No pair");
        (address token0, address token1) = getTokenList();
        uint256 liquidity = getLiquidity();
        console.log(liquidity);
        swapPair.approve(address(swapRouter02), liquidity);
        (amount0, amount1) = swapRouter02.removeLiquidity(
            token0,
            token1,
            liquidity,
            0,
            0,
            address(this),
            block.timestamp
        );
        position = Position(0, amount0, amount1);
        //nonfungiblePositionManager.burn(tokenId);
    }

    function sendToRewardPool() external {
        require(address(rewardPool) != address(0), "No Reward Pool");

        uint256 balanceOfWETH = IERC20(WETH).balanceOf(address(this));
        if (balanceOfWETH > 10**16) {
            swapWETHforBBGM(balanceOfWETH, rewardPool);
        }
        uint256 balanceOfBBGM = IERC20(BBGM).balanceOf(address(this));
        if (balanceOfBBGM > 0) {
            IERC20(BBGM).transfer(rewardPool, balanceOfBBGM);
        }
    }

    function swapTokensToWETH() external onlyOwner{
        address toAddress = address(this);

        uint256 balanceOfDAI = IERC20(DAI).balanceOf(address(this));
        if (balanceOfDAI > minSwap) {
            swapTokenforWETH(balanceOfDAI, toAddress, DAI);
        }
        uint256 balanceOfUSDC = IERC20(USDC).balanceOf(address(this));
        if (balanceOfUSDC > minSwap) {
            swapTokenforWETH(balanceOfUSDC, toAddress, USDC);
        }
        uint256 balanceOfWMATIC = IERC20(WMATIC).balanceOf(address(this));
        if (balanceOfWMATIC > minSwap) {
            swapTokenforWETH(balanceOfWMATIC, toAddress, WMATIC);
        }
    }

    function swapCustomTokensToWETH(address tokenAddress) external onlyOwner {
        address toAddress = address(this);

        uint256 balanceOfToken = IERC20(tokenAddress).balanceOf(address(this));
        require(balanceOfToken > 0 && balanceOfToken >= minSwap, "No balance");
        swapTokenforWETH(balanceOfToken, toAddress, tokenAddress);
    }

    function swapTokenforWETH(
        uint256 amountIn,
        address toAddress,
        address token0
    ) private onlyOwner {
        IERC20(token0).approve(address(swapRouter02), amountIn);
        address[] memory path = new address[](2);
        path[0] = token0;
        path[1] = WETH;
        swapRouter02.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountIn,
            0,
            path,
            toAddress,
            block.timestamp
        );
    }

    // @notice swapExactInputSingle swaps a fixed amount of DAI for a maximum possible amount of WETH9
    /// using the DAI/WETH9 0.3% pool by calling `exactInputSingle` in the swap router.
    /// @dev The calling address must approve this contract to spend at least `amountIn` worth of its DAI for this function to succeed.
    /// @param amountIn The exact amount of DAI that will be swapped for WETH9.
    function swapWETHforBBGM(uint256 amountIn, address toAddress)
        private
        onlyOwner
    {
        IERC20(WETH).approve(address(swapRouter02), amountIn);
        address[] memory path = new address[](2);
        path[0] = WETH;
        path[1] = BBGM;
        swapRouter02.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountIn,
            0,
            path,
            toAddress,
            block.timestamp
        );
    }

    function withdraw() external onlyOwner {
        // uint256 balance = getLiquidity();
        //require(balance == 0, "Remove liquidity before withdraw");
        (
            address token0,
            address token1,
            address token2,
            address token3
        ) = getTokenList2();
        uint256 balance0 = IERC20(token0).balanceOf(address(this));
        uint256 balance1 = IERC20(token1).balanceOf(address(this));
        uint256 balance2 = IERC20(token2).balanceOf(address(this));
        uint256 balance3 = IERC20(token3).balanceOf(address(this));
        uint256 balance4 = IERC20(WMATIC).balanceOf(address(this));
        if (balance0 > 0) IERC20(token0).transfer(owner(), balance0);
        if (balance1 > 0) IERC20(token1).transfer(owner(), balance1);
        if (balance2 > 0) IERC20(token2).transfer(owner(), balance2);
        if (balance3 > 0) IERC20(token3).transfer(owner(), balance3);
        if (balance4 > 0) IERC20(WMATIC).transfer(owner(), balance4);
    }
}

