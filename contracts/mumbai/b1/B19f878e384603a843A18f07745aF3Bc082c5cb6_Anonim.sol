import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "hardhat/console.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@chainlink/contracts/src/v0.8/KeeperCompatible.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

pragma solidity ^0.8.7;

error Anonim__NeedMoreThanZero();
error Anonim__NotSupplied();
error Anonim__CannotWithdrawMoreThanSupplied();
error Anonim__CouldNotBorrowMoreThan80Percent();
error Anonim__ThisTokenIsNotAvailable(address tokenAddress);
error Anonim__CannotWithdrawLargeAmountBeforeRepayingLoan(uint256 amount);

contract Anonim is ReentrancyGuard, KeeperCompatibleInterface, Ownable {
    address[] private allowedTokens;
    address[] private allStakers;
    address[] private allBorrowers;
    uint256 numPriceFeeds = 0;

    uint256 public immutable interval;
    uint256 public lastTimeStamp;

    // token address -> total supply of that token
    mapping(address => uint256) private s_totalSupply;

    // tokenAddress -& user address -> their staked balances
    mapping(address => mapping(address => uint256)) private s_stakedBalances;

    // tokenAddress & user adddress -> their borrowed balance
    mapping(address => mapping(address => uint256)) private s_borrowedBalances;

    // token address -> price feed address
    mapping(address => AggregatorV3Interface) private s_priceFeeds;

    // userAddress -> all of his unique staked tokens
    mapping(address => address[]) private s_stakerUniqueTokens;

    // userAddress -> all of his unique borrowed tokens
    mapping(address => address[]) private s_borrowerUniqueTokens;

    /////////////////////
    ///   Modifiers   ///
    /////////////////////

    modifier haveSupplied() {
        bool success;
        for (uint256 i = 0; i < allowedTokens.length; i++) {
            if (s_stakedBalances[allowedTokens[i]][msg.sender] > 0) {
                success = true;
            }
        }

        if (!success) {
            revert Anonim__NotSupplied();
        }
        _;
    }

    modifier notZero(uint256 amount) {
        if (amount <= 0) {
            revert Anonim__NeedMoreThanZero();
        }
        _;
    }

    modifier isTokenAllowed(address tokenAddress) {
        bool execute;
        for (uint256 i = 0; i < allowedTokens.length; i++) {
            if (allowedTokens[i] == tokenAddress) {
                execute = true;
            }
        }

        if (!execute) {
            revert Anonim__ThisTokenIsNotAvailable(tokenAddress);
        }
        _;
    }

    /////////////////////
    ///   Functions   ///
    /////////////////////

    constructor(
        address[] memory _allowedTokens,
        address[] memory priceFeeds,
        uint256 updateInterval
    ) {
        allowedTokens = _allowedTokens;

        for (uint256 i = 0; i < _allowedTokens.length; i++) {
            s_priceFeeds[_allowedTokens[i]] = AggregatorV3Interface(priceFeeds[i]);
            numPriceFeeds += 1;
        }

        // s_priceFeeds[_allowedTokens[0]] = AggregatorV3Interface(priceFeeds[0]);
        // s_priceFeeds[_allowedTokens[1]] = AggregatorV3Interface(priceFeeds[1]);
        // s_priceFeeds[_allowedTokens[2]] = AggregatorV3Interface(priceFeeds[2]);
        // s_priceFeeds[_allowedTokens[3]] = AggregatorV3Interface(priceFeeds[3]);
        // s_priceFeeds[_allowedTokens[4]] = AggregatorV3Interface(priceFeeds[4]);
        // s_priceFeeds[_allowedTokens[5]] = AggregatorV3Interface(priceFeeds[5]);

        interval = updateInterval;
        lastTimeStamp = block.timestamp;
    }

    function supply(address tokenAddress, uint256 amount)
        external
        payable
        isTokenAllowed(tokenAddress)
        notZero(amount)
    {
        /* 1. Take their permission to use their token
         * 2. If `approve` then stake their token
         * 3. add to total supply
         * 4. give them APY
         */
        IERC20(tokenAddress).transferFrom(msg.sender, address(this), amount);
        s_totalSupply[tokenAddress] += amount;
        s_stakedBalances[tokenAddress][msg.sender] += amount;
        addStaker(msg.sender);
        addUniqueToken(s_stakerUniqueTokens[msg.sender], tokenAddress);
    }

    function withdraw(address tokenAddress, uint256 amount)
        external
        payable
        haveSupplied
        notZero(amount)
    {
        // they are withdrawing
        if (amount > s_stakedBalances[tokenAddress][msg.sender]) {
            revert Anonim__CannotWithdrawMoreThanSupplied();
        }

        uint256 availableWithdrawBalance = getTotalStakeValue(msg.sender) -
            ((uint256(100) / uint256(80)) * getTotalBorrowValue(msg.sender));

        uint256 totalAmountValue = amount * uint256(getLatestPrice(tokenAddress));

        if (totalAmountValue >= availableWithdrawBalance) {
            revert Anonim__CannotWithdrawLargeAmountBeforeRepayingLoan(amount);
        }

        removeStakerAndUniqueToken(tokenAddress, msg.sender);

        s_stakedBalances[tokenAddress][msg.sender] -= amount;
        s_totalSupply[tokenAddress] -= amount;
        IERC20(tokenAddress).transfer(msg.sender, amount);
    }

    function removeStakerAndUniqueToken(address tokenAddress, address userAddress) private {
        if (s_stakerUniqueTokens[userAddress].length == 0) {
            remove(allStakers, userAddress);
        }

        if (s_stakedBalances[tokenAddress][userAddress] <= 0) {
            remove(s_stakerUniqueTokens[userAddress], tokenAddress);
        }
    }

    function borrow(address tokenAddress, uint256 amount)
        external
        payable
        isTokenAllowed(tokenAddress)
        haveSupplied
        notZero(amount)
    {
        uint256 maxBorrow = (uint256(80) / uint256(100)) * getTotalStakeValue(msg.sender);
        uint256 totalAmountValue = amount * uint256(getLatestPrice(tokenAddress));

        if (totalAmountValue > maxBorrow) {
            revert Anonim__CouldNotBorrowMoreThan80Percent();
        }

        addBorrower(msg.sender);
        addUniqueToken(s_borrowerUniqueTokens[msg.sender], tokenAddress);
        s_borrowedBalances[tokenAddress][msg.sender] += amount;
        s_totalSupply[tokenAddress] -= amount;
        IERC20(tokenAddress).transfer(msg.sender, amount);
    }

    function repay(address tokenAddress, uint256 amount) external payable notZero(amount) {
        IERC20(tokenAddress).transferFrom(msg.sender, address(this), amount);
        s_borrowedBalances[tokenAddress][msg.sender] -= amount;
        s_totalSupply[msg.sender] += amount;

        removeBorrowerAndUniqueToken(tokenAddress, msg.sender);
    }

    function removeBorrowerAndUniqueToken(address tokenAddress, address userAddress) private {
        if (s_borrowerUniqueTokens[userAddress].length == 0) {
            remove(allBorrowers, userAddress);
        }

        if (s_borrowedBalances[tokenAddress][userAddress] <= 0) {
            remove(s_borrowerUniqueTokens[userAddress], tokenAddress);
        }
    }

    function chargeAPY() private {
        // APY - 2000 %
        // 10 -> 210 after a year
        // 100 -> 2000 after a year
        // 100 -> 166.6666 / month
        // 100 -> 5.555555 / day
        // 100 -> 0.231 / hour
        // 100 -> 0.0038580 / min
        // 100 -> 0.0000643004 / sec (0.0000634196)
        // 100,000 -> .06

        for (uint256 i = 0; i < allBorrowers.length; i++) {
            for (
                uint256 index = 0;
                index < s_borrowerUniqueTokens[allBorrowers[i]].length;
                index++
            ) {
                s_borrowedBalances[s_borrowerUniqueTokens[allBorrowers[i]][index]][
                    allBorrowers[i]
                ] += (s_borrowedBalances[s_borrowerUniqueTokens[allBorrowers[i]][index]][
                    allBorrowers[i]
                ] * ((uint256(1929) / uint256(20000000)) / 100)); //0.000003215 %
            }
        }
    }

    function rewardAPY() private {
        // 0.00,00,03,215 % per sec
        // APY - 2000 %
        // 0.001929 / 30 sec
        // 0.00009645 % / 30 sec
        for (uint256 i = 0; i < allStakers.length; i++) {
            for (uint256 index = 0; index < s_stakerUniqueTokens[allStakers[i]].length; index++) {
                s_stakedBalances[s_stakerUniqueTokens[allStakers[i]][index]][
                    allStakers[i]
                ] += (s_stakedBalances[s_stakerUniqueTokens[allStakers[i]][index]][allStakers[i]] *
                    ((uint256(1929) / uint256(20000000)) / 100)); // giving APY / 30 sec
            }
        }
    }

    function checkUpkeep(
        bytes calldata /* checkData */
    )
        external
        view
        override
        returns (
            bool upkeepNeeded,
            bytes memory /* performData */
        )
    {
        upkeepNeeded = (block.timestamp - lastTimeStamp) > interval;
        // We don't use the checkData in this example. The checkData is defined when the Upkeep was registered.
    }

    function performUpkeep(
        bytes calldata /* performData */
    ) external override {
        //We highly recommend revalidating the upkeep in the performUpkeep function
        if ((block.timestamp - lastTimeStamp) > interval) {
            rewardAPY();
            chargeAPY();
            lastTimeStamp = block.timestamp;
        }
    }

    function addAllowedTokens(address tokenAddress) external onlyOwner {
        allowedTokens.push(tokenAddress);
    }

    ////////////////////////
    // Helper functions ////
    ///////////////////////

    function addUniqueToken(address[] storage uniqueTokenArray, address tokenAddress) private {
        if (uniqueTokenArray.length == 0) {
            uniqueTokenArray.push(tokenAddress);
        } else {
            bool add = true;
            for (uint256 i = 0; i < uniqueTokenArray.length; i++) {
                if (uniqueTokenArray[i] == tokenAddress) {
                    add = false;
                }
            }
            if (add) {
                uniqueTokenArray.push(tokenAddress);
            }
        }
    }

    function addStaker(address userAddress) private {
        if (allStakers.length == 0) {
            allStakers.push(userAddress);
        } else {
            bool add = true;
            for (uint256 i = 0; i < allStakers.length; i++) {
                if (allStakers[i] == userAddress) {
                    add = false;
                }
            }
            if (add) {
                allStakers.push(userAddress);
            }
        }
    }

    function addBorrower(address userAddress) private {
        if (allBorrowers.length == 0) {
            allBorrowers.push(userAddress);
        } else {
            bool add = true;
            for (uint256 i = 0; i < allBorrowers.length; i++) {
                if (allBorrowers[i] == userAddress) {
                    add = false;
                }
            }
            if (add) {
                allBorrowers.push(userAddress);
            }
        }
    }

    // function removeBorrower(address userAddress) private {
    //     if (s_borrowerUniqueTokens[userAddress].length == 0) {
    //         // remove userAddress from allstakers
    //         remove(allBorrowers, userAddress);
    //     }
    // }

    function remove(address[] storage array, address removingAddress) private {
        for (uint256 i = 0; i < array.length; i++) {
            if (array[i] == removingAddress) {
                array[i] = array[array.length - 1];
                array.pop();
            }
        }
    }

    ////////////////////////////
    ///   getter functions   ///
    ////////////////////////////

    function getTotalSupply(address tokenAddress) external view returns (uint256) {
        return s_totalSupply[tokenAddress];
    }

    function getStakedBalance(address tokenAddress, address userAddress)
        external
        view
        returns (uint256)
    {
        return s_stakedBalances[tokenAddress][userAddress];
    }

    function getBorrowedBalance(address tokenAddress, address userAddress)
        external
        view
        returns (uint256)
    {
        return s_borrowedBalances[tokenAddress][userAddress];
    }

    function getMaxBorrow(address tokenAddress) external view returns (uint256) {
        return (uint256(80) / uint256(100)) * s_stakedBalances[tokenAddress][msg.sender];
    }

    function getLatestPrice(address tokenAddress) public view returns (int256) {
        (, int256 price, , , ) = s_priceFeeds[tokenAddress].latestRoundData();
        return price;
    }

    function getNumPriceFeeds() public view returns (uint256) {
        return numPriceFeeds;
    }

    function getTotalStakeValue(address userAddress) public view returns (uint256) {
        uint256 totalValue = 0;
        for (uint256 i = 0; i < allowedTokens.length; i++) {
            totalValue += (uint256(getLatestPrice(allowedTokens[i])) *
                s_stakedBalances[allowedTokens[i]][userAddress]);
        }
        return totalValue;
    }

    function getTotalBorrowValue(address userAddress) public view returns (uint256) {
        uint256 totalValue = 0;
        for (uint256 i = 0; i < allowedTokens.length; i++) {
            totalValue += (uint256(getLatestPrice(allowedTokens[i])) *
                s_borrowedBalances[allowedTokens[i]][userAddress]);
        }
        return totalValue;
    }

    function getAllowedTokens() public view returns (address[] memory) {
        return allowedTokens;
    }

    function getAllStakers() external view returns (address[] memory) {
        return allStakers;
    }

    function getUniueStakedTokens(address userAddress) external view returns (address[] memory) {
        return s_stakerUniqueTokens[userAddress];
    }

    function getUniueBorrowedTokens(address userAddress) external view returns (address[] memory) {
        return s_borrowerUniqueTokens[userAddress];
    }
}

