pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./interfaces/IForum.sol";
import "./interfaces/IProposal.sol";
import "./interfaces/ICrucibleTokens.sol";
import "./interfaces/IReputation.sol";

contract Proposal is IProposal, AccessControl {
    bytes32 public constant DAO_ADMIN_ROLE = keccak256("DAO_ADMIN_ROLE");
    bytes32 public constant DAO_BANNED_ROLE = keccak256("DAO_BANNED_ROLE");

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setRoleAdmin(DAO_BANNED_ROLE, DAO_ADMIN_ROLE);
    }

    address public forumAddress;
    address public reputationAddress;
    address public crucibleTokens;
    uint256 public proposalsCount;
    mapping(uint256 => mapping(address => uint256)) private stakesAligned;
    mapping(uint256 => mapping(address => uint256)) private stakesNotAligned;
    mapping(uint256 => Vote[]) private stakes;
    mapping(uint256 => uint256) private totalReputationStakedForProposal;
    ProposalType[] private proposals;

    // Voters in informal voting stage
    mapping(uint256 => mapping(address => bool)) private informalVoters;
    mapping(uint256 => uint256) private informalVotersCount;

    //total reputation staked for proposal
    mapping(uint256 => uint256) private stakesAlignedAmount;

    //count of participants who staked
    mapping(uint256 => uint256) private stakesAlignedCount;

    //total reputation staked against for proposal
    mapping(uint256 => uint256) private stakesNotAlignedAmount;

    //count of participants who staked against
    mapping(uint256 => uint256) private stakesNotAlignedCount;

    // Results of proposals
    mapping(uint256 => ResultOfProposal) private resultsOfProposals;
    struct DatesLimit {
        uint256 approvedByAdminDate;
        uint256 completedDate;
        uint256 inDiscussionDueDate;
        uint256 informalVotingDueDate;
        uint256 formalVotingDueDate;
        uint256 creationDate;
    }

    struct ProposalType {
        uint256 id;
        string title;
        string body;
        ProposalStatus status;
        uint256 threadId;
        address owner;
        uint256 reputationToMint;
        uint256 currentAmountUniverseVoters;
        DatesLimit dates;
        bool tied;
    }

    struct Vote {
        address voter;
        uint256 amount;
        VoteType decision;
    }

    event ProposalCreated(uint256 idProposal);

    event ResultInformalVotingStage(uint256 idProposal, ProposalStatus result);

    event ResultFormalVotingStage(uint256 idProposal, ProposalStatus result);

    modifier isTokensHolder() {
        require(ICrucibleTokens(crucibleTokens).balanceOf(msg.sender) > 0, "You do not have tokens");
        _;
    }

    modifier canStake(uint256 proposalId) {
        require(proposals[proposalId].owner != _msgSender(), "Owner can not stake reputation on his proposal");
        require(stakesAligned[proposalId][_msgSender()] == 0, "Only one time voting is allowed");
        require(stakesNotAligned[proposalId][_msgSender()] == 0, "Only one time voting is allowed");
        _;
    }

    modifier isInWhiteList() {
        require(hasRole(DAO_ADMIN_ROLE, msg.sender), "You do not have permissions for this action");
        _;
    }

    modifier formalVotingDueDateIsReach(uint256 proposalId) {
        require(
            block.timestamp >= proposals[proposalId].dates.formalVotingDueDate,
            "The time to voting Stage Due Date isn't reach yet"
        );
        _;
    }

    modifier isNotInBlackList() {
        require(!hasRole(DAO_BANNED_ROLE, msg.sender), "You are banned for inappropriate behaviour");
        _;
    }

    function createProposal(string memory title, string memory body)
        external
        override
        isTokensHolder
        isNotInBlackList
        returns (uint256)
    {
        uint256 proposalId = proposalsCount;
        uint256 newThreadId;

        ProposalType memory proposal;
        proposal.id = proposalId;
        proposal.dates.creationDate = block.timestamp;
        proposal.title = title;
        proposal.body = body;
        proposal.owner = _msgSender();
        proposal.status = ProposalStatus.PENDING_APPROVAL;
        proposal.tied = false;
        newThreadId = IForum(forumAddress).createThread(title, body);

        proposal.threadId = newThreadId;

        proposals.push(proposal);
        proposalsCount++;

        emit ProposalCreated(proposal.id);

        return proposal.id;
    }

    function updateProposal(
        uint256 proposalId,
        string memory title,
        string memory body,
        uint256 reputationAmount
    ) external override isTokensHolder isInWhiteList isNotInBlackList returns (uint256) {
        ProposalType storage proposal = proposals[proposalId];
        require(
            proposal.status == ProposalStatus.IN_DISCUSSION || proposal.status == ProposalStatus.PENDING_APPROVAL,
            "The proposal's state should be In discussion or Pending approval "
        );
        proposal.title = title;
        proposal.body = body;
        proposal.reputationToMint = reputationAmount;

        return proposal.id;
    }

    function stakeReputation(
        uint256 proposalId,
        VoteType voteType,
        uint256 reputationAmount
    ) external override canStake(proposalId) isTokensHolder isNotInBlackList {
        ProposalType storage proposal = proposals[proposalId];
        require(
            reputationAmount < (IReputation(reputationAddress).availableReputationOf(_msgSender()) / 2),
            "You don't have enought reputation to stake. You can't stake more than 50% of reputation"
        );
        require(proposal.status == ProposalStatus.FORMAL_VOTING, "The proposal's state is not Formal Voting Stage");
        require(proposal.dates.formalVotingDueDate > block.timestamp, "The due date of voting stage is expired");
        require(informalVoters[proposalId][_msgSender()] == true, "You have not participated of informal vote");
        Vote memory vote;
        vote.voter = _msgSender();
        vote.amount = reputationAmount;

        if (voteType == VoteType.ALIGNED) {
            stakesAligned[proposalId][_msgSender()] = reputationAmount;
            stakesAlignedAmount[proposalId] = stakesAlignedAmount[proposalId] + reputationAmount;
            stakesAlignedCount[proposalId] = stakesAlignedCount[proposalId] + 1;
            vote.decision = VoteType.ALIGNED;
        } else {
            stakesNotAligned[proposalId][_msgSender()] = reputationAmount;
            stakesNotAlignedAmount[proposalId] = stakesNotAlignedAmount[proposalId] + reputationAmount;
            stakesNotAlignedCount[proposalId] = stakesNotAlignedCount[proposalId] + 1;
            vote.decision = VoteType.NOT_ALIGNED;
        }
        stakes[proposalId].push(vote);
        IReputation(reputationAddress).lockReputationOf(_msgSender(), reputationAmount);
    }

    function approveProposal(uint256 proposalId, uint256 reputation)
        external
        override
        isTokensHolder
        isInWhiteList
        isNotInBlackList
        returns (uint256)
    {
        ProposalType storage proposal = proposals[proposalId];
        require(proposal.status == ProposalStatus.PENDING_APPROVAL, "The proposal status should be Pending approval");

        proposal.reputationToMint = reputation;
        proposal.status = ProposalStatus.IN_DISCUSSION;
        proposal.dates.inDiscussionDueDate = block.timestamp + 1 weeks;
        proposal.dates.approvedByAdminDate = block.timestamp;
        return proposal.id;
    }

    function toInformalVotingProposal(uint256 proposalId)
        external
        override
        isTokensHolder
        isInWhiteList
        isNotInBlackList
        returns (uint256)
    {
        ProposalType storage proposal = proposals[proposalId];
        require(proposal.status == ProposalStatus.IN_DISCUSSION, "The proposal status should be In Discussion");
        require(block.timestamp > proposal.dates.inDiscussionDueDate, "The in discussion due date has not passed yet.");
        proposal.status = ProposalStatus.INFORMAL_VOTING;
        proposal.currentAmountUniverseVoters = getUniverse();
        proposal.dates.informalVotingDueDate = block.timestamp + 1 weeks;
        return proposal.id;
    }

    function toFormalVotingProposal(uint256 proposalId, address[] memory _informalVoters)
        external
        override
        isTokensHolder
        isInWhiteList
        isNotInBlackList
        returns (ProposalStatus)
    {
        ProposalType storage proposal = proposals[proposalId];

        require(proposal.status == ProposalStatus.INFORMAL_VOTING, "The proposal status should be Informal voting");
        require(
            block.timestamp > proposal.dates.informalVotingDueDate,
            "The informal voting due date has not been achieved"
        );
        if (_informalVoters.length >= ((proposal.currentAmountUniverseVoters / 2) + 1)) {
            proposal.status = ProposalStatus.FORMAL_VOTING;
            proposal.dates.formalVotingDueDate = block.timestamp + 1 weeks;
            proposal.currentAmountUniverseVoters = _informalVoters.length;
            for (uint256 i = 0; i < _informalVoters.length; i++) {
                address addrs = address(_informalVoters[i]);
                informalVoters[proposalId][addrs] = true;
                informalVotersCount[proposalId] = _informalVoters.length;
            }
        } else {
            proposal.status = ProposalStatus.NO_QUORUM;
        }
        emit ResultInformalVotingStage(proposalId, proposal.status);

        return proposal.status;
    }

    function discardProposal(uint256 proposalId)
        external
        override
        isTokensHolder
        isInWhiteList
        isNotInBlackList
        returns (uint256)
    {
        ProposalType storage proposal = proposals[proposalId];
        require(proposal.status == ProposalStatus(0), "The proposal status should be Pending approval");
        proposal.status = ProposalStatus(9);
        return proposal.id;
    }

    function completeProposal(uint256 proposalId)
        external
        override
        isTokensHolder
        isInWhiteList
        isNotInBlackList
        returns (uint256)
    {
        ProposalType storage proposal = proposals[proposalId];
        require(
            proposal.status == ProposalStatus.PENDING_IMPLEMENTATION,
            "The proposal status should be Pending implementation"
        );
        proposal.status = ProposalStatus.COMPLETED;
        proposal.dates.completedDate = block.timestamp;
        return proposal.id;
    }

    function resolveProposal(uint256 proposalId)
        external
        override
        isTokensHolder
        isInWhiteList
        formalVotingDueDateIsReach(proposalId)
        isNotInBlackList
        returns (ResultOfProposal memory)
    {
        ProposalType storage proposal = proposals[proposalId];
        require(proposal.status == ProposalStatus(3), "The proposal status should be Formal Voting");

        uint256 votesCount = stakesAlignedCount[proposalId] + stakesNotAlignedCount[proposalId];
        uint256 universe = proposal.currentAmountUniverseVoters;
        uint256 quorum = (universe * 51) / 100;
        if (votesCount > quorum) {
            if (stakesAlignedAmount[proposalId] > stakesNotAlignedAmount[proposalId]) {
                proposal.status = ProposalStatus.PENDING_IMPLEMENTATION;
                _distributeReputation_PassedAndRejected(proposalId, VoteType.ALIGNED);
            } else {
                if (stakesAlignedAmount[proposalId] < stakesNotAlignedAmount[proposalId]) {
                    proposal.status = ProposalStatus.REJECTED;
                    _distributeReputation_PassedAndRejected(proposalId, VoteType.NOT_ALIGNED);
                } else {
                    if (proposal.tied == false && votesCount < informalVotersCount[proposalId]) {
                        proposal.dates.formalVotingDueDate = block.timestamp + 1 weeks;
                        proposal.tied = true;
                    } else {
                        proposal.status = ProposalStatus.TIED;
                        _distributeReputation_NoQuorumAndTied(proposalId);
                    }
                }
            }
        } else {
            proposal.status = ProposalStatus.NO_QUORUM;
            _distributeReputation_NoQuorumAndTied(proposalId);
        }
        ResultOfProposal memory result;
        result.idProposal = proposal.id;
        result.resolveDate = block.timestamp;
        result.finalyStatus = proposal.status;
        result.resolvedFor = _msgSender();
        resultsOfProposals[proposal.id] = result;

        emit ResultFormalVotingStage(proposalId, proposal.status);

        return result;
    }

    function _distributeReputation_PassedAndRejected(uint256 proposalId, VoteType winSide) internal {
        ProposalType storage proposal = proposals[proposalId];
        uint256 totalToDistribute;
        uint256 stakeAmountWinnerSide;
        uint256 reputationToMintInProposal = uint256(((proposal.reputationToMint) / 2));
        if (winSide == VoteType.ALIGNED) {
            totalToDistribute = stakesNotAlignedAmount[proposal.id] + reputationToMintInProposal;
            stakeAmountWinnerSide = stakesAlignedAmount[proposal.id];
            // Mint reputation to OP
            IReputation(reputationAddress).mint(proposal.owner, reputationToMintInProposal);
        } else {
            totalToDistribute = uint256(stakesAlignedAmount[proposal.id]) + reputationToMintInProposal;
            stakeAmountWinnerSide = stakesNotAlignedAmount[proposal.id];
        }
        Vote[] memory voters = stakes[proposal.id];
        for (uint256 i = 0; i < voters.length; i++) {
            Vote memory vote = voters[i];
            // Unlock reputation minted in formal voting
            IReputation(reputationAddress).unLockReputationOf(vote.voter, vote.amount);

            if (vote.decision == winSide) {
                // Calc percentage, get value and mint reputation
                //  percentage = (vote.amount * 100) / stakeAmountWinnerSide;
                //  amountToMint = (totalToDistribute * percentage) / 100;
                // Simplify: amountToMint = totalToDistribute * vote.amount / stakeAmountWinnerSide
                IReputation(reputationAddress).mint(
                    vote.voter,
                    ((totalToDistribute * (vote.amount)) / stakeAmountWinnerSide)
                );
            } else {
                // Remove reputation minted in formal voting
                IReputation(reputationAddress).removeReputation(vote.voter, vote.amount);
            }
        }
    }

    function _distributeReputation_NoQuorumAndTied(uint256 proposalId) internal {
        Vote[] memory voters = stakes[proposalId];
        for (uint256 i = 0; i < voters.length; i++) {
            Vote memory vote = voters[i];
            // Unlock reputation minted in formal voting
            IReputation(reputationAddress).unLockReputationOf(vote.voter, vote.amount);
        }
    }

    //TODO: this function is used only to test. Delete this one when we decided that isn't needed
    function updateProposalById(
        uint256 proposalId,
        uint8 status,
        uint256 inDiscussionDueDate,
        uint256 informalVotingDueDate,
        uint256 formalVotingDueDate
    ) external override onlyRole(DEFAULT_ADMIN_ROLE) isTokensHolder {
        ProposalType storage proposal = proposals[proposalId];
        proposal.status = ProposalStatus(status);
        proposal.dates.inDiscussionDueDate = inDiscussionDueDate;
        proposal.dates.informalVotingDueDate = informalVotingDueDate;
        proposal.dates.formalVotingDueDate = formalVotingDueDate;
    }

    function proposalsLength() external view override returns (uint256) {
        return proposals.length;
    }

    function setForumAddress(address _forumAddress) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        forumAddress = _forumAddress;
    }

    function setReputationAddress(address _reputationAddress) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        reputationAddress = _reputationAddress;
    }

    function setCrucibleTokens(address _crucibleTokens) external override onlyRole(DEFAULT_ADMIN_ROLE) {
        crucibleTokens = _crucibleTokens;
    }

    function addAddressToWhiteList(address _user) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _grantRole(DAO_ADMIN_ROLE, _user);
    }

    function getStakeAlignedAmount(uint256 proposalId) public view returns (uint256) {
        return stakesAlignedAmount[proposalId];
    }

    function getStakeNotAlignedAmount(uint256 proposalId) public view returns (uint256) {
        return stakesNotAlignedAmount[proposalId];
    }

    function getVotersAlignedCountOnProposal(uint256 proposalId) public view returns (uint256) {
        return stakesAlignedCount[proposalId];
    }

    function getVotersNotAlignedCountOnProposal(uint256 proposalId) public view returns (uint256) {
        return stakesNotAlignedCount[proposalId];
    }

    function getAmountOfUsersVotedOnProposal(uint256 proposalId) public view returns (uint256) {
        return stakesNotAlignedCount[proposalId] + stakesAlignedCount[proposalId];
    }

    function getUniverse() public view returns (uint256) {
        uint256 universe;
        address[] memory members = (IReputation(reputationAddress).getMembersWithReputation());
        for (uint256 i = 0; i < members.length; i++) {
            address member = address(members[i]);
            uint256 tokens = ICrucibleTokens(crucibleTokens).balanceOf(member);
            if (tokens > 0) {
                universe = universe + 1;
            }
        }
        return universe;
    }

    function getInDiscussionDueDate(uint256 proposalId) public view returns (uint256) {
        return proposals[proposalId].dates.inDiscussionDueDate;
    }

    function getFormalVotingStageDueDate(uint256 proposalId) public view returns (uint256) {
        return proposals[proposalId].dates.formalVotingDueDate;
    }

    function getInformalVotingStageDueDate(uint256 proposalId) public view returns (uint256) {
        return proposals[proposalId].dates.informalVotingDueDate;
    }

    function getBallotDetails(uint256 proposalId) external view override returns (ProposalBallotDetails memory) {
        ProposalType storage proposal = proposals[proposalId];
        ProposalBallotDetails memory proposalBalotDetail;

        proposalBalotDetail.stakeAlignedAmount = getStakeAlignedAmount(proposalId);
        proposalBalotDetail.stakeNotAlignedAmount = getStakeNotAlignedAmount(proposalId);
        proposalBalotDetail.userVotedOnProposal = getAmountOfUsersVotedOnProposal(proposalId);
        proposalBalotDetail.universe = proposal.currentAmountUniverseVoters;
        proposalBalotDetail.inDiscussionDueDate = getInDiscussionDueDate(proposalId);
        proposalBalotDetail.informalVotingStageDueDate = getInformalVotingStageDueDate(proposalId);
        proposalBalotDetail.formalVotingStageDueDate = getFormalVotingStageDueDate(proposalId);
        proposalBalotDetail.votersAlignedCount = getVotersAlignedCountOnProposal(proposalId);
        proposalBalotDetail.votersNotAlignedCount = getVotersNotAlignedCountOnProposal(proposalId);
        return proposalBalotDetail;
    }

    function getResultOfProposal(uint256 proposalId) external view override returns (ResultOfProposal memory) {
        return resultsOfProposals[proposalId];
    }

    function getProposal(uint256 proposalId) public view returns (ProposalType memory) {
        return proposals[proposalId];
    }

    function getTotalReputationStakedForProposal(uint256 proposalId) public view returns (uint256) {
        return totalReputationStakedForProposal[proposalId];
    }

    function getStakeAligned(uint256 proposalId, address _address) public view returns (uint256) {
        return stakesAligned[proposalId][_address];
    }

    function getStakeNotAligned(uint256 proposalId, address _address) public view returns (uint256) {
        return stakesNotAligned[proposalId][_address];
    }

    function getInformalVoters(uint256 proposalId, address _address) public view returns (bool) {
        return informalVoters[proposalId][_address];
    }

    function isAdmin(address account) public view override returns (bool) {
        return hasRole(DAO_ADMIN_ROLE, account);
    }

    function removeWhitelistAddress(address account) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _revokeRole(DAO_ADMIN_ROLE, account);
    }

    function getProposalStatus(uint256 proposalId) external view override returns (ProposalStatus) {
        return getProposal(proposalId).status;
    }

    function getThreadID(uint256 proposalId) external view override returns (uint256) {
        return getProposal(proposalId).threadId;
    }

    function addAddressToBannedList(address _user) external onlyRole(DAO_ADMIN_ROLE) {
        _grantRole(DAO_BANNED_ROLE, _user);
    }

    function isBanned(address account) public view override returns (bool) {
        return hasRole(DAO_BANNED_ROLE, account);
    }

    function removeBannedAddress(address account) external onlyRole(DAO_ADMIN_ROLE) {
        _revokeRole(DAO_BANNED_ROLE, account);
    }
}

