// File: mynft/contracts/outer.sol


pragma solidity ^0.8.4;







struct MetaData {
    string name;
    string director;
    string local;
    string actor;
    uint level;
}

interface IMovieCard is IERC721, IERC721Enumerable{
    function safeMint(address to, uint256 tokenId, string memory uri, MetaData memory metaData) external;
}

contract outer is AccessControl, DateUtil, ReentrancyGuard {

    address public NFTAddress;
    string mbaseUri = "https://xxx.com/xxx/";  

    struct Card {
        uint index;
        string uri;
    }   
    
    Card[] public cards;
    
    constructor(address nft) {
        NFTAddress = nft;
    }

    function mintOne()  external {
        MetaData memory metaData = MetaData("A better tomorrow", "John Woo", "China/Hong Kong", "Chow Yun Fat/Tommy Tam/Leslie Cheung", 15);
        _mintX(metaData);
    }

    function mintX(MetaData memory data) external {
        _mintX(data);
        emit NFTMint(msg.sender, data);
    }

    function mintY(IRunbitCard.MetaData2 memory meta) external {
        IRunbitCard.MetaData2 memory m =  meta;
        string memory uri = "https://xxx.com/pict/100004-mintY.png";

        emit NFTCardMint(msg.sender, 100004, 2, uri, m);
    }

    function collection(uint256 collectionId, IRunbitCard.CardCollection memory cc) external {

        emit CardCollectionAdd(collectionId, cc);
    }

    function _mintX(MetaData memory data) private {
        IMovieCard movieCard = IMovieCard(NFTAddress); 
        require(movieCard.balanceOf(msg.sender) == 0);  
        MetaData memory metaData = data;   
        uint id = 3 + 100001;
        string memory muri = DateUtil.strConcat(mbaseUri, DateUtil.uintToString(id));
        cards.push(Card(id, DateUtil.strConcat(muri,".png")));

        movieCard.safeMint(msg.sender, id, DateUtil.strConcat(muri,".png"), metaData);
    }
    // ["The White Storm","Benny Chan","China/Hong Kong","ZJH/LQY/GTL",18]

    event NFTMint(address indexed owner, MetaData data);

    event NFTCardMint(address indexed to, uint256 indexed tokenId, uint256 collectionId, string uri, IRunbitCard.MetaData2 meta);

    event CardCollectionAdd(uint256 indexed collectionId, IRunbitCard.CardCollection cc); 
}