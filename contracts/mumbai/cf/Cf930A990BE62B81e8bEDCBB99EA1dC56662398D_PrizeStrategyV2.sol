import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "../binarysearch/IBinarySearch.sol";
import "./UniformRandomNumber.sol";
import "../random/RandomNumberGeneratorV2.sol";
import "hardhat/console.sol";

/// @title Rand.network Ecosystem Reserve
/// @author @adradr - Adrian Lenard
/// @notice Prize Strategy V2 contract to decide the winners at the end of each round for a given vault
/// @dev Utilizes ChainLink VRF to obtain a real random number and uses Binary Search to find the winner based on randomness

// [] Generate IRandomNumberGeneratorV2 and remove this interface
interface IRandomNumberGeneratorV2 {
    function getRandomWords(uint256 _mapping)
        external
        view
        returns (uint256[] memory);

    function requestRandomWords() external returns (uint256);
}

// [] Implement AddressRegistry

contract PrizeStrategyV2 is
    ReentrancyGuardUpgradeable,
    AccessControlUpgradeable,
    PausableUpgradeable,
    UUPSUpgradeable
{
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant BACKEND_ROLE = keccak256("BACKEND_ROLE");

    IBinarySearch binarysearch;
    RandomNumberGeneratorV2 VRFRandomNumberV2;

    event InitiateDistribution(uint256 VRFRequestId);
    event RoundDistribution(
        uint256 distributionTimestamp,
        uint256 vaultId,
        uint256[] prizes,
        uint256[] twBalances,
        uint256 VRFRandomNumber,
        uint256[] pseudoRandomNumbers,
        uint256[] winnerUserIds
    );

    event UpdatedBinarySearchAddress(address binarySearchAddress);
    event UpdatedVRFRandomNumberV2Address(address VRFRandomNumberV2Address);

    struct DistributionData {
        uint256 distributionTimestamp;
        uint256 vaultId;
        uint256[] prizes;
        uint256[] userIds;
        uint256[] twBalances;
        uint256 VRFRequestId;
        uint256 VRFRandomNumber;
        uint256[] pseudoRandomNumbers;
        uint256[] winnerIds;
    }

    // Mapping VRFRequestId to DistributionData array
    mapping(uint256 => DistributionData) distributionMap;
    // Mapping requestIds to VaultIds
    mapping(uint256 => uint256) VRFRequestIdToVaultId;
    // Mappign userIds to _VRFRequestId to bools
    mapping(uint256 => mapping(uint256 => bool)) userHasWonOnVRFRequestId;

    function initialize(
        address _backendAddress,
        address _binarySearchAddress,
        address _VRFRandomNumberV2
    ) public initializer {
        __AccessControl_init();
        __Pausable_init();
        __UUPSUpgradeable_init();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        grantRole(PAUSER_ROLE, _msgSender());
        grantRole(UPGRADER_ROLE, _msgSender());
        grantRole(BACKEND_ROLE, _msgSender());
        grantRole(BACKEND_ROLE, _backendAddress);

        binarysearch = IBinarySearch(_binarySearchAddress);
        VRFRandomNumberV2 = RandomNumberGeneratorV2(_VRFRandomNumberV2);
    }

    /* 
        Flow of distribution happens as follows:
        1. Backend requests to distribute prizes for a given vault
           a. Backend calls initiateDistribution()
           b. This call the RandomNumberGenerator contract to obtain a random number
           c. Storing the requestId in the distribution data
        2. Event of random word fullfilment is received by the backend
           a. Backend calls finishDistribution()
           b. The binary search is used to find the winner based on the random number
           c. Emitting event of round distribution information
     */

    function initiateDistribution(
        uint256 _vaultId,
        uint256[] calldata _prizes,
        uint256[] calldata _userIds,
        uint256[] calldata _twaBalances
    ) public onlyRole(BACKEND_ROLE) {
        // Checking inputs
        require(_vaultId != 0, "PS: vaultId cannot be zero");
        require(
            _userIds.length == _twaBalances.length,
            "PS: userIds must be same lenght at twaBalances"
        );

        // Creating a struct to initiate distribution data
        DistributionData memory dist = DistributionData({
            distributionTimestamp: block.timestamp,
            vaultId: _vaultId,
            prizes: _prizes,
            userIds: _userIds,
            twBalances: _twaBalances,
            VRFRequestId: 0,
            VRFRandomNumber: 0,
            pseudoRandomNumbers: new uint256[](_prizes.length),
            winnerIds: new uint256[](_prizes.length)
        });

        // Fetch random numbers from RandomNumberGeneratorV2
        uint256 VRFRequestId = VRFRandomNumberV2.requestRandomWords();
        dist.VRFRequestId = VRFRequestId;
        // Update data struct and emit event of the VRFRequestId
        distributionMap[VRFRequestId] = dist;
        emit InitiateDistribution(VRFRequestId);
        VRFRequestIdToVaultId[VRFRequestId] = _vaultId;
    }

    function finishDistribution(uint256 _VRFRequestId)
        public
        onlyRole(BACKEND_ROLE)
    {
        // Read new random words from VRF contracts
        uint256[] memory randomWords = VRFRandomNumberV2.getRandomWords(
            _VRFRequestId
        );

        // Read the corresponding distribution data struct
        // Always take the last element
        DistributionData storage dist = distributionMap[_VRFRequestId];
        // Assign new random word
        dist.VRFRandomNumber = randomWords[0];

        // Creating a variables for winner picking
        uint256 loopIterator;
        uint256 winnerIterator;
        uint256 pseudoRandomNumber;
        uint256 prizesLenghtStatic = dist.prizes.length;
        uint256[] memory pseudoRandomArray = new uint256[](prizesLenghtStatic);
        uint256[] memory winnerUserIds = new uint256[](prizesLenghtStatic);
        uint256 maxTwaBalance = dist.twBalances[dist.twBalances.length - 1];

        // Iterate while there are prizes remaining
        // Regenerate random number if the user has already won
        while (winnerIterator < prizesLenghtStatic) {
            // Generate a pseudo random number
            pseudoRandomNumber = UniformRandomNumber.uniform(
                uint256(keccak256(abi.encode(randomWords[0], loopIterator))),
                maxTwaBalance
            );
            // Do a binary search to select a winner
            uint256 winner = binarysearch.binarySearch(
                dist.twBalances,
                pseudoRandomNumber
            );

            // If the user has not won on this particular vaultId
            // Set mapping to true to signal his win
            // Increase iterator and save pseudoRandomNumber
            if (!userHasWonOnVRFRequestId[winner][_VRFRequestId]) {
                userHasWonOnVRFRequestId[winner][_VRFRequestId] = true;
                winnerUserIds[winnerIterator] = winner;
                pseudoRandomArray[winnerIterator] = pseudoRandomNumber;
                winnerIterator++;
            }

            // Increase loopIterator always to get new pseudoRandom
            loopIterator++;
        }

        // Store pseudoRandomNumbers and winnerIds
        dist.pseudoRandomNumbers = pseudoRandomArray;
        dist.winnerIds = winnerUserIds;

        // Announce distribution information as an event
        emit RoundDistribution(
            block.timestamp,
            dist.vaultId,
            dist.prizes,
            dist.twBalances,
            dist.VRFRandomNumber,
            pseudoRandomArray,
            winnerUserIds
        );
    }

    ////////////////////////////////////////////////////////
    //////////////// ADMIN FUNCTIONS  //////////////////////
    ////////////////////////////////////////////////////////

    function updateRandomNumberVRF(address _VRFRandomNumberV2)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        VRFRandomNumberV2 = RandomNumberGeneratorV2(_VRFRandomNumberV2);
        emit UpdatedVRFRandomNumberV2Address(address(VRFRandomNumberV2));
    }

    function updateBinarySearch(address _binarySearchAddress)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        binarysearch = IBinarySearch(_binarySearchAddress);
        emit UpdatedBinarySearchAddress(address(binarysearch));
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(UPGRADER_ROLE)
    {}
}

