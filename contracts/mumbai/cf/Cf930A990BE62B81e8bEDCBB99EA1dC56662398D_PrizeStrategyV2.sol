    Initializable,
    UUPSUpgradeable,
    PausableUpgradeable,
    AccessControlUpgradeable
{
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant VM_ROLE = keccak256("VM_ROLE");

    error OnlyCoordinatorCanFulfill(address have, address want);

    /// @notice ChainLink VRF Random Number Consumer contract to request random numbers for exchange of LINK tokens
    /// @dev Check documentation for V2 at https://docs.chain.link/docs/chainlink-vrf/
    /// @param s_subscriptionId The subscription id of the Chainlink VRF
    /// @param vrfCoordinator The address of the VRF Coordinator
    /// @param keyHash The keyhash used by the Chainlink VRF
    /// @param callbackGasLimit The gas limit for the callback function
    /// @param requestConfirmation Number of confirmations to wait for before sending the random number
    /// @param numWords The number of words to send in the random number
    event ConsumerConfigChanged(
        uint64 s_subscriptionId,
        address vrfCoordinator,
        bytes32 keyHash,
        uint32 callbackGasLimit,
        uint16 requestConfirmation,
        uint32 numWords
    );

    /// @notice Initiated random number request
    /// @param requestId id of the received request
    event RandomWordsRequested(uint256 indexed requestId);

    /// @notice Event to be fired when the random number is ready
    /// @param requestId The request id of the random number
    /// @param numWords The number of words in the random number
    /// @param randomWords The array of random words in the received random number
    event RandomWordsFulfilled(
        uint256 indexed requestId,
        uint256 numWords,
        uint256[] randomWords
    );

    struct ConsumerConfig {
        // Your subscription ID.
        uint64 s_subscriptionId;
        // see https://docs.chain.link/docs/vrf-contracts/#configurations
        // Polygon Mumbai 0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed
        VRFCoordinatorV2Interface vrfCoordinator;
        // The gas lane to use, which specifies the maximum gas price to bump to.
        // For a list of available gas lanes on each network,
        // see https://docs.chain.link/docs/vrf-contracts/#configurations
        // Polygon Mumbai 0x4b09e658ed251bcafeebbc69400383d49f344ace09b9576fe248bb02c003fe9f
        bytes32 keyHash;
        // Depends on the number of requested values that you want sent to the
        // fulfillRandomWords() function. Storing each word costs about 20,000 gas,
        // so 100,000 is a safe default for this example contract. Test and adjust
        // this limit based on the network that you select, the size of the request,
        // and the processing of the callback request in the fulfillRandomWords()
        // function.
        // Polygon Mumbai 100000
        uint32 callbackGasLimit;
        // The default is 3, but you can set this higher.
        // Default 3
        uint16 requestConfirmations;
        // For this example, retrieve 2 random values in one request.
        // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.
        // Default 2, Maximum 500
        uint32 numWords;
    }

    /// @notice Nested array to store the random words received from the VRF Coordinator mapped to the requestId
    mapping(uint256 => uint256[]) public s_randomWords;
    /// @notice Array to store the request ids of the random word requests
    uint256[] public s_requestId;

    /// @notice Struct to hold in all information about the consumer
    ConsumerConfig public config;

    /// @notice Initializer for upgradability
    /// @dev Need to encode the input struct as the example code snippet above in the contract file
    function initialize(
        uint64 _s_subscriptionId,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint32 _callbackGasLimit,
        uint16 _requestConfirmation,
        uint32 _numWords
    ) public initializer {
        __AccessControl_init();
        __Pausable_init();
        __UUPSUpgradeable_init();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        grantRole(PAUSER_ROLE, _msgSender());
        grantRole(UPGRADER_ROLE, _msgSender());
        grantRole(VM_ROLE, _msgSender());

        ConsumerConfig memory _config;
        _config.s_subscriptionId = _s_subscriptionId;
        _config.vrfCoordinator = VRFCoordinatorV2Interface(_vrfCoordinator);
        _config.keyHash = _keyHash;
        _config.callbackGasLimit = _callbackGasLimit;
        _config.requestConfirmations = _requestConfirmation;
        _config.numWords = _numWords;
        config = _config;
        emit ConsumerConfigChanged(
            _s_subscriptionId,
            _vrfCoordinator,
            _keyHash,
            _callbackGasLimit,
            _requestConfirmation,
            _numWords
        );
    }

    function getRandomWords(uint256 _mapping)
        external
        view
        returns (uint256[] memory randomWords)
    {
        randomWords = s_randomWords[_mapping];
    }

    /// @notice Main function to request random words from the VRF Coordinator
    /// @dev Can be only called by the VM_ROLE when not paused, will push the request id to the requestId array. Assumes the subscription is funded sufficiently.
    function requestRandomWords()
        external
        onlyRole(VM_ROLE)
        whenNotPaused
        returns (uint256 requestId)
    {
        // Will revert if subscription is not set and funded.
        ConsumerConfig memory _config = config;
        requestId = _config.vrfCoordinator.requestRandomWords(
            _config.keyHash,
            _config.s_subscriptionId,
            _config.requestConfirmations,
            _config.callbackGasLimit,
            _config.numWords
        );
        // Store in array and emit event for backend
        s_requestId.push(requestId);
        emit RandomWordsRequested(requestId);
    }

    /// @notice Internal function to handle the process when receiving the random words from the VRF Coordinator
    /// @dev Important to note that the gaslimit must be carefully optimized to avoid the transaction failing. Avoid long complex external calls to be made here.
    /// @param randomWords the random words received from the VRF Coordinator
    function fulfillRandomWords(
        uint256 requestId, /* requestId */
        uint256[] memory randomWords
    ) internal {
        s_randomWords[requestId] = randomWords;
    }

    /// @notice Function to check if the random words are correct coming from the VRF Coordinator
    /// @dev rawFulfillRandomness is called by VRFCoordinator when it receives a valid VRF proof. rawFulfillRandomness then calls fulfillRandomness, after validating the origin of the call
    /// @param requestId the request id of the random words
    /// @param randomWords the random words received from the VRF Coordinator
    function rawFulfillRandomWords(
        uint256 requestId,
        uint256[] memory randomWords
    ) external {
        address vrfCoordinator = address(config.vrfCoordinator);
        if (_msgSender() != vrfCoordinator) {
            revert OnlyCoordinatorCanFulfill(_msgSender(), vrfCoordinator);
        }
        emit RandomWordsFulfilled(requestId, randomWords.length, randomWords);
        fulfillRandomWords(requestId, randomWords);
    }

    /// @notice Update function to be used to update the consumer config
    /// @dev Need to use an encoded struct as the example code snippet above in the contract file, can be called when paused and by the UPGRADER_ROLE
    /// @param newConfig the encoded struct containing the whole configuration
    // [] expands arguments and avoid this struct
    function updateConfig(ConsumerConfig memory newConfig)
        public
        onlyRole(UPGRADER_ROLE)
        whenPaused
    {
        config = newConfig;
        emit ConsumerConfigChanged(
            config.s_subscriptionId,
            address(config.vrfCoordinator),
            config.keyHash,
            config.callbackGasLimit,
            config.requestConfirmations,
            config.numWords
        );
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(UPGRADER_ROLE)
    {}
}

