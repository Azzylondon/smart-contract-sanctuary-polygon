pragma solidity >=0.8.0;
pragma abicoder v2;

import {AccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {ERC721Upgradeable, IERC721Upgradeable, IERC721MetadataUpgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import {IERC1155Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import {IERC165Upgradeable} from "@openzeppelin/contracts-upgradeable/utils/introspection/IERC165Upgradeable.sol";
import {IAccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/IAccessControlUpgradeable.sol";
import {Part, IAvatar} from "./interfaces/IAvatar.sol";
import {IUniqueController} from "./interfaces/IUniqueController.sol";
import {IDavaV2} from "./interfaces/IDavaV2.sol";
import {ERC721Account} from "./libraries/ERC721Account.sol";
import {IWearable, Wearable} from "./libraries/Wearable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {AccessControlUpgradeable, IAccessControlUpgradeable} from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {UUPSUpgradeable} from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

interface IPartCollection {
    function categoryId(uint256 tokenId) external view returns (bytes32);
}

contract DavaV2 is
    Initializable,
    IDavaV2,
    OwnableUpgradeable,
    AccessControlUpgradeable,
    Wearable,
    ERC721Upgradeable,
    ERC721Account,
    UUPSUpgradeable
{
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant PART_MANAGER_ROLE = keccak256("PART_MANAGER_ROLE");
    bytes32 public constant UPGRADE_MANAGER_ROLE =
        keccak256("UPGRADE_MANAGER_ROLE");
    bytes32 public constant UNIQUENESS_MANAGER_ROLE =
        keccak256("UNIQUENESS_MANAGER_ROLE");

    string public burntImage;
    string public baseURI;
    uint48 public constant MAX_SUPPLY = 10000;
    IUniqueController public uniqueController;

    // newIdOf(10000 ~ *) => originalId(0~9999)
    mapping(uint256 => uint256) public originalIdOf;

    // originalId(0~9999) => newIdOf(10000 ~ *)
    mapping(uint256 => uint256) public newIdOf;

    // nonce counter for newIdOf;
    uint256 public nonceCounter;

    // avatarAddress => originalId(0 ~ 9999)
    mapping(address => uint256) public originalIdOfAvatar;

    struct AvatarInfo {
        address addr;
        uint256 equippedNum;
        IAvatar avatar;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    // Declare events for logging
    event SetBurntImage(string indexed value);
    event SetUniqueController(IUniqueController indexed value);
    event SetBaseURI(address indexed _from, string indexed value);
    event UpgradeAvatar(address indexed value);
    event MatchTokenId(uint256 indexed _originalId, uint256 _newId);

    // DAO contract owns this registry
    function initialize(address minimalProxy_) public initializer {
        __ERC721_init("dava-humanoids", "DAVA");
        __ERC721Acount_init(minimalProxy_);
        __AccessControl_init();
        __UUPSUpgradeable_init();
        __Ownable_init();

        baseURI = "https://api.davaproject.com";
        burntImage = "https://ipfs.io/ipfs/QmYZ5DRpE2czvCbkrNpVgBpyiGujLqWdojGmVj3RFdGDTe";
        nonceCounter = 10000;

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
        _setRoleAdmin(MINTER_ROLE, DEFAULT_ADMIN_ROLE);
        _setupRole(PART_MANAGER_ROLE, msg.sender);
        _setRoleAdmin(PART_MANAGER_ROLE, DEFAULT_ADMIN_ROLE);
        _setupRole(UPGRADE_MANAGER_ROLE, msg.sender);
        _setRoleAdmin(UPGRADE_MANAGER_ROLE, DEFAULT_ADMIN_ROLE);
        _setupRole(UNIQUENESS_MANAGER_ROLE, msg.sender);
        _setRoleAdmin(UNIQUENESS_MANAGER_ROLE, DEFAULT_ADMIN_ROLE);
    }

    // This is only for upgrade
    function unsafeSetOriginalId(uint256[] calldata originalIds_)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        for (uint256 i = 0; i < originalIds_.length; i += 1) {
            originalIdOfAvatar[super.getAvatar(originalIds_[i])] = originalIds_[
                i
            ];
        }
    }

    function setBurntImage(string calldata burntImage_)
        external
        onlyRole(UPGRADE_MANAGER_ROLE)
    {
        burntImage = burntImage_;
        emit SetBurntImage(burntImage_);
    }

    function setUniqueController(IUniqueController uniqueController_)
        external
        onlyRole(UPGRADE_MANAGER_ROLE)
    {
        uniqueController = uniqueController_;
        emit SetUniqueController(uniqueController_);
    }

    function setBaseURI(string calldata baseURI_)
        external
        onlyRole(UPGRADE_MANAGER_ROLE)
    {
        baseURI = baseURI_;
        emit SetBaseURI(msg.sender, baseURI_);
    }

    function upgradeAvatarImplementationTo(address newImplementation)
        external
        onlyRole(UPGRADE_MANAGER_ROLE)
    {
        _upgradeAvatarImplementationTo(newImplementation);
        emit UpgradeAvatar(newImplementation);
    }

    function registerCollection(address collection)
        external
        onlyRole(PART_MANAGER_ROLE)
    {
        _registerCollection(collection);
    }

    function registerCategory(bytes32 categoryId)
        external
        onlyRole(PART_MANAGER_ROLE)
    {
        _registerCategory(categoryId);
    }

    function registerFrameCollection(address collection)
        external
        onlyRole(PART_MANAGER_ROLE)
    {
        _registerFrameCollection(collection);
    }

    function deregisterCollection(address collection)
        external
        onlyRole(PART_MANAGER_ROLE)
    {
        _deregisterCollection(collection);
    }

    function deregisterCategory(bytes32 categoryId)
        external
        onlyRole(PART_MANAGER_ROLE)
    {
        _deregisterCategory(categoryId);
    }

    function zap(
        uint256 tokenId,
        Part[] calldata partsOn,
        bytes32[] calldata partsOff
    ) external override {
        require(
            partsOn.length <= getAllSupportedCategories().length,
            "Dava: exceeds number of allowed equipment slots"
        );

        (uint256 originalTokenId, uint256 newTokenId) = _getOriginalAndNewId(
            tokenId
        );

        require(
            msg.sender == ownerOf(newTokenId),
            "Dava: msg.sender is not the owner of avatar"
        );

        AvatarInfo memory avatarInfo;
        avatarInfo.addr = getAvatar(originalTokenId);
        avatarInfo.avatar = IAvatar(avatarInfo.addr);
        avatarInfo.equippedNum = 1;

        for (uint256 i = 0; i < partsOff.length; i += 1) {
            Part memory equippedPart = avatarInfo.avatar.part(partsOff[i]);
            IERC1155Upgradeable collection = IERC1155Upgradeable(
                equippedPart.collection
            );
            if (
                equippedPart.collection != address(0x0) &&
                collection.balanceOf(avatarInfo.addr, equippedPart.id) > 0
            ) {
                collection.safeTransferFrom(
                    avatarInfo.addr,
                    msg.sender,
                    equippedPart.id,
                    1,
                    ""
                );
            }
        }

        for (uint256 i = 0; i < partsOn.length; i += 1) {
            IERC1155Upgradeable collection = IERC1155Upgradeable(
                partsOn[i].collection
            );
            require(
                collection.supportsInterface(
                    type(IERC1155Upgradeable).interfaceId
                ),
                "Dava: collection is not an erc1155 format"
            );
            require(
                collection.balanceOf(msg.sender, partsOn[i].id) >= 1,
                "Dava: owner does not hold the part"
            );

            bytes32 categoryId = IPartCollection(partsOn[i].collection)
                .categoryId(partsOn[i].id);

            uint256 convertedCategoryNum = uint256(uint24(bytes3(categoryId)));
            require(
                avatarInfo.equippedNum % convertedCategoryNum != 0,
                "Dava: can not equip same category at once"
            );
            avatarInfo.equippedNum *= convertedCategoryNum;

            Part memory equippedParts = avatarInfo.avatar.part(categoryId);
            if (equippedParts.collection != address(0)) {
                IERC1155Upgradeable(equippedParts.collection).safeTransferFrom(
                    avatarInfo.addr,
                    msg.sender,
                    equippedParts.id,
                    1,
                    ""
                );
            }

            collection.safeTransferFrom(
                msg.sender,
                avatarInfo.addr,
                partsOn[i].id,
                1,
                ""
            );
        }

        IAvatar(getAvatar(originalTokenId)).dress(partsOn, partsOff);
    }

    function mint(address to, uint256 originalId)
        external
        override
        onlyRole(MINTER_ROLE)
        returns (address avatarAddress)
    {
        require(originalId < uint256(MAX_SUPPLY), "Dava: Invalid id");
        uint256 newID = nonceCounter;
        nonceCounter++;
        originalIdOf[newID] = originalId;
        newIdOf[originalId] = newID;

        super._mint(to, newID);
        avatarAddress = _mintWithProxy(originalId);
        originalIdOfAvatar[avatarAddress] = originalId;

        emit MatchTokenId(originalId, newID);
    }

    function registerCombinationData(uint256 tokenId, Part[] calldata parts)
        external
        override
    {
        require(
            hasRole(UNIQUENESS_MANAGER_ROLE, msg.sender) ||
                msg.sender == getAvatar(tokenId),
            "DavaV2: msg.sender is not authorized for registerCombinationData"
        );

        bytes32 hashedData = _hashPartsData(parts);

        if (hashedData != uniqueController.emptyHash()) {
            uniqueController.registerData(tokenId, hashedData);
        }
    }

    function deregisterCombinationData(uint256 tokenId) external override {
        require(
            hasRole(UNIQUENESS_MANAGER_ROLE, msg.sender) ||
                msg.sender == getAvatar(tokenId),
            "DavaV2: msg.sender is not authorized for deregisterCombinationData"
        );
        _deregisterCombinationData(tokenId);
    }

    function updateUniqueness(address avatarAddress) external override {
        require(isAvatar(avatarAddress), "DavaV2: not a avatar");

        uint256 originalId = originalIdOfAvatar[avatarAddress];
        bytes32 hashedData = _hashPartsData(IAvatar(avatarAddress).allParts());

        if (hashedData != bytes32(0)) {
            _deregisterCombinationData(originalId);
        }
    }

    function getPFP(uint256 tokenId)
        external
        view
        override
        returns (string memory)
    {
        (uint256 originalTokenId, uint256 newTokenId) = _getOriginalAndNewId(
            tokenId
        );

        require(
            _exists(newTokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        return IAvatar(getAvatar(originalTokenId)).getPFP();
    }

    function isApprovedOrOwner(address spender, uint256 newTokenId)
        external
        view
        override
        returns (bool)
    {
        return _isApprovedOrOwner(spender, newTokenId);
    }

    function getOriginalAndNewId(uint256 tokenId)
        external
        view
        returns (uint256, uint256)
    {
        return _getOriginalAndNewId(tokenId);
    }

    function isAvailableCombination(Part[] calldata parts)
        external
        view
        override
        returns (bool)
    {
        uint256 totalLayerAmount = getAllSupportedCategories().length;
        bytes32 hashedData = uniqueController.hashPartsData(
            parts,
            totalLayerAmount
        );

        if (hashedData == uniqueController.emptyHash()) {
            return true;
        } else {
            return !uniqueController.isUsed(hashedData);
        }
    }

    function frameCollection()
        external
        view
        override(IDavaV2, IWearable)
        returns (address)
    {
        return _frameCollection;
    }

    function refreshToken(uint256 tokenId) public override {
        (uint256 originalTokenId, uint256 newTokenId) = _getOriginalAndNewId(
            tokenId
        );

        address avatarOwner = ownerOf(newTokenId);
        require(
            msg.sender == avatarOwner ||
                msg.sender == getAvatar(originalTokenId),
            "Dava: msg.sender is not the owner of avatar"
        );
        _refreshToken(originalTokenId, avatarOwner);
    }

    function isAvatar(address avatarAddress)
        public
        view
        override
        returns (bool)
    {
        uint256 avatarId = originalIdOfAvatar[avatarAddress];
        if (avatarId > 0) {
            return true;
        }

        return getAvatar(0) == avatarAddress;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(IERC721MetadataUpgradeable, ERC721Upgradeable)
        returns (string memory)
    {
        (uint256 originalTokenId, uint256 newTokenId) = _getOriginalAndNewId(
            tokenId
        );

        if (_isBurnt(newTokenId)) {
            return burntImage;
        }
        require(
            _exists(newTokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        return IAvatar(getAvatar(originalTokenId)).getMetadata();
    }

    function ownerOf(uint256 tokenId)
        public
        view
        override(IERC721Upgradeable, ERC721Upgradeable)
        returns (address)
    {
        (, uint256 newTokenId) = _getOriginalAndNewId(tokenId);
        if (_isBurnt(newTokenId)) {
            return address(0);
        }
        return super.ownerOf(newTokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(
            IERC165Upgradeable,
            AccessControlUpgradeable,
            ERC721Upgradeable
        )
        returns (bool)
    {
        return
            interfaceId == type(IDavaV2).interfaceId ||
            interfaceId == type(IAccessControlUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function getAvatar(uint256 tokenId) public view override returns (address) {
        (uint256 originalTokenId, ) = _getOriginalAndNewId(tokenId);

        return super.getAvatar(originalTokenId);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}

    function _refreshToken(uint256 originalTokenId, address receiver)
        private
        returns (uint256 newTokenID)
    {
        // remove original tokenID pair
        uint256 oldNewTokenID = newIdOf[originalTokenId];
        _burn(oldNewTokenID);
        delete originalIdOf[oldNewTokenID];

        // mint new token with new ID
        newTokenID = nonceCounter;
        nonceCounter++;

        newIdOf[originalTokenId] = newTokenID;
        originalIdOf[newTokenID] = originalTokenId;

        _mint(receiver, newTokenID);

        emit MatchTokenId(originalTokenId, newTokenID);
    }

    function _deregisterCombinationData(uint256 tokenId) private {
        uniqueController.deregisterData(tokenId);
    }

    function _getOriginalAndNewId(uint256 tokenId)
        private
        view
        returns (uint256 originalId, uint256 newId)
    {
        if (tokenId < uint256(MAX_SUPPLY)) {
            newId = newIdOf[tokenId];
            originalId = tokenId;
        } else {
            newId = tokenId;
            originalId = originalIdOf[tokenId];
        }
    }

    function _isBurnt(uint256 newTokenId) private view returns (bool) {
        if (
            !_exists(newTokenId) &&
            newTokenId >= MAX_SUPPLY &&
            newTokenId < nonceCounter
        ) {
            return true;
        }
        return false;
    }

    function _hashPartsData(Part[] memory parts)
        private
        view
        returns (bytes32)
    {
        uint256 totalLayerAmount = getAllSupportedCategories().length;
        bytes32 hashedData = uniqueController.hashPartsData(
            parts,
            totalLayerAmount
        );

        return hashedData;
    }
}

