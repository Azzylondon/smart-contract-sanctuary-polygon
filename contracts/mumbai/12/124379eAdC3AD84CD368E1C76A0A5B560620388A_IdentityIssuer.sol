pragma solidity 0.8.9;

import "hardhat/console.sol";

struct VerificationBadge {
    address verifier;
    string vType;
    string vData;
    bool valid;
}

struct Identity {
    address owner;
    address issuer;
    string cipheredName;
}

struct Verifier {
    address verifier;
    string name;
    string url;
    string privacyPolicyUrl;
    string securityPolicyUrl;
    uint16 rateOfTrust; // 0.5% = 500; 100% = 10000;
    /** Cost for verification */
    uint256 vCost; // in wei
}

struct Validation {
    address verifier;
    uint256 sum;
    uint256 count;
}

contract IdentityIssuer {
    address public owner;
    address[] public verifiers;
    mapping(address => Verifier) public verifiersData;
    //mapping user address to his/her identity
    mapping(address => Identity) _identities;
    uint256 public identityCount;
    //mapping verifier address to list of identities verified by that verifier
    mapping(address => address[]) _verifiedIdentities;

    // validators are entities that needs to check a user identity, and can rate the trust of a verifier
    mapping(address => Validation) public validators;
    //mapping validator address to identity address mapping to list of verifications
    mapping(address => mapping(address => VerificationBadge[])) _validatorVerifications;

    mapping(address => string[]) issuesOfVerifiers;

    event NewVerifier(
        address indexed verifier,
        string name,
        string url,
        string privacyPolicyUrl,
        string securityPolicyUrl
    );
    event VerifierRemoved(address indexed verifier, string indexed name);
    event VerifierRated(
        address indexed verifier,
        uint256 indexed globalRate,
        address indexed judge,
        uint16 rateOfTrust
    );
    event IdentityVerified(address indexed idAddress, address indexed verifier, string vType, bool indexed verified);

    event IdentityCreated(address indexed idAddress);
    event IdentityDestroyed(address indexed idAddress);
    event AccessGranted(address indexed idAddress, address indexed verifier);
    event AccessRevoked(address indexed idAddress, address indexed verifier);

    event IdentityRead(address indexed idAddress, address indexed verifier);
    event BadgesRead(address indexed idAddress, address indexed verifier);
    event IssueOnIdentityRaised(address indexed idAddress, address indexed verifier, string indexed issue);

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    modifier onlyVerifier() {
        require(isVerifier(msg.sender), "Only verifiers can call this function");
        _;
    }

    modifier onlyVerifierIdentity() {
        require(verifiersData[msg.sender].verifier == msg.sender, "Only the verifier signer can call this function");
        _;
    }

    modifier onlyIdentityOwner() {
        require(_identities[msg.sender].owner == msg.sender, "Only the owner of the identity can call this function");
        _;
    }

    constructor() {
        owner = msg.sender;
    }

    function isVerifier(address _verifier) public view returns (bool) {
        return verifiersData[_verifier].verifier != address(0);
    }

    function addVerifier(
        address verifier,
        string memory name,
        string memory url,
        string memory privacyPolicyUrl,
        string memory securityPolicyUrl,
        uint256 vCost
    ) public onlyOwner {
        require(!isVerifier(verifier), "Verifier already exists");
        verifiers.push(verifier);
        verifiersData[verifier] = Verifier(verifier, name, url, privacyPolicyUrl, securityPolicyUrl, 0, vCost);
        emit NewVerifier(verifier, name, url, privacyPolicyUrl, securityPolicyUrl);
    }

    function removeVerifier(address verifier) public onlyVerifierIdentity {
        require(isVerifier(verifier), "Verifier does not exist");
        require(_verifiedIdentities[verifier].length == 0, "Cannot delete a Verifier that has verified identities");
        delete _verifiedIdentities[verifier];
        emit VerifierRemoved(verifier, verifiersData[verifier].name);
        for (uint256 i = 0; i < verifiers.length; i++) {
            if (verifiers[i] == verifier) {
                verifiers[i] = verifiers[verifiers.length - 1];
                verifiers.pop();
                break;
            }
        }
        delete verifiersData[verifier];
    }

    function getVerifiersCount() public view returns (uint256) {
        return verifiers.length;
    }

    function rateVerifier(address verifier, uint16 rateOfTrust) public onlyVerifier {
        require(rateOfTrust <= 10000 && rateOfTrust >= 0, "Rate of trust must be between 0 and 10000. 100 = 1%");
        require(isVerifier(verifier), "Verifier does not exist");
        require(verifiersData[verifier].verifier != address(0), "Verifier is not registered");
        if (validators[verifier].verifier == address(0)) {
            validators[verifier].verifier = verifier;
        }
        validators[verifier].count++;
        validators[verifier].sum += rateOfTrust;
        verifiersData[verifier].rateOfTrust = uint16((validators[verifier].sum * 100) / validators[verifier].count);
        emit VerifierRated(verifier, verifiersData[verifier].rateOfTrust, msg.sender, rateOfTrust);
    }

    function issueIdentity(string memory cipheredName) public {
        Identity storage id = _identities[msg.sender];
        id.owner = msg.sender;
        id.issuer = address(this);
        id.cipheredName = cipheredName;
        identityCount++;
        emit IdentityCreated(msg.sender);
    }

    function destroyIdentity() public onlyIdentityOwner {
        delete _identities[msg.sender];
        identityCount--;
        emit IdentityDestroyed(msg.sender);
    }

    function grantAccess(address verifier) public onlyIdentityOwner {
        require(isVerifier(verifier), "Verifier does not exist");
        require(!isAccessGranted(msg.sender, verifier), "Identity already verified by this verifier");
        _verifiedIdentities[verifier].push(msg.sender);
        emit AccessGranted(msg.sender, verifier);
    }

    function isAccessGranted(address idAddress, address verifier) public view returns (bool) {
        if (_verifiedIdentities[verifier].length == 0) {
            return false;
        }
        for (uint256 i = 0; i < _verifiedIdentities[verifier].length; i++) {
            if (_verifiedIdentities[verifier][i] == idAddress) {
                return true;
            }
        }
        return false;
    }

    function getIdentity(address idAddress) public returns (Identity memory) {
        require(
            msg.sender == idAddress || isAccessGranted(idAddress, msg.sender),
            "Verifier does not have access to this identity"
        );
        emit IdentityRead(idAddress, msg.sender);
        return _identities[idAddress];
    }

    function getBadgesOfVerifier(address idAddress, address verifier) public returns (VerificationBadge[] memory) {
        require(
            msg.sender == idAddress || isAccessGranted(idAddress, msg.sender),
            "Verifier does not have access to this identity"
        );
        emit BadgesRead(idAddress, msg.sender);
        return _validatorVerifications[verifier][idAddress];
    }

    function revokeAccess(address verifier) public onlyIdentityOwner {
        require(isVerifier(verifier), "Verifier does not exist");
        require(isAccessGranted(msg.sender, verifier), "Identity not verified by this verifier");
        for (uint256 i = 0; i < _verifiedIdentities[verifier].length; i++) {
            if (_verifiedIdentities[verifier][i] == msg.sender) {
                _verifiedIdentities[verifier][i] = _verifiedIdentities[verifier][
                    _verifiedIdentities[verifier].length - 1
                ];
                _verifiedIdentities[verifier].pop();
                break;
            }
        }
        emit AccessRevoked(msg.sender, verifier);
    }

    function verifyIdentity(
        address idAddress,
        string memory vType,
        string memory vData,
        bool verified
    ) public onlyVerifier {
        require(isVerifier(msg.sender), "Verifier does not exist");
        require(isAccessGranted(idAddress, msg.sender), "Identity not verified by this verifier");
        _validatorVerifications[msg.sender][idAddress].push(VerificationBadge(msg.sender, vType, vData, verified));
        if (verified) {
            emit IdentityVerified(idAddress, msg.sender, vType, verified);
        } else {
            emit IssueOnIdentityRaised(idAddress, msg.sender, vData);
        }
    }
}

