// SPDX-License-Identifier: MIT
pragma solidity 0.8.2;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155ReceiverUpgradeable.sol";
import "./Idrop.sol";
import "hardhat/console.sol";

contract Drop is Idrop, Initializable, ContextUpgradeable {
    using AddressUpgradeable for address;

    struct Order {
        uint256 amount;
        uint256 price;
    }
    address public dropSeller;
    uint256 public dropStartTime;
    uint256 public dropEndTime;
    //bytes4 private erc1155Interface = 0xd9b67a26;
    mapping(address => mapping(uint256 => Order)) public ordersByNftId;

    // solhint-disable-next-line func-name-mixedcase
    function __Drop_init(
        uint256 _dropStartTime,
        uint256 _dropEndTime,
        address _dropSeller
    ) public initializer {
        require(_dropSeller != address(0), "Invalid address");
        require(_dropStartTime < _dropEndTime, "Invalid timestamps");
        __Context_init();

        dropStartTime = _dropStartTime;
        dropEndTime = _dropEndTime;
        dropSeller = _dropSeller;
    }

    function createOrders(
        address _nftAddress,
        uint256[] memory _tokenIds,
        uint256[] memory _amounts,
        uint256[] memory _prices
    ) public override beforeStartTime {
        require(_nftAddress.isContract(), "No contract for this address");
        IERC1155 asset = IERC1155(_nftAddress);
        require(
            _tokenIds.length == _amounts.length &&
                _prices.length == _amounts.length,
            "Length mismatch in arrays"
        );
        for (uint256 index = 0; index < _tokenIds.length; index++) {
            require(
                asset.balanceOf(dropSeller, _tokenIds[index]) >=
                    _amounts[index],
                "Insufficient funds"
            );
            require(_prices[index] > 0, "Price should be greater than 0");
            _createOrder(
                _nftAddress,
                _tokenIds[index],
                _amounts[index],
                _prices[index]
            );
        }
    }

    function executeOrder(
        address caller,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _amount,
        uint256 _price
    )
        public
        override
        whenDropActive
        returns (bool isExecuteable, uint256 totalPrice)
    {
        (isExecuteable, totalPrice) = executeBatchOrder(
            caller,
            _nftAddress,
            _asSingletonArray(_tokenId),
            _asSingletonArray(_amount),
            _asSingletonArray(_price)
        );
    }

    function executeBatchOrder(
        address caller,
        address _nftAddress,
        uint256[] memory _tokenIds,
        uint256[] memory _amounts,
        uint256[] memory _prices
    )
        public
        override
        whenDropActive
        returns (bool isExecuteable, uint256 totalPrice)
    {
        require(
            _tokenIds.length == _amounts.length &&
                _prices.length == _amounts.length,
            "Length mismatch in arrays"
        );
        require(dropSeller != caller, "Unauthorized user");

        totalPrice = 0;
        for (uint256 index = 0; index < _tokenIds.length; index++) {
            Order memory order = ordersByNftId[_nftAddress][_tokenIds[index]];
            require(order.price == _prices[index], "The price is not correct");
            totalPrice += _prices[index];
            delete ordersByNftId[_nftAddress][_tokenIds[index]];
        }
        isExecuteable = true;
    }

    function cancelOrder(address _nftAddress, uint256 _tokenId)
        public
        override
    {
        _cancelOrder(_nftAddress, _tokenId);
    }

    function cancelBatchOrder(address _nftAddress, uint256[] memory _tokenIds)
        public
        override
    {
        for (uint256 index = 0; index < _tokenIds.length; index++) {
            _cancelOrder(_nftAddress, _tokenIds[index]);
        }
    }

    function _createOrder(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _amount,
        uint256 _price
    ) internal {
        ordersByNftId[_nftAddress][_tokenId] = Order(_amount, _price);
    }

    function _cancelOrder(address _nftAddress, uint256 _tokenId) internal {
        delete ordersByNftId[_nftAddress][_tokenId];
    }

    function _asSingletonArray(uint256 _element)
        private
        pure
        returns (uint256[] memory array)
    {
        array = new uint256[](1);
        array[0] = _element;
    }

    function getDropSeller()
        public
        view
        override
        returns (address _dropSeller)
    {
        return dropSeller;
    }

    modifier beforeDeadline() {
        require(block.timestamp < dropEndTime, "drop has ended");
        _;
    }
    modifier afterDeadline() {
        require(block.timestamp > dropEndTime, "drop has not yet ended");
        _;
    }
    modifier beforeStartTime() {
        require(block.timestamp < dropStartTime, "drop has started");
        _;
    }
    modifier afterStartTime() {
        require(block.timestamp >= dropStartTime, "drop has not yet started");
        _;
    }
    modifier whenDropActive() {
        require(block.timestamp >= dropStartTime, "drop has not started");
        require(block.timestamp < dropEndTime, "drop has ended");
        _;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity 0.8.2;

library DropData {
    struct Data {
        address nftContractAddress;
        uint256[] tokenIds;
        uint256[] amounts;
        uint256[] prices;
    }

    function splitData(Data memory _dropData)
        internal
        pure
        returns (
            address nftContractAddress,
            uint256[] memory tokenIds,
            uint256[] memory amounts,
            uint256[] memory prices
        )
    {
        nftContractAddress = _dropData.nftContractAddress;
        tokenIds = _dropData.tokenIds;
        amounts = _dropData.amounts;
        prices = _dropData.prices;
    }
}

