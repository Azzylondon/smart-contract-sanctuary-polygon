// SPDX-License-Identifier: MIT
pragma solidity ^0.8.2;
import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165CheckerUpgradeable.sol";
import "../../../collectibles/ERC1155/ICollectible1155.sol";
import "../../../payment-token/IERC20.sol";
import "./Drop.sol";
import "../library/DropData.sol";
import "hardhat/console.sol";
import "../../../helper/RobotdropFee.sol";
import "../../../helper/Beacon.sol";

contract DropFactory is OwnableUpgradeable, ReentrancyGuardUpgradeable {
    using DropData for DropData.Data;
    address public tokenBeacon;
    mapping(bytes32 => address) public cloneAddresses;
    mapping(bytes32 => address) public dropSellers;
    IERC20 public paymentToken;

    event DropCreated(
        bytes32 indexed cloneId,
        address indexed cloneAddress,
        address indexed caller
    );
    event OrderExecuted(
        bytes32 _dropId,
        address _nftAddress,
        uint256[] _tokenIds,
        uint256[] _amounts,
        uint256[] _prices
    );
    event OrderCancelled(
        bytes32 _dropId,
        address _nftAddress,
        uint256 _tokenId
    );
    event BatchOrderCancelled(
        bytes32 _dropId,
        address _nftAddress,
        uint256[] _tokenIds
    );

    // solhint-disable-next-line func-name-mixedcase
    function __DropFactory_init(address _paymentToken) public initializer {
        require(_paymentToken != address(0), "Invalid address");
        __ReentrancyGuard_init();
        __Ownable_init();
        Beacon _tokenBeacon = new Beacon(address(new Drop()));
        tokenBeacon = address(_tokenBeacon);
        paymentToken = IERC20(_paymentToken);
        _tokenBeacon.transferOwnership(_msgSender());
    }

    function createDrop(
        bytes32 _cloneId,
        uint256 _dropStartTime,
        uint256 _dropEndTime,
        DropData.Data[] memory _orders
    ) public nonReentrant returns (address cloneAddress) {
        require(
            cloneAddresses[_cloneId] == address(0),
            "Clone Id already exists"
        );
        require(
            _dropStartTime > block.timestamp && _dropEndTime > block.timestamp,
            "Invalid start or end time"
        );
        require(_dropStartTime < _dropEndTime, "Invalid timestamps");

        IERC1155 asset;
        for (uint256 index = 0; index < _orders.length; index++) {
            asset = IERC1155(_orders[index].nftContractAddress);
            require(
                asset.isApprovedForAll(_msgSender(), address(this)),
                "Approval is missing"
            );
        }
        BeaconProxy dropProxy = new BeaconProxy(
            tokenBeacon,
            abi.encodeWithSelector(
                Drop.__Drop_init.selector,
                _dropStartTime,
                _dropEndTime,
                _msgSender()
            )
        );

        cloneAddresses[_cloneId] = address(dropProxy);
        dropSellers[_cloneId] = _msgSender();

        Idrop drop = Idrop(address(dropProxy));
        for (uint256 index = 0; index < _orders.length; index++) {
            (
                address nftAddress,
                uint256[] memory assetIds,
                uint256[] memory amounts,
                uint256[] memory prices
            ) = _orders[index].splitData();
            drop.createOrders(nftAddress, assetIds, amounts, prices);
        }

        emit DropCreated(_cloneId, address(dropProxy), _msgSender());
        return address(dropProxy);
    }

    function executeOrder(
        bytes32 _dropId,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _amount,
        uint256 _price
    ) public nonReentrant {
        executeBatchOrder(
            _dropId,
            _nftAddress,
            _asSingletonArray(_tokenId),
            _asSingletonArray(_amount),
            _asSingletonArray(_price)
        );
    }

    function executeBatchOrder(
        bytes32 _dropId,
        address _nftAddress,
        uint256[] memory _tokenIds,
        uint256[] memory _amounts,
        uint256[] memory _prices
    ) public nonReentrant {
        require(
            AddressUpgradeable.isContract(_nftAddress),
            "NFT is not a contract"
        );
        require(
            cloneAddresses[_dropId] != address(0),
            "No drop exists for this id"
        );

        address dropClone = cloneAddresses[_dropId];
        Idrop drop = Idrop(dropClone);
        uint256 totalPrice;
        bool isExecutable;
        (isExecutable, totalPrice) = drop.executeBatchOrder(
            _msgSender(),
            _nftAddress,
            _tokenIds,
            _amounts,
            _prices
        );
        require(isExecutable, "The order cannot be executed");
        require(
            paymentToken.balanceOf(_msgSender()) >= totalPrice,
            "Insufficient payment tokens"
        );
        require(
            paymentToken.allowance(_msgSender(), address(this)) >= totalPrice,
            "Not enough allowance"
        );
        uint256 royaltyAmount = 0;
        address nftCreator;
        if (
            ERC165CheckerUpgradeable.supportsInterface(
                _nftAddress,
                type(ICollectible1155).interfaceId
            )
        ) {
            ICollectible1155 collectible = ICollectible1155(_nftAddress);
            nftCreator = collectible.getCreator();
            (uint256 royaltyBps, bool royaltyEnabled) = collectible
                .getRoyalty();
            if (royaltyEnabled) {
                royaltyAmount = RobotdropFee.getFeeAmount(
                    royaltyBps,
                    totalPrice
                );
            }
        }
        if (royaltyAmount > 0 && nftCreator != address(0)) {
            require(
                safeTransferFrom(_msgSender(), nftCreator, royaltyAmount),
                "Transfer of amount failed"
            );
        }
        require(
            safeTransferFrom(
                _msgSender(),
                drop.getDropSeller(),
                totalPrice - royaltyAmount
            ),
            "Transfer of amount failed"
        );
        IERC1155 asset = IERC1155(_nftAddress);
        asset.safeBatchTransferFrom(
            drop.getDropSeller(),
            _msgSender(),
            _tokenIds,
            _amounts,
            "0x00"
        );
        emit OrderExecuted(_dropId, _nftAddress, _tokenIds, _amounts, _prices);
    }

    function getCloneAddress(bytes32 _id)
        public
        view
        returns (address cloneAddress)
    {
        return cloneAddresses[_id];
    }

    function cancelOrder(
        bytes32 _dropId,
        address _nftAddress,
        uint256 _tokenId
    ) public nonReentrant {
        require(
            cloneAddresses[_dropId] != address(0),
            "No drop exists for this id"
        );
        address dropClone = cloneAddresses[_dropId];
        Idrop drop = Idrop(dropClone);
        drop.cancelOrder(_nftAddress, _tokenId);
        emit OrderCancelled(_dropId, _nftAddress, _tokenId);
    }

    function cancelBatchOrder(
        bytes32 _dropId,
        address _nftAddress,
        uint256[] memory _tokenIds
    ) public nonReentrant {
        require(
            cloneAddresses[_dropId] != address(0),
            "No drop exists for this id"
        );
        address dropClone = cloneAddresses[_dropId];
        Idrop drop = Idrop(dropClone);
        drop.cancelBatchOrder(_nftAddress, _tokenIds);
        emit BatchOrderCancelled(_dropId, _nftAddress, _tokenIds);
    }

    function implementation() public view returns (address) {
        Beacon _tokenBeacon = Beacon(tokenBeacon);
        return _tokenBeacon.implementation();
    }

    function beaconOwner() public view returns (address) {
        Beacon _tokenBeacon = Beacon(tokenBeacon);
        return _tokenBeacon.owner();
    }

    function safeTransferFrom(
        address _from,
        address _to,
        uint256 _amount
    ) private returns (bool success) {
        require(_from != address(0) && _to != address(0), "Invalid address");
        paymentToken.transferFrom(_from, _to, _amount);
        assembly {
            switch returndatasize()
            case 0 {
                // This is a non-standard ERC-20
                success := not(0) // set success to true
            }
            case 32 {
                // This is a compliant ERC-20
                returndatacopy(0, 0, 32)
                success := mload(0) // Set `success = returndata` of override external call
            }
            default {
                // This is an excessively non-compliant ERC-20, revert.
                revert(0, 0)
            }
        }
        require(success, "TOKEN_TRANSFER_IN_FAILED");
    }

    function setPaymentToken(address _paymentToken) public onlyOwner {
        require(_paymentToken != address(0), "Invalid address");
        paymentToken = IERC20(_paymentToken);
    }

    function _asSingletonArray(uint256 _element)
        private
        pure
        returns (uint256[] memory array)
    {
        array = new uint256[](1);
        array[0] = _element;
    }

    modifier onlyDropSeller(bytes32 _dropId) {
        require(
            _msgSender() == dropSellers[_dropId],
            "User is not an Drop seller"
        );
        _;
    }
}

