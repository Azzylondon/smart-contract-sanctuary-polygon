contract MarketplaceUpdated is Ownable, IMarketplaceUpdated {

    using Counters for Counters.Counter;
    Counters.Counter private lastListingId;

    using SafeERC20 for IERC20;

    bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;

    address private nft;
    address private PICTOS_WALLET;
    address private PICTOS_MARKET_FEE_WALLET;

    uint256 private pictosFeesPoint = 250; //250 basis points for precision -> 2,5%
    

    struct priceDistribution {
        uint256 sellerPayment;
        uint256 pictosFee;
        address creatorAddress;
        uint256 creatorRoyalties;
    }
    

    struct Listing {
        address seller;
        address currency;
        uint256 sellerId;
        uint256 tokenId;
        uint256 price;
        bool isSold;
        bool exist;
        string paymentMode;
        uint256 paymentId;
    }

    struct ListingOffer {
        address offerend;
        address currency;
        uint256 amount;
    }

    mapping(uint256=>Listing) public listings;
    mapping(uint256=>uint256) public tokensListing;
    mapping(uint256=>ListingOffer[]) public tokenListingOffers;

    modifier onlyItemOwner(uint256 tokenId) {
        isItemOwner(tokenId);
        _;
    }

    modifier onlyTransferApproval(uint256 tokenId) {
        isTransferApproval(tokenId);
        _;
    }

    function isItemOwner(uint256 tokenId) internal view{
        IERC721 token = IERC721(nft);
        require(token.ownerOf(tokenId) == _msgSender(), "Marketplace: Not the item owner");
    }

    function isTransferApproval(uint256 tokenId) internal view {
        IERC721 token = IERC721(nft);
        require(token.getApproved(tokenId) == address(this), "Marketplace: Marketplace is not approved to use this tokenId");
    }

    function setNFTContract(address _nft) external override onlyOwner {
        require(nft != _nft,"Marketplace: New NFT contract address have same value as the old one");
        nft = _nft;
        emit NftSet(_nft, msg.sender);
    }

    function setPictosWallet(address _pictosWallet) external override onlyOwner {
        require(PICTOS_WALLET != _pictosWallet, "Marketplace: new PictosWallet is the same as old one");
        PICTOS_WALLET = _pictosWallet;
        emit PictosWalletSet(_pictosWallet, msg.sender);
    }

    function setPictosMarketFeeWallet(address _pictosMarketFeeWallet) external override onlyOwner {
        require(PICTOS_MARKET_FEE_WALLET != _pictosMarketFeeWallet, "Marketplace: new PictosMarketFeeWallet is the same as old one");
        PICTOS_MARKET_FEE_WALLET = _pictosMarketFeeWallet;
        emit PictosMarketFeeWalletSet(_pictosMarketFeeWallet, msg.sender);
    }

     //Pictos and royalties are determined in basis points (base 10.000)
    function setPictosFeesPoint(uint256 _basisPoints) external override onlyOwner{
        require(_basisPoints != pictosFeesPoint, "New Market Fee is the same as before");
        pictosFeesPoint = _basisPoints;
    }

    

    function calculatePayments(uint256 tokenId, uint256 sellPrice) private view returns (uint256 pictosFee, address creatorAddress, uint256 creatorRoyalties) {
        pictosFee = (sellPrice*pictosFeesPoint)/10000;
        ERC721ControlledUpgradeable token = getToken();
        (creatorAddress, creatorRoyalties) = token.royaltyInfo(tokenId, sellPrice); 
    }

    function supportsInterface(bytes4 interfaceId) internal returns (bool){
        if(interfaceId == LibRoyaltiesV2._INTERFACE_ID_ROYALTIES) {
                return true;
        }

        if(interfaceId == _INTERFACE_ID_ERC2981) {
            return true;
        }

        return supportsInterface(interfaceId);
    }


    function listNative(uint256 userId, uint256 tokenId, uint256 price) external override onlyItemOwner(tokenId) onlyTransferApproval(tokenId) {
        lastListingId.increment();
        uint256 listingId = lastListingId.current();

        require( tokensListing[tokenId] == 0, "Marketplace: invalid listingId");
        require (price >= 0.5 ether, "Minimum Listing price, 0.5 MATIC ");


        
        tokensListing[tokenId] = listingId;

        Listing memory _list = listings[tokensListing[tokenId]];
        require(_list.exist == false, "Marketplace: List already exist");
        require(_list.isSold == false, "Marketplace: Can not list an already sold item");

        Listing memory newListing = Listing(
            msg.sender,
            address(0),
            userId,
            tokenId,
            price,
            false,
            true,
            "fiat",
            0
        );

        listings[listingId] = newListing;

        emit NewListing(listingId, tokenId, msg.sender, userId, price, address(0), block.timestamp);

    }

    function listToken(uint256 userId, uint256 tokenId, uint256 price, address currency) external override onlyItemOwner(tokenId) onlyTransferApproval(tokenId) {
        lastListingId.increment();
        uint256 listingId = lastListingId.current();

        require( tokensListing[tokenId] == 0, "Marketplace: invalid listingId");
        
        tokensListing[tokenId] = listingId;


        Listing memory _list = listings[tokensListing[tokenId]];
        require(_list.exist == false, "Marketplace: List already exist");
        require(_list.isSold == false, "Marketplace: Can not list an already sold item");
        require( currency != address(0), "Marketplace: Currency address is the native address");

        Listing memory newListing = Listing(
            msg.sender,
            currency,
            userId,
            tokenId,
            price,
            false,
            true,
            "crypto",
            0
        );

        listings[listingId] = newListing;


        emit NewListing(listingId, tokenId, msg.sender, userId, price, currency, block.timestamp);
    }

    // For case when Crypto users buy from Crypto users, we should assign ERC20 to seller and ERC721 to buyer
    function buyExactMatchNative(uint256 userId, uint256 tokenId, uint256 paymentId) external payable override{
        Listing storage _list = listings[tokensListing[tokenId]];

        require(_list.exist == true, "Marketplace: item does not exist");
        require(_list.price == msg.value, "Marketplace: not enough tokens send");
        require(_list.isSold == false,"Marketplace: item is already sold");
        require(_list.currency == address(0), "Marketplace: item currency is not the native one");
        require(_list.seller != msg.sender, "Marketplace: seller has the same address as buyer");

        ERC721ControlledUpgradeable token = getToken();
        token.safeTransferFrom(_list.seller, msg.sender, tokenId, "");

        priceDistribution memory price;

        //Marketplace Fees (calculated over total amount)
        (price.pictosFee, price.creatorAddress, price.creatorRoyalties) = calculatePayments(tokenId,msg.value);
        payable(PICTOS_MARKET_FEE_WALLET).transfer(price.pictosFee);
        //Royalties for creator (calculated over total amount)
        payable(price.creatorAddress).transfer(price.creatorRoyalties);
        //Remaining msg.value goes to seller
        price.sellerPayment = msg.value-(price.pictosFee + price.creatorRoyalties);
        payable(_list.seller).transfer(price.sellerPayment);

        console.log("sellerPayments", price.sellerPayment);
        console.log("pictosFee.", price.pictosFee);
        console.log("Creator Royalties", price.creatorRoyalties);

        _list.isSold = true;

        emit Sold(tokenId, _list.seller, msg.sender, _list.sellerId, userId, price.sellerPayment, price.pictosFee, price.creatorRoyalties, price.creatorAddress,
         address(0), "crypto", paymentId , block.timestamp);
        clearStorage(tokenId);
    }

 // For case when Crypto users buy from Fiat users, we should assign ERC20 and ERC721 ownership to our central account
    function buyExactMatchNativeThenAssignToOwner(uint256 userId, uint256 tokenId, uint256 paymentId ) external payable override{
        Listing storage _list = listings[tokensListing[tokenId]];
        require(_list.price == msg.value, "Marketplace: not enough tokens send");
        require(_list.isSold == false,"Marketplace: item is already sold");
        require(_list.exist == true, "Marketplace: item does not exist");
        require(_list.currency == address(0), "Marketplace: item currency is not the native one");
        require(_list.seller != msg.sender, "Marketplace: seller has the same address as buyer");
        ERC721ControlledUpgradeable token = getToken();
        token.safeTransferFrom(_list.seller, PICTOS_WALLET, tokenId, "");

        priceDistribution memory price;

        //Marketplace Fees (calculated over total amount)
        (price.pictosFee, price.creatorAddress, price.creatorRoyalties) = calculatePayments(tokenId,msg.value);
        payable(PICTOS_MARKET_FEE_WALLET).transfer(price.pictosFee);
        //Royalties for creator (calculated over total amount)
        (tokenId, msg.value);
        payable(price.creatorAddress).transfer(price.creatorRoyalties);
        //Remaining msg.value goes to Pictos Central Wallet
        price.sellerPayment = msg.value-(price.pictosFee + price.creatorRoyalties);
        payable(PICTOS_WALLET).transfer(price.sellerPayment);

        console.log("SellerPayments", price.sellerPayment);
        console.log("pictosFee", price.pictosFee);
        console.log("Creator Royalties", price.creatorRoyalties);

        _list.isSold = true;

        emit Sold(tokenId, _list.seller, msg.sender, _list.sellerId, userId, price.sellerPayment, price.pictosFee, price.creatorRoyalties, price.creatorAddress, address(0), "cryptoForOwner", paymentId , block.timestamp);
        clearStorage(tokenId);
    }

    /**
    * this function will be allowed to be called by owner only.
    * for FIAT payments, both from a Crypto or Fiat buyer.
    **/
    function buyExactMatchFiat(uint256 userId, uint256 tokenId, address buyer,uint256 paymentId) external onlyOwner override {

    Listing storage _list = listings[tokensListing[tokenId]];
    require(_list.isSold == false,"Marketplace: item is already sold");
    require(_list.exist == true, "Marketplace: item does not exist");
    require(_list.currency == address(0), "Marketplace: item currency is not the native one");
    require(_list.seller != buyer, "Marketplace: seller has the same address as buyer");

    ERC721ControlledUpgradeable token = getToken();
    token.safeTransferFrom(_list.seller, buyer, tokenId, "");

    priceDistribution memory price;

    //Does not execute transfers, just for the Sold event to have the information
    (price.pictosFee, price.creatorAddress, price.creatorRoyalties) = calculatePayments(tokenId,_list.price);
    (tokenId, _list.price);
    price.sellerPayment = _list.price-(price.pictosFee + price.creatorRoyalties);
    

    _list.isSold = true;

    emit Sold(tokenId, _list.seller, buyer, _list.sellerId, userId, price.sellerPayment, price.pictosFee, price.creatorRoyalties, price.creatorAddress, address(0), "fiat",  paymentId,  block.timestamp);
    clearStorage(tokenId);
}

    function getToken() internal view returns(ERC721ControlledUpgradeable) {
        ERC721ControlledUpgradeable token = ERC721ControlledUpgradeable(nft);
        return token;
    }


    function clearStorage(uint256 tokenId) internal {
        delete listings[tokensListing[tokenId]];
        delete tokensListing[tokenId];
    } 

    function updateListingPrice(uint256 tokenId, uint256 _newPrice) external override onlyOwner {

        Listing storage _list = listings[tokensListing[tokenId]];

        require(_list.isSold == false,"Marketplace: item is already sold");
        require(_list.exist == true, "Marketplace: item does not exist");
        require(_list.currency == address(0), "Marketplace: item currency is not the native one");
    

        _list.price = _newPrice;

        emit UpdatedListing(tokenId, _list.seller, _newPrice, address(0), block.timestamp);
    }

