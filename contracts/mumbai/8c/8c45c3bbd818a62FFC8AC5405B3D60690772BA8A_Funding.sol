                    } else if (funds[_id].currency == 1) {
                    ///@notice refund the backers of USDC
                        usdc.approve(address(this), donations[i].amount);
                        usdc.transferFrom(
                            address(this),
                            donations[_id].backer,
                            donations[_id].amount
                        );
                        donations[i].state = 2;
                        emit Refunded(
                            donations[_id].backer,
                            donations[_id].amount,
                            _id
                        );
