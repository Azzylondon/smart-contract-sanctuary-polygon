pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "./utils/ERC2981.sol";


contract ArtNFT is ERC2981, Initializable, ERC721Upgradeable, ERC721URIStorageUpgradeable, ERC721BurnableUpgradeable, PausableUpgradeable, AccessControlUpgradeable {
  using CountersUpgradeable for CountersUpgradeable.Counter;

  bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
  CountersUpgradeable.Counter private _tokenIdCounter;
  bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;
  bytes4 public constant _INTERFACE_ID_ERC165 = 0x01ffc9a7;
  bytes4 private constant _INTERFACE_ID_ERC721_METADATA = 0x5b5e139f;
  mapping(bytes4 => bool) private _supportedInterfaces;
  uint256 private mintingPrice;
  string public baseURI;

  // pending withdrawal of BNB
  address payable private banker;
  uint256 private pendingBanker;

  function setMintingPrice(uint256 _mintingPrice)  public {
    mintingPrice = _mintingPrice;
  }

  function getMintingPrice()  public view returns(uint256) {
    return mintingPrice;
  }

  event NewArt(
    string uri,
    uint256 indexed tokenId,
    address indexed to,
    uint96 feeNumerator
  );

  event Withdraw(
    uint256 indexed amount
  );

  function initialize(address payable _banker) initializer public {
    __ERC721_init("Art NFT", "UTA");
    __ERC721URIStorage_init();
    __ERC721Burnable_init();
    __Pausable_init();
    __AccessControl_init();

    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(PAUSER_ROLE, msg.sender);
    _setupRole(MINTER_ROLE, msg.sender);
    _registerInterface(_INTERFACE_ID_ERC2981);
    _registerInterface(_INTERFACE_ID_ERC165);
    _registerInterface(_INTERFACE_ID_ERC721_METADATA);
    banker = _banker;
  }

  function pause() public onlyRole(PAUSER_ROLE) {
    _pause();
  }

  function unpause() public onlyRole(PAUSER_ROLE) {
    _unpause();
  }

  // Mint with Artist's fee for BNB
  function mint(address to, string calldata uri, uint96 feeNumerator, uint96 _qty) public payable {
    require(msg.value >= mintingPrice, "Price does not match");

    for (uint i = 0; i < _qty; i++) {
      _tokenIdCounter.increment();
      uint256 tokenId = _tokenIdCounter.current();
      _mint(to, tokenId);
      _setTokenURI(tokenId, uri);
      _setTokenRoyalty(tokenId, to, feeNumerator);

      emit NewArt(
        uri,
        tokenId,
        to,
        feeNumerator
      );
    }

    pendingBanker += msg.value;
  }

  function _burn(uint256 tokenId)
    internal
    override(ERC721Upgradeable, ERC721URIStorageUpgradeable)        
  {
    super._burn(tokenId);
  }

  function tokenURI(uint256 tokenId)
    public
    view
    override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    returns (string memory)
  {
      return super.tokenURI(tokenId);
  }

  function supportsInterface(bytes4 interfaceId) public
    view
    override(ERC721Upgradeable, AccessControlUpgradeable, ERC2981)
    returns (bool)
  {
    return _supportedInterfaces[interfaceId];
  }

  function _registerInterface(bytes4 interfaceId) internal {
    require(interfaceId != 0xffffffff, "ERC165InterfacesSupported: invalid interface id");
    _supportedInterfaces[interfaceId] = true;
  }

  function withdraw() public {
    uint256 toTransfer = pendingBanker;
    pendingBanker = 0;
    // withdraw to banker address
    banker.transfer(toTransfer);
    emit Withdraw(toTransfer);
  }

  function pending() public view returns(uint256) {
    return pendingBanker;
  }

  // This fallback function 
  fallback() external payable
  {
    pendingBanker += msg.value;
  }

  receive() external payable {
    pendingBanker += msg.value;
  }

  function _baseURI() internal view virtual override returns (string memory)  {
    return baseURI;
  }

  function setBaseURI(string calldata baseTokenURI) public onlyRole(MINTER_ROLE) {
    baseURI = baseTokenURI;
  }
}

