pragma solidity ^0.8.9;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC165.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

contract Marketplace is Initializable, OwnableUpgradeable, PausableUpgradeable, AccessControlUpgradeable {
  using Counters for Counters.Counter;
  Counters.Counter private _auctionIds;
  Counters.Counter private _itemsSold;
  Counters.Counter private _itemsCancelled;

  uint private constant marketFeeDenominator = 10000;
  uint private marketFeeNumerator;
  uint256 public listingPrice;
  uint16 public priceTimeframeUpdate;
  address public bnbExchangeToken;
  address[] public exchangeTokens;
  address[] private validNFTs;
  bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;

  address payable private banker;
  uint256 private pendingBanker;

  struct AuctionItem {
    address payable seller;
    address payable owner;
    uint256 auctionId;
    address[] tokenAddresses;
    uint256[] tokenIds;
    uint256 startingPrice;
    uint256 endingPrice;
    address exchangeToken; // eth usdt
    uint256 duration;
    uint256 startingTimestamp;
    bool ongoing;
  }

  mapping(uint256 => AuctionItem) private idToAuctionItem;

  event AuctionCreated(
    address indexed seller,
    uint256 indexed auctionId,
    uint256 startingPrice,
    uint256 endingPrice,
    address exchangeToken,
    uint256 duration,
    uint256 startingTimestamp
  );

  event AuctionSuccessful(
    address indexed seller,
    address indexed buyer,
    uint256 indexed auctionId,
    address token,
    uint256 bidPrice
  );

  event AuctionCancelled(
    address indexed seller,
    uint256 auctionId
  );

  event Withdraw(
    uint256 indexed amount
  );

  function initialize(address[] memory _exchangeTokens, address payable _banker) public initializer {
    __Ownable_init();
    listingPrice = 0.01 ether;
    marketFeeNumerator = 250; // 2.5%
    _setPriceTimeframeUpdate(30); // each 30 minutes by default
    transferOwnership(msg.sender);
    exchangeTokens = _exchangeTokens;
    banker = _banker;
  }

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function setListingPrice(uint256 price) public onlyOwner {
    listingPrice = price;
  }

  function setPriceTimeframeUpdate(uint16 eachMinutes) public onlyOwner {
    _setPriceTimeframeUpdate(eachMinutes);
  }
  
  function _setPriceTimeframeUpdate(uint16 eachMinutes) internal {
    priceTimeframeUpdate = eachMinutes * 60;
  }

  function setMarketFee(uint256 fee) public onlyOwner {
    require(fee < marketFeeDenominator, "Fee should not be 100% or more");
    marketFeeNumerator = fee;
  }

  function setValidNFTs(address[] memory addresses) public onlyOwner {
    validNFTs = addresses;
  }

  function setBNBExchangeToken(address bnb) public onlyOwner {
    bnbExchangeToken = bnb;
  }

  function setExchangeTokens(address[] memory addresses) public onlyOwner {
    exchangeTokens = addresses;
  }

  function createAuction(
    address[] memory tokenAddresses,
    uint256[] memory tokenIds,
    uint256 startingPrice,
    uint256 endingPrice,
    address exchangeToken,
    uint256 duration
  ) public payable whenNotPaused {
    require(startingPrice > 0 && endingPrice > 0, "Price must be at least 1 wei");
    require(msg.value == listingPrice, "Price must be equal to listing price");
    require(tokenAddresses.length > 0 && tokenAddresses.length == tokenIds.length, "Invalid data");
    require(tokenIds.length <= 40, "Too much NFTs for this auction");
    // duration should be one year long or less and must be a multiple of priceTimeframeUpdate
    require(duration % priceTimeframeUpdate == 0 && duration <= 365 * 24 * 60, "Invalid duration");
    require(duration == 0 ? startingPrice == endingPrice : true, "If duration is zero starting and ending prices should be equal");
    require(_validNFTs(tokenAddresses), "Invalid token address");
    require(_validExchangeToken(exchangeToken), "Invalid exchange token");
    // duration is only used when the price is not static
    duration = startingPrice == endingPrice ? 0 : duration;

    _auctionIds.increment();
    uint256 auctionId = _auctionIds.current();
    uint256 startingTimestamp = block.timestamp;

    idToAuctionItem[auctionId] = AuctionItem(
      payable(msg.sender),
      payable(msg.sender),
      auctionId,
      tokenAddresses,
      tokenIds,
      startingPrice,
      endingPrice,
      exchangeToken,
      duration,
      startingTimestamp,
      true
    );

    pendingBanker += listingPrice;

    emit AuctionCreated(
      msg.sender,
      auctionId,
      startingPrice,
      endingPrice,
      exchangeToken,
      duration,
      startingTimestamp
    );
  }

  function settleAuction(
    address seller,
    uint256 bidPrice,
    uint256 auctionId
  ) public payable whenNotPaused {
    // CHECKS https://blog.openzeppelin.com/reentrancy-after-istanbul/
    require(idToAuctionItem[auctionId].seller == seller, "Invalid seller");
    require(msg.sender != seller, "Should not settle its own auction");
    require(idToAuctionItem[auctionId].ongoing == true, "Auction already ended");
    uint price = auctionItemPrice(auctionId);
    require(bidPrice == price, "Please submit the asking price in order to complete the purchase");
    // check if BNB is sended
    bool isMainToken = idToAuctionItem[auctionId].exchangeToken == bnbExchangeToken;
    if (isMainToken) {
      require(msg.value == bidPrice, "Should send enough ether");
    } else {
      require(msg.value == 0, "Should not send any ether");
    }

    // EFFECTS
    idToAuctionItem[auctionId].ongoing = false;
    // set new owner info
    idToAuctionItem[auctionId].owner = payable(msg.sender);
    _itemsSold.increment();

    // split payment between seller and mp (2.5 * money)
    uint256 fee = (bidPrice / marketFeeDenominator) * marketFeeNumerator;

    // ARTNFT  - check if the NFT support 2981 - only if there is one nft at sale
    address artist;
    uint256 artistFee;
    uint256 royaltyAmount;

    if (idToAuctionItem[auctionId].tokenAddresses.length == 1 &&
      IERC165(idToAuctionItem[auctionId].tokenAddresses[0]).supportsInterface(_INTERFACE_ID_ERC2981)) {
      // get royalty amount and artist address
      (artist, royaltyAmount) = IERC2981(idToAuctionItem[auctionId].tokenAddresses[0]).royaltyInfo(idToAuctionItem[auctionId].tokenIds[0], bidPrice); 
      // no royalties should be taken into account if artist is selling its own item
      if (artist == seller) {
        royaltyAmount = 0;
      } else {
        // fees will be distributed between artist and seller
        artistFee = (royaltyAmount / marketFeeDenominator) * marketFeeNumerator;
      }
    }

    // INTERACTIONS
    uint256 sellerAmount = bidPrice - royaltyAmount + artistFee - fee;
    uint256 artistAmount = royaltyAmount - artistFee;

    if (isMainToken) {
      // add to pending
      pendingBanker += fee;
      // transfer to seller
      idToAuctionItem[auctionId].seller.transfer(sellerAmount);
      if (royaltyAmount > 0) {
        // transfer to artist
        payable(artist).transfer(artistAmount);
      }
    } else {
      // transfer to banker
      IERC20(idToAuctionItem[auctionId].exchangeToken)
        .transferFrom(msg.sender, banker, fee);
      // transfer to seller
      IERC20(idToAuctionItem[auctionId].exchangeToken)
        .transferFrom(msg.sender, seller, sellerAmount);
      if (royaltyAmount > 0) {
        // transfer to artist
        IERC20(idToAuctionItem[auctionId].exchangeToken)
          .transferFrom(msg.sender, artist, artistAmount);
      }
    }

    // transfer lots of items
    for (uint i = 0; i < idToAuctionItem[auctionId].tokenAddresses.length; i++) {
      IERC721(idToAuctionItem[auctionId].tokenAddresses[i])
        .transferFrom(idToAuctionItem[auctionId].seller, msg.sender, idToAuctionItem[auctionId].tokenIds[i]);
    }

    emit AuctionSuccessful(
      idToAuctionItem[auctionId].seller,
      msg.sender,
      auctionId,
      idToAuctionItem[auctionId].exchangeToken,
      bidPrice
    );
  }

  function cancelAuction(uint256 auctionId) public whenNotPaused {
    require(idToAuctionItem[auctionId].ongoing == true, "Auction already ended");
    require(idToAuctionItem[auctionId].seller == msg.sender, "Not Authorized");

    _itemsCancelled.increment();
    idToAuctionItem[auctionId].ongoing = false;

    emit AuctionCancelled(
      msg.sender,
      auctionId
    );
  }

  function auctionItem(uint256 auctionId) public view returns (AuctionItem memory) {
    return idToAuctionItem[auctionId];
  }

  function auctionItemPrice(uint256 auctionId) public view returns (uint256) {
    uint256 startingPrice = idToAuctionItem[auctionId].startingPrice;
    uint256 endingPrice = idToAuctionItem[auctionId].endingPrice;
    uint256 duration = idToAuctionItem[auctionId].duration;

    if (duration == 0)
      return startingPrice;

    if (block.timestamp - idToAuctionItem[auctionId].startingTimestamp > duration)
      return endingPrice;

    uint256 remainingTime = duration - (block.timestamp - idToAuctionItem[auctionId].startingTimestamp);

    // change price after certain amount of time
    // giving users a reasonable timeframe to buy
    // without the price moving each block
    if (remainingTime % priceTimeframeUpdate > 0)
      remainingTime += priceTimeframeUpdate - remainingTime % priceTimeframeUpdate;

    uint256 addedPercent = (100 - (100 * remainingTime / duration));

    if (endingPrice > startingPrice)
      return addedPercent * (endingPrice - startingPrice) / 100 + startingPrice;
    else
      return startingPrice - addedPercent * (startingPrice - endingPrice) / 100;
  }

  function _validExchangeToken(address exchangeToken) internal view returns(bool) {
    for (uint i = 0; i < exchangeTokens.length; i++) {
      if (exchangeTokens[i] == exchangeToken) return true;
    }
    return false;
  }

  function _validNFTs(address[] memory addresses) internal view returns(bool) {
    for (uint i = 0; i < addresses.length; i++) {
      bool valid = false;
      for (uint256 j = 0; j < validNFTs.length; j++) {
        if (validNFTs[j] == addresses[i]) {
          valid = true;
          break;
        }
      }
      if (!valid) {
        return false;
      }
    }
    return true;
  }

  function withdraw() public {
    uint256 toTransfer = pendingBanker;
    pendingBanker = 0;
    // withdraw to banker address
    banker.transfer(toTransfer);
    emit Withdraw(toTransfer);
  }

  function pending() public view returns(uint256) {
    return pendingBanker;
  }

  // This fallback function 
  fallback() external payable
  {
    pendingBanker += msg.value;
  }

  receive() external payable {
    pendingBanker += msg.value;
  }
}

