            } else if (typ_hash == keccak256(bytes("sushi / quick - 2x1"))) {
                console.log("sushi / quick - 2x1");
                require(
                    trade_tokens[0] == trade_tokens[3],
                    "Does not end in the same token as startet"
                );
                uint256 amountOut0 = swapExactTokensForTokensSushi(
                    outputs[0],
                    outputs[1],
                    trade_tokens[0],
                    trade_tokens[1],
                    (block.timestamp + 300)
                );
