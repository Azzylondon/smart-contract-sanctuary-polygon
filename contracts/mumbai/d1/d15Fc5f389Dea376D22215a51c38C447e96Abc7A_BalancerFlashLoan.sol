                );
                console.log("Output: ", amountOut3);
                //require(amountOut3 > amount, "No Arbitrage");
            } else if (typ_hash == keccak256(bytes("sushi / quick - 2x2"))) {
                console.log("sushi / quick - 2x2");
                require(
                    trade_tokens[0] == trade_tokens[4],
                    "Does not end in the same token as startet"
                );
                uint256 amountOut0 = swapExactTokensForTokensSushi(
                    outputs[0],
                    outputs[1],
                    trade_tokens[0],
                    trade_tokens[1],
                    (block.timestamp + 300)
                );
                uint256 amountOut1 = swapExactTokensForTokensSushi(
                    amountOut0,
                    outputs[2],
                    trade_tokens[1],
                    trade_tokens[2],
