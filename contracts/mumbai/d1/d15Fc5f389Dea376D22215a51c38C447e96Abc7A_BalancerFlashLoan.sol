                );
                uint256 amountOut2 = swapExactTokensForTokensQuick(
                    amountOut1,
                    outputs[3],
                    trade_tokens[2],
                    trade_tokens[3],
                    (block.timestamp + 300)
                );
                console.log("Output: ", amountOut2);
                require(amountOut2 > amount, "No Arbitrage");
            }

            token.transfer(vault, amount);
            withdraw(tokens[i]);
        }
    }

    function flashLoan(
        IERC20[] memory tokens,
        uint256[] memory amounts,
        bytes memory userData
    ) external {
        IBalancerVault(vault).flashLoan(
            IFlashLoanRecipient(address(this)),
            tokens,
            amounts,
            userData
        );
    }
}

