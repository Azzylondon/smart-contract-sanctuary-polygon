                );
                //require(amountOut1 > amount, "No Arbitrage");
                if (amountOut1 > outputs[0]) {
                    console.log(
                        "We found a arbitrage opportunity - Winning trade"
                    );
                } else {
                    console.log(
                        "We have not found a arbitrage opportunity - Loss trade"
                    );
                }
            } else if (typ_hash == keccak256(bytes("sushi / quick - 1x1"))) {
                require(
