                uint256 amountOut0 = swapExactTokensForTokensSushi(
                    outputs[0],
                    outputs[1],
                    trade_tokens[0],
                    trade_tokens[1],
                    (block.timestamp + 300)
                );
                uint256 amountOut1 = swapExactTokensForTokensQuick(
                    amountOut0,
                    outputs[2],
                    trade_tokens[1],
                    trade_tokens[2],
                    (block.timestamp + 300)
                );
                uint256 amountOut2 = swapExactTokensForTokensQuick(
                    amountOut1,
