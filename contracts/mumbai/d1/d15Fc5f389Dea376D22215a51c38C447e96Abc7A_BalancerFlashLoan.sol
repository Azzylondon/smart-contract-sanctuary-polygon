                uint256 amountOut2 = swapExactTokensForTokensQuick(
                    amountOut1,
                    outputs[3],
                    trade_tokens[2],
                    trade_tokens[3],
                    (block.timestamp + 300)
                );
                uint256 amountOut3 = swapExactTokensForTokensQuick(
                    amountOut2,
                    outputs[4],
                    trade_tokens[3],
                    trade_tokens[4],
                    (block.timestamp + 300)
                );
                console.log("Output: ", amountOut3);
                require(amountOut3 > amount, "No Arbitrage");
            } else if (typ_hash == keccak256(bytes("quick / sushi - 1x1"))) {
                require(
                    trade_tokens[0] == trade_tokens[2],
                    "Does not end in the same token as startet"
