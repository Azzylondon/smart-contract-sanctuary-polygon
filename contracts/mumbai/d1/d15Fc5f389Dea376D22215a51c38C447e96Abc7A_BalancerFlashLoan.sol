                console.log("Output: ", amountOut2);
                require(amountOut2 > amount, "No Arbitrage");
            } else if (typ_hash == keccak256(bytes("sushi / quick - 1x2"))) {
                console.log("sushi / quick - 1x2");
                require(
                    trade_tokens[0] == trade_tokens[3],
                    "Does not end in the same token as startet"
