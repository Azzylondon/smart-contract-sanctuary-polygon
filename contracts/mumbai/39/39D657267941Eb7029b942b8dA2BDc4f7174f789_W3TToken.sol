pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import  "./BancorBondingCurve1.sol";
import "hardhat/console.sol";

contract W3TToken is BancorBondingCurve1, Ownable, ERC20 {
	uint private reserveBalance;
	uint private maxGasPrice = SCALE;
	uint32 private brr; //buying reserved ratio
	uint32 private sd;  // sell discount 0.9 means you are getting 90% from the bonding curve at selling
    bool buyActive = true;
    bool sellActive = false;
	event BuyEvent(string title, address buyer, uint received, uint sent);
    event SellEvent(string title, address seller, uint received, uint sent);
    event WithdrawEvent(string title, uint sent);

    constructor(
    	string memory _name, 
    	string memory _symbol,
        uint _supplies,
    	uint _initPrice,
        uint32 _brr, 
        uint32 _sd    
    ) ERC20(_name, _symbol) {
        _mint(_msgSender(), _supplies);
        brr = _brr;
        sd = _sd;
        reserveBalance = findBalance(_supplies, _initPrice, _brr);
    }

     modifier validGasPrice() {
        require(tx.gasprice <= maxGasPrice, "Too much gas for this transaction");
        _;
    }

    modifier isBuyEnabled(){
        require(buyActive==true, "Buy function has been disabled");
        _;
    }

    modifier isSellEnabled(){
        require(sellActive==true, "Sell function has been disabled");
        _;
    }

    function setMaxGasPrice(uint256 gasPrice) public onlyOwner {
        maxGasPrice = gasPrice;
    }

    function setBuyActive(bool val) public onlyOwner{
        buyActive = val;
    }

    function setSellActive(bool val) public onlyOwner{
        sellActive = val;
    }

    function buy(address receiver) payable public isBuyEnabled validGasPrice{
        require(msg.value > 0, "Insufficient gas Tokens");
        uint supplies = totalSupply();
        uint tokens = calculatePurchaseReturn(supplies, reserveBalance, brr, msg.value);
        _mint(receiver, tokens);
        reserveBalance = reserveBalance + msg.value;
        emit BuyEvent("Token: Buy Transaction", _msgSender(), msg.value, tokens);
    }

    function sell(uint tokens) public isSellEnabled validGasPrice{
        uint supplies = totalSupply();
        require(tokens>0, "Invalid amount");
        require(tokens<=supplies, "Token amount exceeds total supplies");
        uint balance = calculateSaleReturn(supplies, reserveBalance, brr, tokens);
        uint returnBalance = calculateParallelSaleReturn(sd, balance);
        require(address(this).balance >= returnBalance, "Insufficient balance remained in this token Contract");
        _burn(_msgSender(), tokens);
        payable(_msgSender()).transfer(returnBalance);
        reserveBalance = reserveBalance - balance;
        emit SellEvent("Token: Sell Transaction", _msgSender(), tokens, returnBalance);
    }

    function withdraw() payable onlyOwner public{
        payable(_msgSender()).transfer(address(this).balance);
        emit WithdrawEvent("Token: Withdraw Transaction", address(this).balance);
    }

    function tokenInfo() public view returns (uint [] memory data){
        data = new uint[](5);
        data[0] = totalSupply();
        data[1] = reserveBalance;
        data[2] = brr;
        data[3] = sd;
        data[4] = address(this).balance;
    }

    function buyInfo(uint value) public view returns (uint [] memory data){
        data = new uint[](2);
        uint supplies = totalSupply();
        data[0] = supplies;
        uint tokens = calculatePurchaseReturn(supplies, reserveBalance, brr, value); 
        data[1] = tokens;
    }

    function sellInfo(uint tokens) public view returns (uint [] memory data){
        data = new uint[](2);
        uint supplies = totalSupply();
        data[0] = supplies;
        uint balance = calculateSaleReturn(supplies, reserveBalance, brr, tokens);
        uint returnBalance = calculateParallelSaleReturn(sd, balance);
        data[1] = returnBalance;
    }

    function ppt() public view returns (uint256)
    {
        return super.pricePerToken(totalSupply(), reserveBalance, brr);
    }
}

