pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "./erc721/QuantumAccelerator/QuantumAccelerator.sol";
import "./common/CommonLib.sol";

contract NFTSale is
    Initializable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    ERC721HolderUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using AddressUpgradeable for address;

    QuantumAccelerator public quantumAccelerator;

    mapping (address => bool) public _listOperator;
    mapping (address => bool) public _whitelistedWalletSales;
    mapping (address => bool) public _whitelistedCurrency;
    mapping (bytes => address) public _listCouponOfAffiliates;
    mapping (address => uint256) public _listPriceMintNFTByCurrency;
    mapping (address => bool) public _listWalletBuyNFT;

    bool public _isPublicSellNFT;
    uint256 public _maxNFTCanMint;
    uint256 public _totalNFTMinted;
    uint256 public _totalNFTMintedByWalletSale;
    address public _receiveFeeWallet;

    // Event
    event SetOperator(address operator, bool isOperator);
    event SetListWalletSale(address[] listWalletSale);
    event AddWhitelistedCurrencyAndPrice(address currency, bool isCurrencySupport, uint256 price);
    event AddCouponOfAffiliates(string coupon, address walletAffiliate);
    event SetMaxNFTCanMint(uint256 maxNFTCanMint);
    event SetReceiveFeeWallet(address receiveFeeWallet);
    event BuyNFTMint(address owner, address currency, uint256 fee, string coupon);
    event SetIsPublicSellNFT(bool isPublicSellNFT);
    
    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function addOperator(address operator, bool isOperator) public onlyOwner {
        require (!operator.isContract(), "operator-invalid");
        _listOperator[operator] = isOperator;
        emit SetOperator(operator, isOperator);
    }

    function addWhitelistedWallets(address[] memory listWalletSale) external onlyOwner {
        for (uint256 i = 0; i < listWalletSale.length; i++) {
            _whitelistedWalletSales[listWalletSale[i]] = true;
        }
        emit SetListWalletSale(listWalletSale);
    }

    function setWhitelistedCurrencyAndPrice(address currency, bool isCurrencySupport, uint256 price) external onlyOwner {
        if(currency != address(0)){
            require (currency.isContract(), "currency-invalid");
        }
        _whitelistedCurrency[currency] = isCurrencySupport;
        _listPriceMintNFTByCurrency[currency] = price;
        emit AddWhitelistedCurrencyAndPrice(currency, isCurrencySupport, price);
    }

    function setListCouponOfAffiliates (string memory couponCode, address walletAffiliate) external onlyOwner {
        require(_listCouponOfAffiliates[bytes(couponCode)] == address(0), "code-coupon-exists");
        _listCouponOfAffiliates[bytes(couponCode)] = walletAffiliate;
        emit AddCouponOfAffiliates(couponCode, walletAffiliate);
    }

    function setMaxNFTCanMint (uint256 maxNFTCanMint) external onlyOwner {
        require(maxNFTCanMint > 0, "max-nft-can-mint-invalid");
        _maxNFTCanMint = maxNFTCanMint;
        emit SetMaxNFTCanMint(_maxNFTCanMint);
    }

    function setReceiveFeeWallet (address receiveFeeWallet) external onlyOwner {
        require(!receiveFeeWallet.isContract(), "receive-fee-wallet-invalid");
        _receiveFeeWallet = receiveFeeWallet;
        emit SetReceiveFeeWallet(_receiveFeeWallet);
    }

    function setPublicSellNFT (bool isPublicSellNFT) external onlyOwner {
        require(
            _totalNFTMintedByWalletSale == 200, 
            "can-not-set-public-sell-nft"
        );
        _isPublicSellNFT = isPublicSellNFT;
        emit SetIsPublicSellNFT(_isPublicSellNFT);
    }

    function buyNFT (
        address currency,
        string memory couponCode
    ) external whenNotPaused {
        if(!_isPublicSellNFT){
            require(_whitelistedWalletSales[msg.sender], "not-white-list-sale-buy");
        }

        require(!_listWalletBuyNFT[msg.sender],"can-not-buy-nft");

        require(
            _maxNFTCanMint > 0 && _totalNFTMinted <= _maxNFTCanMint,
            "can-not-mint-nft"
        );

        require(_receiveFeeWallet != address(0), "receive-fee-wallet-not-setup");

        require(
            _whitelistedCurrency[currency] && _listPriceMintNFTByCurrency[currency] > 0,
            "currency-not-setup"
        );

        uint256 feeNFTMint = 0;
        if(_whitelistedWalletSales[msg.sender]){
            feeNFTMint = caculateFeeWhenWalletSalerBuyNFT(currency);

            _totalNFTMintedByWalletSale ++;

            _whitelistedWalletSales[msg.sender] = false;

            IERC20Upgradeable(currency).safeTransferFrom(
                msg.sender,
                _receiveFeeWallet,
                feeNFTMint
            );
        }else{
            (uint256 feeMint, uint256 discount, address walletAffiliate) = caculateFeeWhenSellPublic(currency,couponCode);

            feeNFTMint = feeMint;

            if(discount > 0 && walletAffiliate != address(0)){
                IERC20Upgradeable(currency).safeTransferFrom(
                    msg.sender,
                    _receiveFeeWallet,
                    feeNFTMint - discount
                );

                IERC20Upgradeable(currency).safeTransferFrom(
                    msg.sender,
                    walletAffiliate,
                    discount
                );
            }else{
                IERC20Upgradeable(currency).safeTransferFrom(
                    msg.sender,
                    _receiveFeeWallet,
                    feeNFTMint
                );
            }
        }
        quantumAccelerator.mintTo(msg.sender);

        _totalNFTMinted ++;

        _listWalletBuyNFT[msg.sender] = true;

        emit BuyNFTMint(msg.sender, currency, feeNFTMint, couponCode);
    }

    function caculateFeeWhenWalletSalerBuyNFT (address currency) internal returns (uint256 feeMint){
        if(_totalNFTMintedByWalletSale <= 100){ // 5 when test
            feeMint =  _listPriceMintNFTByCurrency[currency] - _listPriceMintNFTByCurrency[currency]/100*10;
        }else if (_totalNFTMintedByWalletSale <= 200){ // 10 when test
            feeMint = _listPriceMintNFTByCurrency[currency] - _listPriceMintNFTByCurrency[currency]/100*7;
        }
    }

    function caculateFeeWhenSellPublic (address currency, string memory couponCode) internal returns (uint256 feeMint, uint256 discount, address walletAffiliate){
        if((keccak256(bytes(couponCode)) != keccak256(bytes("")))){
            require(
                _listCouponOfAffiliates[bytes(couponCode)] != address(0),
                "coupon-code-invalid"
            );
            discount = _listPriceMintNFTByCurrency[currency]/100*5;
            feeMint = _listPriceMintNFTByCurrency[currency] - discount;
            walletAffiliate = _listCouponOfAffiliates[bytes(couponCode)];
        } else{
            feeMint = _listPriceMintNFTByCurrency[currency];
            discount = 0;
            walletAffiliate = address(0);
        }
    }

    // constructor() initializer {}

    function initialize(address quantumAcceleratorAddress) public initializer {
        __UUPSUpgradeable_init();
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        __ERC721Holder_init();
        addOperator(msg.sender, true);
        _totalNFTMinted = 0;
        _totalNFTMintedByWalletSale = 0;
        quantumAccelerator = QuantumAccelerator(quantumAcceleratorAddress);
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}
}

