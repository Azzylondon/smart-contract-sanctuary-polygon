import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "contracts/relayer/BasicMetaTransaction.sol";
import "./upgradeability/CustomOwnable.sol";
import "./interfaces/brainchainEvents.sol";
import "./BadgeToken.sol";
import "./NFT.sol";
import "./interfaces/Iadmin.sol";
import "./interfaces/Iuser.sol";
import "hardhat/console.sol";
import "contracts/relayer/BasicMetaTransaction.sol";


contract Question is BasicMetaTransaction, ReentrancyGuard, CustomOwnable , brainchainEvents{
    
    mapping(string => bool) public questionURI; //QuestionHash => address of creater
    mapping(uint => bool) public question_NFT;


    bool internal isInitialized;
    address public root;

    Iadmin public admin;
    
    function initialize(address _root,address _admin) public {
        require(!isInitialized,"initialized");
        isInitialized = true;
        admin = Iadmin(_admin);
        _setOwner(_root);
        root = _root;
    }

    function createQuestionInBatch(string[] calldata _URI, string[] calldata _hash) external{
        require(Iuser(admin.userContract()).isUserExist(_msgSender()),"user not exists");
        require(_URI.length == _hash.length,"URI length not equal to hash length");
        for(uint i = 0; i < _URI.length; i++){
            require(!questionURI[_hash[i]],"hash id is not there in questionURI");
            NFT(admin.Nft()).mint(_msgSender(), admin.counterId(), _URI[i]);
            questionURI[_hash[i]] = true;
            question_NFT[admin.counterId()] = true;
            admin.increaseCounterID();
            BadgeToken(admin.ExpNTT())._mint(_msgSender(), uint(admin.newQuestion())* 10 ** 18);
            emit MintPoints("newQuestion",_msgSender(), admin.newQuestion());
            emit QuestionCreation(admin.counterId()-1, _URI[i]);
        }
    }
}

