pragma solidity ^0.8.0;

import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "./FanTigerNFT.sol";
import "hardhat/console.sol";

/** 
 *   Purchase a Fan Tiger Digital Asset without a reservation 
 *   but rather with proof that we generate and provide the purchaser offline. 
 *   Which token is being sold is calculated at time of sale. 
*/ 

contract PrimaryPurchase is 
    OwnableUpgradeable,
    PausableUpgradeable {
    
    using SafeERC20 for IERC20;

    // Events
    event BatchOnChainTransfer(address indexed buyer, uint256[] nftIDs, uint256[] amounts, bytes32 paymentID);
    
    event BatchOffChainTransfer(address indexed buyer, uint256[] nftIDs, uint256[] amounts, bytes32 paymentID);
    
    event TierSoldOut(uint256 indexed tierID, address indexed fantigerContract, uint256 numberOfTokenInTier);

    struct Tier {
        
        bool exists;
        bool exhausted;

        // The maximum NFT 
        uint64 maxNftPerWallet;

        // The tokenId of next token to sell in the tier
        uint64 nextTokenToSell;

        // The maximum supply of tier
        uint64 maxSupply;

        // The price of the whole tier
        uint256 purchasePrice;
        
        // Address for erc20 contract whom we would take in for payment
        address erc20PaymentContractForTier;

        // Address for the contract who is holding the data
        address fanTigerContract;

        // The wallet where the pre minted address 
        address nftHolderWallet;        
        
        
    }

    mapping(uint256 => Tier) public tierData;

    // (paymentID => used) track which paymentIDs we've used
    mapping(bytes32=> bool) public paymentIDsUsed;

    // Stores the number of tokens user has for given tier
    // mapping ( buyer address => (tierId => buyer owner quantity))
    mapping(address => mapping (uint256 => uint256)) tokenOwnerQuantity;

    // Payments are sent to this wallet address
    address private erc20PaymentWallet;

    function validateTierParameters(
        uint256 tierID,
        uint64 maxNftPerWallet, 
        address erc20Contract, 
        address fanTigerContract, 
        address nftHolderForTier
    ) private pure
    {
        require(tierID > 0 && tierID < 0x100000000000000000000000000000000 , "Invalid tierID");
        require(maxNftPerWallet > 0 , "maximum nft limit cannot be zero");
        require(erc20Contract != address(0),"Null address passed for the erc20Contract");
        require(fanTigerContract != address(0),"Null address passed for the fanTigerContract");
        require(nftHolderForTier != address(0),"Null address passed for the nftHolderWallet");
    }

    function validateDataForPurchase(
        uint256 tierID, 
        address buyer, 
        uint256 quantity,
        bytes32 paymentID,
        bytes calldata proof
    ) private view returns (Tier memory tierInformation)
    {
        require(tierID > 0 && tierID < 0x100000000000000000000000000000000 , "Invalid tierID");
        tierInformation = tierData[tierID];
        require(tierInformation.exists, "Tier unregistered");

        // console.log("Purchase qty : %d, Next Token to sell : %d, Max Supply : %d",quantity,tierInformation.nextTokenToSell,tierInformation.maxSupply);

        require(tierInformation.nextTokenToSell <= tierInformation.maxSupply, "Tier Exhausted");
        require(!paymentIDsUsed[paymentID],"PaymentID already Used");
        require(quantity -1 + tierInformation.nextTokenToSell <=tierInformation.maxSupply ,"Not Sufficient Supply to buy all the nfts from the tier");
        require(quantity + tokenOwnerQuantity[buyer][tierID]  <= tierInformation.maxNftPerWallet,'Buyer buying more than max allowed');

        requireValidSignature(tierID, buyer, paymentID, proof);
    }

    
    function prepareDataForNFTTransfer(
        Tier memory tierInformation,
        uint256 tierID,
        uint256 quantity
    ) private pure returns(Tier memory,uint256[] memory,uint256[] memory )
    {
        uint256[] memory nftIDs = new uint256[](quantity);
        uint256[] memory amounts = new uint256[](quantity);
        uint256 nextTokenToSell = uint256(tierInformation.nextTokenToSell);

        for(uint i = 0 ; i < quantity;i++) {
            amounts[i] = 1;
            nftIDs[i] = uint256(tierID << 128)+(nextTokenToSell);
            nextTokenToSell++;
        }

        // Preventing Reentrancy Attacks
        tierInformation.nextTokenToSell = uint64(nextTokenToSell);
        tierInformation.exhausted = (tierInformation.maxSupply < uint256(tierInformation.nextTokenToSell)) ? true : false;
        return (tierInformation,nftIDs,amounts);
    }

    function initialize(address paymentWallet) public initializer {
        __Ownable_init();
        __Pausable_init();
        erc20PaymentWallet = paymentWallet;
    }

    function updatePaymentWallet(address newWallet) external onlyOwner whenNotPaused {
        erc20PaymentWallet = newWallet;
    }

    function decomposeTokenID(uint256 tokenID) public pure returns(uint256 tierID,uint256 tokenNo){
        tierID = tokenID>>128;
        tokenNo = tokenID & 0xffffffffffffffff;
    }
    function composeTokenID(uint256 tierID,uint256 tokenNo) public pure returns (uint256 tokenID) {
        require(tierID > 0 && tierID < 0x100000000000000000000000000000000 , "tierID outside range");
        require(tokenNo > 0 && tokenNo < 0x100000000000000000000000000000000 , "tokenNo outside range");
        tokenID= (tierID << 128) + tokenNo;
    }

    // @note Puts a tier up for sale 
    // setBuyPrice in wei units for this ERC-20 contract
    // The priceWei can be 0, for the case of the airdrop
    function registerTierForPurchase(uint256 tierID, uint256 priceWei, uint64 maxNftPerWallet, address erc20Contract, address fanTigerContract, address nftHolderForTier) external onlyOwner whenNotPaused {
        
        validateTierParameters(tierID, maxNftPerWallet, erc20Contract, fanTigerContract, nftHolderForTier);

        Tier memory tierInformation = tierData[tierID];
        (bool fanTigerTierExists,,,uint64 maxSupply) = FanTigerNFT(fanTigerContract).readTierData(uint256(tierID));

        //The tier cannot be registered for purchase if it has not itself been created, in the FanTigerNFT contract
        require(fanTigerTierExists,"NFT Tier has not been created");

        //The tier has already been registered for purchase 
        require(tierInformation.exists == false,"Tier already registered");

        // Track the state
        tierData[uint256(tierID)] = Tier(
            true,
            false,
            maxNftPerWallet,
            uint64(1),
            maxSupply,
            priceWei,
            erc20Contract,
            fanTigerContract,
            nftHolderForTier            
        );
    }

    function updateTierPurchaseRegistration(uint256 tierID, uint256 priceWei, uint64 maxNftPerWallet ,address erc20Contract, address fanTigerContract, address nftHolderForTier) external onlyOwner whenNotPaused {

        validateTierParameters(tierID, maxNftPerWallet, erc20Contract, fanTigerContract, nftHolderForTier);

        Tier memory tierInformation = tierData[tierID];
        require(tierInformation.exists == true,"Tier has not been registered");
        require(tierInformation.exhausted == false,"Tier has been exhausted");
        
        tierInformation.erc20PaymentContractForTier = erc20Contract;
        tierInformation.purchasePrice = priceWei;
        tierInformation.fanTigerContract = fanTigerContract;
        tierInformation.nftHolderWallet = nftHolderForTier;
        tierInformation.maxNftPerWallet = maxNftPerWallet;

        tierData[tierID] = tierInformation;
    }
    

    // Need to check for not sufficient balance
    function BatchTransferForOnChainPurchase(
        uint256 tierID, 
        address buyer, 
        uint256 quantity,
        bytes32 paymentID,
        bytes calldata proof
    ) external onlyOwner whenNotPaused {
        // Validate that all the parameters entered are correct
        Tier memory tierInformation = validateDataForPurchase(tierID,buyer,quantity,paymentID,proof);

        // Seeting the paymentID variable so as to prevent payment from same paymentID
        paymentIDsUsed[paymentID] = true;
        tokenOwnerQuantity[buyer][tierID] += quantity;
        
        uint256[] memory nftIDs;
        uint256[] memory amounts;
        (tierInformation,nftIDs, amounts) = prepareDataForNFTTransfer(tierInformation,tierID,quantity);
        // Setting all the values to prevent re-entrancy attack
        tierData[tierID] = tierInformation;
        

        address paymentContractAddress = tierInformation.erc20PaymentContractForTier;
        
        ERC1155Upgradeable fanTigerContract = ERC1155Upgradeable(tierInformation.fanTigerContract);
        // uint256 startingBalance = IERC20(tierInformation.erc20PaymentContractForTier).balanceOf(erc20PaymentWallet);
        uint256 totalPurchaseAmount = tierInformation.purchasePrice * quantity;

        // Check that we have an allowance and balance of atleast the purchase price
        require(IERC20(paymentContractAddress).allowance(buyer, address(this)) >= tierInformation.purchasePrice, "insufficient allowance remaining for purchase");
        require(IERC20(paymentContractAddress).balanceOf(buyer) >= totalPurchaseAmount,"insufficient balance remaining for purchase");

        IERC20(paymentContractAddress).safeTransferFrom(buyer, erc20PaymentWallet,totalPurchaseAmount);
        
        fanTigerContract.safeBatchTransferFrom(tierInformation.nftHolderWallet, buyer, nftIDs, amounts, "");
        // require(IERC20(tierInformation.erc20PaymentContractForTier).balanceOf(erc20PaymentWallet) == (startingBalance + totalPurchaseAmount), "not paid the exact amount");

        emit BatchOnChainTransfer(buyer, nftIDs, amounts, paymentID);
        
       if(tierInformation.exhausted){
            emit TierSoldOut(tierID, tierInformation.fanTigerContract, tierInformation.maxSupply);
        }
    }

    // The proof is Signature of (tierID || buyer || paymentID) using contract owner's private key
    // If changing the owner of the contract, then for all the pending buys, 
    // the proof should be reconstructed as the owner's private key has been changed

    /**
     * @dev Transferring multiple nfts of same tier to the given user.
     * tierID : The ID of tier of the nfts to be bought
     * buyer  : The address of buyer who is going to buy the nfts
     * quantity : The number of nfts, the user wants to buy for the tierID
     * proof : The proof generated by the FanTiger.com that user is the buyer
     */
    function BatchTransferForOffChainPurchase(
        uint256 tierID,
        address buyer, 
        uint256 quantity,
        bytes32 paymentID,
        bytes calldata proof
    ) external onlyOwner whenNotPaused {
        // Validate that all the parameters entered are correct
        Tier memory tierInformation = validateDataForPurchase(tierID,buyer,quantity,paymentID,proof);

        // Seeting the paymentID variable so as to prevent payment from same paymentID
        paymentIDsUsed[paymentID] = true;
        tokenOwnerQuantity[buyer][tierID] += quantity;

        uint256[] memory nftIDs;
        uint256[] memory amounts;
        (tierInformation,nftIDs, amounts) = prepareDataForNFTTransfer(tierInformation,tierID,quantity);

        // Setting all the values to prevent re-entrancy attack
        tierData[tierID] = tierInformation;

        // Getting the address of the deployed ERC1155 NFT contract, so as to facilitate purchase
        ERC1155Upgradeable fanTigerContract = ERC1155Upgradeable(tierInformation.fanTigerContract);
        fanTigerContract.safeBatchTransferFrom(tierInformation.nftHolderWallet, buyer, nftIDs, amounts, "");

        emit BatchOffChainTransfer(buyer, nftIDs, amounts, paymentID);

        if(tierInformation.exhausted){
            emit TierSoldOut(tierID, tierInformation.fanTigerContract, uint256(tierInformation.maxSupply));
        }
    }

    /**
     * @dev Checking that the proof presented by the user is valid.
     * First Encoding the parameters and then recovering the public address from the signature and then matching it with owner
     */
    function requireValidSignature(uint256 tierID, address buyer,bytes32 paymentID,bytes memory messageSignature) internal view {
        // Recreate the msg hash
        bytes32 messageHash = keccak256(abi.encode(tierID, buyer, paymentID)); 
        bytes32 ethSignedHash = ECDSA.toEthSignedMessageHash(messageHash);
        address messageSigner = ECDSA.recover(ethSignedHash, messageSignature);
        require(messageSigner == owner(), "Signature Invalid");
    }
    
    function pause() public onlyOwner whenNotPaused  {
        _pause();
    }
    function unpause() public onlyOwner whenPaused  {
        _unpause();
    }
    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[47] private __gap;
}

