pragma solidity ^0.8.4;

import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@unlock-protocol/contracts/dist/PublicLock/IPublicLockV10.sol";
import "./chainlink/ChainlinkClientUpgradeable.sol";

contract ImpactSelf is 
  OwnableUpgradeable,
  UUPSUpgradeable,
  ChainlinkClientUpgradeable
{
  using Chainlink for Chainlink.Request;

  string public baseURI;
  string public apiBaseUrl;
  string private jobId;
  uint256 private fee;
  address private lockAddress;
  string private requestUrl;
  bytes32 private requestId;
  address private target;

  mapping(address => bool) private privacyList;
  mapping(address => uint256) private scoreList;
  mapping(bytes32 => address) private tracker;

  event RequestScore(address owner, uint256 score);
  event LockUpdated(address oldLock, address newLock);

  function initialize(address linkTokenAddress, address oracleAddress, string memory _jobId, string memory _apiBaseUrl, string memory _baseURI) external virtual initializer {
    __Context_init_unchained();
    __Ownable_init_unchained();
    __ChainlinkClient_init();

    setChainlinkToken(linkTokenAddress);
    setChainlinkOracle(oracleAddress);

    jobId = _jobId;
    fee = (1 * LINK_DIVISIBILITY) / 10; // 0,1 * 10**18 (Varies by network and job)
    apiBaseUrl = _apiBaseUrl;
    baseURI = _baseURI;
  }

  function _authorizeUpgrade(address newImplementation)
    internal
    virtual
    override
    onlyOwner
  {}

  modifier tokenOwner() {
    IPublicLockV10 Lock = IPublicLockV10(lockAddress);
    require(Lock.getHasValidKey(msg.sender) == true, 'Not a token owner');
    _;
  }

  function setLockAddress(address _lockAddress) public onlyOwner {
    address oldLock = lockAddress;
    require(_lockAddress != oldLock, 'Already set');

    lockAddress = _lockAddress;
    emit LockUpdated(oldLock, _lockAddress);
  }

  function tokenURI(
      address lockAddress,
      address operator,
      address owner,
      uint256 keyId,
      uint256 expirationTimestamp
  ) external view returns (string memory) {       
    return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, StringsUpgradeable.toString(keyId))) : "";
  }

  function isPrivate(address sender) external view returns (bool) {
    return privacyList[sender];
  }

  function getScore(address sender) external view returns (uint256) {
    return scoreList[sender];
  }

  function recalculate() external tokenOwner {
    if (privacyList[msg.sender] == false) {
      requestScore(msg.sender);
    } 
  }

  function createScoreRequest(address owner) internal returns (bytes32 _requestId) {
    Chainlink.Request memory req = buildChainlinkRequest(
      stringToBytes32(jobId),
      address(this),
      this.receiveScore.selector
    );

    string memory ownerAddress = StringsUpgradeable.toHexString(uint256(uint160(owner)), 20);
    // Setting URL to perform GET request for score
    requestUrl = string(abi.encodePacked(apiBaseUrl, ownerAddress));
    req.add("get", requestUrl);

    // Setting the path for the score JSON response
    req.add("path", "score");
    int256 multiplier = 1;
    req.addInt("times", multiplier);

    // add any data cleaning here
    return sendChainlinkRequest(req, fee);
  }

  function receiveScore(
      bytes32 _requestId,
      uint256 _score
  ) public recordChainlinkFulfillment(_requestId) {
    address _target = tracker[_requestId];

    target = _target;
    scoreList[target] = _score;

    delete(tracker[_requestId]);

    emit RequestScore(target, _score);
  }

  function setPrivacy(bool value) external tokenOwner {
    address sender = msg.sender;

    privacyList[sender] = value;

    if (value == true) {
      scoreList[sender] = 0;

      emit RequestScore(sender, 0);
    } else {
      requestScore(sender);
    }
  }

  function requestScore(address recipient) internal {
    bytes32 id = createScoreRequest(recipient);
    tracker[id] = recipient;
    requestId = id;
  }

  function keyPurchasePrice(address from,
    address recipient,
    address referrer,
    bytes calldata data
  ) external view
    returns (uint minKeyPrice) {
      return 0;
  }

  function onKeyPurchase(
    uint tokenId,
    address from,
    address recipient,
    address referrer,
    bytes calldata data,
    uint minKeyPrice,
    uint pricePaid
  ) external {
    privacyList[recipient] = false;

    requestScore(recipient);
  }

  function withdrawLink() public onlyOwner {
    LinkTokenInterface link = LinkTokenInterface(chainlinkTokenAddress());
    require(link.transfer(msg.sender, link.balanceOf(address(this))), 'Unable to transfer');
  }

  function stringToBytes32(string memory source)
    private
    pure
    returns (bytes32 result)
  {
    bytes memory tempEmptyStringTest = bytes(source);
    if (tempEmptyStringTest.length == 0) {
      return 0x0;
    }

    assembly {
      // solhint-disable-line no-inline-assembly
      result := mload(add(source, 32))
    }
  }
}

