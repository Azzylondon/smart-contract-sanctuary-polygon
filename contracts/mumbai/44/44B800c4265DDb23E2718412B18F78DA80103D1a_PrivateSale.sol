     * @param percentReward Array of percents that will go to referrals
     * @notice For specifying amount of levels, you should create `percentReward` array of `n` length.
     */
    function _setSystemParameters(uint256[] memory percentReward)
        internal
    {
        delete (_percentReward);
        uint256 level = percentReward.length;
        for (uint256 i; i < level; i++) {
            _allReferralPercent += percentReward[i];
            _percentReward.push(percentReward[i]);
        }
    }

    /**
     * @dev Distribute fees to the referrers of `n` levels
     * @param referral Address of the account was invited
     * @param amount Amount of the asset from which percents will be calculated
     * @param token Address of the asset. In case of the native currency should be equal zero address
     */
    function _distributeTheFee(
        address referral,
        uint256 amount,
        address token
    )
        internal
        returns (
            uint256 feeToPeople
        )
    {
        uint256 level = _percentReward.length;
        address[] memory addresses = new address[](level);
        uint256[] memory fees = new uint256[](level);

        address newReferral = referral;
        uint256 fee;

        for (uint256 i; i < level; ++i) {
            uint256 value = _calcPercent(amount, _percentReward[i]);
            
            if (_referralList[newReferral] != address(0)) {
                newReferral = _referralList[newReferral];
                fee = value;
                feeToPeople += value;

                if(token != address(0)) {
                    sendUSD(newReferral, value, token);
                } else {
                    sendMATIC(newReferral, value);
                }
            } else {
                newReferral = address(0);
                fee = 0;
            }

            addresses[i] = newReferral;
            fees[i] = fee;
        }

        emit DistributedRewards(referral, addresses, fees);
    }

    /**
     * @dev Send `value` amount of the asset with address `token` to `account`
     */
    function sendUSD(
        address account,
        uint256 value,
        address token
    ) internal {
        IERC20(token).safeTransfer(account, value);
    }

    /**
     * @dev Send `value` amount of the native currency to `account`
     */
    function sendMATIC(address account, uint256 value) internal nonReentrant {
        payable(account).call{value: value}("");
    }

    function _calcPercent(uint256 value, uint256 percent)
        internal
        pure
        returns (uint256 res)
    {
        return ((percent * value) / (100));
    }

    /**
     * @dev Get referral system parameters was set with `_setSystemParameters` function
     */
    function getDataRefSystem()
        external
        view
        returns (uint256[] memory percentReward)
    {
        return _percentReward;
    }

    /**
     * @dev Get referrer for the referral
     * @param referral Address of the account to get referrer
     */
    function getReferrer(address referral)
        external
        view
        returns (address referrer)
    {
        return _referralList[referral];
    }
}

