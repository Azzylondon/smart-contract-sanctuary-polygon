/**
 * @title Referral system of `n` levels.
 */
contract ReferralSystem is Whitelist, ReentrancyGuard {
    using SafeERC20 for IERC20;

    event DistributedRewards(address referral, address[] referrers, uint256[] rewards);

    uint256[] internal _percentReward;
    uint256 internal _allReferralPercent;

    mapping(address => address) internal _referralList;

    /**
     * @dev Checks if sender or account is whitelisted
     */
