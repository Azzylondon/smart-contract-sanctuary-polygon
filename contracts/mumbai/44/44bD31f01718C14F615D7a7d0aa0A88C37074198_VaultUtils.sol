    function calLpMargin(uint256 mc, int256 piNet, uint256 beta) public view returns (uint256) {
        if(piNet >= 0) {
            uint256 uPiNet = uint256(piNet);

            uint256 a = sqr(mc.add(uPiNet));
            uint256 b = sqr(uPiNet).mul(beta).mul(2).div(BASIS_POINTS_DIVISOR);
            // console.log("calLpMargin condition:", a > b);
            if(a < b) return 0;
            return mc.add(uPiNet).add( sqrt(a.sub( b ))).div(2);
        } else {
            uint256 uPiNet = uint256(-piNet);

            uint256 a = mc > uPiNet ? sqr(mc.sub(uPiNet)): sqr(uPiNet.sub(mc));
            uint256 b = sqr(uPiNet).div(BASIS_POINTS_DIVISOR).mul(beta).mul(2);
            if(a < b) return 0;

            // return mc.add(uPiNet).add( sqrt(sqr(mc.add(uPiNet)).sub( sqr(uPiNet).div(BASIS_POINTS_DIVISOR).mul(beta).mul(2) )) ).div(2);
            return mc.add( sqrt(a.sub( b )) ).sub(uPiNet).div(2);
        }
    }

    function calBestPrice(uint256 pi, int256 piNet, uint256 m, uint256 alpha, uint256 beta, bool isLong) public view returns (uint256) {
        uint256 mid = isLong ? BASIS_POINTS_DIVISOR.sub(alpha.div(2)): BASIS_POINTS_DIVISOR.add(alpha.div(2));
        if(piNet >= 0) {
            uint256 end = BASIS_POINTS_DIVISOR.sub(beta.mul(uint256(piNet)).div(m));
            return pi.mul(mid).mul(end).div(sqr(BASIS_POINTS_DIVISOR));
        } else {
            uint256 end = BASIS_POINTS_DIVISOR.add(beta.mul(uint256(-piNet)).div(m));
            return  pi.mul(mid).mul(end).div(sqr(BASIS_POINTS_DIVISOR));
        }
    }

    // function isOpenTrade(uint256 mc, int256 piNet, uint256 beta) public view returns (bool) {
    //     if(piNet >= 0) {
    //         uint256 uPiNet = uint256(piNet);
    //         return uPiNet.add(uPiNet) >
    //     } else {
    //         uint256 uPiNet = uint256(-piNet);
    //     }

    // }

    function validateOpenPosition(address _token, bool _isRaise) public override view returns (bool) {
        uint256 margin = getLpMargin(_token);
        if(margin != 0) return true;
        if(_isRaise) revert("Vault: not enough margin");
        return false;
    }

    function calBaseAveragePrice(uint256 pi, int256 piNet, uint256 m, uint256 delta, uint256 beta, bool isLong) public view returns (uint256) {
        
        int256 sub = isLong? 2*piNet-int256(delta): 2*piNet+int256(delta); // need to validate

        // int256 sub = isLong? 2*net - int256(delta): 2*net + int256(delta); // need to validate
        if(sub > 0) {
            uint256 a = beta.mul(uint256(sub)).div(m.mul(2));
            return pi.mul(BASIS_POINTS_DIVISOR.sub(a)).div(BASIS_POINTS_DIVISOR);
        } else {
            uint256 a = beta.mul(uint256(-sub)).div(m.mul(2));
            return pi.mul(BASIS_POINTS_DIVISOR.add(a)).div(BASIS_POINTS_DIVISOR);
        }
    }

    function getMarketPrice(address indexToken, uint256 delta, bool isLong) public override view returns (uint256) {
        // uint256 decimals = vault.tokenDecimals(targetToken);
        if(isLong) {
            uint256 pi =  vault.getMaxPrice(indexToken);
            // uint256 mc = vault.poolAmounts(targetToken) * pi / 10**vault.tokenDecimals(targetToken);
            uint256 aum = getMarginUsd(true);
            uint256 beta = vault.openBetaBasisPoints();
            int256 piNet = getPiNetUsd(indexToken, pi);
            return calMarketPirce(pi, aum, piNet, beta, delta, isLong);
        }  else {
            uint256 pi =  vault.getMinPrice(indexToken);
            // uint256 mc = vault.poolAmounts(targetToken) * pi / 10**vault.tokenDecimals(targetToken);
            uint256 aum = getMarginUsd(false);
            uint256 beta = vault.closeBetaBasisPoints();
            int256 piNet = getPiNetUsd(indexToken, pi);
            return calMarketPirce(pi, aum, piNet, beta, delta, isLong);
        }
    }

    function calMarketPirce(uint256 pi, uint256 mc, int256 piNet, uint256 beta, uint256 delta, bool isLong) public view returns (uint256) {
        
        uint256 margin = calLpMargin(mc, piNet, beta);
        if(margin == 0) return pi;
        // require(margin > 0, "Vault: No liquidity");
        uint256 bestPrice = calBestPrice(pi, piNet, margin, vault.alphaBasisPoints(), beta, isLong);
        // console.log("bestPrice:", bestPrice);
        uint256 baseAvgPrice = calBaseAveragePrice(pi, piNet, margin, delta, beta, isLong);
        // console.log("baseAvgPrice:", baseAvgPrice);

        return isLong ? max(bestPrice, baseAvgPrice) : min(bestPrice, baseAvgPrice);
    }

    function getPiNetUsd(address _token, uint256 _price) public view returns (int256) {
        // if (_tokenAmount == 0) { return 0; }
        uint256 decimals = vault.tokenDecimals(_token);
        int256 net = vault.netPosition(_token);
        return net.mul(int256(_price)).div(int256(10 ** decimals));
    }
    

    function getLpMargin(address _token) public view returns (uint256) {
        uint256 aum = getMarginUsd(true);
        uint256 beta = vault.openBetaBasisPoints();
        uint256 price = vault.getMaxPrice(_token);
        int256 piNet = getPiNetUsd(_token, price);
        return calLpMargin(aum, piNet, beta);
    }



    function getMarginUsd(bool maximise) public view returns (uint256) {
        uint256 length = vault.allWhitelistedTokensLength();
        uint256 aum = 0;

        for (uint256 i = 0; i < length; i++) {
            address token = vault.allWhitelistedTokens(i);
            bool isWhitelisted = vault.whitelistedTokens(token);

            if (!isWhitelisted) {
                continue;
            }

            uint256 price = maximise ? vault.getMaxPrice(token) : vault.getMinPrice(token);
            uint256 poolAmount = vault.poolAmounts(token);
            uint256 decimals = vault.tokenDecimals(token);

            if (vault.stableTokens(token)) {
                aum = aum.add(poolAmount.mul(price).div(10 ** decimals));
            } else {
                // uint256 reservedAmount = vault.reservedAmounts(token);
                // aum = aum.add(poolAmount.sub(reservedAmount).mul(price).div(10 ** decimals));
                aum = aum.add(poolAmount.mul(price).div(10 ** decimals));
            }
        }

        return aum;
    }

    function getNextBorrowRate(address _token) public override view returns (uint256) {
        if (vault.lastFundingTimes(_token).add(vault.fundingInterval()) > block.timestamp) { return 0; }

        uint256 intervals = block.timestamp.sub(vault.lastFundingTimes(_token)).div(vault.fundingInterval());
        uint256 poolAmount = vault.poolAmounts(_token);
        if (poolAmount == 0) { return 0; }

        uint256 _fundingRateFactor = vault.stableTokens(_token) ? vault.stableFundingRateFactor() : vault.fundingRateFactor();
        return _fundingRateFactor.mul(vault.reservedAmounts(_token)).mul(intervals).div(poolAmount);
    }



    function getNextFundingRate(address _token) public override view returns (int256) {
        if (vault.lastFundingTimes(_token).add(vault.fundingInterval()) > block.timestamp) { return 0; }

        int256 fundingRate = getFundingRate(_token);
        // if(fundingRate >= 0) {
        //     console.log("update fundingRates+:", uint256(fundingRate));
        // } else {
        //     console.log("update fundingRates-:", uint256(-fundingRate));
        // }
        return fundingRate;
    }

    function getFundingRate(address _token) public view returns (int256) {
        uint256 margin = getLpMargin(_token);
        int256 fundingRateCoefficient = vault.fundingRateCoefficient();
        uint256 price = vault.getMaxPrice(_token);
        int256 piNet = getPiNetUsd(_token, price);
        // if(piNet >= 0) {
        //     console.log("update fundingRates+:", uint256(piNet), margin);
        // } else {
        //     console.log("update fundingRates-:", uint256(-piNet), margin);
        // }
        return -fundingRateCoefficient.mul(piNet.mul(int256(FUNDING_RATE_PRECISION))).div(int256(margin)).div(BASIS_POINTS_DIVISOR_INT);
    }


    

    // source: https://ethereum.stackexchange.com/a/2913
    function sqrt(uint x) private pure returns (uint y) {
        uint z = (x + 1) / 2;
        y = x;
        while (z < y) {
            y = z;
            z = (x / z + z) / 2;
        }
    }

    function sqr(uint x) private pure returns (uint) {
        return x**2;
    }

    function max(uint a, uint b) private pure returns (uint) {
        return a > b ? a : b;
    }
    function min(uint a, uint b) private pure returns (uint) {
        return a < b ? a : b;
    }
}

