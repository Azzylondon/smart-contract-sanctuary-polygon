pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/finance/PaymentSplitter.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./OpenSeaCompatibility.sol";
import "hardhat/console.sol";

contract NFT1155 is ERC1155, ContextMixin, NativeMetaTransaction, PaymentSplitter, Ownable {
    using Counters for Counters.Counter;

    // Contract name
    string public name;

    // The address of the token that is accepted as payment for minting a new NFT.
    // Could be, for example, the address of the USDC stable coin.
    address public paymentTokenAddress;

    // The price to mint a new token, denominated in the coin specified by paymentTokenAddress.
    uint256 public mintPrice;

    uint256 public totalSupply;

    uint256 public numberOfTokensMinted;

    constructor(string memory _name, address _paymentTokenAddress, address[] memory _payees, uint256[] memory _shares, uint256 _totalSupply, uint256 _mintPrice) ERC1155("<insert base URI>") PaymentSplitter(_payees, _shares) {
        name = _name;
        paymentTokenAddress = _paymentTokenAddress;
        totalSupply = _totalSupply;
        mintPrice = _mintPrice;
        _initializeEIP712(name);
    }

    function mintTo(address recipient, uint256 amount) public {
        numberOfTokensMinted += amount;
        require(numberOfTokensMinted <= totalSupply, "Max supply reached");

        //TODO: Use USDC's receiveWithAuthorization from EIP-3009
        SafeERC20.safeTransferFrom(IERC20(paymentTokenAddress), _msgSender(), address(this), mintPrice * amount);
        
        _mint(recipient, 0, amount, "");
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }

    /**
    * As another option for supporting trading without requiring meta transactions, override isApprovedForAll to whitelist OpenSea proxy accounts on Matic
    */
    function isApprovedForAll(
        address _owner,
        address _operator
    ) public override view returns (bool isOperator) {
        if (_operator == address(0x207Fa8Df3a17D96Ca7EA4f2893fcdCb78a304101)) {
            return true;
        }
        
        return ERC1155.isApprovedForAll(_owner, _operator);
    }
}

