pragma solidity ^0.8.1;

import "hardhat/console.sol";

import { LibArmonia , Modifiers , Gente} from "../ArmShared.sol";
import { EnumerableSetString } from "../../shared/libraries/CaiLibEnumerateString.sol";

contract PerFacet is Modifiers {
  mapping (address => Gente)  PER_PerByAddr;
  mapping (string => Gente )  PER_PerByAlias;
  EnumerableSetString.StringSet aAlias ;
 
  function PER_register( string memory _nomalias , address  _addr , string memory  _parentalias , string memory _name , string memory _ipfs , string memory _pubK  ) public  onlyAdmins  {
    address addrParent;
    if (keccak256(abi.encodePacked(("root"))) == keccak256(abi.encodePacked((_parentalias)))){
      addrParent = s.diamond;  
    }else{
      _parentalias   = LibArmonia.normalizeAlias('arm.per', _parentalias);
      addrParent =  PER_AliasToAddr(_parentalias);
    }
    _nomalias      = LibArmonia.normalizeAlias('arm.per', _nomalias);
    EnumerableSetString.add( aAlias , _nomalias);
    PER_PerByAlias[_nomalias] = Gente( _addr , addrParent,_nomalias,_name,_ipfs,_pubK,true);
    PER_PerByAddr[_addr] =  PER_PerByAlias[_nomalias];
  }

  function PER_allAlias() public view returns(string[] memory ) {
      return  EnumerableSetString.values(aAlias);
  }

  function PER_allAddr() public view returns(address[] memory ) {
    address[] memory aAddrs = new address[](EnumerableSetString.length(aAlias));
      for( uint i = 1 ; i <= EnumerableSetString.length(aAlias) ; i++){
        Gente memory oGente = PER_PerByAlias[EnumerableSetString.at(aAlias,i)];
        aAddrs[i-1] = oGente.addr;
      }
    return aAddrs;
  }

  function PER_allGente() public view returns(Gente[] memory ) {
    Gente[] memory aGente = new Gente[](EnumerableSetString.length(aAlias));
    
      for( uint i = 1 ; i <= EnumerableSetString.length(aAlias) ; i++){
        Gente memory oGente = PER_PerByAlias[EnumerableSetString.at(aAlias,i)];
        aGente[i-1] = oGente;
      }
    return aGente;
  }

      
  function PER_AliasToAddr(string memory _nomalias ) public view returns(address){
    _nomalias      = LibArmonia.normalizeAlias('arm.per',_nomalias);
    return PER_PerByAlias[_nomalias].addr;
  }

  function PER_AliasToGente(string memory _nomalias ) public view returns(Gente memory){
    _nomalias      = LibArmonia.normalizeAlias('arm.per',_nomalias);
    return PER_PerByAlias[_nomalias];
  }

  function PER_AddrToGente(address _addr ) public view returns(Gente memory ){
    return PER_PerByAddr[_addr];
  }

  function PER_getSilverByAlias(string memory _nomalias ) public returns(uint256) {
      _nomalias = LibArmonia.normalizeAlias('arm.per', _nomalias);
      return LibArmonia.getSilverByAlias(_nomalias);
  }
  function PER_getSilverByAddr(address _addr ) public view   returns(uint256) {
    console.log("PER_getSilverByAddr",_addr);
  
      return LibArmonia.getSilverByAddr(_addr);
  }
  function PER_test(address _addr ) public view   returns(uint256) {
      return 123;
  }

}

