pragma solidity ^0.8.17;

import {SBTBase} from "./SBTBase.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "hardhat/console.sol";

contract SBT is SBTBase {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenID;

    constructor() SBTBase("data:application/json;base64,") {
        _tokenID.increment();
    }

    function mintNewSBT(address to, string memory cid) public virtual {
        uint256 _currentTokenID = _tokenID.current();

        _mint(to, _currentTokenID, cid);

        _tokenID.increment();
        console.log("SBT #%s has been minted to %s", _currentTokenID, to);
    }

    function mintExistingSBT(address to, uint256 tokenId) public virtual {
        _mint(to, tokenId);
        console.log("SBT #%s has been minted to %s", tokenId, to);
    }

    function mintBatchNewSBT(address[] memory tos, string memory cid) public {
        uint256 _currentTokenID = _tokenID.current();

        _mintBatch(tos, _currentTokenID, cid);

        _tokenID.increment();
        console.log("SBT #%s has been minted", _currentTokenID);
    }

    function mintBatchExistingSBT(address[] memory tos, uint256 tokenId) public {
        _mintBatch(tos, tokenId);
        console.log("SBT #%s has been minted", tokenId);
    }

    function burn(uint256 tokenId) external {
        _burn(_msgSender(), tokenId);
        console.log("SBT #%s has been burnt", tokenId);
    }
}

