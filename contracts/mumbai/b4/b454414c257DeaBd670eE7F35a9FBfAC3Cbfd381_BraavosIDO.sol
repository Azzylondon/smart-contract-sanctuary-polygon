interface ITreasury {
    function deposit(
        uint256 _amount,
        address _token,
        uint256 _profit
    ) external returns (uint256 send_);

    function valueOfToken(address _token, uint256 _amount)
        external
        view
        returns (uint256 value_);
}

interface IStaking {
    function stake(uint256 _amount, address _recipient) external returns (bool);
}

contract BraavosIDO is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address public BRAVO;
    address public FRAX;
    address public addressToSendFRAX;
    address public staking;
    address public NFT;

    uint256 public totalAmount;

    bool public initialized;
    bool public cancelled;
    bool public finalized;

    uint256 public totalParticipants;
    uint256 public amountOfFraxAvailable;

    mapping(address => bool) public hasParticipated;

    address[] buyers;
    mapping(address => uint256) public purchasedAmounts;
    
    mapping(address => uint256) public allocations;

    address treasury;
    address DAO;

    uint256 public borrowed;

    constructor(
        address _BRAVO,
        address _FRAX,
        address _treasury,
        address _staking,
        address _NFT,
        address _DAO
    ) {
        require(_BRAVO != address(0));
        require(_FRAX != address(0));
        require(_treasury != address(0));
        require(_staking != address(0));
        require(_NFT != address(0));
        require(_DAO != address(0));

        BRAVO = _BRAVO;
        FRAX = _FRAX;
        treasury = _treasury;
        staking = _staking;
        NFT = _NFT;
        DAO = _DAO;
        cancelled = false;
        finalized = false;
    }

    function saleStarted() public view returns (bool) {
        return initialized;
    }

    function setAllocations(address[] memory _buyers, uint256 _amount)
        external
        onlyOwner
        returns (bool)
    {
        require(saleStarted() == false, 'Already started');
        for (uint256 i; i < _buyers.length; i++) {
          allocations[_buyers[i]] = _amount;
        }
        return true;
    }

    function setAllocation(address _buyer, uint256 _amount)
        external
        onlyOwner
        returns (bool)
    {
        require(saleStarted() == false, 'Already started');
        allocations[_buyer] = _amount;
        return true;
    }

    function initialize() external onlyOwner returns (bool) {
        require(initialized == false, 'Already initialized');
        initialized = true;
        return true;
    }

    function purchaseBRAVO(uint256 _amountFRAX) external returns (bool) {
        require(saleStarted() == true, 'IDO has not yet started');
        require(allocations[msg.sender] > 0, 'Address has no allocation');
        require(hasParticipated[msg.sender] == false, 'Already participated');
        require(_amountFRAX == allocations[msg.sender], 'Amount must match allocation');

        hasParticipated[msg.sender] = true;

        uint256 _purchaseAmount = _amountFRAX;

        amountOfFraxAvailable = amountOfFraxAvailable.add(_purchaseAmount);

        purchasedAmounts[msg.sender] = _purchaseAmount;
        buyers.push(msg.sender);

        IERC20(FRAX).safeTransferFrom(msg.sender, address(this), _amountFRAX);
        // INFT(NFT).mint(msg.sender, '');

        return true;
    }

    function _calculateSalePrice()
        internal
        view
        returns (uint256)
    {
        return amountOfFraxAvailable.div(100000);
    }

    /// @dev Only Emergency Use
    /// cancel the IDO and return the funds to all buyer
    function cancel() external onlyOwner {
        cancelled = true;
    }

    function recover() external {
        require(cancelled, 'IDO has not been cancelled');
        uint256 amount = purchasedAmounts[msg.sender];
        IERC20(FRAX).transfer(msg.sender, purchasedAmounts[msg.sender]);
    }

    function claim(address _recipient) public {
        require(finalized, 'Claim only allowed after finalized');
        require(purchasedAmounts[_recipient] > 0, 'Has not participated in IDO');
        uint256 treasuryReserve = ITreasury(treasury).valueOfToken(
            FRAX,
            amountOfFraxAvailable
        );
        uint256 canStake = purchasedAmounts[_recipient].div(treasuryReserve.div(100000));
        IStaking(staking).stake(canStake, _recipient);
        purchasedAmounts[_recipient] = 0;
    }

    function finalize(address _receipt) external onlyOwner {
        require(borrowed == 0, 'all borrowed funds should be returned');
        // require(amountOfFraxAvailable > 100000 * 1e18, 'should have enough FRAX for backing');

        IERC20(FRAX).approve(treasury, amountOfFraxAvailable);

        uint256 treasuryReserve = ITreasury(treasury).valueOfToken(
            FRAX,
            amountOfFraxAvailable
        );

        uint256 tokensToDistribute = treasuryReserve.sub(uint256(100000).mul(1e9));
        uint256 clamMinted = ITreasury(treasury).deposit(amountOfFraxAvailable, FRAX, tokensToDistribute);

        require(clamMinted == 100000 * 1e9);

        IERC20(BRAVO).approve(staking, clamMinted);

        finalized = true;

        // claim(_receipt);
    }

    function borrow(uint256 _amount) external onlyOwner {
        borrowed = borrowed.add(_amount);
        IERC20(FRAX).transfer(DAO, _amount);
    }

    function deposit(uint256 _amount) external onlyOwner {
        IERC20(FRAX).safeTransferFrom(msg.sender, address(this), _amount);
        borrowed = borrowed.sub(_amount);
    }

}

