pragma solidity ^0.8.0;

interface IPoolBetting {

    enum EventStatus{ OPEN, CLOSED, CANCELLED }

    // Structs
    struct Event {
        uint256 eventStarts; // Time event starts
        uint256 eventStops; // Time event stops
        uint256 bettingStarts; // Time betting starts
        uint256 bettingStops; // Time betting stops
        uint256 totalBetAmount; // Total amount of all the bets
        uint256 totalCommission; // Total commission for all the bets
        uint16 totalPlayers; // Total number of players
        uint16 winner;
        EventStatus status; // If event is open, closed or cancelled
        uint256[] playersBetAmount; // Total amount bets amount for every player
    }

    struct Bet {
        uint256 betAmount;
        uint256 commission;
        bool withdrawn;
    }

    // Events
    event EventCreatedNotification(
        bytes32 indexed eventId,
        uint256 _bettingStarts,
        uint256 _bettingStops,
        uint256 indexed eventStarts,
        uint256 _eventStops,
        uint16 indexed _amountOfPlayers
    );

    event EventClosedNotification(
        bytes32 indexed _eventId,
        uint16 _winner
    );

    event EventCancelledNotification(
        bytes32 indexed _eventId
    );

    event BetIsPlacedNotification(
        bytes32 indexed _eventId,
        uint16 indexed _playerId,
        uint256 _amount,
        uint256 _commission,
        address _bettor
    );

    event WithdrawNotification(
        bytes32 indexed _eventId,
        uint256 indexed _amount
    );

    event WithdrawCommissionNotification(
        uint256 indexed _amount
    );

    // Functions
    function createNewEvent(
        bytes32 _eventId,
        uint256 _bettingStarts,
        uint256 _bettingStops,
        uint256 _eventStarts,
        uint256 _eventStops,
        uint16 _amountOfPlayers
    ) external;

    function closeEvent(bytes32 _eventId, uint16 _winnerPlayerID) external;

    function cancelEvent(bytes32 _eventId) external;

    function placeBet(bytes32 _eventId, uint16 _playerId, uint256 _amount) external;

    function withdraw(bytes32 _eventId) external;

    // View
    function eventDetailsView(bytes32 _eventId) external view returns (Event memory);
    function betView(bytes32 eventId, address user, uint16 player) external view returns (Bet memory);
    function availableForWithdraw(bytes32 _eventId, address user) external view returns (uint256);

    /// Admin
    function withdrawCommission() external;
}

