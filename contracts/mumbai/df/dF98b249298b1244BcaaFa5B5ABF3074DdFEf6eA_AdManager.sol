pragma solidity ^0.8.2;

library AdUtil {
    function compareStrHash(string memory a, string memory b) public pure returns (bool) {
      return keccak256(bytes(a)) == keccak256(bytes(b));
    }
}

//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.2;

contract Accounts {

    mapping (address => uint256) balance;
    mapping (address => uint) index;

    // Array with address 
    address payable[] store;

    function appendAccount(address payable _addr) public {
        if (!exists(_addr)) {
            // Append
            index[_addr] = store.length;
            store.push(_addr);
        }
    }
    
    function getAccountsLength() public view returns (uint) {
        return store.length;
    }

    function getAccountAddress(uint i) public view returns (address payable) {
        return store[i];
    }

    function exists(address payable _addr) private view returns (bool) {
        // address 0x0 is not valid if pos is 0 is not in the array
        if (index[_addr] > 0) {
            return true;
        }
        return false;
    }

    function addAccountBalance(address payable _addr, uint256 amount) public {
        //if (msg.sender == owner) {
            if (exists(_addr)) {
                balance[_addr] += amount;
            }
        //}
    }

    function resetAccountBalance(address payable _addr) public {
        //if (msg.sender == owner) {
            if (exists(_addr)) {
                balance[_addr] = 0;
            }
        //}
    }

    function getAccountBalance(address payable _addr) public view returns (uint256) {
        return balance[_addr];
    }

}