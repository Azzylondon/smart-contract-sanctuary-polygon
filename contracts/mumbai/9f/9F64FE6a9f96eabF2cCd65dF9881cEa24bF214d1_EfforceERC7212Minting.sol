/*
 * @title EfforceERC7212Minting
 * @author Efforce.io
 * @notice Contract to allow WOZX locker to mint NFT for specific projects
 */
contract EfforceERC7212Minting is
    Initializable,
    OwnableUpgradeable,
    ERC721URIStorageUpgradeable,
    UUPSUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;
    /// @notice Counter for minted tokens
    uint256 public tokenIds;
    /// @notice Price of the nft in USDC
    uint256 public nftPrice;
    /// @notice Holds all the realeved NFT
    mapping(uint256 => bool) private _revealedNft;
    /// @notice USDC token address
    IERC20Upgradeable public usdcToken;
    /// @notice Addresses that can mint for free
    mapping(address => bool) private _freeMint;
    /// @notice Addresses that can particate on the first pre sale
    mapping(address => bool) private _firstPreSale;
    /// @notice Addresses that can particate on the second pre sale
    mapping(address => bool) private _secondPreSale;
    /// @notice NFT minted per user
    mapping(address => uint256) public nftMinted;
    /// @notice Project ID
    uint256 private _projectId;
    /// @notice Projects manager contract
    ProjectsManagerContract public manager;
    /// @notice Orchestrator
    address private _orchestrator;
    /// @notice Timestamp to indicate the start of the start of the pre-sale
    uint256 public startFirstPreSale;
    /// @notice Timestamp to indicate the start of the end of the pre-sale
    uint256 public endFirstPreSale;
    /// @notice Timestamp to indicate the start of the end of the second pre-sale
    uint256 public endSecondPreSale;
    /// @notice Pre-sale minted NFT for each user
    mapping(address => uint256) public preSalesNft;
    /// @notice minted NFT for each whitelisted user
    mapping(address => uint256) public mintedNftForWhitelistedUsers;

    string private tokenUri;
    uint256 private wozxToLock;

    function setSaleDates(
        uint256 _startPreSale,
        uint256 _endPreSale,
        uint256 endSecondPresale
    ) external onlyOwner {
        startFirstPreSale = _startPreSale;
        endFirstPreSale = _endPreSale;
        endSecondPreSale = endSecondPresale;
    }

    function addFreeMintWhitelist(address _address) external onlyOwner {
        _freeMint[_address] = true;
    }

    function addPreSalesMintWhitelist(address _address) external onlyOwner {
        _firstPreSale[_address] = true;
    }

    function addSecondPreSalesMintWhitelist(address _address)
        external
        onlyOwner
    {
        _secondPreSale[_address] = true;
    }


    function setWozxToLock(uint256 amount)
        external
        onlyOwner
    {
        wozxToLock = amount;
    }

    /// @notice Event for a newly minted NFT
    event NewEfforceNFTMinted(address sender, uint256 tokenId);
    /// @notice Event when an NFT is revealed
    event NFTrevealed(address sender, uint256 tokenId);

    /// @dev Modifier to not allow the reveal of a NFT more than 1 time
    modifier isNotReveiled(uint256 tokenId) {
        require(!_revealedNft[tokenId], "This NFT has already been revealed");
        _;
    }

    /// @notice Initialize the contract
    /// @dev Using the initialize function since this is an upgradable contract
    /// @param _nftPrice (uint256)
    /// @param _usdcToken (address)
    /// @param projectId (uint256)
    /// @param _address (address)
    function initialize(
        uint256 _nftPrice,
        IERC20Upgradeable _usdcToken,
        uint256 projectId,
        //@todo, this should be a static address once deployed
        address _address
    ) public initializer {
        usdcToken = _usdcToken;
        nftPrice = _nftPrice;
        tokenIds = 0;
        _projectId = projectId;
        manager = ProjectsManagerContract(_address);
        tokenUri = "https://ipfs.io/ipfs/QmR57rnt6WYokVmHib8CkWHsLHw3pNmn86dSbpzU5fRyxu?filename=efforce-nft";
        __ERC721_init("EfforceNFT", "ENFT");
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    /// @notice Private function to check if enough wozx was locked to mint NFT
    /// @param _address (address)
    /// @return  (bool)
    function _canMintNft(address _address) private view returns (bool) {
        uint256 lockedWozx = manager.isWhitelisted(_projectId, _address);

        return
            lockedWozx > 0 &&
            ((lockedWozx / ((nftMinted[_address] + 1) * wozxToLock)) >=
                (nftMinted[_address] + 1));
    }

    /// @notice Function to mint NFT
    /// @param _address (address)
    /// @param amount (uint256)
    function mintGeneralNft(address _address, uint256 amount) external {
        require(
            startFirstPreSale != 0 &&
                endFirstPreSale != 0 &&
                endSecondPreSale != 0,
            "Sale is not open"
        );

        require(_address != address(0x0), "Can't mint NFT");
        if (
            block.timestamp >= startFirstPreSale &&
            block.timestamp < endFirstPreSale
        ) {
            require(_firstPreSale[_address]);

           // @question why did i do this???
            // require(
            //     preSalesNft[_address] > mintedNftForWhitelistedUsers[_address]
            // );
        } else if (
            block.timestamp >= endFirstPreSale &&
            block.timestamp < endSecondPreSale
        ) {
            require(_secondPreSale[_address]);
             // @question why did i do this???
            // require(
            //     preSalesNft[_address] > mintedNftForWhitelistedUsers[_address]
            // );
        } else if (block.timestamp >= endSecondPreSale) {
            if (!_freeMint[_address]) {
                require(amount == nftPrice, "Not enough funds where approved");
            }
        }
        require(
                _canMintNft(_address),
                "Not enough wozx were locked to mint this NFT"
            );

        _mintGeneralNft(_address, amount);
    }

    /// @notice Function to mint NFT
    /// @param _address (address)
    /// @param amount (uint256)
    function _mintGeneralNft(address _address, uint256 amount) private {
        if (_freeMint[_address]) {
            usdcToken.safeTransferFrom(_address, address(this), amount);
        }
        uint256 newItemId = tokenIds;

        _safeMint(msg.sender, newItemId);

        _setTokenURI(newItemId, tokenUri);
        nftMinted[_address]++;
        emit NewEfforceNFTMinted(msg.sender, newItemId);
        tokenIds++;
    }

    /// @notice Function to reveal NFT
    /// @param tokenId (uint256)
    /// @param revealTokenUri (string)
    function revealNft(uint256 tokenId, string memory revealTokenUri)
        external
        onlyOwner
        isNotReveiled(tokenId)
    {
        _revealedNft[tokenId] = true;
        _setTokenURI(tokenId, revealTokenUri);
        emit NFTrevealed(msg.sender, tokenId);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        virtual
        override
        onlyOwner
    {}
}

