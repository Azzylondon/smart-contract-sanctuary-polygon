    uint256 constant maximumLoanDuration = 90 days; // setPeriod / Timestamp of loan completion 91 days = 13 weeks
    uint256 public maxLoanValue  = 4600000000000000000; // 7500$ = 4.60 ETH = 4600000000000000000 wei
    uint256 public minLoanValue  = 3070000000000000000; // 5000$ = 3.07 ETH = 3070000000000000000 wei
    uint256 constant entryFee = 200; // always be 2 percent = 200 in solidity 20 percent = 2000 
    uint256 constant NFT_PRICE = 7510000000000000000; // 10,000$ = 7.51 in USDT = 7510000000000000000 wei

