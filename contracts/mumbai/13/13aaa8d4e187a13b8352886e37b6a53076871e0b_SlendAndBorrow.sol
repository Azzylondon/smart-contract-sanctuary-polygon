    struct LOAN {
        uint256 tokenId; // NFT token id

        uint256 nftCollateralId;
        address Lender; // NFT token owner/Lender (loan initiator or 0x0 for repaid) //
        address borrower; // must

        uint64 loanStartTime; // block.timestamp
        uint256 maximumLoanDuration; // setPeriod / Timestamp of loan completion
        uint256 endingTime;
      
//        uint256 entryFee; // always 200
        uint256 APR;
        uint256 minLoanAmount; // Minimum value
        uint256 maxLoanAmount; // Maximum value
        
        address nftCollateralContract;
        address loanERC20Denomination;
    }

