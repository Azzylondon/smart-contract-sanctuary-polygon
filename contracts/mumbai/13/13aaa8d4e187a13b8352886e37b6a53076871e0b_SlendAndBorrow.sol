    using SafeMath for uint256;

    constructor()ERC721("lendAndBorrow", "LBT") {
        
        contractOwner = msg.sender; // CONTRACT OWNER address

        erc20CurrencyIsWhitelisted[address(0x2B401F9F04D205770Dc5EA5A84f8799F0Ac90402)] = true;
        nftContractIsWhitelisted[address(0x6F132b31A08A5A8127B1CE794C780972906A01aC)] = true;
    }
   
    // ============ Admin Section ============
    address public contractOwner; // need to make private     // CONTRACT OWNER/Admin address

    mapping (address => bool) public erc20CurrencyIsWhitelisted; // The ERC20 contract of the currency being used as principal/interest for this loan
    mapping (address => bool) public nftContractIsWhitelisted;

    uint256 public lenderAmount;


    // ============ Mutable variables storage ============
    uint256 public numLoans = 0 ; // A continuously increasing counter that simultaneously allows every loan to have a unique ID and provides a running count of how many loans have been started by this contract.
    uint256 public offerEscrowExpirationTime;


