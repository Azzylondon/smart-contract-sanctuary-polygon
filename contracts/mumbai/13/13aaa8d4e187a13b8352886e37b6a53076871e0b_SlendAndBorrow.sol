    // ============ Events ============
    // address nftcollateralContract; => tokenAddress // NFT token owner (loan initiator or 0x0 for repaid) => _lender
    event LoanCreated(uint256 loanId, address tokenAddress,  address _lender, uint256 loanStartTime, uint256 maximumLoanDuration, uint256 entryFee, uint256 APR,uint256 minLoanAmount, uint256 maxLoanAmount);
    event LoanAccepted(uint256 loanId, uint256 nftCollateralId, address nftcollateralContract, uint256 loanprincipalAmount, address loanERC20Denomination, uint256 loanStartTime);

    event LoanRepaid(uint256 loanId,address borrower,address _lender,uint256 loanPrincipalAmount,uint256 nftCollateralId,address nftCollateralContract,address loanERC20Denomination);

    // ============ Functions ============    
    function lenderOffering( uint256 _nftCollateralId, address _nftcollateralContract,uint256 _maximumLoanDuration, uint256 _entryFees, uint256 annualPercentageRate, uint256 loanMinimumValue, uint256 loanMaximumValue, address _loanERC20Denomination) external onlyOwner  returns (uint256)
    {   

        uint256 loanId = ++numLoans;

        require(uint256(loanIdToLoan[loanId].maximumLoanDuration) <= maximumLoanDuration, "Loan duration exceeds maximum loan duration");
        require(entryFee == _entryFees, "Entry fee Always be 2% (200 in solidity)");
//        require(loanIdToLoan[loanId].minLoanAmount <= minLoanValue, "Loan Minimum value should be less than equal to 5000$");
//        require(loanIdToLoan[loanId].maxLoanAmount <= maxLoanValue, "Loan Maximum Value should be less than equal to 7500$");
        
        // Create loan
        loanIdToLoan[loanId].tokenId = loanId;
        loanIdToLoan[loanId].nftCollateralId = _nftCollateralId;
        loanIdToLoan[loanId].nftCollateralContract = _nftcollateralContract;
        loanIdToLoan[loanId].Lender = msg.sender; // not used param: automatically makeoffer is calling lender
        loanIdToLoan[loanId].maximumLoanDuration = uint32(_maximumLoanDuration); // set Period // Timestamp of loan completion 
        loanIdToLoan[loanId].APR = annualPercentageRate;
        loanIdToLoan[loanId].minLoanAmount = loanMinimumValue;
        loanIdToLoan[loanId].maxLoanAmount = loanMaximumValue;
        loanIdToLoan[loanId].loanERC20Denomination = _loanERC20Denomination;
        
        offerEscrowExpirationTime = block.timestamp + 72 hours; // Offer expireTime // 3 days = 72 hours
        
        // Transfer NFT from owner to contract // assuming token transfer is approved
        IERC721(_nftcollateralContract).transferFrom(contractOwner, address(this), _nftCollateralId);

        // Issue an ERC721 promissory note to the lender that gives them the // right to either the principal-plus-interest or the collateral.
        _mint(loanIdToLoan[loanId].Lender, loanId);

        // Emit creation event // _nftcollateralContract => NFT token owner (loan initiator or 0x0 for repaid)
        emit LoanCreated( _nftCollateralId,_nftcollateralContract, msg.sender, offerEscrowExpirationTime, _maximumLoanDuration, _entryFees, annualPercentageRate, loanMinimumValue, loanMaximumValue); 
        
        // Return loan id
        return loanId;
    }

    function acceptLoanOffer(uint256 _loanId, uint256 _nftCollateralId, address _nftcollateralContract, uint256 _loanprincipalAmount, address _loanERC20Denomination) external nonReentrant{

        require(idAccepted[_loanId] == false, "User Already Accepted an Offer");

        // set the values    
        LOAN storage loan = loanIdToLoan[_loanId]; 
        loan.tokenId = _loanId;
        loan.nftCollateralId = _nftCollateralId;
        loan.nftCollateralContract = _nftcollateralContract;
        loan.maxLoanAmount = _loanprincipalAmount;
        loan.borrower = msg.sender;
        loan.loanERC20Denomination = _loanERC20Denomination;
        loan.loanStartTime = uint64(block.timestamp); // added from lenderFunction

        idAccepted[loan.tokenId] = true; 
        
        // Escrow offer expired after 3 days / 72 hours       
        // offerEscrowExpirationTime (72) >=  (current = 73) block.timestamp 
        require (offerEscrowExpirationTime >= block.timestamp,"Offer will be expire in 72 hours");
        
        // Loan Offering from 5000$ to 7500$
        // require(msg.value >=minLoanValue || msg.value <= maxLoanValue , "Loan Offering from 5000$ to 7500$");
        
        // Whitelist: Ensuring principal come from supported contracts.
        require(erc20CurrencyIsWhitelisted[loan.loanERC20Denomination], "Currency denomination is not whitelisted to be used by this contract");

        // Whitelist: Ensuring NFT Contract come from supported contracts.
        require(nftContractIsWhitelisted[loan.nftCollateralContract], "NFT collateral contract is not whitelisted to be used by this contract");

        // The original sum of money transferred from lender to borrower when borrower accepts the offer, measured in loanERC20Denomination's smallest units.
        // Transfer principal from lender/owner to borrower.

        // Transfer principal from lender to borrower.
        IERC20(loan.loanERC20Denomination).transferFrom(loan.Lender, loan.borrower, loan.maxLoanAmount);

        // Lender should receive 500$ Back
        lenderAmount = maxLoanValue - loan.maxLoanAmount; // 3,400,000,000,000,000,000
        IERC20(loan.loanERC20Denomination).transferFrom(loan.borrower, loan.Lender, lenderAmount); // 

        emit LoanAccepted(_loanId, _nftCollateralId, _nftcollateralContract, _loanprincipalAmount, _loanERC20Denomination, loan.loanStartTime);
    }
     
     uint256 public totalMinutes;// I need to make it private
     
     // Repay => loanacceptAmount => _loanprincipalAmount + 2% Entry + 1% Monthly 
    function payBackLoan(uint256 _loanId) external nonReentrant {
        
        require(!loanRepaidOrLiquidated[_loanId], "Loan has already been repaid or liquidated");

        // Fetch loan details from storage, but store them in memory for the
        // sake of saving gas.
        LOAN storage loan = loanIdToLoan[_loanId];

        // Check that the borrower is the caller, only the borrower is entitled to the collateral.
        require(msg.sender == loan.borrower, "Only the borrower can pay back a loan and reclaim the underlying NFT");

        // Fetch current owner of loan promissory note.
        address Lender = ownerOf(_loanId);
        Lender;

        // Repay => loanacceptAmount => _loanprincipalAmount + 2% Entry + 1% Monthly after It can be called at any time after the loan has begun. 
        // entryFeeAndAccumulatedDebt = 2e^17 times 0
        uint256 entryFeeAndAccumulatedDebt = calculateEntryFeePercentage(loan.maxLoanAmount).add(calculateAccumulatedDebtPercentage(loan.maxLoanAmount));
        
        // New Debt => assetSoldNewDebtPercentage

        // Loan Ending time 
        loan.endingTime = block.timestamp;

        // Total time: 
        totalMinutes =  calculateTotalMinutes(loan.endingTime, loan.loanStartTime);

        // Loan total time duration of loan in minutes
        loan.maximumLoanDuration = totalMinutes;

        // Mark loan as repaid before doing any external transfers to follow
        // the Checks-Effects-Interactions design pattern.
        loanRepaidOrLiquidated[_loanId] = true;

        --numLoans;

        // Transfer entryFeeAndAccumulatedDebt = 7350$ from borrower to ContractOwner
        IERC20(loan.loanERC20Denomination).transferFrom(loan.borrower, contractOwner, entryFeeAndAccumulatedDebt);

        // Owna sending 7350$ to Lender    
        IERC20(loan.loanERC20Denomination).transferFrom(contractOwner, loan.Lender, entryFeeAndAccumulatedDebt);

        // Owna Taking 10%
        //  ownaTakesPercentage()

        // Transfer collateral from this contract to borrower.
        IERC721(loan.nftCollateralContract).transferFrom(address(this), loan.borrower, loan.tokenId);

        // Destroy the lender's promissory note.
        _burn(_loanId);

       // Emit an event with all relevant details from this transaction.
        emit LoanRepaid(_loanId,loan.borrower,loan.Lender,loan.maxLoanAmount,loan.nftCollateralId,loan.nftCollateralContract,loan.loanERC20Denomination);

     }

    // 2 % of 7000$ (4340000000000000000 wei) = 86800000000000000 wei = 0.0868 ETH = 139.88 $
    function calculateEntryFeePercentage(uint256 borrowersAcceptedAmount) public pure returns (uint256)
    {
        uint256 _entryFee = 200; // 2 %
        uint256 entryfeedeductedAmount = borrowersAcceptedAmount.mul(_entryFee).div(10000); // 2% = (2 * theNumber) / 100 = 140 -------- => 100 = 10000
        return entryfeedeductedAmount;
    }

    // 1% of 7000$ (4340000000000000000 wei) = 130200000000000000 wei =  0.1302 ETH = 
    function calculateAccumulatedDebtPercentage(uint256 borrowersAcceptedAmount) public pure returns (uint256)
    {
        uint256 _accumulatedDebt = 100; // 1 % = (1 * thenumber) / 1,000 = 7 * 30 = 210$ ------- => 100 = 100000
        // deductedAmount = theNumber.mul(_accumulatedDebt).div(10000);
        uint256 accumulatedDeductedAmount = (borrowersAcceptedAmount.mul(_accumulatedDebt).div(10000)) * 3 ;
        return accumulatedDeductedAmount;
    }

    // 1% of 7000$ (4340000000000000000 wei) = 130200000000000000 wei =  0.1302 ETH = 
    function totalDebt(uint256 EntryFeePercentage, uint256 AccumulatedDebtPercentage) public pure returns (uint256)
    {
        uint256 totalAmount = EntryFeePercentage.add(AccumulatedDebtPercentage);
        return totalAmount;
    }

    // 10% of Owna Takes => 900$
    function ownaTakesPercentage(uint256 soldItemPrice) public pure returns (uint256)
    {
        uint256 _ownaTakes = 1000; // 10 %
        uint256 ownaTakesAmount = soldItemPrice.mul(_ownaTakes).div(10000);
        return ownaTakesAmount;
    }

    // 1499.5 $
    function assetSoldNewDebtPercentage(uint256 assetSoldTime, uint256 borrowersAcceptedAmount) public pure returns (uint256)
    {
        uint256 twelvePercent  = 1200; // 12%
        uint256 newDebtAmount = ((borrowersAcceptedAmount.mul(twelvePercent).div(10000)).mul(borrowersAcceptedAmount).div(365 days)).mul(assetSoldTime);
        return newDebtAmount;
    }

    // 7350 + 149.5 = 7499.5 $ send it from => Owna to => Lender
    function totalDebtAndNewDebt(uint256 _totalDebt, uint256 _newDebt) public pure  returns (uint256)
    {
        uint256 lendersAmount = _totalDebt.add(_newDebt);
        return lendersAmount;
    }
    
    // 9000$ - 7499.5$ – 900$ =  600.5
    function restBorrowerPercentage (uint256 assetSoldPrice,  uint256 _totalDebtAndNewDebt, uint256 _ownaTakesPercentage) public pure returns (uint256)
    {
        uint borrowerAmount = assetSoldPrice.sub(_totalDebtAndNewDebt).sub(_ownaTakesPercentage);
        return borrowerAmount;
    }

    function calculateTotalMinutes(uint256 _endingTime, uint256 _startingTime) public pure returns (uint256 _totalMinutes){
        _totalMinutes = ((_endingTime - _startingTime) / 60); // in minutes!
        return _totalMinutes;
    }


