import "./ERC2771ContextUpgradeable.sol";

interface IDeviceAccount {
    function isActiveDevice(address _address) external view returns (bool);
    function getDeviceStatus(address _address) external view returns (uint);
}

contract SeedVault is Initializable, UUPSUpgradeable, OwnableUpgradeable, EIP712Upgradeable, ERC2771ContextUpgradeable {
    using ECDSAUpgradeable for bytes32;

    event EDHVaultKeyUpdated(address indexed _account, address _updater, bytes32 publicKey, address [] devices);

    // Structs

    // -- state models
    struct SeedItem {
        bytes32 encryptionPublicKey;

        bytes32 previousEncryptionPublicKey;
        // nonce|nacl.secretBox(previousSi, newSeedDerivedSecretKey)
        bytes previousSeed; // bytes(0) on first item

        // map of device-address=> nonce|nacl.box(account-seed, deviceEncryptionPublicKey, encryptionPrivateKey).
        mapping(address => bytes) deviceSeeds;

        uint256[16] _reserved;
    }

    struct Vault {
        SeedItem[] seedItems;
    }

    // -- end state models

    // -- read models

    struct DeviceSeedView {
        bytes32 encryptionPublicKey;

        bytes32 previousEncryptionPublicKey;
        // nonce|nacl.secretBox(previousSi, newSeedDerivedSecretKey)
        bytes previousSeed;

        bytes deviceSeed;
    }

    // States

    mapping(address => Vault) private accountVaults;

    // Initialize
    function initialize(address forwarder) public initializer {
        // Initializes the contract setting the deployer as the initial owner.
        __Ownable_init();
        __UUPSUpgradeable_init();

        // Initializes the domain separator and parameter caches
        __EIP712_init("SeedVault", "1");

        __ERC2771Context_init(forwarder);

    }

    // UUPSUpgradeable
    function _authorizeUpgrade(address) internal override onlyOwner {}

    // ERC2771 - Meta Transactions
    function _msgSender() internal view virtual override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (address sender) {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData() internal view virtual override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (bytes calldata) {
        return ERC2771ContextUpgradeable._msgData();
    }

    function setTrustedForwarder(address forwarder) public onlyOwner {
        _setTrustedForwarder(forwarder);
    }

    function addAccountSeed(
        bytes32 encryptionPublicKey,
        bytes32 previousEncryptionPublicKey,
        bytes memory previousSeed,
        address[] memory devices,
        bytes[] memory seeds
    ) public {
        address accountAddress = _msgSender();
        _addAccountSeedBy(
            accountAddress, accountAddress,
            encryptionPublicKey,
            previousEncryptionPublicKey,
            previousSeed,
            devices, seeds
        );
    }

    function addDeviceAccountSeed(
        address accountAddress,
        bytes32 encryptionPublicKey,
        bytes32 previousEncryptionPublicKey,
        bytes memory previousSeed,
        address[] memory devices,
        bytes[] memory seeds
    ) public {
        address device = _msgSender();
        require(accountAddress != address(0), "Account is address(0)");
        require(AddressUpgradeable.isContract(accountAddress), "Account not EDHAccount");
        require(IDeviceAccount(accountAddress).isActiveDevice(device), "Caller not active device");
        _addAccountSeedBy(
            accountAddress, device,
            encryptionPublicKey,
            previousEncryptionPublicKey,
            previousSeed,
            devices, seeds
        );
    }

    function _addAccountSeedBy(
        address accountAddress,
        address updater,
        bytes32 encryptionPublicKey,
        bytes32 previousEncryptionPublicKey,
        bytes memory previousSeed,
        address[] memory devices,
        bytes[] memory seeds
    ) internal {
        require(devices.length > 0 && seeds.length > 0, "Empty devices or seeds");
        require(devices.length == seeds.length, "Mismatched devices/seeds length");
        require(encryptionPublicKey != bytes32(0), "Empty public key");

        Vault storage vault = accountVaults[accountAddress];

        if (keccak256(previousSeed) != keccak256("")) {
            // "Transferring from %s to %s %s tokens",
            // console.log(previousSeed);
            require(previousEncryptionPublicKey != bytes32(0), "Empty previous Public Key");
        }

        vault.seedItems.push();
        SeedItem storage sItem = vault.seedItems[vault.seedItems.length-1];

        sItem.encryptionPublicKey = encryptionPublicKey;
        sItem.previousEncryptionPublicKey = previousEncryptionPublicKey;
        sItem.previousSeed = previousSeed;

        uint newSeedLength = devices.length;
        for(uint i=0; i < newSeedLength; i++) {
            sItem.deviceSeeds[devices[i]] = seeds[i];
        }

        emit EDHVaultKeyUpdated(accountAddress, updater, encryptionPublicKey, devices);
    }

    function getDeviceVaultSeeds(address deviceAddress) public view returns(DeviceSeedView[] memory) {
        return getVaultSeeds(_msgSender(), deviceAddress);
    }

    function getVaultSeeds(address accountAddress, address deviceAddress) public view returns (DeviceSeedView[] memory seeds) {
        return getVaultSeedsAt(accountAddress, deviceAddress, 0, 1000);
    }

    function getVaultSeedsAt(address accountAddress, address deviceAddress, uint offset, uint count) public view returns (DeviceSeedView[] memory seeds) {
        IDeviceAccount dvAccount = IDeviceAccount(accountAddress);
        require(dvAccount.getDeviceStatus(deviceAddress) != 0, "Mismatched device and account");

        Vault storage vault = accountVaults[accountAddress];
        uint vaultSeedLength = vault.seedItems.length;

        if (offset >= vaultSeedLength) {
            return seeds;
        }
        count = Math.min(count, vaultSeedLength - offset);
        seeds = new DeviceSeedView[](count);

        for(uint i=0; i < count && i+offset < vaultSeedLength; i++) {
            SeedItem storage sItem = vault.seedItems[i+offset];
            seeds[i].encryptionPublicKey = sItem.encryptionPublicKey;
            seeds[i].previousEncryptionPublicKey = sItem.previousEncryptionPublicKey;
            seeds[i].previousSeed = sItem.previousSeed;
            seeds[i].deviceSeed = sItem.deviceSeeds[deviceAddress];
        }
        return seeds;
    }
}

