            "Access to Request Denied"
        );

        _withdraw(requestId);
        _deleteSwapRequest(swapRequests[requestId].poolId, requestId);
    }

    // Need to take care of the pool state while doing this, currently it should be locked
    function distributeFundsManually(bytes32 requestId)
        external
        onlyOwner
        onlyDuringPoolState(swapRequests[requestId].poolId, PoolStatus.LOCKED)
        nonReentrant
    {
        _withdraw(requestId);
        _callReceiveFunds(requestId);
        _deleteSwapRequest(swapRequests[requestId].poolId, requestId);
    }

    function distributeFundsForcefully(bytes32 requestId)
        external
        onlyOwner
        onlyDuringPoolState(swapRequests[requestId].poolId, PoolStatus.LOCKED)
