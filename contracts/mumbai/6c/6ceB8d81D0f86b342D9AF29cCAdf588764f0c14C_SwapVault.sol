    function mulPrice(
        uint256 _price1,
        uint8 _decimals1,
        uint256 _price2,
        uint8 _decimals2,
        uint8 _outDecimals
    ) internal pure returns (uint256) {
        uint8 multiplier = 18 - _decimals1;
        uint8 multiplier2 = 18 - _decimals2;
        uint8 outMultiplier = 18 - _outDecimals;

        _price1 *= 10**multiplier;
        _price2 *= 10**multiplier2;

