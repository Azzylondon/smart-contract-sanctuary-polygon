    /**
     * @notice Distribute amount proportionatly to token holders
     * PayAmount is in $POD
     */
    function payoutHolders(
        address _payer,
        uint256 _tokenID, 
        uint256 _payAmount, 
        address [] memory _holders
        ) public nonReentrant onlyContractAdmin
    {
        // Make sure Marketplace is allowed to spend $POD, then spend it
        require(
            POD.allowance(_payer, address(this)) > _payAmount,
            "PodiumMarketplace: POD spend allowance or balance is too low"
        );

        bool successTo = POD.transferFrom(
            _payer, 
            address(this), 
            _payAmount
        );
        require(successTo, "POD transfer failed.");

        uint256 totalPayed = 0;

        uint256 marketTransactionFee = (_payAmount * marketPayoutFee) / 10000;
        salesBalance_[feeTaker] += marketTransactionFee;

        totalPayed += marketTransactionFee;
        uint256 payAfterFees = _payAmount - marketTransactionFee;

        // Add balance to all holders
        for(uint i = 0; i < _holders.length; i++){
            uint256 holderPay = payAfterFees * (
                AdSpaceMint.balanceOf(_holders[i], _tokenID) /
                AdSpaceMint.getTokenInfo(_tokenID).tokenSupply) / 10000;
            salesBalance_[_holders[i]] += holderPay;
            totalPayed += holderPay;
        }

        // Refund if overpaid
        if(totalPayed < _payAmount){
            bool successFrom = POD.transferFrom( 
                address(this), 
                _payer,
                (_payAmount - totalPayed)
            );
            require(successFrom, "POD refund failed.");
        }
    }

    /**
     * @notice Internal list specified token and amounts
     * at specific price for specific Owner address
     * @dev list duration is in epochs
     *
     * Emits a {TokenListed} event.
     */
    function _listTokensTo(
        OwnerToken memory _ownerToken,
        AdSpaceListing memory _adSpace
    )   private 
        sufficientTokenBalance(_ownerToken, _adSpace.amount)
        listPriceNotZero(_ownerToken, _adSpace.unitPrice)
        listDurationPositive(_ownerToken, _adSpace.listDuration)
    {

        // Check marketplace approved
        if(!AdSpaceMint.isApprovedForAll(_ownerToken.owner, address(this))){
            revert MarketPlaceNotApproved(_ownerToken.owner);
        }

        // Check that token has not been listed
        if(listings_[_ownerToken.tokenID][_ownerToken.owner].unitPrice > 0) {
            revert ListingAlreadyExists(_ownerToken.owner, _ownerToken.tokenID);
        }

        listings_[_ownerToken.tokenID][_ownerToken.owner] = _adSpace;
        tokenListers_[_ownerToken.tokenID].push(_ownerToken.owner);

        emit TokenListed(
            _ownerToken.owner, 
            _ownerToken.tokenID, 
            _adSpace.unitPrice,
             _adSpace.amount,
            _adSpace.listDuration
        );
    }

    /**
     * @notice Internal update token listing that performs no checks
     */
    function _updateListing (
        uint256 _tokenID,
        uint256 _unitPrice,
        uint256 _amount,
        uint256 _listDuration
    ) internal 
    {
        listings_[_tokenID][msg.sender].unitPrice = _unitPrice;
        listings_[_tokenID][msg.sender].amount = _amount;
        listings_[_tokenID][msg.sender].listDuration = _listDuration;

        emit UpdatedTokenListing(
            msg.sender, 
            _tokenID, 
            _unitPrice, 
            _amount, 
            _listDuration
        );
    }

    /**
     * @notice find element from list and remove
     * Look for listing, shift all others to left and pop final dup
     */
    function _removeItem(address _sellerAddress, uint256 _tokenID) internal
    {

        for(uint i = 0; i < tokenListers_[_tokenID].length; i++) {
            if(tokenListers_[_tokenID][i] == _sellerAddress){
                for(uint j = i; j < tokenListers_[_tokenID].length - 1; j++) {
                    tokenListers_[_tokenID][j] = tokenListers_[_tokenID][j+1];
                }
                tokenListers_[_tokenID].pop();
            }
        }
    }

    // ------------------------------------------------------------------------
    // Utility Get Methods

    /**
     * @notice Getter that returns balance for each address that can be withdrawn
     */
    function getSalesBalance(address _account) 
        public view returns(uint256)
    {
        return salesBalance_[_account];
    }

    /** 
     * @notice get listers by address
     */
    function getTokenListers(uint256 _tokenID) 
        public view returns(address [] memory)
    {
        return tokenListers_[_tokenID];
    }

    /** 
     * @notice Getter listing by token by owner
     */
    function getListing(address _owner, uint256 _tokenID) 
        public view returns(AdSpaceListing memory)
    {
        return listings_[_tokenID][_owner];
    }

    // ------------------------------------------------------------------------
    // Marketplace operations and admin

    /** 
     * @notice attach new mint contract to mint from
     *
     * Emits a {MintContractChange} event.
     */
    function newMintContract(address _newMintContract) 
        public onlyContractAdmin
    {
        AdSpaceMint = IPodiumAdSpaces(_newMintContract);

        emit MintContractChange(msg.sender, _newMintContract);

    }

    /** 
     * @notice attach new mint contract to mint from
     *
     * Emits a {PodContractChange} event.
     */
    function newPODContract(address _newPODContract) 
        public onlyContractAdmin
    {
        POD = IERC20(_newPODContract);

        emit PODContractChange(msg.sender, _newPODContract);
    }

    /**
     * @notice Setter that changes marketplace sales fee
     *
     * Emits a {MarketFeeChange} event.
     */
    function setMarketSaleFee(uint256 _newFee) 
        external onlyContractAdmin
    {
        marketSaleFee = _newFee;
        
        emit MarketSaleFeeChange(msg.sender, _newFee);
    }

    /**
     * @notice Setter that changes marketplace payout fee
     *
     * Emits a {HERE} event.
     */
    function setMarketPayoutFee(uint256 _newFee) 
        external onlyContractAdmin
    {
        marketPayoutFee = _newFee;
        
        // HERE EMIT
        // emit MarketSaleFeeChange(msg.sender, _newFee);
    }

    /**
     * @notice Setter that changes marketplace fee receiver
     * that can withdraw fees
     *
     * Emits a {FeeTakerChange} event.
     */
    function setFeeTaker(address _newFeeTaker) 
        external onlyContractAdmin
    {
        feeTaker = _newFeeTaker;
        
        emit FeeTakerChange(msg.sender, _newFeeTaker);
    }

}

