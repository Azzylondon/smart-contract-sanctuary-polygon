// Throw error when does account does not own enough Tokens
error InsufficientTokenBalance(address account, uint256 tokenID, uint256 amount);

// Throw error when trying to list at price 0
error ListPriceCannotBeZero(address caller, uint256 tokenID, uint256 unitPrice);

// Throw error when this contract is not an approved operator
error MarketPlaceNotApproved(address caller);

// Throw error when listing already exists for user token
error ListingAlreadyExists(address caller, uint256 tokenID);

// Throw error when listing already exists for user token
error ListingDNE(address caller, uint256 tokenID);

// Throw error if list duration is 0
error ListDurationCannotBeZero (address caller, uint256 tokenID, uint256 listDuration);

// Throw error if listing is not active
error ListingNotActive(address owner, uint256 tokenID, address caller);

contract PodiumMarketplace is ReentrancyGuard, Admin {

    // PodiumAdSpace Token contract
    IPodiumAdSpaces public AdSpaceMint;
    IERC20 public POD;

    /* @notice Listing information including seller, amount and unit price
     * @dev listing may sell any amount <= amount
     *
     * unitPrice is the price in USD (* some multiple)
     * amount is in bps. See {AdSpaceMint-maxTokenSupply}. 
     * createdAt is the block.timestamp of transaction
     * listDuration is how long the listing is active for
     * sold indicated if a token was sold
    */
    struct AdSpaceListing {
        uint256 unitPrice;
        uint256 amount;
        uint256 listDuration;
        uint256 createdAt;
        bool sold;
    }

    struct OwnerToken {
        address owner;
        uint256 tokenID;
    }

    // Marketplace fee (2.5% = 250)
    uint256 public marketSaleFee;

    // Marketplace payout fee (i.e. brand deals)
    uint256 public marketPayoutFee;

    // Mapping from token ID and owner address to Listings
    mapping(uint256 => mapping(address => AdSpaceListing)) private listings_;

    // Mapping from token ID to all active listings sellers
    mapping(uint256 => address []) private tokenListers_;

    // Mapping from owner to sales balance
    mapping(address => uint256) private salesBalance_;

    // Marketplace fee address
    address public feeTaker;

    // Default listing duration if not specified
    uint256 private defaultDuration_ = 365 days;

    // Uniswap router v3 ETH
    // ISwapRouter public constant uniswapRouter = ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);
    // IQuoter public constant quoter = IQuoter(0xb27308f9F90D607463bb33eA1BeBb41C27CE5AB6);
    // address private multiDaiKovan = 0x6B175474E89094C44Da98b954EedeAC495271d0F;
    // address private WETH9 = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;

    ISwapRouter public constant uniswapRouter = ISwapRouter(0xE592427A0AEce92De3Edee1F18E0157C05861564);
    IQuoter public constant quoter = IQuoter(0xb27308f9F90D607463bb33eA1BeBb41C27CE5AB6);
    address private usdc = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    address private matic = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270;


    constructor(
        address _AdSpaceMint, 
        address _POD, 
        uint256 _marketSaleFee, 
        uint256 _marketPayoutFee,
        address _feeTaker
    )
    {
        AdSpaceMint = IPodiumAdSpaces(_AdSpaceMint);
        POD = IERC20(_POD);
        marketSaleFee = _marketSaleFee;
        marketPayoutFee = _marketPayoutFee;
        feeTaker = _feeTaker;
    }

    // ------------------------------------------------------------------------
    // Events emitted

    // Emitted when a new listing is created
    event TokenListed(
        address indexed seller, 
        uint256 indexed tokenId,
        uint256 unitPrice,
        uint256 amount,
        uint256 listDuration
    );

   // Emitted when a new listing is updated
    event UpdatedTokenListing(
        address indexed seller, 
        uint256 indexed tokenId,
        uint256 unitPrice,
        uint256 amount,
        uint256 listDuration
    );

    // Emitted when a listing is deleted
    event TokenDeListed(address indexed owner, uint256 indexed tokenId);

    // Emitted when balance is withdrawn from contract
    event WithdrawBalance(address indexed caller, uint256 amount);

    // Emitted when a token is sold
    event TokenSale(
        address indexed seller, 
        address indexed buyer, 
        uint256 tokenID, 
        uint256 amount, 
        uint256 totalPODPrice
    );

    // Emitted when the marketplace fee taker address is changed
    event FeeTakerChange(address indexed admin,address indexed newTaker);

   // Emitted when the fee ammount is changed
    event MarketSaleFeeChange(address indexed admin, uint256 indexed newFee);

   // Emitted when the $POD ERC20 Token address is changed
    event PODContractChange(
        address indexed admin, 
        address indexed newPODContract
    );

   // Emitted when the Mint ERC1155 contract is changed
    event MintContractChange(
        address indexed admin, 
        address indexed newMintContract
    );

    // ------------------------------------------------------------------------
    // Modifiers used 

    // @dev throws if owner does not have enough tokens to list requested
    // amount. Also inherently checks if _owner is an actual owner
    modifier sufficientTokenBalance(
        OwnerToken memory _ownerToken, 
        uint256 _amount
        )
    {
        if(AdSpaceMint.balanceOf(_ownerToken.owner,_ownerToken.tokenID) < _amount ){
            revert InsufficientTokenBalance(_ownerToken.owner, _ownerToken.tokenID, _amount);
        }
        _;
    }

    // @dev throws is token not listed
    modifier tokenIsListed (OwnerToken memory _ownerToken){
        if(listings_[_ownerToken.tokenID][_ownerToken.owner].unitPrice <= 0) {
            revert ListingDNE(_ownerToken.owner, _ownerToken.tokenID);
        }
        _;
    }

    // @dev throws if list price is zero
    modifier listPriceNotZero (
        OwnerToken memory _ownerToken, 
        uint256 _unitPrice
        )
    {
        if(_unitPrice <= 0) {
            revert ListPriceCannotBeZero(_ownerToken.owner, _ownerToken.tokenID, _unitPrice);
        }
        _;
    }

    // @dev throws is token listduration is in the past
    modifier listDurationPositive (OwnerToken memory _ownerToken, uint256 _listDuration){
        if(_listDuration <= 0) {
            revert ListDurationCannotBeZero(_ownerToken.owner, _ownerToken.tokenID, _listDuration);
        }
        _;
    }

    // ------------------------------------------------------------------------
    // Contrzct main function logic

    /**
     * @notice List specified token and amounts at specific price
     */
    function listTokens(
        uint256 _tokenID,
        uint256 _unitPrice,
        uint256 _amount,
        uint256 _listDuration
    ) external nonReentrant
    {
        _listTokensTo(
            OwnerToken(msg.sender,_tokenID), 
            AdSpaceListing(
                _unitPrice, 
                _amount, 
                _listDuration, 
                block.timestamp, 
                false
            )
        );
    }

    /**
     * @notice update token listing
     */
    function updateListing(
        uint256 _tokenID,
        uint256 _unitPrice,
        uint256 _amount,
        uint256 _listDuration
    )   external nonReentrant
        tokenIsListed(OwnerToken(msg.sender, _tokenID))
        listPriceNotZero(OwnerToken(msg.sender, _tokenID), _unitPrice)
        sufficientTokenBalance(OwnerToken(msg.sender, _tokenID), _amount)     
        listDurationPositive(OwnerToken(msg.sender, _tokenID), _listDuration)   
    {
        _updateListing(_tokenID, _unitPrice, _amount,_listDuration);

    }

    /**
     * @notice De-list specified token 
     * @dev If owned or has been transfered out, listings
     * are associated by token and owner. So may be canceled
     * even without explicit current ownership
     *
     * Rather than deleting listings, we set the duration
     * to zero meaning that given current block.timestamp
     * always > createdAt the listing will have "expired"
     *
     * When finding listings, we must check non-expired listings
     * and so adding a bool for active rather that listDuration 
     * would not reduce work
     *
     * Emits a {TokenDeListed} event.
     */
    function delistTokens(
        uint256 _tokenID
    ) public nonReentrant tokenIsListed(OwnerToken(msg.sender, _tokenID))
    {
        listings_[_tokenID][msg.sender].listDuration = 0 days;
        listings_[_tokenID][msg.sender].unitPrice = 0;

        _removeItem(msg.sender, _tokenID);

        emit TokenDeListed(msg.sender, _tokenID);


    }

    /**
     * @notice Mint token for creator and list all tokens
     * @dev Mint function is restricted to Podium
     * Marketplace allowed addresses (i.e. admins)
     * _totalSupply is in bps. See {AdSpaceMint-maxTokenSupply}.  
     */
    function mintAndList(
        IPodiumAdSpaces.TokenInfo memory _tokenInfo,
        address _AllowedPodiumOperator,
        uint256 _unitPrice
    ) public onlyContractAdmin returns(uint256)
    {
        return mintAndList(
            _tokenInfo,
            _AllowedPodiumOperator,
            _unitPrice,
            _tokenInfo.tokenSupply,
            defaultDuration_
        );
    }

    /**
     * @notice Mint token for creator and list specified amount of tokens
     * @dev Mint function is restricted to Podium
     * Marketplace allowed addresses (i.e. admins)
     * _totalSupply is in bps. See {AdSpaceMint-maxTokenSupply}.  
     */
    function mintAndList(
        IPodiumAdSpaces.TokenInfo memory _tokenInfo,
        address _AllowedPodiumOperator,
        uint256 _unitPrice,
        uint256 _listamount,
        uint256 _listDuration
    ) public onlyContractAdmin returns(uint256)
    {

        // confirm that total_supply > 0
        uint256 tokenID = AdSpaceMint.mint(
            _tokenInfo,
            _AllowedPodiumOperator
        );

        if(_listamount > 0){
            _listTokensTo(
                OwnerToken(_tokenInfo.tokenCreator, 
                tokenID), 
                AdSpaceListing(
                    _unitPrice, 
                    _listamount, 
                    _listDuration, 
                    block.timestamp,                     
                    false
                )
            );
        }

        return tokenID;
    }

    /**
     * @notice Send $POD to user based on sales balance
     * Making a withdrawal is safer than transfering funds automatically
     * See https://fravoll.github.io/solidity-patterns/pull_over_push.html
    *
     * Emits a {WithdrawBalance} event.
     */
    function withdrawSalesBalance() 
        external nonReentrant
    {
        require(
            salesBalance_[msg.sender] > 0, 
            "PodiumMarketplace: Balance is 0"
        );

        uint256 withdrawAmount = salesBalance_[msg.sender];

        salesBalance_[msg.sender] = 0;

        bool success = POD.transferFrom(
            address(this), 
            msg.sender, 
            withdrawAmount
        );
        require(success, "Withdrawal failed.");

        emit WithdrawBalance(msg.sender, withdrawAmount);
    }

    /**
     * @notice Buy listed Token
    *
     * Emits a {TokenSale} event.
     */
    function buyListedToken(uint256 _tokenID, address _seller, uint256 _amount) 
        external nonReentrant 
        sufficientTokenBalance(OwnerToken(_seller, _tokenID), _amount)
    {
        AdSpaceListing memory Listing = listings_[_tokenID][_seller];

        // Make sure listing is active
        if(block.timestamp > (Listing.createdAt + Listing.listDuration)){
            revert ListingNotActive(_seller, _tokenID, msg.sender);
        }
        IPodiumAdSpaces.TokenInfo memory tokenInfo = AdSpaceMint.getTokenInfo(_tokenID);
        // uint256 totalPODPrice = _amount * Listing.unitPrice * 1;
        uint256 totalPODPrice = _amount * Listing.unitPrice * getPODPrice();
        
        // Make sure Marketplace is allowed to spend $POD
        require(
            POD.allowance(msg.sender, address(this)) > totalPODPrice,
            "PodiumMarketplace: POD spend allowance or balance is too low"
        );

        bool success = POD.transferFrom(
            msg.sender, 
            address(this), 
            totalPODPrice
        );

        require(success, "POD transfer failed.");

        uint256 marketTransactionFee = (totalPODPrice * marketSaleFee) / 10000;
        uint256 creatorRoyalties = (totalPODPrice * tokenInfo.royalties) / 10000;

        Listing.sold = true;
        delistTokens(_tokenID);
        
        salesBalance_[tokenInfo.tokenCreator] += creatorRoyalties;
        salesBalance_[feeTaker] += marketTransactionFee;
        salesBalance_[_seller] += (totalPODPrice - creatorRoyalties - marketTransactionFee);

        AdSpaceMint.safeTransferFrom(_seller, msg.sender, _tokenID, _amount, "");

        emit TokenSale(_seller, msg.sender, _tokenID, _amount, totalPODPrice);
    }

