        bytes sig;              // signed hash that was computed with 'batchMintPayableHash'
    }

    /**
     * @dev constructor with default admin of extension as parameter  
     */
    constructor(address _admin) {
        _grantRole(DEFAULT_ADMIN_ROLE, _admin);
    }


    /**
     * @dev Mint function that can be used by an external source if the used request data can be verified 
     * @param _params struct that contains all data to verify the given mint request     
     */
    function batchMintPayable(BatchMintPayable calldata _params) public nonReentrant payable {
        require(_params.tos.length == _params.tokenIds.length && _params.tos.length == _params.tokenUris.length && _params.tos.length == _params.prices.length, "Size input arrays is not equal!");
        require(_params.expiresAt >= block.timestamp, "minting request expired");
        require(_params.signer != address(0), "zero signer address not allowed");
        require(_params.payout != address(0), "zero payout address not allowed");
        _checkRole(DEFAULT_ADMIN_ROLE, _params.signer);

        uint256 priceSum = 0;
        uint256 len = _params.prices.length;
        for (uint256 i; i < len; ++i) {
            priceSum += _params.prices[i];
        }

        require(msg.value >= priceSum, "insufficient funds sent for minting");

        bytes32 hash = batchMintPayableHash(_params.baseContract, _params.tos, _params.tokenIds, _params.tokenUris, _params.prices, _params.expiresAt, _params.payout);
        require(validateSignature(hash, _params.sig, _params.signer), "signature invalid");

        IBaseERC721Extendable(_params.baseContract).batchMintExtension(_params.tos, _params.tokenIds, _params.tokenUris);

        // if buyer sends too much money, we will refund the difference
        if (msg.value > priceSum) {
            uint256 change = msg.value - priceSum;
            address payable refundAddress = payable(msg.sender);
            (bool refundSent,) = refundAddress.call{value : change}("");
            require(refundSent, "refunding of excess Ether failed");
        }

        address payable payoutAddress = payable(_params.payout);
        (bool payoutSent,) = payoutAddress.call{value : priceSum}("");
        require(payoutSent, "send of payment to payout failed");
    }

    /**
     * @dev Support function that can be called by an external provider to validate if minting of the given tokens is possible 
     * @param baseContract erc721 contract where the extension is registered and where the token check should be executed     
     * @param tokenIds that should be checked. If at least one token is minted a information will be returned
     * @param expiresAt timestamp that will compared to current block time.
     */
    function checkClaimEligibility(address baseContract, uint256[] calldata tokenIds, uint256 expiresAt) external view returns (string memory){

        if (block.timestamp >= expiresAt) {
            return "minting request expired";
        }

        uint256 len = tokenIds.length;
        for (uint256 i; i < len; ++i) {
            try IERC721(baseContract).ownerOf(tokenIds[i]) returns (address) {
                return "At least one token was already minted.";
            }catch{}
        }
        return "";
    }

    /**
     * @dev Mint function that takes all input parameters to create a hash of them. This hash can the be signed by an admin account and used for minting      
     * @param _baseContract;    erc721 contract where the extension will connect
     * @param _tos;             list of receiver for the tokens
     * @param _tokenIds;        list of token ids that should be minted
     * @param _tokenUris;       list of token uris of the tokens
     * @param _prices;          list of prices of the tokens
     * @param _expiresAt;       block time when the mint offer expires
     * @param _payout;          payout address that receives the the eth that was send with minting      
     */
    function batchMintPayableHash(address _baseContract, address[] calldata _tos, uint256[] calldata _tokenIds, string[] calldata _tokenUris, uint256[] calldata _prices, uint256 _expiresAt, address _payout) public pure returns (bytes32) {
        require(_tos.length == _tokenIds.length && _tos.length == _tokenUris.length && _tos.length == _prices.length, "Size input arrays is not equal!");
        bytes memory encodedParams = abi.encode(
            _baseContract,
            _tos,
            _tokenIds,
            _tokenUris,
            _prices,
            _expiresAt,
            _payout
        );
        return keccak256(encodedParams);
    }

    /**
     * @dev Updates the uris of the given tokens. The extension must be the creator of the token      
     * @param _baseContract;    erc721 contract where the extension will connect     
     * @param _tokenIds;        list of token ids that should be updated
     * @param _tokenUris;       list of token uris that should be set           
     */
    function batchUpdateTokenUri(address _baseContract, uint256[] calldata _tokenIds, string[] calldata _tokenUris) public onlyRole(DEFAULT_ADMIN_ROLE) {
        IBaseERC721Extendable(_baseContract).batchUpdateTokenUriExtension(_tokenIds, _tokenUris);
    }

    /**
     * @dev Updates the base uri in the contract of the extension      
     * @param _baseContract;    erc721 contract where the extension will connect     
     * @param _baseURI;         base uri that should be updated                
     */
    function updateBaseURI(address _baseContract, string memory _baseURI) public onlyRole(DEFAULT_ADMIN_ROLE) {
        IBaseERC721Extendable(_baseContract).updateBaseURIExtension(_baseURI);
    }

    /**
     * @dev Updates the base uri in the contract of the extension      
     * @param _hash;            Hash value that should be validated      
     * @param _sig;             signature of the hash that should be validated                
     * @param _expectedSigner   expected signer of the hash                
     */
    function validateSignature(
        bytes32 _hash,
        bytes memory _sig,
        address _expectedSigner
    ) public pure returns (bool) {
        bytes32 ethSignedHash = ECDSA.toEthSignedMessageHash(_hash);
        address actualSigner = ECDSA.recover(ethSignedHash, _sig);
        return actualSigner == _expectedSigner;
    }


}