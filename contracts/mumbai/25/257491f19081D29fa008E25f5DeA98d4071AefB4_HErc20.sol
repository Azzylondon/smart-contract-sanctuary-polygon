  /**
   * @notice used to deposit collateral
   * @param _tokenId to be deposited as collateral
   */
  function depositCollateralInternal(uint256 _tokenId) internal {
    doCollateralTransferIn(msg.sender, _tokenId);

    _mintCoupon(msg.sender, _tokenId, 0);
  }

  /**
   * @notice minting a coupon when depositing collateral, this coupon will accrue borrows
   * @param _to the receiver (who deposits the collateral)
   * @param _id the id of the token that is deposited
   * @param _borrowAmount the amount that was borrowed
   */
  function _mintCoupon(
    address _to,
    uint256 _id,
    uint256 _borrowAmount
  ) internal {
    uint256 currentId = idCounter.current();
    idCounter.increment();

    // Construct a coupon
    Coupon memory coupon;
    coupon.id = _id;
    coupon.borrowAmount = _borrowAmount;
    coupon.active = true;
    coupon.index = borrowIndex;
    coupon.couponId = currentId;

    borrowCoupons[currentId] = coupon;

    // Track user ownership of the ticket
    userToCoupons[_to].push(currentId);

    // Mint NFT
    _mint(_to, currentId, 1, "");

    // TODO: Set URI
  }

  /**
   * @notice borrows an amount and accrued it to the coupon
   * @param _borrowAmount borrowed amount
   * @param _tokenId nft that you want to use as collateral
   */
  function borrowInternal(uint256 _borrowAmount, uint256 _tokenId) internal {
    // Find the user's coupon
    uint256 activeIndex = searchCouponByNftId(msg.sender, _tokenId);

    // Can't be the null coupon
    if (activeIndex == 0) revert HTokenError(Error.COUPON_LOOKUP);

    // Sanity check to make sure the coupon was found correctly
    if (borrowCoupons[activeIndex].id != _tokenId) revert WrongParams();

    // Our contract must own the relevant tokenId
    if (collateralToken.ownerOf(_tokenId) != address(this)) revert NotOwner();

    // Check for borrow allowed
    hivemind.borrowAllowed(HTokenI(address(this)), msg.sender, _tokenId, _borrowAmount);

    // Add the borrow amount to the token and set index
    borrowCoupons[activeIndex].borrowAmount += _borrowAmount;
    borrowCoupons[activeIndex].index = borrowIndex;

    // Add the amount to total borrows
    totalBorrows += _borrowAmount;

    emit Borrow(msg.sender, _borrowAmount, _tokenId, borrowCoupons[activeIndex].borrowAmount);
    // Transfer the funds to borrower
    doUnderlyingTransferOut(msg.sender, _borrowAmount);
  }

  /**
   * @notice does collateral transfer out, transfer nft to the _to
   * @param _to where to transfer the collateral
   * @param _tokenId what to transfer, the collateral id
   */
  function doCollateralTransferOut(address _to, uint256 _tokenId) internal {
    collateralToken.transferFrom(address(this), _to, _tokenId);
  }

  /**
   * @notice does collateral transfer in, transfer nft to the hToken
   * @param _from where to get the collateral
   * @param _tokenId what to transfer, the collateral id
   */
  function doCollateralTransferIn(address _from, uint256 _tokenId) internal {
    collateralToken.transferFrom(_from, address(this), _tokenId);
  }

  /**
   * @notice repais borrow for a collateral
   * @param _repayAmount the amount to repay
   * @param _tokenId to want collateral id to pay for
   */
  function repayBorrowInternal(uint256 _repayAmount, uint256 _tokenId) internal {
    hivemind.repayBorrowAllowed(HTokenI(address(this)), msg.sender, _tokenId, _repayAmount);

    // Accrue interest
    accrueInterest();

    // console.log("repayAmount ", _repayAmount);

    // Can't repay 0 amount
    if (_repayAmount == 0) revert HTokenError(Error.AMOUNT_ZERO);

    // Find the user's coupon
    uint256 activeIndex = searchCouponByNftId(msg.sender, _tokenId);

    // Sanity check to make sure the coupon was found correctly
    if (borrowCoupons[activeIndex].id != _tokenId) revert WrongParams();

    // Our contract must already own the relevant tokenId
    if (collateralToken.ownerOf(_tokenId) != address(this)) revert NotOwner();

    // Get outstanding debt
    uint256 debt = getBorrowFromCoupon(activeIndex);
    if (debt == 0) revert HTokenError(Error.NO_DEBT);

    // Set amount to repay
    uint256 payment;
    if (_repayAmount > debt) {
      payment = debt;
    } else {
      payment = _repayAmount;
    }

    // Take in repayment monies
    uint256 repayment = doUnderlyingTransferIn(msg.sender, payment);

    // Credit their account
    borrowCoupons[activeIndex].borrowAmount -= repayment;

    // TODO: Set borrow index here to ensure correct compounding

    // Decrease total borrows
    totalBorrows -= repayment;

    emit RepayBorrow(msg.sender, payment, debt, totalBorrows, _tokenId);
  }

  /**
   * @notice repais borrow for a collateral for someone else
   * @param _borrower for whom to pay
   * @param _repayAmount the amount to repay
   * @param _tokenId to want collateral id to pay for
   */
  function repayBorrowBehalfInternal(
    address _borrower,
    uint256 _repayAmount,
    uint256 _tokenId
  ) internal {
    // Accrue interest
    accrueInterest();

    // Can't repay 0 amount
    if (_repayAmount == 0) revert HTokenError(Error.AMOUNT_ZERO);

    // Find the user's coupon
    uint256 activeIndex = searchCouponByNftId(_borrower, _tokenId);

    // Sanity check to make sure the coupon was found correctly
    if (borrowCoupons[activeIndex].id != _tokenId) revert HTokenError(Error.COUPON_NOT_FOUND);

    // Our contract must already own the relevant tokenId
    // require(collateralToken.ownerOf(_tokenId)==address(this));
    // TODO: move this elsewhere if needed

    // Get outstanding debt
    uint256 debt = getBorrowFromCoupon(activeIndex);
    if (debt == 0) revert HTokenError(Error.NO_DEBT);

    // Set amount to repay
    uint256 payment;
    if (_repayAmount > debt) {
      payment = debt;
    } else {
      payment = _repayAmount;
    }

    // Take in repayment monies
    uint256 repayment = doUnderlyingTransferIn(msg.sender, payment);

    // Credit their account
    borrowCoupons[activeIndex].borrowAmount -= repayment;

    // Decrease total borrows
    totalBorrows -= repayment;
  }

  /**
   * @notice liquidate borrw once the health factor is over the collateral factor.
   *         This transfers the nft to the liquidator where you can clawback
   * @param _couponNFTId what to liquidate
   */
  function liquidateBorrowInternal(uint256 _couponNFTId) internal {
    if (!liquidationAllowed(_couponNFTId)) revert HTokenError(Error.LIQUIDATION_NOT_ALLOWED);
    Coupon memory activeCoupon = borrowCoupons[_couponNFTId];
    uint256 _tokenId = activeCoupon.id;

    bytes memory data = abi.encode(address(this));

    collateralToken.safeTransferFrom(address(this), liquidator, _tokenId, data);
  }

  /**
   * @notice check if liquidation is allowed for this coupon, meaning it s active
   * @param _couponNFTId what to check for
   * @return true if it's allowed
   */
  function liquidationAllowed(uint256 _couponNFTId) internal view returns (bool) {
    return borrowCoupons[_couponNFTId].active;
  }

  /**
   * @notice closing liquidation, meaning that you pay the borrow and burn the coupon
   * @param _borrower borrower who pays
   * @param _tokenId what to pay for
   */
  // TODO: Make extra sure to have good return checks and requires. Critical function.
  function closeoutLiquidation(address _borrower, uint256 _tokenId) external {
    if (msg.sender != liquidator) revert Unauthorized();

    uint256 activeIndex = searchCouponByNftId(_borrower, _tokenId);

    uint256 repayAmount = getBorrowFromCoupon(activeIndex);

    // Repay borrow
    repayBorrowBehalfInternal(_borrower, repayAmount, _tokenId);

    // Burn the NFT coupon
    burnAndDelete(_borrower, activeIndex);
  }

  /**
   * @notice checks if withdrawal are allowed for this token id
   * @param _user borrower who pays
   * @param _couponNFTId what to pay for
   * @return returns true if it's allowed
   */
  function withdrawalAllowed(address _user, uint256 _couponNFTId) public view returns (bool) {
    uint256 activeIndex = searchCouponByNftId(_user, _couponNFTId);

    return getBorrowFromCoupon(activeIndex) == 0;
  }

  /**
   * @notice burns a coupon and deletes it from the all coupons
   * @param _user user to whom to burn
   * @param _couponNFTId what to burn
   */
  function burnAndDelete(address _user, uint256 _couponNFTId) internal {
    // Remove coupon from user by overwriting with null coupon
    uint256[] memory userIndices = userToCoupons[_user];
    uint256 i;
    for (i; i < userIndices.length; i++) {
      if (userIndices[i] == _couponNFTId) {
        userIndices[i] = 0;
        break;
      }
    }

    // Overwrite all stored metadata with blank
    delete borrowCoupons[_couponNFTId];

    // Burn coupon NFT
    _burn(_user, _couponNFTId, 1);
  }

  /**
   * @notice withdraw collateral
   * @param _couponNFTId what to withdraw
   */
  // Notably, this takes the ID of the coupon NFT issued by the protocol, NOT the collateral project tokenId
  function withdrawCollateralInternal(uint256 _couponNFTId) internal {
    Coupon memory activeCoupon = borrowCoupons[_couponNFTId];

    uint256 _tokenId = activeCoupon.id;

    if (!withdrawalAllowed(msg.sender, _tokenId)) revert HTokenError(Error.WITHDRAW_NOT_ALLOWED);

    burnAndDelete(msg.sender, _couponNFTId);

    doCollateralTransferOut(msg.sender, _tokenId);
  }

