  /**
   * @notice get borrowed amount from a coupon
   * @param _index index of the borrwed coupon
   * @return borrwed amount of underlying
   */
  function getBorrowFromCoupon(uint256 _index) public view returns (uint256) {
    return borrowCoupons[_index].borrowAmount;
  }

  /**
   * @notice get user's coupon
   * @param _user the user to search for
   * @return user's coupons
   */
  function getUserCoupons(address _user) public view returns (Coupon[] memory) {
    uint256[] memory indices = userToCoupons[_user];
    Coupon[] memory userCoupons = new Coupon[](indices.length);

    uint256 i;
    uint256 index;
    for (i; i < indices.length; i++) {
      index = indices[i];
      userCoupons[i] = borrowCoupons[index];
    }

    return userCoupons;
  }

  /**
   * @notice search a coupon by token id
   * @param _user to serach for
   * @param _nftTokenId nft id to search for
   * @return coupon index in the borrowCoupons
   */
  function searchCouponByNftId(address _user, uint256 _nftTokenId) public view returns (uint256) {
    // Find the user's coupon
    uint256[] memory indices = userToCoupons[_user];
    uint256 activeIndex;

    uint256 i;
    for (i; i < indices.length; i++) {
      activeIndex = indices[i];
      if ((borrowCoupons[activeIndex].id == _nftTokenId) && (borrowCoupons[activeIndex].active)) {
        return activeIndex;
      }
    }

    return 0;
  }

  /**
   * @notice search a coupon by token id
   * @param _user to serach for
   * @param _couponId coupon id to search for
   * @return coupon index in the borrowCoupons
   */
  function searchCouponById(address _user, uint256 _couponId) public view returns (uint256) {
    // Find the user's coupon
    uint256[] memory indices = userToCoupons[_user];
    uint256 activeIndex;

    uint256 i;
    for (i; i < indices.length; i++) {
      activeIndex = indices[i];
      if ((borrowCoupons[activeIndex].couponId == _couponId) && (borrowCoupons[activeIndex].active)) {
        return activeIndex;
      }
    }

    return 0;
  }

  /**
   * @notice get a specific coupon for an NFT
   * @param _couponNFTId nft id
   * @return coupon
   */
  function getSpecificCoupon(uint256 _couponNFTId) external view returns (Coupon memory) {
    return borrowCoupons[_couponNFTId];
  }

  /**
   * @notice accruels interests to all coupons
   * @return all accrued interest
   */
  function accrueInterestToCoupons() public returns (uint256) {
    uint256 index;
    uint256 runningTotal;

    // We may start at position 1 as position 0 is reserved for the null coupon
    for (index = 1; index <= idCounter.current(); ) {
      if (borrowCoupons[index].active) {
        runningTotal += accrueInterestToCoupon(index);
      }
      unchecked {
        index++;
      }
    }

    return runningTotal;
  }

  /**
   * @notice accruels interest to coupon
   * @param _index borrow index
   * @return new total accrued
   */
  function accrueInterestToCoupon(uint256 _index) public returns (uint256) {
    Coupon storage activeCoupon = borrowCoupons[_index];

    uint256 initialAmount = activeCoupon.borrowAmount;
    uint256 initialIndex = activeCoupon.index;
    uint256 currentIndex = borrowIndex;

    uint256 indexDelta = currentIndex - initialIndex;
    uint256 interestIncurred = mul_ScalarTruncate(Exp({ mantissa: indexDelta }), initialAmount);
    uint256 newTotal = initialAmount + interestIncurred;

    // Effects
    activeCoupon.borrowAmount = newTotal;
    activeCoupon.index = currentIndex;

    emit AccrueCouponInterest(activeCoupon, initialAmount, interestIncurred, initialIndex, currentIndex);

    return newTotal;
  }

