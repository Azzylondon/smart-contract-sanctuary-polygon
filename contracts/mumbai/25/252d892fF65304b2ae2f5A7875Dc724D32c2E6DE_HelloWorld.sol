pragma solidity ^0.8.11;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract HelloWorld {
    using SafeMath for uint;

    mapping(uint256 => uint) public alephversaryTokenIdMap;

    function mint(uint256 tokenId, uint alephversary) public {
        alephversaryTokenIdMap[tokenId] = alephversary;
    }

    function getAge(uint256 tokenId) public view returns (string memory range) {
        uint age = block.timestamp.sub(alephversaryTokenIdMap[tokenId]).div(60 * 60 * 24 * 365);
        if (age < 1) {
            range = "less than one";
        } else if (age >= 1 && age < 2) {
            range = "less than two";
        } else if (age >= 2 && age < 4) {
            range = "less than four";
        } else if (age >= 4 && age < 8) {
            range = "less than eight";
        } else if (age >= 8 && age < 16) {
            range = "less than sixteen";
        } else if (age >= 16 && age < 32) {
            range = "less than thirty two";
        } else {
            range = "more than thirty two";
        }
    }
}

