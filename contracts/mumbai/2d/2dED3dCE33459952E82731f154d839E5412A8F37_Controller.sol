pragma solidity ^0.7.5;

import "./libraries/Ownable.sol";
import "./libraries/Address.sol";
import "./libraries/SafeMath.sol";
import "@quant-finance/solidity-datetime/contracts/DateTime.sol";

interface IISA {
    function claim(uint256 tokensToMint) external returns (bool);

    function transfer(address recipient, uint256 amount)
        external
        returns (bool);
    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external
        returns (bool);
}

contract Controller is Ownable {
    using Address for address;
    using SafeMath for uint256;

    uint256 public lastClaim;

    IISA public isa;
    address public treasury;
    address public development;
    mapping(string => uint256) public claimedTokens;

    constructor(address _treasury,address _development) {
        treasury = _treasury;
        development = _development;
        lastClaim = block.timestamp;
    }

    function setISA(address _isa) external onlyOwner {
        require(_isa != address(0));
        require(_isa.isContract());
        isa = IISA(_isa);
    }

    function claim() external {
        uint256 Days = DateTime.diffDays(lastClaim, block.timestamp);
        require(Days > 0);
        uint256 tokensToMint = Days.mul(144000 ether);
        bool isMinted = isa.claim(tokensToMint);
        if (isMinted) {
            claimedTokens["treasury"] = claimedTokens["treasury"].add(
                tokensToMint.mul(1500).div(10000)
            );
            claimedTokens["development"] = claimedTokens["development"].add(
                tokensToMint.mul(2500).div(10000)
            );
            claimedTokens["bonds"] = claimedTokens["bonds"].add(
                tokensToMint.mul(6000).div(10000)
            );
        }
        lastClaim = block.timestamp;
    }

    function distribute() external {
        require(isa.transfer(treasury, claimedTokens["treasury"]));
        require(isa.transfer(development, claimedTokens["development"]));
        claimedTokens["treasury"] = 0;
        claimedTokens["development"] = 0;
    }

    function fundToBonds(address bond, uint256 amount) external {
        claimedTokens["bonds"] = claimedTokens["bonds"].sub(amount);
        require(isa.transfer(bond, amount));
    }

    // testing function for isa fee
    // remove it before deployment
    function testIsaFee(uint256 amount) external {
        isa.transferFrom(msg.sender, address(this), amount);
    }
}

