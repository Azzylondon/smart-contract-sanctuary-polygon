struct DataStorage {
   address diamond;
   address coin;

   EnumerableSetString.StringSet aAdmins;
   EnumerableSetString.StringSet aComunidades;

   mapping (address => string )   AddrToAdmin ;
   mapping (string => address )   AdminToAddr ;
  
}

 struct Gente {
        address addr;
        address parent;
        string nomalias;
        string name;
        string ipfs;
        string pubK;
        bool active ;
    }





 
 library LibArmonia {
    function LibArmoniaStorage() internal pure returns (DataStorage storage ds) {
        assembly {
            ds.slot := 0
        }
    }

    function addAdmin( string memory _nomalias ) internal  returns( bool ) {
       string memory aliasAdmin = normalizeAlias("arm.per",_nomalias);
       address addr = getAddr(aliasAdmin);
       LibArmoniaStorage().AddrToAdmin[addr] = aliasAdmin;
       LibArmoniaStorage().AdminToAddr[aliasAdmin]= addr;
       return  EnumerableSetString.add( LibArmoniaStorage().aAdmins, aliasAdmin);
    }

    function allAdminsAlias() internal view returns (string[] memory )   {
         return EnumerableSetString.values(LibArmoniaStorage().aAdmins);
    }

    function allAdminsAddr() internal view returns(address[] memory ) {
    address[] memory aAddrs = new address[](EnumerableSetString.length(LibArmoniaStorage().aAdmins));
      for( uint i = 1 ; i <= EnumerableSetString.length(LibArmoniaStorage().aAdmins) ; i++){
        aAddrs[i-1] =  LibArmoniaStorage().AdminToAddr[EnumerableSetString.at(LibArmoniaStorage().aAdmins,i)];
      }
      return aAddrs;
    }

    function allAdminsGente() internal returns(Gente[] memory ) {
        Gente[] memory aGente = new Gente[](EnumerableSetString.length(LibArmoniaStorage().aAdmins));
        for( uint i = 1 ; i <= EnumerableSetString.length(LibArmoniaStorage().aAdmins) ; i++){
            aGente[i-1] = getGenteByAlias(EnumerableSetString.at(LibArmoniaStorage().aAdmins,i));
        }
        return aGente;
    }


    function normalizeAlias(string memory ent , string memory value ) internal pure returns (string memory){
        require(bytes(value).length > 0 ," No se permite vacio");
        if( LibCai.compareStrings('arm.', LibCai.substring(value,0,4) ) ){
            return value;
        }
        else{
            return LibCai.concat(ent,'.',value);
        }
    }

   function getAddr(string memory _nomalias ) internal returns(address) {
    LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
    bytes4 functionSelector = bytes4(keccak256("PER_AliasToAddr(string)"));
    LibDiamond.FacetAddressAndPosition storage facet = ds. selectorToFacetAndPosition[functionSelector]; 
    address facetAddr = facet.facetAddress ;
    bytes memory myFunctionCall = abi.encodeWithSelector(functionSelector, _nomalias);
    (bool success, bytes memory result) = facetAddr.delegatecall(myFunctionCall);
    require(success, "Error en GetAddr");
    return abi.decode(result, (address));  
   }
   function getGenteByAlias(string memory _nomalias ) internal returns(Gente memory) {
    LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
    bytes4 functionSelector = bytes4(keccak256("PER_AliasToGente(string)"));
    LibDiamond.FacetAddressAndPosition storage facet = ds. selectorToFacetAndPosition[functionSelector]; 
    address facetAddr = facet.facetAddress ;
    bytes memory myFunctionCall = abi.encodeWithSelector(functionSelector, _nomalias);
    (bool success, bytes memory result) = facetAddr.delegatecall(myFunctionCall);
    require(success, "Error en GetAddr");
    return abi.decode(result, (Gente));  
   }

   function getCoin() internal returns(address) {
    LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
    bytes4 functionSelector = bytes4(keccak256("DAO_GetCoin()"));
    LibDiamond.FacetAddressAndPosition storage facet = ds. selectorToFacetAndPosition[functionSelector]; 
    address facetAddr = facet.facetAddress ;
    bytes memory myFunctionCall = abi.encodeWithSelector(functionSelector);
    (bool success, bytes memory result) = facetAddr.delegatecall(myFunctionCall);
    require(success, "Error en GetCoin");
    console.log( "getCoin", string(abi.encodePacked(result)));
    return abi.decode(result, (address));  
   }

   function getSilverByAddr(address _addr) internal  returns(uint256)  {
     DataStorage storage ds = LibArmoniaStorage();
     return  IERC20(ds.coin).balanceOf(_addr);
   }

   function getSilverByAlias(string memory _nomalias) internal  returns(uint256)  {
    Gente memory oGente =  getGenteByAlias(_nomalias);
    return getSilverByAddr(oGente.addr);
   }
   

}

contract Modifiers {
   DataStorage internal s;

    modifier onlyOwner() {
        LibDiamond.enforceIsContractOwner();
        _;
    }
    modifier onlyAdmins() {
      // string  memory nomalias  = s.AddrToAdmin[msg.sender];
      // require ( EnumerableSetString.contains(s.aAdmins, nomalias ) || msg.sender == LibDiamond.contractOwner()  ,"Solo los Admins pueden hacer esa opcion" );
      require ( LibCai.compareStrings('', s.AddrToAdmin[msg.sender])  !=  false  || msg.sender == LibDiamond.contractOwner()  ,"Solo los Admins pueden hacer esa opcion" );
      _;
    }
    
}

