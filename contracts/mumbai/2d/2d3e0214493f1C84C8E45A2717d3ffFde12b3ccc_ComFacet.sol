contract ComFacet is Modifiers {
  mapping (address => ArmComunidad)  public COM_ComByAddr;
  mapping (string => ArmComunidad )  public COM_ComByAlias;


  function COM_register( string memory _aliasComunidad , string memory _ipfs , string memory _sector , string memory _mecenasalias  ) public  onlyAdmins returns (address)  {
    _aliasComunidad      = LibArmonia.normalizeAlias('arm.com',_aliasComunidad);
    _sector              = LibArmonia.normalizeAlias('arm.sec',_sector);
    _mecenasalias        = LibArmonia.normalizeAlias('arm.per',_mecenasalias);
   
    COM_ComByAlias[_aliasComunidad] = new ArmComunidad( s.diamond , s.coin, _aliasComunidad, _ipfs, _sector , _mecenasalias , LibArmonia.getAddr(_mecenasalias)); 
    address myAddr = address(COM_ComByAlias[_aliasComunidad]);
    COM_ComByAddr[myAddr] = COM_ComByAlias[_aliasComunidad];
    EnumerableSetString.add( s.aComunidades, _aliasComunidad);
    return myAddr;
  }

  function COM_allComunidades() public view returns(string[] memory ) {
     return EnumerableSetString.values(s.aComunidades);
  }
  function COM_allComunidadesAddr() public view returns(address[] memory ) {
    address[] memory aAddrs = new address[](EnumerableSetString.length(s.aComunidades));
    for( uint i = 1 ; i <= EnumerableSetString.length(s.aComunidades) ; i++){
      aAddrs[i-1] = address(COM_ComByAlias[EnumerableSetString.at(s.aComunidades,i)]);
    }
    return aAddrs;
  }

  function COM_ByAlias(string memory _nomalias) public view returns( Comunidad memory ) {
     return COM_ComByAlias[_nomalias].detail();
  }
  
}

