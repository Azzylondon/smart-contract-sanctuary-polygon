    }

    modifier restricted(){
        require(msg.sender == SO); // changeTGaddress should only be called by the system operator
        _;
    }

    modifier onlyByTG(){
        require(msg.sender == TGaddress); // getAcceptedoffer() and delOffer() can only be called from TradingGame contract
        _;
    }

    // set the right TradingGame address
    function changeTGaddress(address _new) public restricted {
      TGaddress = _new;
    }

    function changeAPIurl(string memory _new) public restricted {
        url = _new;
    }

    address PQC = 0x4069a91aeb204D9C8644B81189Ba963df3a1C7BB; // public key of PQC token
    ERC20 public ERC20Interface = ERC20(PQC); // Make token functions available in this contract

    struct Offer {
        string AS; // Type of ancillary service that the SO needs
        uint256 ASAmount; // How much of the AS is needed
        uint256 startTime; // When does the SO need it
        uint256 endTime; // When does the SO doesn't need it anymore
        uint256 tokenAmount; // Remuneration for the client
        bool accept; // Is offer acccepted by client?
        bool finish; // Is the validation process finished?
    }
    mapping(address=>Offer) public acceptedOfferList; // Mapping accepted offer on the public key of SM
    mapping(address => address) public SMList; // Mapping of the PK of client on the public key of SM

    // function called by TradingGame.acceptOffer() so it can be mapped on the smart meter in the validation contract
    function getAcceptedoffer(address _client, address _ID_SM, string calldata _AS, uint256 _ASAmount, uint256 _startTime, uint256 _endTime, uint256 _tokenAmount, bool _accept) external onlyByTG {
        acceptedOfferList[_ID_SM] = Offer(_AS, _ASAmount, _startTime, _endTime, _tokenAmount, _accept, false);
        SMList[_ID_SM] = _client;
    }

    // function called by TradingGame.denyOffer() or TradingGame.deleteOffer()
    function delOffer(address _ID_SM, address _recipient, uint256 _tokenAmount) external onlyByTG {
        if (!acceptedOfferList[_ID_SM].finish) { // If the offer was not yet validated, tokens will be transfered to its rightfull owner
            ERC20Interface.transfer(_recipient, _tokenAmount);
            delete acceptedOfferList[_ID_SM];
        }
        else return;
    }

    // Function that requests the right api call (depending on the job) from the oracle contract
    function requestData(Job memory _job, address _ID_SM) public returns(bytes32) {
        Chainlink.Request memory request;
        if (_job.ID == "99b1b806a8f84b14a254230ccf094747") {
            // fulfillfinish is the function that should be executed when calling this job
            request = buildChainlinkRequest(_job.ID, address(this), this.fulfillfinish.selector);
        }
        else if (_job.ID == "bbf0badad29d49dc887504bacfbb905b") {
            // fulfillproof is the function that should be executed when calling this job
            request = buildChainlinkRequest(_job.ID, address(this), this.fulfillproof.selector);
        }
        // url of the api
        request.add("get", string(abi.encodePacked(url, _job.name))); // Set the URL to perform the GET request on
        bytes32 rid = sendChainlinkRequestTo(oracle, request, _job.fee);
        SM_requests[rid] = _ID_SM;
        return rid;
    }

    // fulfill functitons will update the proof and finish variables of the accepted offer
    function fulfillfinish(bytes32 _requestId, bool _finish) public recordChainlinkFulfillment(_requestId) {
        address ID_SM = SM_requests[_requestId];
        if (_finish) { // if validation is finished, we can also call the measured amount of the provided ancillary service
            requestData(jobs["SM1_Enow"], ID_SM);
        } else return;
    }

    function fulfillproof(bytes32 _requestId, uint256 _proof) public recordChainlinkFulfillment(_requestId) {
        address ID_SM = SM_requests[_requestId];
        require(!acceptedOfferList[ID_SM].finish, "Offer is already validated");
        uint256 _ref = acceptedOfferList[ID_SM].ASAmount;
        uint256 tokenAmount = acceptedOfferList[ID_SM].tokenAmount;
        uint256 clientAmount = (_proof * tokenAmount) / _ref;
        if (clientAmount >= tokenAmount) {
            ERC20Interface.transfer(SMList[ID_SM], tokenAmount);
        }
        else {
            if (clientAmount != 0) {
                ERC20Interface.transfer(SMList[ID_SM], clientAmount);
            } // otherwise error because requirement in transfer tokens that amount > 0 //only if proof == 0
            ERC20Interface.transfer(SO, tokenAmount-clientAmount);
        }
        acceptedOfferList[ID_SM].finish = true;

    }

    // Function to validate the offer by the smart meter
    function validate() public {
        LinkTokenInterface linkToken = LinkTokenInterface(chainlinkTokenAddress());
        require(linkToken.balanceOf(address(this)) >= jobs["SM1_idle"].fee + jobs["SM1_Enow"].fee, "LINK balance not sufficient");  // to call requestData, you need some LINK
        require(acceptedOfferList[msg.sender].accept, "No offer was detected"); // offer needs to be accepted first (+ you have to be a SM!)
        require(!acceptedOfferList[msg.sender].finish, "Offer is already validated");
        requestData(jobs["SM1_idle"], msg.sender);
    }

    function withdrawLink() external {
        LinkTokenInterface linkToken = LinkTokenInterface(chainlinkTokenAddress());
        require(linkToken.transfer(msg.sender, linkToken.balanceOf(address(this))), "Unable to transfer");
    }

}