        // contractAddress = marketplaceAddress;
    }

    function getAllRegisters() public view returns (string[] memory) {
        string[] memory allNames = new string[](_tokenIds.current());
        for (uint256 i = 0; i <_totalHolders; i++) {
            allNames[i] = names[i+1];
        }

        return allNames;
    }

    function createToken(string calldata _bandName) public returns (uint) {
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();

        string memory finalSvg = string(
            abi.encodePacked(svgPartOne, _bandName, svgPartTwo)
        );

        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "',
                        _bandName,
                        '", "description": "Distro Fank IP is a token generated and stored on chain to check and assess music rights, IP, among others assets ",',
                        '"image": "data:image/svg+xml;base64,',
                        Base64.encode(bytes(finalSvg)),
                        '"}'
                    )
                )
            )
        );

        string memory finalTokenUri = string(
            abi.encodePacked("data:application/json;base64,", json)
        );

        _mint(msg.sender, newItemId);
        _setTokenURI(newItemId, finalTokenUri);
        setApprovalForAll(contractAddress, true);
        names[newItemId] = _bandName;
        _totalHolders++;
        return newItemId;
    }

    function setRecord(string calldata record, string calldata _bandName)
        public
    {
        require(balanceOf(msg.sender) > 0, "You don not own a minted NFT");
        records[_bandName] = record;
    }

    function getRecord(string calldata _bandName)
        public
        view
        returns (string memory)
    {
        return records[_bandName];
    }
}

