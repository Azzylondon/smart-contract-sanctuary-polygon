// SPDX-License-Identifier: MIT
pragma solidity ^0.8.10;

import "Strings.sol";
import "SafeMath.sol";
import "ERC165.sol";
import "ERC165Checker.sol";
import "IERC165.sol";

import "Initializable.sol";
import "ViciAccess.sol";
import "Mintable.sol";
import "IDropManager.sol";
import "IUriManager.sol";

contract DropManager is IDropManager, IUriManager, ViciAccess, Initializable {
    using Strings for string;
    using SafeMath for uint256;
    using ERC165Checker for address;

    // Creator can create a new token type and mint an initial supply.
    bytes32 public constant URI_MANAGER = "URI Manager";

    // Creator can create a new token type and mint an initial supply.
    bytes32 public constant DROP_MANAGER = "Drop Manager";

    // Number of tokens minted so far in the current drop
    uint256 public dropMintCount;

    // Which drop is a token from
    mapping(uint256 => uint256) internal dropIndexForToken;
    string internal constant NO_ACTIVE_DROP = "No active drop";

    string public defaultBaseURI;

    Drop[] public completedDrops;
    Drop public currentDrop = Drop(NO_ACTIVE_DROP, 0, 0, "");

    mapping(uint256 => string) internal customURIs;

    Mintable public tokenFactory;

    /// @inheritdoc	ERC165
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ViciAccess, IERC165)
        returns (bool)
    {
        return
            interfaceId == type(IUriManager).interfaceId ||
            interfaceId == type(IDropManager).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    /**
     * @notice initializes the token factory.
     * @param _tokenFactory address of the token factory contract.
     *
     * Requirements:
     *
     * - Caller MUST be the contract owner
     * - `_tokenFactory` MUST  be a contract that supports the Mintable interface
     * - This function MUST NOT be called more than once.
     */
    function setTokenFactory(address _tokenFactory)
        public
        onlyOwner
        initializer
    {
        require(_tokenFactory.supportsInterface(type(Mintable).interfaceId));
        tokenFactory = Mintable(_tokenFactory);
        _setupRole(URI_MANAGER, address(_tokenFactory));
    }

    /**
     * @notice Returns the number of completed drops.
     */
    function completedDropCount() public view returns (uint256) {
        return completedDrops.length;
    }

    function getCompletedDrop(uint256 _index)
        external
        view
        returns (Drop memory)
    {
        return completedDrops[_index - 1];
    }

    function getCurrentDrop() external view returns (Drop memory) {
        return currentDrop;
    }

    function baseURI() external view returns (string memory) {
        return defaultBaseURI;
    }

    function baseURI(uint256 _tokenId) external view returns (string memory) {
        uint256 dropIndex = dropIndexForToken[_tokenId];
        return baseURIForDropIndex(dropIndex);
    }

    function getDropIndexForToken(uint256 _tokenId) public view returns (uint256) {
        return dropIndexForToken[_tokenId];
    }

    function setBaseURI(string memory _baseURI)
        public
        onlyOwnerOrRole(URI_MANAGER)
    {
        defaultBaseURI = _baseURI;
    }

    function setBaseURI(uint256 _dropIndex, string memory _baseURI)
        public
        onlyOwnerOrRole(URI_MANAGER)
    {
        require(_dropIndex > 0, "_dropIndex is 1 based.");
        _dropIndex--;
        require(_dropIndex <= completedDrops.length);
        if (_dropIndex == completedDrops.length) {
            require(
                keccak256(bytes(currentDrop.dropName)) !=
                    keccak256(bytes(NO_ACTIVE_DROP))
            );
            currentDrop.baseURI = _baseURI;
        } else {
            completedDrops[_dropIndex].baseURI = _baseURI;
        }
    }

    function setCustomURI(uint256 tokenId, string memory newURI)
        public
        onlyOwnerOrRole(URI_MANAGER)
    {
        customURIs[tokenId] = newURI;
        emit URI(newURI, tokenId);
    }

    function getTokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        // We have to convert string to bytes to check for existence
        bytes memory customUriBytes = bytes(customURIs[_tokenId]);
        if (customUriBytes.length > 0) {
            return customURIs[_tokenId];
        }

        uint256 dropIndex = dropIndexForToken[_tokenId];
        string memory base = baseURIForDropIndex(dropIndex);

        return string(abi.encodePacked(base, Strings.toString(_tokenId)));
    }

    function baseURIForDropIndex(uint256 dropIndex) public view returns (string memory) {
        if (dropIndex == 0) {
            return defaultBaseURI;
        } else if (dropIndex == completedDrops.length+1) {
            return currentDrop.baseURI;
        } else {
            return completedDrops[dropIndex-1].baseURI;
        }

    }

    /**
     * @notice Ends the current drop immediately. It's not necessary to call this.
     * The current drop ends automatically once the last token is sold.
     *
     * Requirements:
     *
     * - Calling user MUST be owner or have the creator role.
     * - There MUST be an active drop.
     */
    function deactivateDrop() public onlyOwnerOrRole(DROP_MANAGER) {
        require(
            keccak256(bytes(currentDrop.dropName)) !=
                keccak256(bytes(NO_ACTIVE_DROP))
        );
        _endDrop();
    }

    /**
     * @notice Starts a new drop. Ends the current drop, if any.
     * @param _dropName The name of the new drop
     * @param _dropStartTime The unix timestamp of when the drop is active
     * @param _dropSize The number of NFTs in this drop
     * @param _baseURI The base URI for the tokens in this drop
     *
     * Requirements:
     *
     * - Calling user MUST be owner or have the creator role.
     * - _dropSize + totalSuppy() MUST NOT be greater than MAX_SUPPLY
     */
    function startNewDrop(
        string memory _dropName,
        uint32 _dropStartTime,
        uint32 _dropSize,
        string memory _baseURI
    ) public onlyOwnerOrRole(DROP_MANAGER) {
        require(_dropSize <= tokenFactory.maxSupply());

        _endDrop();
        currentDrop = Drop(_dropName, _dropStartTime, _dropSize, _baseURI);
        emit DropAnnounced(currentDrop);
    }

    /**
     * @notice Returns the number of tokens that may still be minted in the current drop.
     *
     * Requirements:
     *
     * - This function MAY be called when there is no active drop. The answer will be 0.
     */
    function amountRemainingInDrop() public view returns (uint256) {
        if (currentDrop.dropSize <= dropMintCount) {
            return 0;
        }
        return currentDrop.dropSize - dropMintCount;
    }

    function _endDrop() internal {
        if (
            keccak256(bytes(currentDrop.dropName)) !=
            keccak256(bytes(NO_ACTIVE_DROP))
        ) {
            completedDrops.push(currentDrop);
            dropMintCount = 0;
            currentDrop = Drop(NO_ACTIVE_DROP, 0, 0, "");
            emit DropEnded(currentDrop);
        }
    }

    function mint(address _toAddress, uint256 _tokenId)
        public
        onlyOwnerOrRole(DROP_MANAGER)
    {
        _pre_mint(_tokenId);
        require(amountRemainingInDrop() > 0, "sold out");

        tokenFactory.mint(_toAddress, _tokenId);
    }

    function batchMint(
        address[] memory _toAddresses,
        uint256[] memory _tokenIds
    ) public onlyOwnerOrRole(DROP_MANAGER) {
        require(_toAddresses.length == _tokenIds.length);
        require(amountRemainingInDrop() >= _toAddresses.length);

        for (uint256 i = 0; i < _toAddresses.length; i++) {
            _pre_mint(_tokenIds[i]);
        }

        tokenFactory.batchMint(_toAddresses, _tokenIds);
    }

    function _pre_mint(uint256 _tokenId) internal {
        require(block.timestamp >= currentDrop.dropStartTime, "early");

        dropIndexForToken[_tokenId] = completedDrops.length+1;
        dropMintCount++;
        if (dropMintCount >= currentDrop.dropSize) {
            _endDrop();
        }
    }
}

