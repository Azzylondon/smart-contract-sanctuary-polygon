contract Inventory is ERC721TokenReceiver, UUPSUpgradeable {
    address public owner;

    event ItemAdded(address owner, address itemAddress, uint256 itemId, uint256 number, bytes data);
    event ItemReturned(address owner, address itemAddress, uint256 itemId, uint256 number, bytes data);

    struct Item {
        address itemAddress;
        uint256 tokenId;
        uint256 itemId;
        uint256 number;
        bytes data;
    }

    Item[] internal ownedItems;

    mapping(uint256 => uint256) internal _tokenIdx;

    modifier onlyOwner() {
        require(owner == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    function initialize(address _owner) external initializer {
        owner = _owner;
        _changeAdmin(msg.sender);
    }

    function itemCount() external view returns (uint256) {
        return ownedItems.length;
    }

    // Triggered when ERC721 item is transfered to the address by "safeTransferFrom"
    function onERC721Received(
        address, /* _operator */
        address _from,
        uint256 _tokenId,
        bytes calldata _data
    ) external returns (bytes4) {
        address itemAddress = msg.sender;

        require(_from == owner, "Collection: Item can be only sent from the owner");

        uint256 itemId = IMaplestoryEquip(itemAddress).getTokenItemId(_tokenId);
        uint256 number = IMaplestoryEquip(itemAddress).getTokenNumber(_tokenId);

        Item memory item = Item(itemAddress, _tokenId, itemId, number, _data);
        ownedItems.push(item);

        _tokenIdx[_tokenId] = ownedItems.length; // itemIdx is 1-based (starts from 1)

        emit ItemAdded(owner, itemAddress, itemId, number, _data);

        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }

    function returnItem(uint256 tokenId) external onlyOwner {
        uint256 idx = _tokenIdx[tokenId];
        Item memory item = ownedItems[idx];

        // OwnedItems의 마지막 원소를 Idx번째로 이동
        ownedItems[idx] = ownedItems[ownedItems.length - 1];
        // 마지막 원소였던 Item의 인덱스 조정
        _tokenIdx[ownedItems[idx].tokenId] = idx;
        // OwnedItems의 길이 -= 1
        ownedItems.pop();

        // 아이템 주인에게 다시 전송
        IERC721(item.itemAddress).safeTransferFrom(address(this), owner, item.tokenId);

        emit ItemReturned(owner, item.itemAddress, item.itemId, item.number, item.data);
    }

    function getItemList() external view returns (Item[] memory) {
        return ownedItems;
    }

    // Inherited from UUPSUpgradeable contract
    function _authorizeUpgrade(address newImplementation) internal view override {
        require(msg.sender == _getAdmin(), "Only owner can upgrade.");
        require(_getImplementation() != newImplementation, "Can't upgrade to same implementation");
    }
}

