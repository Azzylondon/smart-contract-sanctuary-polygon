pragma solidity ^0.8.13;

import {Strings} from "@openzeppelin/contracts/utils/Strings.sol";
import {Context} from "@openzeppelin/contracts/utils/Context.sol";
import {IAdminAndExecutors} from "./IAdminAndExecutors.sol";

contract AdminAndExecutors is IAdminAndExecutors, Context {
    address private _admin;
    mapping(address => bool) private _isExecutor;

    modifier onlyAdmin() {
        _checkAdmin(_msgSender());
        _;
    }

    modifier onlyExecutor() {
        _checkExecutor(_msgSender());
        _;
    }

    /*
    TODO: https://gitlab.nexon.com/projecta/bunit/smartcontract/maple-contract/-/merge_requests/55#note_425872
     애초에 admin을 부여할 때 executor도 같이 부여되게 만들면 되지 않나
     논의하기
    */
    modifier onlyAtLeastExecutor() {
        _checkAtLeastExecutor(_msgSender());
        _;
    }

    constructor() {
        _transferAdmin(_msgSender());
    }

    /*
     * TODO: changeAdmin으로 이름 바꾸기
     * 나머지 renounceAdmin도 자연스러운 이름으로 변경
     * grantExecutor도 자연스러운 이름으로 (add/remove executor?)
     * 등등 이름 고민. 아니면 동사 대신 admin, executor를 adminRole, executorRole로?
     */
    function transferAdmin(address newAdmin) public virtual onlyAdmin {
        require(newAdmin != address(0), "AdminAndExecutors: new admin is the zero address");
        _transferAdmin(newAdmin);
    }

    function renounceAdmin() public virtual onlyAdmin {
        _transferAdmin(address(0));
    }

    function grantExecutor(address executor) public virtual onlyAdmin {
        _setExecutor(executor, true);
    }

    function revokeExecutor(address executor) public virtual onlyAdmin {
        _setExecutor(executor, false);
    }

    function admin() public view virtual returns (address) {
        return _admin;
    }

    function isExecutor(address account) public view virtual returns (bool) {
        return _isExecutor[account];
    }

    function isAtLeastExecutor(address account) public view virtual returns (bool) {
        return _isExecutor[account] || account == _admin;
    }

    function _transferAdmin(address newAdmin) internal virtual {
        address previousAdmin = _admin;
        _admin = newAdmin;
        emit AdminTransferred(previousAdmin, newAdmin);
    }

    function _setExecutor(address executor, bool grant) internal virtual {
        if (_isExecutor[executor] != grant) {
            _isExecutor[executor] = grant;
            if (grant) {
                emit ExecutorGranted(executor);
            } else {
                emit ExecutorRevoked(executor);
            }
        }
    }

    function _checkAdmin(address account) internal view virtual {
        if (account != _admin) {
            revert(
                string(
                    abi.encodePacked(
                        "AdminAndExecutors: account ",
                        Strings.toHexString(uint160(account), 20),
                        " is not the admin"
                    )
                )
            );
        }
    }

    function _checkExecutor(address account) internal view virtual {
        if (!_isExecutor[account]) {
            revert(
                string(
                    abi.encodePacked(
                        "AdminAndExecutors: account ",
                        Strings.toHexString(uint160(account), 20),
                        " is not an executor"
                    )
                )
            );
        }
    }

    function _checkAtLeastExecutor(address account) internal view virtual {
        if (!_isExecutor[account] && account != _admin) {
            revert(
                string(
                    abi.encodePacked(
                        "AdminAndExecutors: account ",
                        Strings.toHexString(uint160(account), 20),
                        " is neither the admin nor an executor"
                    )
                )
            );
        }
    }
}

