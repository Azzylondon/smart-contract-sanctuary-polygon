import {IERC1271} from "@openzeppelin/contracts/interfaces/IERC1271.sol";
import {ECDSA} from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import {EIP712} from "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";

abstract contract ERC1271SmartWallet is IERC1271, EIP712 {
    struct ERC1271SmartWalletStruct {
        bytes32 hash;
    }

    bytes32 private constant TYPE_HASH = keccak256("ERC1271SmartWallet(bytes32 hash)");

    // solhint-disable-next-line
    constructor() EIP712("ERC1271SmartWallet", "1") {}

    function isValidSignature(bytes32 hash, bytes memory signature) external view returns (bytes4 magicValue) {
        (address recovered, ECDSA.RecoverError err) = ECDSA.tryRecover(_wrapHash(hash), signature);
        if (err == ECDSA.RecoverError.NoError && _canSmartWalletSign(recovered))
            return IERC1271.isValidSignature.selector;
        return 0xffffffff;
    }

    function _canSmartWalletSign(address account) internal view virtual returns (bool);

    function _wrapHash(bytes32 originalHash) private view returns (bytes32 hashToSign) {
        return _hashTypedDataV4(keccak256(abi.encode(TYPE_HASH, ERC1271SmartWalletStruct({hash: originalHash}))));
    }
}

