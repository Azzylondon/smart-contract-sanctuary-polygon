pragma solidity ^0.8.16;

import {Address} from "@openzeppelin/contracts/utils/Address.sol";
import {ISmartWalletExec} from "../../interfaces/ISmartWalletExec.sol";

/*
TODO: https://gitlab.nexon.com/projecta/bunit/smartcontract/maple-contract/-/merge_requests/55#note_425201
 exec 컨트랙트 분리하지 않고 합치는 건에 대하여 한번만 생각해보소
 에 관하여 논의하기
*/
abstract contract SmartWalletExec is ISmartWalletExec {
    modifier whenSmartWalletExecutable() {
        _checkSmartWalletExecutable();
        _;
    }

    // solhint-disable-next-line no-empty-blocks
    receive() external payable {}

    function exec(
        address to,
        bytes memory data,
        uint256 value
    ) external whenSmartWalletExecutable {
        _exec(to, data, value);
    }

    function batchExec(Call[] calldata calls) external whenSmartWalletExecutable {
        for (uint256 i = 0; i < calls.length; i++) {
            Call calldata call = calls[i];
            _exec(call.to, call.data, call.value);
        }
    }

    function _checkSmartWalletExecutable() internal view virtual;

    function _exec(
        address to,
        bytes memory data,
        uint256 value
    ) private {
        // solhint-disable-next-line avoid-low-level-calls
        (bool success, bytes memory returnData) = to.call{value: value}(data);
        Address.verifyCallResult(success, returnData, "SmartWalletExec: low-level call failed");
    }
}

