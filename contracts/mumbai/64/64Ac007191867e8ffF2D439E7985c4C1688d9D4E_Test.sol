// SPDX-License-Identifier: MIT
pragma solidity ^0.8.13;

import "./IApproveController.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract ApproveController is IApproveController, Ownable {
    struct Approval {
        bool approved;
        uint256 approveDate;
    }

    mapping(address => Approval) private _approvals;

    function setApproveByOwner(address account, bool newApproved) public override onlyOwner {
        _approvals[account] = Approval(newApproved, block.timestamp);

        emit SetApprove(account, newApproved);
    }

    function setApprove(bool newApproved) public override {
        _approvals[_msgSender()] = Approval(newApproved, block.timestamp);

        emit SetApprove(_msgSender(), newApproved);
    }

    function isApproved(address account) public view override returns (bool) {
        return _approvals[account].approved;
    }

    function getApproveDate(address account) public view override returns (uint256) {
        return _approvals[account].approveDate;
    }
}

