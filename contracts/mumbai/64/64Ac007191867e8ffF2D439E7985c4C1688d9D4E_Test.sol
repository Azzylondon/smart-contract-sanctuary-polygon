        bytes memory data
    ) public onlyCreator {
        _mint(to, id, amount, data);

        totalSupply[id] += amount;
        currentSupply[id] += amount;
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyCreator {
        _mintBatch(to, ids, amounts, data);

        for (uint256 i = 0; i < ids.length; i++) {
            totalSupply[ids[i]] += amounts[i];
            currentSupply[ids[i]] += amounts[i];
        }
    }

    function burn(
        address from,
        uint256 id,
        uint256 amount
    ) public {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()) || _msgSender() == owner(),
            "caller is not owner nor approved"
        );

        _burn(from, id, amount);

        unchecked {
            currentSupply[id] -= amount;
        }
    }

    function burnBatch(
        address from,
        uint256[] memory ids,
        uint256[] memory amounts
    ) public {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()) || _msgSender() == owner(),
            "caller is not owner nor approved"
        );

        _burnBatch(from, ids, amounts);

        for (uint256 i = 0; i < ids.length; i++) {
            unchecked {
                currentSupply[ids[i]] -= amounts[i];
            }
        }
    }

    function uri(uint256 id) public view virtual override returns (string memory) {
        require(totalSupply[id] > 0, "token didn't minted");

        return
            bytes(tokenURI[id]).length > 0
                ? tokenURI[id]
                : string(abi.encodePacked(super.uri(id), id.toString(), ".json"));
    }

    function isApprovedForAll(address account, address operator) public view override returns (bool) {
