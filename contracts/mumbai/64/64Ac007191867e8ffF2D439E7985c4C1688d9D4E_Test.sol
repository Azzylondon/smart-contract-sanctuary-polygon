        bytes32 salt,
        bytes memory data,
        bool forceCall
    ) external onlyOwner returns (address contractAddress) {
        contractAddress = Create2.deploy(0, salt, type(FactoryBeaconProxy).creationCode);

        if (data.length > 0 || forceCall) {
            Address.functionCall(contractAddress, data);
        }
    }

    function getAddress(bytes32 salt) public view returns (address) {
        return Create2.computeAddress(salt, INIT_CODE_HASH);
    }
}

