abstract contract Claimable is IClaimable, TokenAddressData, Ownable {
    uint256 private _claimPeriod;
    uint256 private _earlyClaimFeeRatio;

    mapping(address => uint256) private _lastClaimTime;

    constructor() {
        _setClaimPeriod(1);
        _setEarlyClaimFeeRatio(1);
    }

    function claim(address to, uint256 amount) public override onlyOwner {
        _claim(to, amount);

        emit Claim(to, amount);
    }

    function batchClaim(address[] memory to, uint256[] memory amounts) public override onlyOwner {
        require(to.length == amounts.length, "Claimable: Can't Claim Batch");

        for (uint256 i = 0; i < to.length; i++) {
            _claim(to[i], amounts[i]);
        }

        emit BatchClaim(to, amounts);
    }

    function earlyClaim(address to, uint256 amount) public override onlyOwner {
        _earlyClaim(to, amount);

        emit EarlyClaim(to, amount, _earlyClaimFeeRatio);
    }

    function batchEarlyClaim(address[] memory to, uint256[] memory amounts) public override onlyOwner {
        require(to.length == amounts.length, "Claimable: params length is not matched");

        for (uint256 i = 0; i < to.length; i++) {
            _earlyClaim(to[i], amounts[i]);
        }

        emit BatchEarlyClaim(to, amounts, _earlyClaimFeeRatio);
    }

    function setClaimPeriod(uint256 newClaimPeriod) public onlyOwner {
        _setClaimPeriod(newClaimPeriod);
    }

    function setEarlyClaimFeeRatio(uint256 newEarlyClaimFeeRatio) public onlyOwner {
        _setEarlyClaimFeeRatio(newEarlyClaimFeeRatio);
    }

    // TODO : 실 스펙에 적용할지 여부 결정
    function setLastClaimTime(address account, uint256 newLastClaimTime) public onlyOwner {
        _lastClaimTime[account] = newLastClaimTime;
    }

    function claimPeriod() public view returns (uint256) {
        return _claimPeriod;
    }

    function earlyClaimFeeRatio() public view returns (uint256) {
        return _earlyClaimFeeRatio;
    }

    function lastClaimTime(address account) public view returns (uint256) {
        return _lastClaimTime[account];
    }

    function canClaim(address account) public view returns (bool) {
        return block.timestamp - _lastClaimTime[account] >= _claimPeriod;
    }

    function _setClaimPeriod(uint256 newClaimPeriod) internal virtual {
        require(newClaimPeriod > 0, "Vault: claimPeriod must be at least 1");

        emit ClaimPeriod(_claimPeriod, newClaimPeriod);

        _claimPeriod = newClaimPeriod;
    }

    function _setEarlyClaimFeeRatio(uint256 newEarlyClaimFeeRatio) internal virtual {
        emit EarlyClaimFeeRatio(_earlyClaimFeeRatio, newEarlyClaimFeeRatio);

        _earlyClaimFeeRatio = newEarlyClaimFeeRatio;
    }

    function _claim(address to, uint256 amount) internal virtual {
        require(canClaim(to), "Vault: Can't claim now");

        _lastClaimTime[to] = block.timestamp;

        IERC20(tokenAddress).transfer(to, amount);
    }

    function _earlyClaim(address to, uint256 amount) internal virtual {
        require(amount / 10**18 > 0, "Vault: amount have to over 1 token");

        uint256 tax = ((amount / 100) * _earlyClaimFeeRatio);

        IERC20(tokenAddress).transfer(to, amount - tax);
        IERC20(tokenAddress).transfer(address(this), tax);
        // TODO : 따로 tax를 저장하는 EOA 혹은 CA를 생성하여 관리할 것인지 결정
    }
}

