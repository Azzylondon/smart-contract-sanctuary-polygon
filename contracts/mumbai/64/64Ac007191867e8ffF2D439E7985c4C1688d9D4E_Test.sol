import "@openzeppelin/contracts/access/Ownable.sol";
import "../utils/TokenAddressData.sol";
import "./IDepositable.sol";

abstract contract Depositable is IDepositable, TokenAddressData, Ownable {
    uint256 private _depositTime;
    uint256 private _depositAmount;
    uint96 private _depositPeriod;

    constructor() {
        _setDepositTime(block.timestamp);
        _setDepositAmount(10000000 * (10**18));
        _setDepositPeriod(1 days);
    }

    function deposit() public override onlyOwner {
        require(depositableCount() > 0, "Depositable: can't deposit now");

        _deposit();
    }

    function setDepositTime(uint256 newDepositTime) public override onlyOwner {
        if (depositableCount() > 0) _deposit();
        _setDepositTime(newDepositTime);
    }

    function setDepositAmount(uint256 newDepositAmount) public override onlyOwner {
        if (depositableCount() > 0) _deposit();
        _setDepositAmount(newDepositAmount);
    }

    function setDepositPeriod(uint96 newDepositPeriod) public override onlyOwner {
        require(newDepositPeriod > 0, "Depositable: depositPeriod must be at least 1");

        if (depositableCount() > 0) _deposit();
        _setDepositPeriod(newDepositPeriod);
    }

    function depositableCount() public view override returns (uint256) {
        return (block.timestamp - _depositTime) / _depositPeriod;
    }

    function depositTime() public view override returns (uint256) {
        return _depositTime;
    }

    function depositAmount() public view override returns (uint256) {
        return _depositAmount;
    }

    function depositPeriod() public view override returns (uint96) {
        return _depositPeriod;
    }

    function _setDepositTime(uint256 newDepositTime) internal {
        emit DepositTime(_depositTime, newDepositTime);

        _depositTime = newDepositTime;
    }

    function _setDepositAmount(uint256 newDepositAmount) internal {
        emit DepositAmount(_depositAmount, newDepositAmount);

        _depositAmount = newDepositAmount;
    }

    function _setDepositPeriod(uint96 newDepositPeriod) internal {
        emit DepositPeriod(_depositPeriod, newDepositPeriod);

        _depositPeriod = newDepositPeriod;
    }

    function _deposit() internal {
        uint256 _depositableCount = depositableCount();
        uint256 currentTime = block.timestamp;
        require(_depositableCount > 0, "Depositable: can't deposit now");

        // solhint-disable-next-line avoid-low-level-calls
        (bool success, ) = tokenAddress.call(
            abi.encodeWithSignature("mint(address,uint256)", address(this), _depositAmount * _depositableCount)
        );

        if (!success) revert("Depositable: minting failed");
        _setDepositTime(_depositTime + _depositPeriod * _depositableCount);

        emit Deposit(currentTime, _depositAmount * _depositableCount);
    }
}

