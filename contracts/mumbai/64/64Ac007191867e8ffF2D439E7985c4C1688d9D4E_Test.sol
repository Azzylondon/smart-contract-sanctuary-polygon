import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "../ItemOwnable/ItemOwnable.sol";
import "../ApproveController/IApproveController.sol";

contract Packaging is ERC721, ERC721Burnable, ItemOwnable, ERC1155Holder {
    struct PackageData {
        address account;
        address[] contracts;
        uint256[][] values;
    }

    uint256 public constant ERC20TYPE = 1;
    uint256 public constant ERC721TYPE = 2;
    uint256 public constant ERC1155TYPE = 3;

    uint256 private _tokenIndex;

    mapping(address => uint256) public _contracts;
    mapping(uint256 => PackageData) private _packageData;

    // solhint-disable-next-line no-empty-blocks
    constructor() ERC721("Packaging", "PACK") {}

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC1155Receiver)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setERC20(address newERC20) public onlyOwner {
        require(_contracts[newERC20] == 0, "ERC20 already exists");
        _setContract(newERC20, ERC20TYPE);
    }

    function setERC721(address newERC721) public onlyOwner {
        require(_contracts[newERC721] == 0, "ERC721 already exists");
        _setContract(newERC721, ERC721TYPE);
    }

    function setERC1155(address newERC1155) public onlyOwner {
        require(_contracts[newERC1155] == 0, "ERC1155 already exists");
        _setContract(newERC1155, ERC1155TYPE);
    }

    function pack(
        address account,
        address[] memory contracts,
        uint256[][] memory values
    ) public onlyOwner {
        require(contracts.length == values.length, "ERR");
        for (uint256 i = 0; i < contracts.length; i++) {
            uint256 contractType = _contracts[contracts[i]];
            require(contractType > 0, "Invalid contract type");

            if (contractType == ERC20TYPE) {
                IERC20(contracts[i]).transferFrom(account, address(this), values[i][0]);
            } else if (contractType == ERC721TYPE) {
                IERC721(contracts[i]).transferFrom(account, address(this), values[i][0]);
            } else {
                IERC1155(contracts[i]).safeTransferFrom(account, address(this), values[i][0], values[i][1], ""); // [5, 1], [1, 5] 착각했을 때
            }
        }

        _packageData[_tokenIndex] = PackageData(account, contracts, values);
        _safeMint(account, _tokenIndex++);
    }

    function unpack(address account, uint256 tokenId) public onlyOwner {
        PackageData storage packagedData = _packageData[tokenId];

        for (uint256 i = 0; i < packagedData.contracts.length; i++) {
            uint256 contractType = _contracts[packagedData.contracts[i]];
            if (contractType == ERC20TYPE) {
                IERC20(packagedData.contracts[i]).transfer(account, packagedData.values[i][0]);
            } else if (contractType == ERC721TYPE) {
                IERC721(packagedData.contracts[i]).transferFrom(address(this), account, packagedData.values[i][0]);
            } else {
                IERC1155(packagedData.contracts[i]).safeTransferFrom(
                    address(this),
                    account,
                    packagedData.values[i][0],
                    packagedData.values[i][1],
                    ""
                ); // [5, 1], [1, 5] 착각했을 때
            }
        }

        _burn(tokenId);
    }

    function isApprovedForAll(address owner, address operator) public view override returns (bool) {
        return
            creator[_msgSender()]
                ? IApproveController(controllerAddress).isApproved(owner)
                : super.isApprovedForAll(owner, operator);
    }

    function _setContract(address account, uint256 contractType) private {
        _contracts[account] = contractType;
    }
}

