pragma solidity ^0.8.16;

import {FactoryBeacon} from "../../utils/FactoryBeacon.sol";
import {ISmartWallet} from "../interfaces/ISmartWallet.sol";
import {ISmartWalletFactoryFrontend} from "../interfaces/ISmartWalletFactoryFrontend.sol";

abstract contract SmartWalletFactoryCore is ISmartWalletFactoryFrontend {
    address public immutable backend;

    event SmartWalletCreated(ISmartWallet indexed wallet, uint256 indexed id);

    modifier whenExecutable() {
        _checkExecutable(msg.sender);
        _;
    }

    modifier onlyAdmin() {
        _checkAdmin(msg.sender);
        _;
    }

    constructor(address backend_) {
        backend = backend_;
    }

    function createWallet(uint256 id) external whenExecutable returns (ISmartWallet wallet) {
        bytes32 salt = _salt(id);
        wallet = ISmartWallet(_backend().create(salt, "", false));
        emit SmartWalletCreated(wallet, id);
    }

    function migrateTo(address newFactory) external onlyAdmin {
        _backend().transferOwnership(newFactory);
    }

    function upgradeTo(address newImplementation) external onlyAdmin {
        _backend().upgradeTo(newImplementation);
    }

    function getWallet(uint256 id) external view returns (ISmartWallet) {
        return ISmartWallet(_backend().getAddress(_salt(id)));
    }

    function _backend() internal view returns (FactoryBeacon) {
        return FactoryBeacon(backend);
    }

    function _checkExecutable(address account) internal view virtual;

    function _checkAdmin(address account) internal view virtual;

    function _salt(uint256 id) private pure returns (bytes32) {
        return bytes32(id);
    }
}

