contract FactoryBeacon is UpgradeableBeacon {
    bytes32 public constant INIT_CODE_HASH = keccak256(type(FactoryBeaconProxy).creationCode);

    constructor(address implementation_) UpgradeableBeacon(implementation_) {
        emit Upgraded(implementation_); // manually emit event as the constructor does not automatically
