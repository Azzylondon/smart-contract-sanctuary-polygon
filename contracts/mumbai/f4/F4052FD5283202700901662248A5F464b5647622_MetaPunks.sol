    ) public onlyOwner {
        rewardList[_owner].push(_id);
        customUri[_id] = _uri;
    } 

    // 重置质押奖励
    function resetReward(
        address _owner,
        uint256[] memory _ids
    ) public onlyOwner {
        rewardList[_owner] = _ids;
    }

    // 获取奖励列表
    function rewardOfOwner(
        address _owner
    ) public view returns (uint256[] memory) {
        return rewardList[_owner];
    }    

    // 领取质押所获的奖励
    function claim() public {
        address _own = _msgSender();
        uint256[] memory _ids = rewardList[_own];
        require(_ids.length > 0, "Nothing");
        for (uint256 i = 0; i < _ids.length; i++) {
            _mint(_own, _ids[i]);
        }
        delete rewardList[_own];
    }

    function clubWithdraw() public onlyOwner {
        // get the amount of Ether stored in this contract
        // uint amount = address(this).balance;

        // send all Ether to owner
        // Owner can receive Ether since the address of owner is payable
        (bool success, ) = payable(msg.sender).call{value: address(this).balance}("");
        require(success, "Failed");
    }         
}