pragma solidity >=0.8.0;

import "./interfaces/IUniswapV3Factory.sol";
import "./interfaces/INonfungiblePositionManager.sol";
import "./interfaces/IUniswapV3Pool.sol";
import "./interfaces/IERC20Minimal.sol";
import "./libraries/PoolAddress.sol";
import "./libraries/RewardMath.sol";
import "./libraries/TransferHelperExtended.sol";

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

contract YeildFarmingKingRivalV3 is ReentrancyGuard, AccessControl, Pausable  {
    using SafeMath for uint256;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant MANAGERMENT_ROLE = keccak256("MANAGERMENT_ROLE");

    struct IncentiveKey {
        bool status;
        IERC20Minimal rewardToken;
        IUniswapV3Pool pool;
        uint256 startTime;
        uint256 endTime;
        uint256 lockTimeReward;
        address refundee;
    }

    /// @notice Represents a staking incentive
    struct Incentive {
        bool status;
        uint256 totalRewardUnclaimed;
        uint160 totalSecondsClaimedX128;
        uint96 numberOfStakes;
        uint256 totalFarmed;
        uint256 totalRewarded;
    }

    /// @notice Represents the deposit of a liquidity NFT
    struct Deposit {
        address owner;
        uint48 numberOfStakes;
        int24 tickLower;
        int24 tickUpper;
    }

    /// @notice Represents a staked liquidity NFT
    struct Stake {
        uint160 secondsPerLiquidityInsideInitialX128;
        uint96 liquidityNoOverflow;
        uint128 liquidityIfOverflow;
    }

    struct UserRewardLock {
        uint256 amount; //Amount reward lock
        uint256 timeUnlock; //Time unlock reward 
        bool isClaim; //Status claimed
    }

    struct OverView {
        uint256 amount; //Amount reward lock
        uint256 timeUnlock; //Time unlock reward 
    }

    struct InformationUser {
        uint256 countUserRewardLock;
        uint256 indexRewardNotClaim;
        uint256 totalRewardLockUser;
        uint256 [] listTokenIdOfUser;
    }

    IUniswapV3Factory public immutable factory;
    INonfungiblePositionManager public immutable nonfungiblePositionManager;
    
    /// @dev incentiveKey[key] => IncentiveKey
    mapping(uint256 => IncentiveKey) public incentiveKey;

    /// @dev incentives[key] => Incentive
    mapping(uint256 => Incentive) public incentives;

    /// @dev deposits[tokenId] => Deposit
    mapping(uint256 => Deposit) public deposits;

    /// @dev stakes[tokenId][key] => Stake
    mapping(uint256 => mapping(uint256 => Stake)) private _stakes;

    /// @dev rewards[key][IERC20Minimal][address] => Stake
    mapping(uint256 => mapping(IERC20Minimal => mapping(address => uint256))) public rewards;
    
    /// @dev userLockReward[IERC20Minimal][address][count] => UserRewardLock
    mapping(IERC20Minimal => mapping(address => mapping(uint256 => UserRewardLock))) private userLockReward;

    /// @dev informationOfUser[key][address] => InformationUser
    mapping(uint256 => mapping(address => InformationUser)) public informationOfUser;

    event IncentiveCreatedEvent(uint256 key, IERC20Minimal rewardToken, IUniswapV3Pool pool, uint256 startTime, uint256 endTime, uint256 lockTimeReward, address refundee);
    event IncentiveEndedEvent(uint256 key, uint256 refund);
    event TokenUnstakedEvent(uint256 tokenId, uint256 key);
    event LudenaRewardEvent(address user, uint256 RewardId, uint256 Reward, uint256 timeUnlock);
    event GatherRewardEvent(address user, uint256 rewardId, uint256 rewardClaim);

    function stakes(uint256 tokenId, uint256 key) public view returns (uint160 secondsPerLiquidityInsideInitialX128, uint128 liquidity)
    {
        Stake storage stake = _stakes[tokenId][key];
        secondsPerLiquidityInsideInitialX128 = stake.secondsPerLiquidityInsideInitialX128;
        liquidity = stake.liquidityNoOverflow;
        if (liquidity == type(uint96).max) {
            liquidity = stake.liquidityIfOverflow;
        }
    }

    /// @param _factory the Uniswap V3 factory
    /// @param _nonfungiblePositionManager the NFT position manager contract address
    constructor(
        IUniswapV3Factory _factory,
        INonfungiblePositionManager _nonfungiblePositionManager
    ) {
        factory = _factory;
        nonfungiblePositionManager = _nonfungiblePositionManager;
        _setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE);
        _setRoleAdmin(MANAGERMENT_ROLE, MANAGERMENT_ROLE);
        _setupRole(ADMIN_ROLE, _msgSender());
        _setupRole(MANAGERMENT_ROLE, _msgSender());

    }

   function pause() public onlyRole(MANAGERMENT_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(MANAGERMENT_ROLE) {
        _unpause();
    }

    function createIncentive(uint256 key, IERC20Minimal rewardToken, IUniswapV3Pool pool, uint256 startTime, uint256 endTime, uint256 lockTimeReward, address refundee) external onlyRole(ADMIN_ROLE) {
        require(!incentiveKey[key].status, "Package already exists");
        require(startTime < endTime, "Start time must be before end time");
        require(refundee != address(0), "Invalid refundee");

        incentiveKey[key] = IncentiveKey({
                                    status: true,
                                    rewardToken: rewardToken,
                                    pool: pool,
                                    startTime: startTime,
                                    endTime: endTime,
                                    lockTimeReward: lockTimeReward,
                                    refundee: refundee
                                });
        emit IncentiveCreatedEvent(key, rewardToken, pool, startTime, endTime, lockTimeReward, refundee);
    }

    function createReward(uint256 key, uint256 reward) external onlyRole(ADMIN_ROLE) {
        require(reward > 0, "Reward must be positive");
        require(incentiveKey[key].status, "Package does not exist");

        TransferHelperExtended.safeTransferFrom(address(incentiveKey[key].rewardToken), msg.sender, address(this), reward);
        incentives[key].totalRewardUnclaimed += reward;
    }

    function endIncentive(uint key) external onlyRole(ADMIN_ROLE) returns (uint256 refund) {
        require(block.timestamp >= incentiveKey[key].endTime, "Cannot end incentive before end time");
        Incentive storage incentive = incentives[key];

        refund = incentive.totalRewardUnclaimed;

        require(refund > 0, "No refund available");
        require(incentive.numberOfStakes == 0, "Cannot end incentive while deposits are staked");

        incentive.totalRewardUnclaimed = 0;
        incentiveKey[key].status = false;
        TransferHelperExtended.safeTransfer(address(incentiveKey[key].rewardToken), incentiveKey[key].refundee, refund);
        emit IncentiveEndedEvent(key, refund);
    }


    function stakeToken(uint256 key, uint256 tokenId) external whenNotPaused {
        require(incentiveKey[key].status, "Package does not exist");
        nonfungiblePositionManager.transferFrom(msg.sender, address(this), tokenId);
        informationOfUser[key][msg.sender].listTokenIdOfUser.push(tokenId);
        _stakeToken(key, tokenId);
    }

    function _stakeToken(uint256 key, uint256 tokenId) private {
        require(block.timestamp >= incentiveKey[key].startTime, "Incentive not started");
        require(block.timestamp < incentiveKey[key].endTime, "Incentive ended");

        require(incentives[key].totalRewardUnclaimed > 0, "Non-existent incentive");
        require(_stakes[tokenId][key].liquidityNoOverflow == 0, "Token already staked");

        address token0;
        address token1;
        uint24 fee;
        int24 tickLower;
        int24 tickUpper;
        uint128 liquidity;
        IUniswapV3Pool pool;
        
        (, , token0, token1, fee, tickLower, tickUpper, liquidity, , , , ) = nonfungiblePositionManager.positions(tokenId);

        pool = IUniswapV3Pool(
            PoolAddress.computeAddress(
                address(factory),
                PoolAddress.PoolKey({token0: token0, token1: token1, fee: fee})
            )
        );

        require(pool == incentiveKey[key].pool, "Token pool is not the incentive pool");
        require(liquidity > 0, "Cannot stake token with 0 liquidity");

        deposits[tokenId] = Deposit({owner: msg.sender, numberOfStakes: 0, tickLower: tickLower, tickUpper: tickUpper});

        deposits[tokenId].numberOfStakes++;
        incentives[key].numberOfStakes++;

        (, uint160 secondsPerLiquidityInsideX128, ) = pool.snapshotCumulativesInside(tickLower, tickUpper);

        if (liquidity >= type(uint96).max) {
            _stakes[tokenId][key] = Stake({
                secondsPerLiquidityInsideInitialX128: secondsPerLiquidityInsideX128,
                liquidityNoOverflow: type(uint96).max,
                liquidityIfOverflow: liquidity
            });
        } else {
            Stake storage stake = _stakes[tokenId][key];
            stake.secondsPerLiquidityInsideInitialX128 = secondsPerLiquidityInsideX128;
            stake.liquidityNoOverflow = uint96(liquidity);
        }
        emit TokenUnstakedEvent(tokenId, key);
    }

   function unstakeToken(uint256 key, uint256 tokenId) external nonReentrant {
        Deposit memory deposit = deposits[tokenId];
        require(_stakes[tokenId][key].liquidityNoOverflow != 0, "Token staked does not exist");
        require(deposit.owner == msg.sender, "Only owner can withdraw token");

        (uint160 secondsPerLiquidityInsideInitialX128, uint128 liquidity) = stakes(tokenId, key);

        require(liquidity != 0, "Liquidity does not exist");

        Incentive storage incentive = incentives[key];

        deposits[tokenId].numberOfStakes--;
        incentive.numberOfStakes--;

        (, uint160 secondsPerLiquidityInsideX128, ) = incentiveKey[key].pool.snapshotCumulativesInside(deposit.tickLower, deposit.tickUpper);
        (uint256 reward, uint160 secondsInsideX128) =
            RewardMath.computeRewardAmount(
                incentive.totalRewardUnclaimed,
                incentive.totalSecondsClaimedX128,
                incentiveKey[key].startTime,
                incentiveKey[key].endTime,
                liquidity,
                secondsPerLiquidityInsideInitialX128,
                secondsPerLiquidityInsideX128,
                block.timestamp
            );

        // if this overflows, e.g. after 2^32-1 full liquidity seconds have been claimed,
        // reward rate will fall drastically so it's safe
        incentive.totalSecondsClaimedX128 += secondsInsideX128;
        // reward is never greater than total reward unclaimed
        incentive.totalRewardUnclaimed -= reward;
        // this only overflows if a token has a total supply greater than type(uint256).max
        rewards[key][incentiveKey[key].rewardToken][deposit.owner] += reward;

        incentive.totalFarmed += reward;

        Stake storage stake = _stakes[tokenId][key];
        delete stake.secondsPerLiquidityInsideInitialX128;
        delete stake.liquidityNoOverflow;
        if (liquidity >= type(uint96).max) delete stake.liquidityIfOverflow;

        _withdrawToken(key, tokenId, msg.sender);
    }

    function _withdrawToken(uint256 key, uint256 tokenId, address to) internal {
        require(to != address(this), "Cannot withdraw to staker");
        Deposit memory deposit = deposits[tokenId];
        require(deposit.numberOfStakes == 0, "Cannot withdraw token while staked");
        require(deposit.owner == msg.sender, "Only owner can withdraw token");

        for (uint256 i = 0; i < informationOfUser[key][msg.sender].listTokenIdOfUser.length - 1; i++) {
            if (informationOfUser[key][msg.sender].listTokenIdOfUser[i] == tokenId) {
                informationOfUser[key][msg.sender].listTokenIdOfUser[i] = informationOfUser[key][msg.sender].listTokenIdOfUser[i + 1];
            }
        }
        informationOfUser[key][msg.sender].listTokenIdOfUser.pop();

        delete deposits[tokenId];
        nonfungiblePositionManager.safeTransferFrom(address(this), to, tokenId);
    }

    function ludenaReward(uint256 key) external nonReentrant returns (uint256 totalRewardUser) {
        totalRewardUser = rewards[key][incentiveKey[key].rewardToken][msg.sender]; // Total Reward of user
        require(totalRewardUser > 0, "Amount reward does not exist");

        uint256 count = informationOfUser[key][msg.sender].countUserRewardLock;
        uint256 timeNow = block.timestamp;

        UserRewardLock memory userReward;
        userReward.amount = totalRewardUser;
        userReward.timeUnlock = timeNow.add(incentiveKey[key].lockTimeReward);
        userReward.isClaim = false;

        userLockReward[incentiveKey[key].rewardToken][msg.sender][count] = userReward;

        informationOfUser[key][msg.sender].countUserRewardLock = count + 1;
        rewards[key][incentiveKey[key].rewardToken][msg.sender] = 0;
        informationOfUser[key][msg.sender].totalRewardLockUser += totalRewardUser;
        emit LudenaRewardEvent(msg.sender, count, userReward.amount, userReward.timeUnlock);
    }

    function gatherReward(uint256 key) external nonReentrant returns (uint256 rewardClaim) {
        uint256 indexClaim = informationOfUser[key][msg.sender].indexRewardNotClaim;
        uint256 totalLockReward = informationOfUser[key][msg.sender].countUserRewardLock;

        require(indexClaim < totalLockReward, "Index claim is not bigger than total lock reward");
        require(!userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].isClaim, "Reward is claimed");
        require(userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].timeUnlock < block.timestamp, "Token is locked");

        rewardClaim = userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].amount;
        userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].isClaim = true;
        userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].amount = 0;

        require(rewardClaim > 0, "Reward claim must bigger than zero");
        informationOfUser[key][msg.sender].indexRewardNotClaim = indexClaim.add(1);
        incentives[key].totalRewarded += rewardClaim;
        informationOfUser[key][msg.sender].totalRewardLockUser -= rewardClaim;
        TransferHelperExtended.safeTransfer(address(incentiveKey[key].rewardToken), msg.sender, rewardClaim);
        emit GatherRewardEvent(msg.sender, indexClaim, rewardClaim);
    }

    function getRewardInfo(uint256 key, uint256 tokenId) external view returns (uint256 reward, uint160 secondsInsideX128) {
        (uint160 secondsPerLiquidityInsideInitialX128, uint128 liquidity) = stakes(tokenId, key);
        require(liquidity > 0, "Stake does not exist");

        Deposit memory deposit = deposits[tokenId];
        Incentive memory incentive = incentives[key];

        (, uint160 secondsPerLiquidityInsideX128, ) =
            incentiveKey[key].pool.snapshotCumulativesInside(deposit.tickLower, deposit.tickUpper);

        (reward, secondsInsideX128) = RewardMath.computeRewardAmount(
            incentive.totalRewardUnclaimed,
            incentive.totalSecondsClaimedX128,
            incentiveKey[key].startTime,
            incentiveKey[key].endTime,
            liquidity,
            secondsPerLiquidityInsideInitialX128,
            secondsPerLiquidityInsideX128,
            block.timestamp
        );
    }

    function getListOverallViewOfUser(uint256 key) external view returns (OverView [] memory) {
        uint256 indexClaim = informationOfUser[key][msg.sender].indexRewardNotClaim;
        uint256 i = 0;

        OverView [] memory listView = new OverView[](informationOfUser[key][msg.sender].countUserRewardLock - indexClaim);
        while(indexClaim < informationOfUser[key][msg.sender].countUserRewardLock) {
            OverView memory data = OverView ({
                amount: userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].amount,
                timeUnlock: userLockReward[incentiveKey[key].rewardToken][msg.sender][indexClaim].timeUnlock
            });
            listView[i] = data;
            indexClaim ++;
            i++;
        }
        return listView;
    }

    function getTotalRewardOfUser(uint256 key, address _address) external view returns (uint256 totalReward) {
        totalReward = rewards[key][incentiveKey[key].rewardToken][_address];
    }

    function getTotalFarmed(uint256 key) external view returns (uint256 totalFarmed) {
        totalFarmed = incentives[key].totalFarmed;
    }

    function getTotalRewarded(uint256 key) external view returns (uint256 totalRewarded) {
        totalRewarded = incentives[key].totalRewarded;
    }

    function getAmountRewardIndexOfUser(uint256 key, address _address) external view returns (uint256) {
        uint256 indexClaim = informationOfUser[key][_address].indexRewardNotClaim;
        return userLockReward[incentiveKey[key].rewardToken][_address][indexClaim].amount;
    }

    function getTimeUnlockRewardIndexOfUser(uint256 key, address _address) external view returns (uint256) {
        uint256 indexClaim = informationOfUser[key][_address].indexRewardNotClaim;
        return userLockReward[incentiveKey[key].rewardToken][_address][indexClaim].timeUnlock;
    }

    function getTotalRewardLockOfUser(uint256 key, address _address) external view returns (uint256) {
        return informationOfUser[key][_address].totalRewardLockUser;
    }

    function getListTokenIdOfUser(uint256 key, address _address) external view returns (uint256 [] memory) {
        return informationOfUser[key][_address].listTokenIdOfUser;
    }

    function viewStake(uint256 key, uint256 tokenId) public view returns (Stake memory){
        return _stakes[tokenId][key];
    }

    function viewDeposit(uint256 tokenId) public view returns (Deposit memory){
        return deposits[tokenId];
    }

    function viewPackage(uint256 key) public view returns (IncentiveKey memory){
        return incentiveKey[key];
    }

    function viewIncentive(uint256 key) public view returns (Incentive memory){
        return incentives[key];
    }

}

