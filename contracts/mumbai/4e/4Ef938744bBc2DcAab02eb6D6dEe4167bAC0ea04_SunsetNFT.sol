pragma solidity =0.8.15;

import "./ERC721Enumerable.sol";
import "./ERC721URIStorage.sol";
import "./utils/Ownable.sol";

contract SunsetNFT is ERC721Enumerable, ERC721URIStorage, Ownable {
    constructor() ERC721("Sunset NFT", "SUNSET") Ownable() {}

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public pure override(ERC721, ERC721Enumerable) returns (bool){
        return super.supportsInterface(interfaceId);
    }

    function safeMint(address to, uint256 tokenId, string memory uri) public onlyOwner {
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
    }

    function contractURI() public view returns (string memory) {
        return "ipfs://QmT49f7EfTtmjJGrharZxQ5W2bqGnHg1yBF3hEtPDmrFAX/";
    }

    function _baseURI() internal pure override returns (string memory) {
        return "ipfs://QmVm1NrZ3RwE5a6gNZ4tH5KGFaXMyWa3imM3EGWPGXybxE/";
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

}

