    uint32 numWords =  2;

    uint256[] public s_randomWords;
    uint256 public s_requestId;
    address s_owner;


    constructor(uint64 subscriptionId) VRFConsumerBaseV2(vrfCoordinator){
        owner = msg.sender;
        badgeAddress = IERC721(0x1c6702ac01E875b353F3670eD57f294A09F7d5Bf);
        certificateAddress = IERC721(0xB0fD14b2e7A0613217263Bd91823981e055cA273);
        degreeAddress = IERC721(0x22cd121c976CcaffD9441ff63a9BF79722b03F97);
        jobTitleAddress = IERC721(0x8134Ed12d5ccaee3e80bD12967276CbE70C6E278);
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_owner = msg.sender;
        s_subscriptionId = subscriptionId;
    }

    function getBadgeMembership() public payable {
        memberCount = memberCount.add(1);
        Member memory member = Member(memberCount, msg.sender, MembershipType.BADGE, block.timestamp);
        memberDetails[msg.sender] = member;
        require(msg.value == MEMBERSHIP_FEES, "Invalid Membership Fees sent");
        payable(address(owner)).transfer(MEMBERSHIP_FEES);
    }

    function isBadgeMember() public view returns(bool){
        return memberDetails[msg.sender].membershipStartAt.add(365 days) > block.timestamp && memberDetails[msg.sender].choice == MembershipType.BADGE;
    }

    function getCertificateMembership() public payable{
        memberCount = memberCount.add(1);
        Member memory member = Member(memberCount, msg.sender, MembershipType.CERTIFICATE, block.timestamp);
        memberDetails[msg.sender] = member;
        require(msg.value == MEMBERSHIP_FEES, "Invalid Membership Fees sent");
        payable(address(owner)).transfer(MEMBERSHIP_FEES);
    }

    function isCertificateMember() public view returns(bool){
        return memberDetails[msg.sender].membershipStartAt.add(365 days) > block.timestamp && memberDetails[msg.sender].choice == MembershipType.CERTIFICATE;
