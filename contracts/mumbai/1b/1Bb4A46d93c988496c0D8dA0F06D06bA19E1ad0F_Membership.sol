    }

    function rewardCertificate(string memory userMail, uint256 tokenID) public {
        require(isCertificateMember(), "Not a Certificate Member");
        certificateCount = certificateCount.add(s_randomWords[0]);
        Certificate memory certificateObject = Certificate(tokenID, msg.sender, userMail, s_randomWords[0]);
        userCertificates[tokenID] = (certificateObject);
        requestRandomWords();
    }

    function rewardDegree(string memory userMail, uint256 tokenID) public  {
        require(isDegreeMember(), "Not a Degree Member");
        degreeCount = degreeCount.add(1);
        Degree memory degreeObject = Degree(tokenID, msg.sender, userMail, s_randomWords[0]);
        userDegrees[tokenID] = (degreeObject);
        requestRandomWords();
    }

    function rewardJobTitle(string memory userMail, uint256 tokenID) public {
        require(isJobTitleMember(), "Not a Job Title Member");
        jobCount = jobCount.add(1);
        JobTitle memory jobObject = JobTitle(tokenID, msg.sender, userMail, s_randomWords[0]);
        userJobTitles[tokenID] = (jobObject);
        requestRandomWords();
    }

    function claimBadge(uint256 secretKey, uint256 tokenID) public {
        require(userBadges[tokenID].secretKey == secretKey, "Secret Key not Matched");
        badgeAddress.safeClaimFrom(userBadges[tokenID].by, msg.sender, tokenID);
    }

    function claimCertificate(uint256 secretKey, uint256 tokenID) public {
        require(userCertificates[tokenID].secretKey == secretKey, "Secret Key not Matched");
        certificateAddress.safeClaimFrom(userCertificates[tokenID].by, msg.sender, tokenID);
    }

    function claimDegree(uint256 secretKey, uint256 tokenID) public {
        require(userDegrees[tokenID].secretKey == secretKey, "Secret Key not Matched");
        degreeAddress.safeClaimFrom(userDegrees[tokenID].by, msg.sender, tokenID);
    }

    function claimJobTitle(uint256 secretKey, uint256 tokenID) public {
        require(userJobTitles[tokenID].secretKey == secretKey, "Secret Key not Matched");
        jobTitleAddress.safeClaimFrom(userJobTitles[tokenID].by, msg.sender, tokenID);
    }

    // Assumes the subscription is funded sufficiently.
    function requestRandomWords() public {
        // Will revert if subscription is not set and funded.
        s_requestId = COORDINATOR.requestRandomWords(
        keyHash,
        s_subscriptionId,
        requestConfirmations,
        callbackGasLimit,
        numWords
        );
    }
    
    function fulfillRandomWords(
        uint256, /* requestId */
        uint256[] memory randomWords
    ) internal override {
        s_randomWords = randomWords;
    }

}