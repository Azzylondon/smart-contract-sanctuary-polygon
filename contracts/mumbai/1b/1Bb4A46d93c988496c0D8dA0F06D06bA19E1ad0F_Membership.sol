        uint256 id;
        address memberAddress;
        MembershipType choice;
        uint256 membershipStartAt;
    }

    struct Badge {
        uint256 tokenID;
        address by;
        string userMail;
        uint256 secretKey;
    }

    struct Certificate {
        uint256 tokenID;
        address by;
        string userMail;
        uint256 secretKey;
    }

    struct Degree {
        uint256 tokenID;
        address by;
        string userMail;
        uint256 secretKey;
    }

    struct JobTitle {
        uint256 tokenID;
        address by;
        string userMail;
        uint256 secretKey;
    }

    uint256 public memberCount;
    mapping(address => Member) memberDetails;

    uint256 public badgeCount;
    mapping(uint256 => Badge) public userBadges;

    uint256 public certificateCount;
    mapping(uint256 => Certificate) public userCertificates;

    uint256 public degreeCount;
    mapping(uint256 => Degree) public userDegrees;

    uint256 public jobCount;
    mapping(uint256 => JobTitle) public userJobTitles;

    uint256 public constant MEMBERSHIP_FEES = .01 ether;
    address public owner;

    IERC721 badgeAddress;
    IERC721 certificateAddress;
    IERC721 degreeAddress;
    IERC721 jobTitleAddress;

// Chainlink VRF 
    VRFCoordinatorV2Interface COORDINATOR;

    // Your subscription ID.
    uint64 s_subscriptionId;

    // Polygon coordinator. For other networks,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
    address vrfCoordinator = 0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed;

    // The gas lane to use, which specifies the maximum gas price to bump to.
    // For a list of available gas lanes on each network,
    // see https://docs.chain.link/docs/vrf-contracts/#configurations
