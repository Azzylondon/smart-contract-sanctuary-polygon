    // function to prevent insertion of duplicates in array
    function isDuplicate(uint itemId) internal view returns (bool) {
        MarketItem storage item = idToMarketItem[itemId];
        for(uint i = 0; i < item.copyOwners.length; i++){
        if(item.copyOwners[i] == msg.sender) return true;
    }

    return false;
}
    function addAddressToCopyArray(uint itemId) internal  {
        
        MarketItem storage item = idToMarketItem[itemId];
            if(!isDuplicate(itemId)){
            item.copyOwners.push(msg.sender);
            }
       
    }
    function deleteOwnerFromCopyOwners(uint itemId,uint _tokenId) internal  {
         MarketItem storage item = idToMarketItem[itemId];

         for (uint256 i = 0; i < item.copyOwners.length; i++) {
            if (
                
                item.copyOwners[i] == msg.sender && item.tokenId == _tokenId
                
            ) {
            item.copyOwners[i] = item.copyOwners[item.copyOwners.length - 1];
            item.copyOwners.pop();
            }
        }

    }
    
