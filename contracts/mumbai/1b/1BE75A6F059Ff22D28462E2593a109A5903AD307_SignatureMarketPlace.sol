       //submit hoodie size for token(only one) id
    function submitHoodieSizeForTokenId(
        address nftContract,
        uint256 tokenId,
        uint256 itemId,
        string memory size
    ) external onlyFirstBuyer(itemId) {
        require(
            tokenId == SignatureNFT(payable(address(nftContract))).Gold() ||
            tokenId == SignatureNFT(payable(address(nftContract))).Platinum() ||
            tokenId == SignatureNFT(payable(address(nftContract))).Legendary(),
            "submitHoodieSize: only Gold, Platinum and Legendary tokens buyers can submit hoodie size"
        );
        require(isHoodieSizeSubmittedForTokenId[msg.sender][tokenId] != true,"you have already submited hoodie size");
        require(
            keccak256(bytes(size)) == keccak256(bytes("Small"))
         || keccak256(bytes(size)) == keccak256(bytes("Medium")) 
         || keccak256(bytes(size)) == keccak256(bytes("Large")) 
         || keccak256(bytes(size)) == keccak256(bytes("XL"))  
         || keccak256(bytes(size)) == keccak256(bytes("XXL")) ,
          "submitHoodieSize: You can only submit Small, Medium, Large , XL and XXL as sized in string"
          );
        hoodieSizeForToken[msg.sender][tokenId] = size;
        isHoodieSizeSubmittedForTokenId[msg.sender][tokenId] = true; 
    }
     function createMarketItem(
        address nftContract,
        uint256 tokenId,
        uint amount,
        uint256 price
    )public nonReentrant  {
        require(price > 0 && amount != 0, "!= 0,wrong price OR wrong token amount");

        _createMarketItem(nftContract,tokenId,amount,price);
    }

    function _createMarketItem(
        address nftContract,
        uint256 tokenId,
        uint amount,
        uint256 price
        ) internal  {

        _itemsIds.increment();
        uint256 itemId = _itemsIds.current();

        bool copy;
        if(amount>1){
            copy = true;
        }

        idToMarketItem[itemId] = MarketItem(
            itemId,
            nftContract,
            tokenId,
            payable(msg.sender),
            amount,
            payable(msg.sender),
            payable(address(0)),
            payable(address(0)),
            price,
            false,
            new address[](0),
            payable(iMarketplaceSettings.getAthleteAddress()),
            copy

        );
        //pushing msg.sender to array in item struct
        MarketItem storage item = idToMarketItem[itemId];
        item.copyOwners.push(msg.sender);
        //transferring token from user address to this contract address
         SignatureNFT(payable(address(nftContract))).safeTransferFrom(
            msg.sender,
            address(this),
            tokenId,
            amount,
            ""
        );
        emit MarketItemCreated(
            itemId,
            nftContract,
            tokenId,
            payable(msg.sender),
            amount,
            msg.sender,
            address(0),
            price,
            payable(iMarketplaceSettings.getAthleteAddress()),
            copy
            
        );
    }

    function updateMarketItemPrice(uint256 itemId, uint256 newPrice)
        public 
        onlyProductSeller(itemId)
    {
        MarketItem storage item = idToMarketItem[itemId];
        uint256 oldPrice = item.price;
        item.price = newPrice;

        emit ProductUpdated(itemId, oldPrice, newPrice);
    }

    //transferring token from contract to user
    function transferToken(address nftContract,address to,uint tokenId,uint amount ) internal{

     SignatureNFT(payable(address(nftContract))).safeTransferFrom(
            address(this),
            to,
            tokenId,
            amount,
            "" 
        );
    }
        
 
    function createMarketSale( uint  amount, uint256 itemId, address _buyer)
        public
        payable
        nonReentrant
    {     
        require(!idToMarketItem[itemId].sold, "Item is Sold");
        uint256 price = idToMarketItem[itemId].price;
        uint256 tokenId = idToMarketItem[itemId].tokenId;
        address nftAddress =  idToMarketItem[itemId].nftContract;
        address payee = idToMarketItem[itemId].seller;
        bool _isCopy = idToMarketItem[itemId].isCopy;
        address tokenCreator = iERC1155CreatorRoyalty.tokenCreator(nftAddress,tokenId);
        //Check that enough ether was sent.
        require(
            tokenPriceFeeIncluded(nftAddress,itemId,tokenId,amount) == msg.value,
            "createMarketSale::Must purchase the token for the correct price"
        );
       
        
        // transfer token 
        transferToken( nftAddress,_buyer,tokenId,amount );
        // payout all parties
        Payments.payout(
            price,
            !iMarketplaceSettings.hasERC1155TokenSold(nftAddress, tokenId),
            iMarketplaceSettings.getMarketplaceFeePercentage(),
            iERC1155CreatorRoyalty.getERC1155TokenRoyaltyPercentage(
                nftAddress,
                tokenId
            ),
            iMarketplaceSettings.getERC1155ContractPrimarySaleFeePercentage(
                nftAddress
            ),
            iMarketplaceSettings.getERC1155ContractSecondarySaleFeePercentage(
                nftAddress
            ),
            iMarketplaceSettings.getERC1155ContractAthletePrimarySaleFeePercentage(
                nftAddress
            ),
            iMarketplaceSettings.getERC1155ContractAthleteSecondarySaleFeePercentage(
                nftAddress
            ),
            //_seller
            payable(payee),
            //_marketplacePayee owner()
            payable(owner()),
            // tokenCreator for ryoalty
            payable(tokenCreator),
            //_creator (us)
            payable(iMarketplaceSettings.getSignatureAiAddress()),
            // athlete
            payable(iMarketplaceSettings.getAthleteAddress())

        );
        
        // Set token as sold
        iMarketplaceSettings.markERC1155Token(nftAddress, tokenId, true);
        // add first buyer in strcut if there is 0 adress before
        if(idToMarketItem[itemId].firstBuyer == address(0)){
            idToMarketItem[itemId].firstBuyer = payable(_buyer);
        }
        if(_isCopy){
        //updating current amount of tokens after selling
        idToMarketItem[itemId].amount = getCurrentNFTBalance(nftAddress, tokenId, address(this));
        saleEvent(itemId,amount);
        //adding address to copyOwners Array in iten Struct
        addAddressToCopyArray(itemId);
        isCopyOwner[_buyer][itemId] = true;
        //reading balance of user from nft contract
        copyOwnerBalance[_buyer][itemId] = getCurrentNFTBalance(nftAddress, tokenId, _buyer);
        if(idToMarketItem[itemId].amount == 0){
        idToMarketItem[itemId].sold = true;
        }
        return;
        }

        if(!_isCopy){
        require(idToMarketItem[itemId].amount == 1,"wrong amount of tokens");
        idToMarketItem[itemId].owner = payable(_buyer);
        idToMarketItem[itemId].sold = true;
        idToMarketItem[itemId].amount == amount;
        _itemsSold.increment();
    
        //emit
        saleEvent(itemId,amount);

        return;
        }
}

    function saleEvent(uint itemId, uint _amount) internal {
        emit ProductSold(
            idToMarketItem[itemId].itemId,
            idToMarketItem[itemId].nftContract,
            idToMarketItem[itemId].tokenId,
            idToMarketItem[itemId].creator,
            _amount,
            idToMarketItem[itemId].seller,
            payable(msg.sender),
            idToMarketItem[itemId].price,
            idToMarketItem[itemId].signedBy,
            idToMarketItem[itemId].isCopy
        );
    }

    function getCurrentNFTBalance(address _nftContract, uint tokenId, address _address) internal view returns(uint){
       return SignatureNFT(payable(address(_nftContract))).balanceOf(_address,tokenId);
    }

    function putItemToResell(address nftContract, uint256 itemId,uint amount, uint256 newPrice)
        public
        nonReentrant
        onlyItemOwner(itemId)
    {
        require(newPrice > 0, "Price must be at least 1 wei");

        uint256 tokenId = idToMarketItem[itemId].tokenId;
        bool    _isCopy = idToMarketItem[itemId].isCopy;

        if(_isCopy){
        require(amount <= copyOwnerBalance[msg.sender][itemId],"wrong amount");
       
        if(amount <=copyOwnerBalance[msg.sender][itemId]){
            copyOwnerBalance[msg.sender][itemId] -=amount;
            if(amount == copyOwnerBalance[msg.sender][itemId]){
             isCopyOwner[msg.sender][itemId] = false;
        }
        }
        
        deleteOwnerFromCopyOwners( itemId, tokenId);
        require(newPrice > 0 && amount != 0, "!= 0,wrong price OR wrong token amount");

        _createMarketItem( nftContract, tokenId, amount, newPrice);
       }

        if(!_isCopy){
        
         SignatureNFT(payable(address(nftContract))).safeTransferFrom(
            msg.sender,
            address(this),
            tokenId,
            amount,
            ""
        );
        //getting last owner address
        address payable oldOwner = idToMarketItem[itemId].owner;
        idToMarketItem[itemId].owner = payable(address(0));
        idToMarketItem[itemId].seller = oldOwner;
        idToMarketItem[itemId].price = newPrice;
        idToMarketItem[itemId].sold = false;
        _itemsSold.decrement();
        emit ProductListed(itemId);
        }
    }

    /////////////////////////////////////////////////////////////////////////
    // unListItem
    /////////////////////////////////////////////////////////////////////////
    /**
     * @dev unListItem the MarketItem
     * @param itemId of NFT to unlist
     * @param to addres to transfer NFT
     * conditions, owner must be seler or mp owner
     */
    function unListItem(uint256 itemId, address to) onlyOwner public{
        MarketItem storage tokenMarketData = idToMarketItem[itemId];
        uint256 tokenId = tokenMarketData.tokenId;
        address nftContract = tokenMarketData.nftContract;
        uint amount = tokenMarketData.amount;
        require(tokenMarketData.seller == msg.sender || msg.sender == owner(),"Error: Not Owner not Admin");
        // transfer token 
        transferToken( nftContract,to,tokenId,amount );
        delete idToMarketItem[itemId];
        _itemsIds.decrement();

        emit MarketItemUnlisted(
            tokenMarketData.itemId,
            tokenMarketData.nftContract,
            tokenMarketData.tokenId,
            tokenMarketData.seller,
            tokenMarketData.owner,
            tokenMarketData.amount
        );
    }
    //fetch all marketItems
    function fetchMarketItems() public view returns (MarketItem[] memory) {
        uint256 itemCount = _itemsIds.current();
        uint256 unsoldItemCount = _itemsIds.current() - _itemsSold.current() - _itemsDeleted.current();
        uint256 currentIndex = 0;

        MarketItem[] memory items = new MarketItem[](unsoldItemCount);
        for (uint256 i = 0; i < itemCount; i++) {
            if (
                
                idToMarketItem[i + 1].sold == false
                
            ) {
            uint256 currentId = i + 1;
            MarketItem storage currentItem = idToMarketItem[currentId];
            items[currentIndex] = currentItem;
            currentIndex += 1;
            }
        }
        return items;
    }
    //dev fetch market items based on conditon, if true, it will returen all copy items
    // if false, it will return all rare items
    //pankaj
    function fetchMarketItemsToggle(bool toggle) public view returns (MarketItem[] memory) {
        uint256 itemCount = _itemsIds.current();
        uint256 unsoldItemCount = _itemsIds.current() - _itemsSold.current() - _itemsDeleted.current();
        uint256 currentIndex = 0;

        MarketItem[] memory items = new MarketItem[](unsoldItemCount);
        for (uint256 i = 0; i < itemCount; i++) {
            if (
                
                idToMarketItem[i + 1].isCopy == toggle
                
            ) {
            uint256 currentId = i + 1;
            MarketItem storage currentItem = idToMarketItem[currentId];
            items[currentIndex] = currentItem;
            currentIndex += 1;
            }
        }
        return items;
    }

    /* Returns only copy items that a user has purchased */
    function fetchMyNFTs() public view returns (MarketItem[] memory) {
        uint256 totalItemCount = _itemsIds.current();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if(idToMarketItem[i + 1].owner == msg.sender ){
                itemCount += 1;
            }
            else{
            //inner loop for iterating all addresses in array
            for(uint256 j = 0; j<idToMarketItem[i + 1].copyOwners.length; j++){
                if (idToMarketItem[i + 1].copyOwners[j] == msg.sender) {
                    itemCount += 1;
                    break;
            }
            }
            }
        }
        MarketItem[] memory items = new MarketItem[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            if (idToMarketItem[i + 1].owner == msg.sender) {
                uint256 currentId = i + 1;
                MarketItem storage currentItem = idToMarketItem[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
            }
            else{
            for(uint256 j = 0; j<idToMarketItem[i + 1].copyOwners.length; j++){
                if(idToMarketItem[i + 1].copyOwners[j] == msg.sender){
                uint256 currentId = i + 1;
                MarketItem storage currentItem = idToMarketItem[currentId];
                items[currentIndex] = currentItem;
                currentIndex += 1;
                break;
            }
            }   
            }            
            }
        return items;
    }

