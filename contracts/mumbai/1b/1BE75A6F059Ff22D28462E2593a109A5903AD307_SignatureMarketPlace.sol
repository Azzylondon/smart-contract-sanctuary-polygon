    event MarketItemDeleted(uint256 itemId);

    event ProductSold(
        uint256 indexed itemId,
        address indexed nftContract,
        uint256 indexed tokenId,
        address creator,
        uint256 amount,
        address seller,
        address owner,
        uint256 price,
        address signedBy,
        bool isCopy

    );
    event ProductListed(
        uint256 indexed itemId
