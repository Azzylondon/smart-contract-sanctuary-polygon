    /**
     * @dev Checks that the token is owned by the market place
     * @param nftContract to check token balance
     * @param _tokenId uint256 id of the.
     * @param itemId to check Is token sold
     */
    function _marketPlaceStillOwnsTheToken(address nftContract,uint256 _tokenId, uint itemId) 
    public view returns (bool status) {
        if(SignatureNFT(payable(address(nftContract))).balanceOf(address(this),_tokenId) != 0
         && idToMarketItem[itemId].sold == false){
        return true;
        }
    }

    /////////////////////////////////////////////////////////////////////////
    // tokenPrice
    /////////////////////////////////////////////////////////////////////////
    /**
     * @dev Gets the sale price of the token
     * @param nftContract to check token balance
     * @param itemId uint256 ID of the token
     * @return uint256 sale price of the token
     */
    function tokenPrice(address nftContract,uint256 itemId, uint256 _tokenId)
        external
        view
        returns (uint256)
    {
        
        if (_marketPlaceStillOwnsTheToken(nftContract,_tokenId,itemId) ) {
            return idToMarketItem[itemId].price;
        }
    }

    /////////////////////////////////////////////////////////////////////////
    // tokenPriceFeeIncluded
    /////////////////////////////////////////////////////////////////////////
     /**
     * @dev Gets the sale price of the token
     * @param nftContract to check token balance
     * @param itemId uint256 ID of the token
     * @return uint256 sale price of the token
     */
    function tokenPriceFeeIncluded(address nftContract,uint256 itemId, uint256 _tokenId,uint amount)
        public
        view
        returns (uint256)
    {

        
        require(_marketPlaceStillOwnsTheToken(nftContract,_tokenId,itemId),"_marketPlaceStillOwnsTheToken: token is not owned by Market Place" );

            if(amount==0){
                return
                idToMarketItem[itemId].price.add(
                iMarketplaceSettings.calculateMarketplaceFee(
                idToMarketItem[itemId].price
                )
                );
            }
            if(amount>0){
                return
                idToMarketItem[itemId].price.mul(amount).add(
                    iMarketplaceSettings.calculateMarketplaceFee(
                        idToMarketItem[itemId].price.mul(amount)
                    )
                );

            }           
        
            }

   
   


