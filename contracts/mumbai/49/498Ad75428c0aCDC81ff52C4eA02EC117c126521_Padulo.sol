pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./PaduloModifier.sol";
import "./PaduloAdministrative.sol";
import "hardhat/console.sol";

abstract contract Jackpot is PaduloAdministrative {
	using SafeMath for uint256;
	using SafeERC20 for IERC20;

    struct Winner {
        address playerAddress;
        uint256 jackpotWin;
    }

	uint256 public jackpot;
    uint256 public jackpotLeft;
    uint256 public jackpotParticipants = 50; //number of players who will receive a portion of the jackpot
    uint8 public startDistribution;
    uint256 public singleValue; //valore utile per il calcolo ponderato della reddistribuzione del jackpot
    uint256 public jackpotWaived; //Payout of jackpots waived by players with withdrawal

    mapping(uint256 => Winner) public winners;
    mapping(address => uint256) public _winnersNumberList;

	event JackptoWin(address winner, uint256 amount);
    event NewNumberOfParticipants(uint256 participants);
    event WithdrawJackpot(address owner, uint256 amount);

    function getJackpot() public view returns(uint256)
    {
        return jackpot;
    }

    // function getParticipants() public view returns(uint256)
    // {
    //     return jackpotParticipants;
    // }

    // function setParticipants(uint256 number) external onlyAdmin {
	// 	require(number != 0, "Cannot be zero");
    //     //if the jackpot distribution has already started, it is not possible to change the value because it would change all the calculations of the individual jackpots
    //     require(startDistribution == 0, "cannot change value, Jackpot distribution already started");
        
	// 	participants = number;

	// 	emit NewNumberOfParticipants(number);
	// }

    function jackpotAward(uint256 playersInGame, address playerAddress) internal
    {
        //start distribution jackpot
        if(playersInGame==jackpotParticipants+1)
        {
            startDistribution=1;
        }

        if(playersInGame<=jackpotParticipants)
        {
            uint256 jackpotPosition = jackpotParticipants - playersInGame + 1;
            
            uint256 amount = (singleValue*jackpotPosition) + jackpotWaived;

            _winnersNumberList[playerAddress] = playersInGame;

            Winner storage winner =  winners[playersInGame];
            winner.playerAddress = playerAddress;
            winner.jackpotWin = amount;

            jackpotLeft = jackpotLeft - amount;

            emit JackptoWin(winner.playerAddress, winner.jackpotWin);
        }
    }

    function getJackpotWin(address account) public view returns (uint256)
    {
        uint256 position = _winnersNumberList[account];

        if(position==0)
        {
            return 0;
        }

        Winner memory winner = winners[position];

        return winner.jackpotWin;
    }
    function withdrawJackpot(address account, uint256 playersInGame) internal
    {
        uint256 position = _winnersNumberList[account];

        if(position != 0)
        {
            Winner storage winner = winners[position];

            emit WithdrawJackpot(winner.playerAddress, winner.jackpotWin);

            winner.jackpotWin = 0;
        }
        else
        {
            //se un giocatore preleva e aveva diritto a una porzione di jackpot, rinuncia alla sua porzione, che viene distribuita in maniera equa tra i giocatori rimasti
            if(startDistribution==1)
            {
                uint256 jackpotPosition = jackpotParticipants - playersInGame + 1;
                uint256 amount = singleValue*jackpotPosition;

                jackpotWaived = jackpotWaived + (amount / (playersInGame-1));
            }
        }
    }
}

