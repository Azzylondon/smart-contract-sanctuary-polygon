	using SafeMath for uint256;
	using SafeERC20 for IERC20;

	uint256 public timeInterval = 60;

	event NewAdminAddress(address admin);
	event NewOperatorAddress(address operator);
	event NewIntervalDuration(uint256 duration);
    event NewTokenPlayed(address token);

	function setAdmin(address _adminAddress) external onlyAdmin {
		require(_adminAddress != address(0), "Cannot be zero address");
		adminAddress = _adminAddress;

		emit NewAdminAddress(_adminAddress);
	}

	function setOperator(address _operatorAddress) external onlyAdmin {
		require(_operatorAddress != address(0), "Cannot be zero address");
		operatorAddress = _operatorAddress;

		emit NewOperatorAddress(_operatorAddress);
	}

	function setInterval(uint256 duration) external onlyAdmin {
		require(duration > 0, "Cannot be zero or lower");
		timeInterval = duration;
		emit NewIntervalDuration(duration);
	}

    // function setTokenPlayed(address token) external onlyAdmin {
    //     require(token != address(0), "Cannot be zero address");
    //     paduloToken = token;

    //     emit NewTokenPlayed((token));
    // }

}

