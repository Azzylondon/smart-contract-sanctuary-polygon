pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./ITotalSupply.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "hardhat/console.sol";


abstract contract PaduloNFTManager {
	using SafeMath for uint256;
	using SafeERC20 for IERC20;
   
    uint8 private titanTurn = 1; //for the distribution of fees

    mapping(uint256 => uint256) public _nftUsed; //map degli nft usati
    mapping(address => uint256) public _nftActivated; //map dei giocatori che hanno attivato un nft
    mapping(uint256 => uint256) public _nftInGame; //map necessario poi per bloccare il transfer degli nft attivis
    mapping(uint256 => uint256) public _titanFees;

    uint constant assSaverStartIndex = 1;
    uint constant assSaverEndIndex = 100;
    uint constant assBreakerStartIndex = 101;
    uint constant assBreakerEndIndex = 200;

    address usdtAddress;
    address public nftTitanContract;
    address public nftSkillContract;

    //event NewMint(address factoryContract, uint256 tokenId);
    event NewMaxTotalSupply(uint256 maxTotalSupply);
    event NewPaduloAddress(address paduloAddress);
    event NewStatusSkill(address player, uint256 tokenId, bool isActivated);   
    event TitanFeeUpdated(uint256 indexed titanID, int256 amount);
    event NewNftUsed(address owner, uint256 padulationTimestamp, uint256 tokenIdUsed);


    //life nft activaction
    function assSaverNftActivaction(uint256 tokenId) external
    {
        require(tokenId>=assSaverStartIndex && tokenId<=assSaverEndIndex, "you don't have Ass Saver");
        //check if nft already used
        uint256 checkUsed = _nftUsed[tokenId];
        require(checkUsed == 0, "nft already used");

        IERC721 nftFactory = IERC721(nftSkillContract);
		address nftOwner = nftFactory.ownerOf(tokenId);
        require(nftOwner == msg.sender, "Caller doesn't own life token");

        require(_nftActivated[nftOwner] == 0, "you already have an active nft");

        _nftActivated[nftOwner] = tokenId;
        _nftInGame[tokenId] = tokenId;

        emit NewStatusSkill(nftOwner, tokenId, true);
    }

    //killer nft activaction
    function assBreakerNftActivaction(uint256 tokenId) external
    {
        require(tokenId>=assBreakerStartIndex && tokenId<=assBreakerEndIndex, "you don't have Ass Breaker");
        //check if nft already used
        uint256 checkUsed = _nftUsed[tokenId];
        require(checkUsed == 0, "nft already used");

        IERC721 nftFactory = IERC721(nftSkillContract);
		address nftOwner = nftFactory.ownerOf(tokenId);
        require(nftOwner == msg.sender, "Caller doesn't own life token");

        require(_nftActivated[nftOwner] == 0, "you already have an active nft");

        _nftActivated[nftOwner] = tokenId;
        _nftInGame[tokenId] = tokenId;


        emit NewStatusSkill(nftOwner, tokenId, true);
    }

    function deactivationNftSkill() external
    {
        uint256 tokenId = _nftActivated[msg.sender];

        require(tokenId != 0, "There are no active skills");

        IERC721 nftFactory = IERC721(nftSkillContract);
		address nftOwner = nftFactory.ownerOf(tokenId);
        require(nftOwner == msg.sender,"Caller doesn't own killer token");

        _nftActivated[msg.sender] = 0;
        _nftInGame[tokenId] = 0;

        emit NewStatusSkill(nftOwner, tokenId, false);
    }

    //se il giocatore ha un nft attivo restituisce un valore diverso da 0
    function getNftActivated() public view returns(uint256)
    {
        uint256 tokenId = _nftActivated[msg.sender];

        return tokenId;
    }

    

    function _nftSkillUsage(address owner, uint256 tokenId) internal
    {
        //inseriamo l'nft tra quelli usati
        _nftUsed[tokenId] = tokenId;

        //cancelliamo parametri, così il giocatore può attivare un altro nft
        _nftActivated[owner] = 0;
        _nftInGame[tokenId] = 0;

        emit NewNftUsed(owner, block.timestamp, tokenId);
    }

    function getNftUsed(uint256 tokenId) public view returns (uint256)
    {
        return _nftUsed[tokenId];
    }

	function _setTitanFee(uint256 fee) internal {

		uint256 titanTokenId = titanTurn;

		if(uint256(_titanFees[titanTokenId]) > 0 )
		{
			_titanFees[titanTokenId] = _titanFees[titanTokenId] + fee;
		}
		else
		{
			_titanFees[titanTokenId] = fee;
		}

        titanTurn++;
        ITotalSupply nftFactory = ITotalSupply(nftTitanContract);
        uint256 titanSupply = nftFactory.totalSupply();

        if(titanTurn>titanSupply){
            titanTurn = 1;
        }

		emit TitanFeeUpdated(titanTokenId, int256(_titanFees[titanTokenId]));	
	}

    function titanFees(uint256 tokenId) public view returns (uint256) {
        uint256 fee = _titanFees[tokenId];
        return fee;
    }

    function getTotalTitanRevenue() public view returns (uint256)
    {
        ITotalSupply titanContract = ITotalSupply(nftTitanContract);
        uint256 totalSupply = titanContract.totalSupply();
        uint256 ris;
        for(uint8 i=1; i<=totalSupply; i++)
        {
            uint256 fee = _titanFees[i];
            ris = ris + fee;
        }

        return ris;
    }

	function ClaimTitanFee(uint256 titanTokenId) public {

		IERC721 nftFactory = IERC721(nftTitanContract);
		address nftOwner = nftFactory.ownerOf(titanTokenId);
		require(nftOwner == msg.sender,"Caller doesn't own arena token");

		uint256 titanFeeAmount = _titanFees[titanTokenId];
		_titanFees[titanTokenId] = 0;
		IERC20(usdtAddress).safeTransfer(address(msg.sender), titanFeeAmount);	

		emit TitanFeeUpdated(titanTokenId, -int256(titanFeeAmount) );	
	}
}

