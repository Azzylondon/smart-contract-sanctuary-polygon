pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "./Jackpot.sol";
import "./NFT/PaduloNFTManager.sol";
import "./INFTActivated.sol";
// import "hardhat/console.sol";

contract Padulo is Context, Jackpot, PaduloNFTManager, INFTActivated {
    using SafeERC20 for IERC20;
    using SafeMath for uint;
    using SafeMath for uint256;

    struct Player {
        address playerAddress;
        // uint ticketBuyed;s
        uint enterEpoch;
    }

    //usdt use 6 decimal
    uint constant ticketPrice = 5 * 1e6;
    uint256 constant public titanFeePercentage = 10;
    uint256 constant public jackpotContributionPercentage = 10;

    uint currentEpoch; //numero epoca in cui stanno entrando i giocatori
    uint lastPadulation; //momento in cui è avvenuta l'ultima padulazione
    uint totalEpoch; //somma delle epoche di tutti i giocatori in gioco
    uint totalBalance; //somma dell'entrate, meno la percentuale per il jackpot e le fee
    uint openEnrollment; //paramatro che indica se le iscrizioni sono aperte
    uint256 playersInGame; //numero giocatori in gioco
    bool gameStarted; //parametro che indica se il gioco è iniziato (serve per bloccare il tansfer degli NFT)
    uint256 PadulationTurnsMaxRange;
    uint256 PadulationTurnsMinRange;

    mapping(uint256 => Player) public players;
    mapping(address => uint256) private _playerNumberList;

    event Deposit(address from, uint256 amount, uint256 timestamp);
    event Withdrawal(address to, uint256 amount, uint256 timestamp);
    event Padulation(address padulo, uint256 padulationTimestamp);
    event CloseRegistration(uint256 playersInGame, uint256 timestamp);
    event GameStarted(bool, uint256 timestamp);

    constructor(
		address _adminAddress,
		address _operatorAddress,
        address _usdtAddress,
        address _nftTitantContract,
        address _nftSkillContract
	) {
		require(_adminAddress != address(0), "Cannot be zero address");
		require(_operatorAddress != address(0), "Cannot be zero address");

		adminAddress = _adminAddress;
		operatorAddress = _operatorAddress;
        usdtAddress = _usdtAddress;
        currentEpoch = 1;
        openEnrollment = 1;
        gameStarted = false;
        nftTitanContract = _nftTitantContract;
        nftSkillContract = _nftSkillContract;
        PadulationTurnsMaxRange = 100;
        PadulationTurnsMinRange = 0;
        startDistribution = 0;
        singleValue = 0;
        jackpotWaived = 0;
	}

    function addPlayer() public  {

        require(gameStarted==true, "game don't start");
        require(openEnrollment==1, "registration closed");
        //todo: move usdt to current contract
        address playerAddress = _msgSender();
        uint256 position = _playerNumberList[playerAddress];
        require(position==0, "player already in game");
        //potrebbe fallire!!!! lo sposto sopra per non cambiare le variabili in caso di mancato budget
        IERC20(usdtAddress).safeTransferFrom(playerAddress, address(this), ticketPrice);
        emit Deposit(playerAddress, ticketPrice, block.timestamp);

        //aggiorno il totale epoche per il calcolo budget
        totalEpoch = totalEpoch + 1;
        //aggiorno totale giocatori
        playersInGame = playersInGame + 1;
        //inserisco il giocatore a fine lista
        _playerNumberList[playerAddress] = playersInGame;
        Player storage player =  players[playersInGame];
        player.playerAddress = playerAddress;
        // player.ticketBuyed = ticketPrice;
        player.enterEpoch = currentEpoch;

        uint256 titanFee = (ticketPrice/100)*titanFeePercentage;
        _setTitanFee(titanFee);

        uint256 jackpotContribution = (ticketPrice/100)*jackpotContributionPercentage;
        jackpot = jackpot + jackpotContribution;

        totalBalance = totalBalance + ticketPrice - jackpotContribution - titanFee;
    }

    function balanceOf(address account) public view returns (uint256) {
        uint256 _balance = 0;
        uint256 jackpotAmount = 0;

        uint256 position = _playerNumberList[account];

        if(openEnrollment==0 && playersInGame==1 && position!=0)
        {
            _balance = totalBalance + jackpotLeft;
        }
        else
        {
            Player memory player = players[position];
            uint playedEpochs = (currentEpoch - player.enterEpoch) + 1;

            _balance = totalBalance/totalEpoch*playedEpochs;

            //se il giocatore non ha mai giocato o è stato padulato
            if(position==0)
            {
                _balance = 0;
            }

            //se sono state chiuse le iscrizioni, bisogna controllare se il giocatore è stato padulato e se ha vinto una porzione del jackpot
            if(openEnrollment==0)
            {
                jackpotAmount = getJackpotWin(account);
            }

            _balance = _balance + jackpotAmount;
        }

        return _balance;
    }

    function getPlayersInGame() public view returns(uint256)
    {
        return playersInGame;
    }

    function getTotalbalance() public view returns(uint256)
    {
        return totalBalance;
    }

    function closeRegistration() external onlyAdmin
    {
        require(openEnrollment==1, "registration already closed");
        openEnrollment = 0;

        //se ha chiusura delle iscrizione, il numero dei giocatori è inferiore a 50
        if(playersInGame < jackpotParticipants)
        {
            jackpotParticipants = playersInGame;
            startDistribution = 1;
        }
        
        uint256 divider = 1275;
       
        singleValue = jackpot/divider;

        jackpotLeft = jackpot;

        emit CloseRegistration(playersInGame, block.timestamp);
    }

    function random(address account) private view returns(uint256)
    {
        uint256 var1 = uint256(keccak256(abi.encodePacked(block.difficulty, account, block.timestamp, totalBalance)));
        uint256 var2 = uint256(keccak256(abi.encodePacked(block.difficulty, account, block.timestamp, playersInGame)));
        uint256 var3 = uint256(keccak256(abi.encodePacked(block.difficulty, account, block.timestamp, totalBalance, playersInGame)));
        uint256 ris1 = var1/var2;
        uint256 ris2 = var2/var1;
        uint256 ris3 = var1/var3;

        uint256 ris = (ris1*3)+ris2+(ris3*(totalBalance/1000000000000000000));

        ris = (ris % playersInGame) + 1;
        return  ris;
    }

    function padulation() external
    {
        // require(block.timestamp > lastPadulation, "try again in a few minutes");
        require(playersInGame > 1, "There are no players to padulated");
        require(gameStarted == true, "game don't start");

        uint256 padulationTurns = 1;

        if(playersInGame>100 && playersInGame<=500){
            padulationTurns = uint256(playersInGame/100) + 1;
        }

        if(playersInGame>500){
            padulationTurns=5;
        }

        //estraggo random da 1 a playersInGame
        address account = _msgSender();
        uint256 padulatedPosition = random(account);

        for(uint256 i=1; i<=padulationTurns; i++)
        {

            //recupero l'utente padulato
            Player memory player = players[padulatedPosition];
            _checkActiveSkill(player, padulatedPosition);
            padulatedPosition = padulatedPosition + 1;
            if(padulatedPosition > playersInGame){
                padulatedPosition = 1;
            }
            
        }

        lastPadulation = block.timestamp + timeInterval;
    }

    function _padulactionProcess(Player memory player, uint256 padulatedPosition) internal
    {
        currentEpoch = currentEpoch + 1;

        uint playedEpochs = (currentEpoch - player.enterEpoch) + 1;
        totalEpoch = totalEpoch + playersInGame - playedEpochs;
        //se le iscrizioni sono aperte, iniziamo a distribuire il jackpot
        if(openEnrollment==0)
        {
            jackpotAward(playersInGame, player.playerAddress);
        }

        playersInGame = playersInGame - 1;
        
        shiftLastUserInPosition(padulatedPosition);
        // updatePlayersAndTotalEpochOnExit(player);
        emit Padulation(player.playerAddress, block.timestamp);
        
    }

    function _checkActiveSkill(Player memory player, uint256 padulatedPosition) internal
    {
        uint256 tokenId = _nftActivated[player.playerAddress];

        //giocatore non ha nft attivi
        if(tokenId == 0)
        {
            _padulactionProcess(player, padulatedPosition);
        }

        //giocatore ha life
        if(tokenId>=1 && tokenId<=100)
        {
            _nftSkillUsage(player.playerAddress, tokenId);
        }

        if(tokenId>=101 && tokenId<=200 && playersInGame>1)
        {
            _activationNftKiller(player.playerAddress, tokenId, padulatedPosition);
        }
    }

    function _activationNftKiller(address playerAddress, uint256 tokenId, uint256 padulatedPosition) internal
    {
        uint256 loop = 4;
        _nftSkillUsage(playerAddress, tokenId);

        address safuPlayer = playerAddress;

        if(playersInGame<5)
        {
            loop = playersInGame - 1;
        }

        for(uint256 i=0; i<loop; i++)
        {
            //prendiamo i giocatori iscritti subito dopo
            padulatedPosition = padulatedPosition + 1;

            //eventualmente uscissimo fuori dalla lista dei giocatori
            if(padulatedPosition > playersInGame && playersInGame>=5){
                padulatedPosition = padulatedPosition - 5;
            }

            if(padulatedPosition > playersInGame && playersInGame<5){
                padulatedPosition = 1;
            }

            Player memory killed = players[padulatedPosition];
            
            //se ricapita il padulato, si salta
            if(killed.playerAddress == safuPlayer){
                loop++;
            }
            else
            {
                //controlliamo se hanno nft attivi. In tal caso, si comportano tutti come saver, se no viene eliminato
                uint256 killedTokenId = _nftActivated[killed.playerAddress];
                if(killedTokenId!=0)
                {
                    _nftSkillUsage(killed.playerAddress, killedTokenId);
                }
                else
                {
                    _padulactionProcess(killed, padulatedPosition);
                }
            }           

        }
    }

    function withdrawal() external
    {
        console.log("withdrawal in");
        address to = _msgSender();

        uint256 tokenId = _nftActivated[to];
        require(tokenId==0, "nft active in play");
        
         //calcolo l'importo prima di modificare gli storage
        uint256 amount = balanceOf(to);
        require(amount !=0, "It is not possible to make the withdrawal, balance = 0");

        //check che un utente non possa uscire nella stessa epoch in cui è entrato
        uint256 position = _playerNumberList[to];

        Player memory player = players[position];
        require(player.enterEpoch <= currentEpoch, "must wait at least 1 epoch to exit");

        uint256 jackpotPosition = _winnersNumberList[to];

        if(jackpotPosition == 0 && playersInGame>1)
        {
            //calcolo delle epoche rimaste. Non necessario se il giocatore è già stato padulato e deve ritirare il suo jackpot
            uint playedEpochs = (currentEpoch - player.enterEpoch) + 1;
            totalEpoch = totalEpoch - playedEpochs;

            playersInGame = playersInGame - 1;

            totalBalance = totalBalance - amount;
        }

        console.log("calcolo epoche ok");

        if(playersInGame==1 && position!=0)
        {
            totalBalance = 0;
            jackpotLeft = 0;
        }
        else
        {
            withdrawJackpot(to, playersInGame);
        }

        console.log("withdrawJackpot");

        //scambio l'utente con l'ultimo per avere sempre la lista allineata
        shiftLastUserInPosition(position);
        // updatePlayersAndTotalEpochOnExit(player);
        IERC20(usdtAddress).transfer(to, amount);
        emit Withdrawal(to, amount, block.timestamp);
    }

    

    function shiftLastUserInPosition(uint position) internal{
        Player memory padulatedPlayer = players[position];
        _playerNumberList[padulatedPlayer.playerAddress] = 0;

        if(position!=playersInGame+1)
        {
            Player storage lastPlayer = players[playersInGame + 1];
            players[position] = lastPlayer;
            _playerNumberList[lastPlayer.playerAddress] = position;
            lastPlayer.playerAddress = address(0);
        }
    }
    

    function isGameStarted() public view returns(bool)
    {
        return gameStarted;
    }

    function startGame() public onlyAdmin
    {
        gameStarted = true;
        emit GameStarted(gameStarted, block.timestamp);
    }
    
    //vediamo se il token è in uso da qualche giocatore
    function isNftActivated(uint256 tokenId) public view returns (uint256)
    {
        uint256 nftInGame = _nftInGame[tokenId];
        return nftInGame;
    }
}

