pragma solidity >=0.7.0 <0.9.0;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";

contract TokenSaleManager is Ownable, ReentrancyGuard {
    int256 public version = 1;
    using SafeMath for uint256;

    //@notice Token sale details, it is constant and does not change
    TokenSale public tokenSale;
    //@notice whitelisted addresses, mapping is not beeing updating while token sale
    mapping(address => bool) public whitelist;
    //@notice keep track of whitelistedUsers;
    address[] whitelistedAddresses;

    //@notice Investment for specific addresses
    mapping(address => uint256) public contributions;
    //@notice Investment for specific addresses
    mapping(address => uint256) public tokensToClaim;
    //@notice Total amount of tokens which are left for sale
    uint256 public totalTokensLeft;
    //@notice Total investment amount
    uint256 public totalContribution;

    struct TokenSale {
        address tokenContract;
        address saleCurrency; // send 0x0 when WETH needs to be used
        uint256 pricePerToken; // price per 1 token in currency wei
        uint256 totalCap; //total cap of gathered funds in ETH wei / ERC20 wei
        address owner;
        uint256 startTime;
        uint256 endTime;
        uint256 lockTime; // equals to endTime if no lock after sale
        uint256 softCap; // softcap in ERC20wei / ETHwei
        uint256 minInvestment; // 0 if no min
        uint256 maxInvestment; // 0 if no max
        bool whitelistEnabled;
    }

    event TokenSaleCreated(address tokenContract, uint256 price, address owner);

    /**
     * @notice Constructor allows to create token sale contract
     * @param newTokenSale New token sale struct
     * @param feeReceiver Receiver of the fee, fee is equal to msg.value
     */
    constructor(TokenSale memory newTokenSale, address payable feeReceiver)
        payable
    {
        require(msg.value > 0, "Deployment fee required");
        feeReceiver.transfer(msg.value);

        tokenSale = newTokenSale;

        if (tokenSale.owner != msg.sender) {
            transferOwnership(tokenSale.owner);
        }

        emit TokenSaleCreated(
            tokenSale.tokenContract,
            tokenSale.pricePerToken,
            tokenSale.owner
        );
    }

    /**
     * @notice Method for updating whitelist
     * @param updatedAddresses New token sale struct
     */
    function updateWhitelist(
        address[] memory updatedAddresses,
        address payable feeReceiver
    ) public payable onlyOwner {
        require(tokenSale.whitelistEnabled, "Whitelist not enabled");
        require(msg.value > 0, "Deployment fee required");
        feeReceiver.transfer(msg.value);

        _removeFromWhitelist(whitelistedAddresses);
        _addManyToWhitelist(updatedAddresses);
        whitelistedAddresses = updatedAddresses;
    }

    function getWhitelistedAddresses() public view returns (address[] memory) {
        return whitelistedAddresses;
    }

    /**
     * @notice Update whitelisted addresses.
     * Possible to invoke only when current block timestamp is
     * approximately 10 minutes before current sale starts.
     * Not possible to update after tokens for sale were deposited.
     * @param updatedTokenSale New token sale struct
     */
    function updateSaleDetails(
        TokenSale memory updatedTokenSale,
        address payable feeReceiver
    ) public payable onlyOwner isSaleUpdatable {
        require(totalTokensLeft == 0, "Tokens already deposited");
        require(msg.value > 0, "Deployment fee required");
        feeReceiver.transfer(msg.value);

        tokenSale = updatedTokenSale;
    }

    /**
     * @notice Method for depositing tokens for sale
     * You need to give allowance to this contract so
     * it may receive your tokens.
     * It is required to deposit tokens for sale and without them
     * token sale won't start.
     * Preferable way of depositing tokens to contract is by this method.
     * @param amountToken Token amount
     */
    function depositTokens(uint256 amountToken) public onlyOwner {
        require(totalTokensLeft == 0, "Tokens already deposited");
        require(
            amountToken == requiredAmountOfTokensForSale(),
            "You need to deposit required amount of tokens for sale"
        );

        IERC20(tokenSale.tokenContract).transferFrom(
            msg.sender,
            address(this),
            amountToken
        );
        totalTokensLeft = amountToken;
    }

    /**
     * @notice Calculate amount for tokens required for sale
     */
    function requiredAmountOfTokensForSale() public view returns (uint256) {
        return _getTokenAmount(tokenSale.totalCap);
    }

    /**
     * @notice Method for buying tokens from sale
     * Possible to invoke only after sale has started and tokens
     * has been deposited successfuly.
     * @param tokenContract Token address used to participate in sale, pass
     * 0x0000000000000000000000000000000000000000 when you want to pay with WETH.
     * @param contribution Amount of tokenContract in WEI
     */
    function buy(uint256 contribution, address tokenContract)
        public
        payable
        nonReentrant
    {
        _preValidatePurchase(contribution);
        _handleIncomingPayment(contribution, tokenContract);

        contributions[msg.sender] = SafeMath.add(
            contributions[msg.sender],
            contribution
        );

        uint256 tokensBeingBought = _getTokenAmount(contribution);

        tokensToClaim[msg.sender] = SafeMath.add(
            tokensToClaim[msg.sender],
            tokensBeingBought
        );

        totalContribution = SafeMath.add(totalContribution, contribution);
        totalTokensLeft = SafeMath.sub(totalTokensLeft, tokensBeingBought);
    }

    /**
     * @notice Method for claiming tokens from sale
     * Possible to invoke only when softcap has been reached, lock is finished
     * and user has participed in the sale.
     */
    function claim(uint256 amountToClaim)
        public
        nonReentrant
        onlyWhenLockFinished
        softcapReached
    {
        require(
            tokensToClaim[msg.sender] > 0,
            "You have no tokens to claim!"
        );

        tokensToClaim[msg.sender] = SafeMath.sub(
            tokensToClaim[msg.sender],
            amountToClaim
        );
        IERC20(tokenSale.tokenContract).transfer(msg.sender, amountToClaim);
    }

    /**
     * @notice Investors can claim refunds here if crowdsale is unsuccessful
     * and has finished. Tokens being refund are based on the sale currency.
     */
    function claimRefund() public saleFinished softcapNotReached nonReentrant {
        if (tokenSale.saleCurrency == address(0)) {
            payable(msg.sender).transfer(contributions[msg.sender]);
        } else {
            IERC20(tokenSale.saleCurrency).transfer(
                msg.sender,
                contributions[msg.sender]
            );
        }
        contributions[msg.sender] = 0;
        tokensToClaim[msg.sender] = 0;
    }

    /**
     * @notice Method for withdrawing funds from sale by owner if crowdsale is successful
     * and finished.
     * Type of the withdrawn funds are based on the sale currency.
     */
    function withdrawFunds() public saleFinished softcapReached onlyOwner {
        if (tokenSale.saleCurrency == address(0)) {
            require(address(this).balance > 0, "No funds to withdraw");
            payable(tokenSale.owner).transfer(address(this).balance);
        } else {
            IERC20 currency = IERC20(tokenSale.saleCurrency);
            require(
                currency.balanceOf(address(this)) > 0,
                "No funds to withdraw"
            );
            currency.transfer(msg.sender, currency.balanceOf(address(this)));
        }
    }

    /**
     * @notice Method for withdrawing tokens from sale by owner if crowdsale is successful
     * and finished.
     * If softcap has been reached, we use "totaltokensLeft" as amount of tokens free to withdraw
     * because rest of the tokens is reserved for investors to claim.
     * If softcap has not been reached, owner is eligible to withdraw all the tokens deposited
     * previously to the contract.
     */
    function withdrawTokens() public saleFinished onlyOwner {
        if (totalContribution >= tokenSale.softCap) {
            IERC20(tokenSale.tokenContract).transfer(
                msg.sender,
                totalTokensLeft
            );
        } else {
            IERC20(tokenSale.tokenContract).transfer(
                msg.sender,
                requiredAmountOfTokensForSale()
            );
        }
        totalTokensLeft = 0;
    }

    /**
     * @notice Method for checking funds amount to withdraw by owner.
     */
    function fundsToWithdraw()
        public
        view
        saleFinished
        softcapReached
        onlyOwner
        returns (uint256)
    {
        if (tokenSale.saleCurrency == address(0)) {
            return address(this).balance;
        }
        return IERC20(tokenSale.saleCurrency).balanceOf(address(this));
    }

    /**
     * @dev Get token amount by multiplying funds in wei by token decimals
     * and dividing result by price per token in wei
     */
    function _getTokenAmount(uint256 fundsInWei)
        internal
        view
        returns (uint256)
    {
        return
            SafeMath.div(
                SafeMath.mul(
                    fundsInWei,
                    10**IERC20(tokenSale.tokenContract).decimals()
                ),
                tokenSale.pricePerToken
            );
    }

    /**
