import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract TokenLocker is Ownable {
    int256 public version = 1;

    using SafeMath for uint256;
    IERC20 token;
    uint256 public lockTime;
    uint256 public unlockTime;

    event TokenLockerCreated(address owner, address token);
    event TokensLocked(
        address token,
        uint256 tokenAmount,
        uint256 unlockTimeToSet
    );
    event TokensReLocked(address token, uint256 newUnlockTimeToSet);

    /**
     * @notice Constructor allows to create token locker contract
     * @param tokenAddress Address of ERC20 token which is going to be locked.
     * It's not possible to lock any other token standard.
     * @param owner owner of token locker, can lock, relock and withdraw tokens
     * after unlock time while locking tokens.
     */
    constructor(address tokenAddress, address owner) {
        token = IERC20(tokenAddress);
        transferOwnership(owner);

        emit TokenLockerCreated(msg.sender, tokenAddress);
    }

    /**
     * @notice Method for receiving token address
     */
    function getTokenAddress() public view returns (address) {
        return address(token);
    }

    /**
     * @notice Method allowing to lock ERC20 tokens.
     * @param tokenAmount amount of tokens to lock.
     * @param unlockTimeToSet unlock timestamp
     * @param feeReceiver receiver of the fee. Fee amount is always equal to msg.value
     */
    function lock(
        uint256 tokenAmount,
        uint256 unlockTimeToSet,
        address payable feeReceiver
    ) public payable onlyOwner {
        require(
            unlockTimeToSet > block.timestamp,
            "Unlock time should be future date"
        );
        require(tokenAmount > 0, "No tokens to lock");

        require(msg.value > 0, "Lock fee required");
        feeReceiver.transfer(msg.value);

        lockTime = block.timestamp;
        unlockTime = unlockTimeToSet;
        token.transferFrom(msg.sender, address(this), tokenAmount);

        emit TokensLocked(address(token), tokenAmount, unlockTimeToSet);
    }

    /**
     * @notice Method allowing to relock currently locked tokens.
     * Possible to relock only when tokens were deposited and new unlock
     * timestamp is after current one.
     * @param newUnlockTime new unlock time to be set
     */
    function relock(uint256 newUnlockTime) public onlyOwner {
        require(
            token.balanceOf(address(this)) > 0,
            "No locked tokens to relock"
        );
        require(
            newUnlockTime > unlockTime,
            "New unlock time must be after current one!"
        );

        unlockTime = newUnlockTime;

        emit TokensReLocked(address(token), newUnlockTime);
    }

    /**
     * @notice Method allowing to withdraw locked tokens.
     * Possible to unlock only after unlock time.
     * @param amount amount of tokens
     * @param receiverAddress receiver of tokens
     */
    function withdraw(address receiverAddress, uint256 amount)
        public
        onlyOwner
    {
        require(
            block.timestamp > unlockTime,
            "Cannot unlock the tokens before unlock time!"
        );

        token.transfer(receiverAddress, amount);
    }

    /**
     * @notice Locked tokens amount
     */
    function getLockedTokensAmount() public view returns (uint256) {
        return token.balanceOf(address(this));
    }
}

interface IERC20 {
    function balanceOf(address account) external view returns (uint256);

    function transfer(address recipient, uint256 amount)
        external
        returns (bool);

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);
}

