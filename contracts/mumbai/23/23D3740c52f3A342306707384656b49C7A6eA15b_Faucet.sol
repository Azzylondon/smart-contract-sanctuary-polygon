        globalSupply = _globalSupply;
        _mint(_msgSender(), _localSupply);
    }

    function setLzEndpoint(ILayerZeroEndpoint _lzEndpoint) external onlyOwner {
        lzEndpoint = _lzEndpoint;
    }

