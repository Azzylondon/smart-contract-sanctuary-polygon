pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./ERC20Mintable.sol";

contract Faucet is AccessControl {
    using EnumerableSet for EnumerableSet.AddressSet;

    EnumerableSet.AddressSet internal tokens;
    mapping(address => uint256) public claimAmounts;
    uint256 public minClaimDelay;
    mapping (address => uint256) public lastClaim;

    constructor (uint256 minClaimDelay_) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        minClaimDelay = minClaimDelay_;
    }

    function setMinClaimDelay(uint256 minClaimDelay_) external onlyRole(DEFAULT_ADMIN_ROLE) {
        minClaimDelay = minClaimDelay_;
    }

    function addClaimTokens(IERC20TestnetMintable[] calldata newTokens_, uint256[] calldata claimAmounts_) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newTokens_.length == claimAmounts_.length, "Faucet: Wrong array len");
        for (uint256 i = 0; i < newTokens_.length; i++) {
            require(newTokens_[i].hasRole(DEFAULT_ADMIN_ROLE, address(this)), "Faucet: No admin role");
            require(tokens.add(address(newTokens_[i])), "Faucet: Already added");
            claimAmounts[address(newTokens_[i])] = claimAmounts_[i];
        }
    }

    function addClaimToken(IERC20TestnetMintable newToken_, uint256 claimAmount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newToken_.hasRole(DEFAULT_ADMIN_ROLE, address(this)), "Faucet: No admin role");
        require(tokens.add(address(newToken_)), "Faucet: Already added");
        claimAmounts[address(newToken_)] = claimAmount;
    }

    function removeClaimToken(IERC20TestnetMintable removeToken_) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(tokens.remove(address(removeToken_)), "Faucet: Not exists");
        delete claimAmounts[address(removeToken_)];
    }

    function canClaim(address user) public view returns(bool) {
        // solhint-disable-next-line not-rely-on-time
        return (block.timestamp - lastClaim[user]) >= minClaimDelay;
    }

    function leftToNextClaim(address user) external view returns(uint256) {
        if (canClaim(user)) {
            return 0;
        }
        // solhint-disable-next-line not-rely-on-time
        return lastClaim[user] + minClaimDelay - block.timestamp;
    }

    function claim() external {
        require(canClaim(msg.sender), "Faucet: Cannot claim yet");
        // solhint-disable-next-line not-rely-on-time
        lastClaim[msg.sender] = block.timestamp;

        address[] memory tokens_ = tokens.values();
        for(uint256 i = 0; i < tokens_.length; i++) {
            IERC20TestnetMintable(tokens_[i]).mintTo(msg.sender, claimAmounts[tokens_[i]]);
        }
    }

    function getSupportedTokens() external view returns(address[] memory) {
        return tokens.values();
    }
}

