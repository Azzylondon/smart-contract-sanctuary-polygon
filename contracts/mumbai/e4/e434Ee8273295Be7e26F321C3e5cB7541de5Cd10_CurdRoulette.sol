pragma solidity ^0.8.4;

import "hardhat/console.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/draft-ERC20PermitUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

/// @custom:security-contact [emailÂ protected]
contract CurdToken is
    Initializable,
    ERC20Upgradeable,
    PausableUpgradeable,
    AccessControlUpgradeable,
    ERC20PermitUpgradeable,
    UUPSUpgradeable
{
    struct Token {
        IERC20Upgradeable tokenAddress;
        uint8 decimals;
    }

    bytes32 public constant LOCKER_ROLE = keccak256("LOCKER_ROLE");
    Token public tokenPairing;
    uint256 public exchangeRate;
    mapping(address => uint256) private _lockedBalance;

    error InsufficientBalance(
        address account,
        address token,
        uint256 amountAvailable,
        uint256 amountWanted
    );

    event Locked(address indexed account, uint256 indexed amount);
    event Unlocked(address indexed account, uint256 indexed amount);
    event WinningDistributed(address indexed account, uint256 indexed amount);
    event LostIncurred(address indexed account, uint256 indexed amount);

    modifier validAccountsAndAmounts(
        address[] memory _accounts,
        uint256[] memory _amounts
    ) {
        require(
            _accounts.length == _amounts.length,
            "_accounts and _amounts param differ in length"
        );
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(Token calldata _token, uint256 _exchangeRate)
        public
        initializer
    {
        __ERC20_init("CurdToken", "CTK");
        __Pausable_init();
        __AccessControl_init();
        __ERC20Permit_init("CurdToken");
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(LOCKER_ROLE, msg.sender);

        exchangeRate = _exchangeRate;
        tokenPairing = _token;
    }

    function setTokenPairing(Token calldata _newTokenPair)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        tokenPairing = _newTokenPair;
    }

    function setExchangeRate(uint256 _exchangeRate)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        exchangeRate = _exchangeRate;
    }

    function mint(address _payer, uint256 _mintAmount) public {
        if (
            !tokenPairing.tokenAddress.transferFrom(
                _payer,
                address(this),
                _mintAmount
            )
        ) {
            revert InsufficientBalance(
                _payer,
                address(tokenPairing.tokenAddress),
                tokenPairing.tokenAddress.balanceOf(_payer),
                _mintAmount
            );
        }
        uint256 mintAmount = getAmountToMintFrom(_mintAmount);
        _mint(_payer, mintAmount);
    }

    function getAmountToMintFrom(uint256 _paidAmount)
        public
        view
        returns (uint256)
    {
        return
            _paidAmount *
            exchangeRate *
            (10**decimals() / 10**tokenPairing.decimals);
    }

    function withdraw(address _account, uint256 _tokenToBurn)
        external
        returns (bool)
    {
        _burn(_account, _tokenToBurn);
        uint256 withdrawTokenAmount = getAmountToWithdrawFrom(_tokenToBurn);
        require(
            tokenPairing.tokenAddress.transfer(_account, withdrawTokenAmount),
            "Error transferring token to user"
        );
        return true;
    }

    function getAmountToWithdrawFrom(uint256 _balance)
        public
        view
        returns (uint256)
    {
        return
            ((_balance / exchangeRate) * 10**tokenPairing.decimals) /
            10**decimals();
    }

    function lock(address _account, uint256 _amount)
        public
        onlyRole(LOCKER_ROLE)
    {
        if (unlockedBalanceOf(_account) < _amount) {
            revert InsufficientBalance(
                _account,
                address(this),
                unlockedBalanceOf(_account),
                _amount
            );
        }
        _lockedBalance[_account] += _amount;
        emit Locked(_account, _amount);
    }

    function lockBatch(
        address[] calldata _accounts,
        uint256[] calldata _amounts
    )
        public
        validAccountsAndAmounts(_accounts, _amounts)
        onlyRole(LOCKER_ROLE)
    {
        for (uint256 i = 0; i < _accounts.length; ++i) {
            address account = _accounts[i];
            uint256 amount = _amounts[i];
            lock(account, amount);
        }
    }

    function unlockBatch(address[] memory _accounts, uint256[] memory _amounts)
        public
        onlyRole(LOCKER_ROLE)
        validAccountsAndAmounts(_accounts, _amounts)
    {
        for (uint256 i = 0; i < _accounts.length; ++i) {
            address account = _accounts[i];
            uint256 amount = _amounts[i];
            unlock(account, amount);
        }
    }

    function unlock(address _account, uint256 _amount)
        public
        onlyRole(LOCKER_ROLE)
    {
        if (lockedBalanceOf(_account) < _amount) {
            revert InsufficientBalance(
                _account,
                address(this),
                lockedBalanceOf(_account),
                _amount
            );
        }
        _lockedBalance[_account] -= _amount;
        emit Unlocked(_account, _amount);
    }

    function distributeWinnings(
        address[] calldata _accounts,
        uint256[] calldata _amounts
    )
        external
        onlyRole(LOCKER_ROLE)
        validAccountsAndAmounts(_accounts, _amounts)
    {
        for (uint256 i = 0; i < _accounts.length; ++i) {
            address account = _accounts[i];
            uint256 winnings = _amounts[i];
            uint256 lockedBalance = lockedBalanceOf(account);

            unlock(account, lockedBalance);
            if (winnings > lockedBalance) {
                _mint(account, winnings - lockedBalance);
                emit WinningDistributed(account, winnings - lockedBalance);
            } else if (winnings < lockedBalance) {
                _burn(account, lockedBalance - winnings);
                emit LostIncurred(account, lockedBalance - winnings);
            }
        }
    }

    function lockedBalanceOf(address _account) public view returns (uint256) {
        return _lockedBalance[_account];
    }

    function unlockedBalanceOf(address _account) public view returns (uint256) {
        return balanceOf(_account) - lockedBalanceOf(_account);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);

        require(!paused(), "ERC20Pausable: token transfer while paused");
        if (from == msg.sender && unlockedBalanceOf(from) < amount) {
            revert InsufficientBalance(
                from,
                address(this),
                unlockedBalanceOf(from),
                amount
            );
        }
    }

    function pause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}
}

