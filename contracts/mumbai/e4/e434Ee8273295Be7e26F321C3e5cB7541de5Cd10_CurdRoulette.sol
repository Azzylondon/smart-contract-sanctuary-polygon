import "hardhat/console.sol";
import "./CurdToken.sol";
import "./CurdRouletteNft.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

/// @custom:security-contact [emailÂ protected]
contract CurdRoulette is
    Initializable,
    PausableUpgradeable,
    AccessControlUpgradeable,
    UUPSUpgradeable
{
    enum BetType {
        Single,
        Odd,
        Even,
        Red,
        Black
    }

    enum GameStage {
        WaitingToStart,
        OpenToBet,
        PendingDistribution
    }

    struct PlayerBet {
        address player;
        uint256 betAmount;
        BetType betType;
    }

    mapping(uint256 => bytes32) private gameHash;
    mapping(uint256 => mapping(address => PlayerBet[])) public roomBets;
    mapping(uint256 => address[]) playersPerRoom;
    mapping(uint256 => GameStage) public gameStage;
    CurdRouletteTable public rouletteNft;
    CurdToken public curdTokenContract;

    error InvalidRoomId(uint256 roomId);

    event BettingOpened(uint256 indexed roomId);
    event Bet(
        address indexed better,
        BetType indexed betType,
        uint256 indexed amount
    );
    event BettingClosed(uint256 indexed roomId);
    event Spined(uint256 indexed roomId, uint8 indexed spinValue);

    modifier onlyOperators(uint256 _roomId) {
        // calling the ownerOf method will often revert on its own if the address is 0
        if (
            rouletteNft.userOf(_roomId) == msg.sender ||
            rouletteNft.ownerOf(_roomId) == msg.sender
        ) {
            revert InvalidRoomId(_roomId);
        }
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        CurdRouletteTable _rouletteNft,
        CurdToken _curdTokenContract
    ) public initializer {
        __Pausable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        rouletteNft = _rouletteNft;
        curdTokenContract = _curdTokenContract;
    }

    function startGame(uint256 _roomId, bytes32 _hash)
        external
        onlyOperators(_roomId)
        returns (bool)
    {
        require(
            gameStage[_roomId] == GameStage.WaitingToStart,
            "Game Underway. Please end betting and/or distribute winnings first"
        );

        gameHash[_roomId] = _hash;
        gameStage[_roomId] = GameStage.OpenToBet;
        emit BettingOpened(_roomId);
        return true;
    }

    function bet(uint256 _roomId, PlayerBet calldata _bet)
        external
        returns (bool)
    {
        require(
            gameStage[_roomId] == GameStage.OpenToBet,
            "Roulette Game Room not open to bet yet"
        );
        if (roomBets[_roomId][_bet.player].length == 0) {
            // New player joining the room
            require(
                playersPerRoom[_roomId].length < 7,
                "Too many players in current room:"
            );
            playersPerRoom[_roomId].push(_bet.player);
        }
        curdTokenContract.lock(_bet.player, _bet.betAmount);
        PlayerBet[] storage bets = roomBets[_roomId][_bet.player];
        bets.push(_bet);
        emit Bet(_bet.player, _bet.betType, _bet.betAmount);
        return true;
    }

    function closeBetting(uint256 _roomId)
        external
        onlyOperators(_roomId)
        returns (bool)
    {
        require(
            gameStage[_roomId] == GameStage.OpenToBet,
            "Cannot close a game room that is not open of betting"
        );
        gameStage[_roomId] = GameStage.PendingDistribution;
        emit BettingClosed(_roomId);
        return true;
    }

    function distributeWinnings(
        uint256 _roomId,
        uint256 _seed,
        bytes32 _salt
    ) external returns (bool) {
        require(
            gameStage[_roomId] == GameStage.PendingDistribution,
            "Roulette Game Room is not ready to have winnings distributed"
        );
        require(
            keccak256(abi.encodePacked(msg.sender, _seed, _salt)) ==
                gameHash[_roomId],
            "invalid hash"
        );
        uint8 spin = uint8(_seed % 37);
        emit Spined(_roomId, spin);

        address[] memory players = playersPerRoom[_roomId];
        for (uint8 i = 0; i < players.length; ++i) {
            address player = players[i];
            PlayerBet[] memory playerBets = roomBets[_roomId][player];
            uint256 winnings = 0;
            uint256 betAmount = 0;
            for (uint16 j = 0; j < playerBets.length; ++j) {
                PlayerBet memory playerBet = playerBets[j];
                betAmount += playerBet.betAmount;
            }
            // curdTokenContract.distributeWinnings([player], [winnings]);
        }

        address[] memory winners = new address[](7);
        uint256[] memory amounts = new uint256[](7);

        curdTokenContract.distributeWinnings(winners, amounts);

        gameStage[_roomId] = GameStage.WaitingToStart;
        return true;
    }

    function pause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}
}

