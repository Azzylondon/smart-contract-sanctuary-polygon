import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

contract CurdRouletteTable is
    IERC721AUpgradeable,
    ERC721AUpgradeable,
    ERC721ABurnableUpgradeable,
    ERC721AQueryableUpgradeable,
    ERC4907AUpgradeable,
    Initializable,
    AccessControlUpgradeable,
    UUPSUpgradeable
{
    string public baseURI;
    IERC20 public currency;
    uint256 public price;
    address rouletteOperator;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        address _currency,
        uint256 _price,
        address _rouletteOperator,
        string calldata _newBaseURI
    ) public initializerERC721A initializer {
        __ERC721A_init("Curd Roulette", "CRT");
        __ERC4907A_init();
        __ERC721ABurnable_init();
        __ERC721AQueryable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        currency = IERC20(_currency);
        price = _price;
        rouletteOperator = _rouletteOperator;
        baseURI = _newBaseURI;
    }

    function mint(address _to, uint256 _quantity) external payable {
        require(
            currency.transferFrom(_to, address(this), _quantity * price),
            "Insufficient amount"
        );
        uint64 expires = 1924934400; // 2030 dec 31st
        setUser(_nextTokenId(), rouletteOperator, expires);
        _mint(_to, _quantity);
    }

    function adminMint(address _to, uint256 _quantity)
        external
        payable
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _mint(_to, _quantity);
    }

    function setRouletteOperator(address _rouletteOperator)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        rouletteOperator = _rouletteOperator;
    }

    function setPrice(uint256 _newPrice) external onlyRole(DEFAULT_ADMIN_ROLE) {
        price = _newPrice;
    }

    function setCurrency(address _currency)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        currency = IERC20(_currency);
    }

    function setBaseURI(string calldata _newBaseURI)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        baseURI = _newBaseURI;
    }

    function _baseURI()
        internal
        view
        override(ERC721AUpgradeable)
        returns (string memory)
    {
        return baseURI;
    }

    // The following functions are overrides required by Solidity.
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(
            IERC721AUpgradeable,
            ERC721AUpgradeable,
            ERC4907AUpgradeable,
            AccessControlUpgradeable
        )
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfers(
        address from,
        address to,
        uint256 tokenId,
        uint256 quantity
    ) internal override(ERC721AUpgradeable) {
        // if (from != to && _users[tokenId].user != address(0)) {
        //     delete _users[tokenId];
        //     emit UpdateUser(tokenId, address(0), 0);
        // }
        super._beforeTokenTransfers(from, to, tokenId, quantity);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(DEFAULT_ADMIN_ROLE)
    {}
}

