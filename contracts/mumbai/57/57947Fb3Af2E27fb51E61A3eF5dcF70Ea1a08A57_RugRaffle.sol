pragma solidity ^0.8.9;

import "@0xessential/contracts/fwd/EssentialERC2771Context.sol";
import "@0xessential/contracts/fwd/IForwardRequest.sol";
import "hardhat/console.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract RugRaffle is EssentialERC2771Context, VRFConsumerBase {
    /// @notice gas lane for VRF
    bytes32 internal KEY_HASH;
    /// @notice Link address wrapped in ERC20 interface
    IERC20 public immutable LINK_TOKEN;
    /// @notice end time for raffle
    uint256 public raffleEndTime;
    /// @notice Gas limit for VRF
    uint256 internal vrfFee;
    /// @notice randomness for raffle drawing from Chainlink VRF
    uint256 public entropy;
    /// @notice VRF entropy set for raffle clearing
    bool public clearingEntropySet = false;
    /// @notice raffle should only be shuffled once
    bool shuffled = false;

    event RaffleEntry(uint256 indexed entryId, address indexed entrant, address indexed nftContract, uint256 tokenId);
    event RaffleShuffled(address indexed shuffler, uint256 indexed numberOfWinners);

    error TokenAlreadyEntered(address nftContract, uint256 tokenId);
    error ProjectNotAllowed(address nftContract);

    mapping(address => mapping(uint256 => address)) public enteredTokens;
    mapping(address => uint256) public raffleEntriesForAddress;
    mapping(bytes => bool) public allowedProjects;
    uint256[] public raffleEntries;

    using Counters for Counters.Counter;
    Counters.Counter private _entryIds;

    constructor(
        address _trustedForwarder,
        bytes32 _keyhash,
        address _VRFCoordinator,
        address _LinkToken,
        uint256 _raffleEndTime
    ) EssentialERC2771Context(_trustedForwarder) VRFConsumerBase(_VRFCoordinator, _LinkToken) {
        LINK_TOKEN = IERC20(_LinkToken);
        KEY_HASH = _keyhash;
        vrfFee = 0.0001 * 10**18;
        raffleEndTime = _raffleEndTime;
    }

    function enterRaffleWithToken() external onlyForwarder {
        IForwardRequest.NFT memory nft = _msgNFT();

        // ensure raffle hasnt ended
        require(block.timestamp <= raffleEndTime, "Raffle has ended");

        // ensure project is allowed
        bytes memory id = buildAllowedProjectId(nft.chainId, nft.contractAddress);
        if (allowedProjects[id] == false) {
            revert ProjectNotAllowed(nft.contractAddress);
        }

        // ensure token hasn't already been entered
        if (enteredTokens[nft.contractAddress][nft.tokenId] != address(0)) {
            revert TokenAlreadyEntered(nft.contractAddress, nft.tokenId);
        }

        enteredTokens[nft.contractAddress][nft.tokenId] = _msgSender();
        uint256 entryId = enterEOAInRaffle(_msgSender());

        emit RaffleEntry(entryId, _msgSender(), nft.contractAddress, nft.tokenId);
    }

    function enterEOAInRaffle(address _eoa) internal returns (uint256) {
        _entryIds.increment();

        uint256 entryId = _entryIds.current();
        raffleEntries.push(entryId);
        raffleEntriesForAddress[_eoa] += 1;
        return entryId;
    }

    function addAllowedProject(uint256 chainId, address allowedProjectAddress) external onlyOwner {
        bytes memory id = buildAllowedProjectId(chainId, allowedProjectAddress);
        allowedProjects[id] = true;
    }

    function buildAllowedProjectId(uint256 chainId, address allowedProjectAddress)
        internal
        pure
        returns (bytes memory)
    {
        bytes memory id = new bytes(28);
        assembly {
            mstore(add(id, 28), allowedProjectAddress)
            mstore(add(id, 4), chainId)
            mstore(id, 28)
        }

        return id;
    }

    function setClearingEntropy() external {
        //Ensure contract has enough LINK to make VRF randomness call
        require(LINK_TOKEN.balanceOf(address(this)) >= vrfFee, "Insufficient LINK");
        //Ensure entropy not already set
        require(!clearingEntropySet, "Entropy already set");
        requestRandomness(KEY_HASH, vrfFee);
    }

    function getRaffleEntries() public view returns (uint256[] memory) {
        return raffleEntries;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        entropy = randomness;
        clearingEntropySet = true;
    }

    function setRaffleEndTime(uint256 _raffleEndTime) external onlyOwner {
        raffleEndTime = _raffleEndTime;
    }

    function clearRaffle(uint256 numOfWinners) external {
        // Ensure entropy has been set
        require(clearingEntropySet, "Entropy not set");
        // Ensure raffle hasn't already been cleared
        require(!shuffled, "Raffle has already been cleared");
        // Ensure raffle requires clearing (entries !< numOfWinners)
        require(raffleEntries.length > numOfWinners, "Raffle does not need clearing");

        // Run Fisher-Yates shuffle for AVAILABLE_SUPPLY
        for (uint256 i = 0; i < numOfWinners; i++) {
            // Generate a random index to select from
            uint256 randomIndex = i + (entropy % (raffleEntries.length - i));
            // Collect the value at that random index
            uint256 randomTmp = raffleEntries[randomIndex];
            // Update the value at the random index to the current value
            raffleEntries[randomIndex] = raffleEntries[i];
            // Update the current value to the value at the random index
            raffleEntries[i] = randomTmp;
        }

        // Update number of shuffled entries
        shuffled = true;

        //Emit successful shuffle
        emit RaffleShuffled(msg.sender, numOfWinners);
    }

    /*
    I think we'll allow winners to mint ERC1155 Mint Passes - another thing we are
    considering is mint passes with various levels - i.e. mint cost at various levels
    would be Pass + Free, Pass + 0.04 ETH, Pass + 0.08 ETH, so working through the
    winner selection it would be nice to be able to be able to assign the level.
    */
}

