// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts/interfaces/IERC1155.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "./interfaces/ISpaceShip.sol";

contract GamePool is IERC721Receiver, Initializable, ContextUpgradeable, IERC1155Receiver {

    using SafeMath for uint256;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    event NftStaked(address staker, uint256 tokenId, uint8 shipTypeId);
    event NftUnstaked(address staker, uint256 tokenId, uint128 shipTypeId);

    event NftCardStaked(address staker, uint256 tokenId, uint256 amount);
    event NftCardUnstaked(address staker, uint256 tokenId, uint256 amount);

    event PlanetNftStaked(address staker, uint256 planetTokenId);
    event PlanetNftUnstaked(address staker, uint256 planetTokenId);


    /**
     * Used to represent the current staking status of a ship.
     * Optimised for use in storage.
     */
    struct ShipInfo {
        uint256 shipId;
        address owner;
        uint8 shipTypeId;
    }

    
    /**
     * Used to represent the current staking status of a planet.
     * Optimised for use in storage.
     */
    struct PlanetInfo {
        uint256 planetId;
        address owner;
    }

    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

     /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(_owner == _msgSender(), "Ownable: caller is not the owner");
        _;
    }
    
    ISpaceShip public whitelistedShip;


    //--------Main State---------//

    /* ship Id => is in gaming */
    mapping(uint256 => bool) public gaming;

    /* ship nft Id => token info */
    mapping(uint256 => ShipInfo) public ships;

    /* staker =>ship nft token ids */
    mapping(address => EnumerableSetUpgradeable.UintSet) stakedShips;

    IERC1155 public nftCard;
    /* staker => card id => amount*/
    mapping(address=>mapping(uint256=>uint256)) public cardStakers;
    
        
    //--------Main State---------//
    IERC721 public whitelistedPlanet;

    /* planet Id => is in gaming */
    mapping(uint256 => bool) public planet_gaming;

    /* planet nft Id => token info */
    mapping(uint256 => PlanetInfo) public planets;

    /* staker =>planet nft token ids */
    mapping(address => EnumerableSetUpgradeable.UintSet) stakedPlanets;

    //---------------------------//

    constructor() {
    }
    /**
     * @dev initialize
     */
    function initialize(ISpaceShip whitelistedShip_, IERC721 whitelistedPlanet_) public initializer {
        whitelistedShip = whitelistedShip_;
        whitelistedPlanet = whitelistedPlanet_;
        _owner=_msgSender();
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }

    /**
     * @dev Returns the bep token owner.
     */
    function getOwner() external view returns (address) {
        return _owner;
    }

    /* get staked ship list of a staker */
    function stakedShipsOf(address _staker) 
        external 
        view
        returns( uint256[] memory )
    {
        uint256[] memory tokens = new uint256[](stakedShips[_staker].length());
        for(uint i = 0; i < stakedShips[_staker].length(); i++){
            tokens[i] = stakedShips[_staker].at(i);
        }
        return tokens;
    }

    /* get staked planet list of a staker */
    function stakedPlanetsOf(address _staker) 
        external 
        view
        returns( uint256[] memory )
    {
        uint256[] memory tokens = new uint256[](stakedPlanets[_staker].length());
        for(uint i = 0; i < stakedPlanets[_staker].length(); i++){
            tokens[i] = stakedPlanets[_staker].at(i);
        }
        return tokens;
    }

    /**
     * ERC721Receiver hook for single transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC721Received(
        address, /*operator*/
        address from,
        uint256 id,
        bytes calldata /*data*/
    ) external override returns (bytes4) {
        require(address(whitelistedShip) == _msgSender() || address(whitelistedPlanet) == _msgSender(), "Game Pool: not a spaceship or planet");
        if (address(whitelistedShip) == _msgSender()) {
            _stakeNft(id, from, whitelistedShip.getType(id));
        } else {
            _stakePlanetNft(id, from);
        }
        return this.onERC721Received.selector;
    }

    /*                                             ERC1155TokenReceiver                                             */

    /**
     * ERC1155Receiver hook for single transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC1155Received(
        address, /*operator*/
        address from,
        uint256 id,
        uint256 value,
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        require(address(nftCard) == _msgSender(), "contract not whitelisted");
        cardStakers[from][id]+=value;
        emit NftCardStaked(from, id, value);        
        return this.onERC1155Received.selector;
    }

    /**
     * ERC1155Receiver hook for batch transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC1155BatchReceived(
        address, /*operator*/
        address, /*from*/
        uint256[] calldata, /* ids*/
        uint256[] calldata, /* values*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function supportsInterface(bytes4 interfaceId) external pure override returns (bool) {
        return interfaceId == type(IERC1155Receiver).interfaceId;
    }

    function setNftCard(IERC1155 _nftCard) external onlyOwner {
        nftCard = _nftCard;
    }

    function setWhitelistedPlanet(IERC721 _planet) external onlyOwner {
        whitelistedPlanet = _planet;
    }

    function unstakeNftCard(uint256 tokenId, uint256 amount) external {
        require(cardStakers[_msgSender()][tokenId]>=amount, 'Game Pool: too big amount than yours staked');
        cardStakers[_msgSender()][tokenId]-=amount;
        nftCard.safeTransferFrom(address(this), _msgSender(), tokenId, amount, "");
        emit NftCardUnstaked(_msgSender(), tokenId, amount);
    }

    function _stakeNft(uint256 shipId, address from, uint8 shipTypeId) private
    {
        stakedShips[from].add(shipId);
        ships[shipId] = ShipInfo(shipId, from, shipTypeId);
        gaming[shipId] = true;
        emit NftStaked(from, shipId, shipTypeId);
    }

    function unstakeNft(uint256 shipId) external {
        require(ships[shipId].owner == _msgSender(), "Game Pool: not staked for owner");
        gaming[shipId] = false;
        stakedShips[_msgSender()].remove(shipId);
        whitelistedShip.safeTransferFrom(address(this), _msgSender(), shipId);
        emit NftUnstaked(_msgSender(), shipId, whitelistedShip.getType(shipId));
        delete ships[shipId];
    }


    
    function _stakePlanetNft(uint256 planetId, address from) private
    {
        stakedPlanets[from].add(planetId);
        planets[planetId] = PlanetInfo(planetId, from);
        planet_gaming[planetId] = true;
        emit PlanetNftStaked(from, planetId);
    }

    function unstakePlanetNft(uint256 planetId) external {
        require(planets[planetId].owner == _msgSender(), "Game Pool: not staked for owner");
        planet_gaming[planetId] = false;
        stakedPlanets[_msgSender()].remove(planetId);
        whitelistedPlanet.safeTransferFrom(address(this), _msgSender(), planetId);
        emit PlanetNftUnstaked(_msgSender(), planetId);
        delete planets[planetId];
    }


    function withdrawToken(address token) external onlyOwner {
        IERC20(token).transfer(_owner, IERC20(token).balanceOf(address(this)));
    }
}

