/*

Deploy Guide:

Create subscription for chainlink on mainnet.   
https://vrf.chain.link/mumbai
https://vrf.chain.link/polygon

Testnet Verification Tx:
https://mumbai.polygonscan.com/tx/0x747b927a3213c0dd4eac47f46db1c31696baf358931a3932d829f3e461273796






Create and fund a subscription

For this example, create a new subscription on the Rinkeby testnet.

Open MetaMask and set it to use the Rinkeby testnet. The Subscription Manager detects your network based on the active network in MetaMask.
Check MetaMask to make sure you have testnet ETH and LINK on Rinkeby. If you need testnet funds, you can get them from faucets.chain.link.
Open the Subscription Manager page.
Click Create Subscription and follow the instructions to create a new subscription account. MetaMask opens and asks you to confirm payment to create the account on-chain. After you approve the transaction, the network confirms the creation of your subscription account on-chain.
After the subscription is created, click Add funds and follow the instructions to fund your subscription. For this example, a balance of 2 LINK is sufficient. MetaMask opens to confirm the LINK transfer to your subscription. After you approve the transaction, the network confirms the transfer of your LINK token to your subscription account.
After you add funds, click Add consumer. A page opens with your account details and subscription ID.
Record your subscription ID, which you need for your consumer contract. You will add the consumer to your subscription later.

You can always find your subscription IDs, balances, and consumers on the Subscription Manager page.

Now that you have a funded subscription account and your subscription ID, create and deploy a VRF v2 compatible contract.












*/


contract RiotFactoryParts is ERC721, ERC721Enumerable, ERC721URIStorage, ERC721Burnable, Ownable, VRFConsumerBaseV2 {

    using Counters for Counters.Counter;
    


    









    // Chainlink VRF
    VRFCoordinatorV2Interface COORDINATOR;
    uint64 s_subscriptionId;

    // address vrfCoordinator = 0xAE975071Be8F8eE67addBC1A82488F1C24858067;    // TODO - uncomment this, it is live polygon coordinator
    address vrfCoordinator = 0x7a1BaC17Ccc5b313516C5E16fb24f7659aA5ebed;    // TODO - delete this testnet polygon coordinator

    // TODO - just use the keyhash Fast for this, so the 1000 gwei gas premium for MATIC
    // delete the below probably
    // bytes32 keyHashFast = 0xd729dc84e21ae57ffb6be0053bf2b0668aa2aaf300a2a7b2ddf7dc0bb6e875a8;   // TODO - uncomment this, it is live polygon coordinator
    // bytes32 keyHashMedium = 0xcc294a196eeeb44da2888d17c0625cc88d70d9760a69d58d853ba6581a9ab0cd;   // TODO - uncomment this, it is live polygon coordinator
    // bytes32 keyHashSlow = 0x6e099d640cde6de9d40ac749b4b594126b0169747122711109c9985d47751f93;   // TODO - uncomment this, it is live polygon coordinator

    // bytes32 keyHash = 0xd729dc84e21ae57ffb6be0053bf2b0668aa2aaf300a2a7b2ddf7dc0bb6e875a8;   // TODO - uncomment this, it is live polygon coordinator
    bytes32 keyHash = 0x4b09e658ed251bcafeebbc69400383d49f344ace09b9576fe248bb02c003fe9f;   // TODO - delete this testnet polygon coordinator

    uint32 callbackGasLimit = 100000;
    uint16 requestConfirmations = 5;
    uint32 numWords =  3;
    uint256[] public s_randomWords;
    uint256 public s_requestId;
    address s_owner;

    


    Counters.Counter private _tokenIdCounter;
    mapping(uint256 => address) public mappingRequestIdToAddressToMintTo;
    mapping(uint256 => string) public mappingIpfsIdToIpfsHashStringForTokenUri;
    mapping(uint256 => bool) public mappingIpfsIdToHasTokenUri;
    mapping(uint256 => bool) public mappingIpfsIdToHasMinted;
    mapping(address => bool) public isMinting;
    uint256 public maxSupplyAllParts = 10000;

    // bool public isMintingEnabled = false;   // TODO - set this to false on release, for testing should be on true, reason is because we don't want someone to try to instantly mint when it is deployed
    bool public isMintingEnabled = true;









    constructor() ERC721("RIOT FACTORY", "RF") VRFConsumerBaseV2(vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        s_owner = msg.sender;
        s_subscriptionId = 1020;       // TODO - update with the correct subscription ID here.
    }


    // Standard Overrides for ERC721

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable){
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory){
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool){
        return super.supportsInterface(interfaceId);
    }

    // set base to ipfs
    function _baseURI() internal pure override returns (string memory) {
        return "ipfs://";
    }





    
    function setIsMintingEnabled(bool enableOrDisable) public onlyOwner {
        isMintingEnabled = enableOrDisable;
    }



    function earliestUnmintedIpfsId() private view returns (uint256) {
        for(uint256 i = 0; i < 10000; i++){
            if(!mappingIpfsIdToHasMinted[i]){
                return i;
            }
        }
        return 1234567890;
    }



    // Initialization can only happen once per NFT otherwise it would be open to explotiation
    // owner can only initialize, this can be see as pre-minting
    bool public isInitializationAllowed = true;
    function setIsInitializationAllowedToFalse() public onlyOwner {
        isInitializationAllowed = false;        // set to false after Initialization is completed
    }

    function initializeIpfs(uint256 ipfsHashId, string memory ipfsHashString) public onlyOwner {
        require(isInitializationAllowed == true, "Initialiation is no longer allowed, the NFT IPFS data has been assigned.");
        mappingIpfsIdToIpfsHashStringForTokenUri[ipfsHashId] = ipfsHashString;     // ipfs hash id will range from 0 to 9999.
        // this means you only need a random number from 0 to 9999, then just have it mint to that.
        // the initialization process handles all the rarity you need.

        mappingIpfsIdToHasTokenUri[ipfsHashId] = true;
    }






    function safeMint(address to) public onlyOwner {

        require(_tokenIdCounter.current() <= maxSupplyAllParts, "Max is 10,000 Part NFTs.");

        require(isMintingEnabled == true, "Minting must be enabled");

        // re-entrancy per address
        require(!isMinting[to], "Minter must not already have a mint in progress.");
        require(!isMinting[_msgSender()], "Minter must not already have a mint in progress.");
        isMinting[to] = true;
        isMinting[_msgSender()] = true;

        requestRandomWords(to);

        isMinting[to] = false;
        isMinting[_msgSender()] = false;

    }

    // call chainlink VRF to determine which part you will get
    // check to see how many exist of that part type
    // if the part type has reached its max
    // if it has not reached the max, then mint it
    // if it has reached the max, then check which part types have not hit the max
    // add these part types, using ints to a mapping
    // generate a random number based on the number that has not reached the max limit
    // use that random number to determine which part type they will mint
    // mint that part type






    

    // Chainlink VRF functions
    // Assumes the subscription is funded sufficiently.
    function requestRandomWords(address to) private {
        uint256 requestId = COORDINATOR.requestRandomWords(keyHash, s_subscriptionId, requestConfirmations, callbackGasLimit, numWords);  // Will revert if subscription is not set and funded.
        mappingRequestIdToAddressToMintTo[requestId] = to;
        s_requestId = requestId;
    }


    // TODO - if this doesn't work, make two different functions
    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        s_randomWords = randomWords;

        uint256 randomNum1 = (randomWords[0] % 10000);  // 0 to 9999
        uint256 randomNum2 = (randomWords[1] % 10000);
        uint256 randomNum3 = (randomWords[2] % 10000);



        // rarity of 50%
        // rarity of 30%
        // rarity of 15%
        // rarity of 4%
        // rarity of 1%


        // finish the minting
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        // _safeMint(to, tokenId);
        // _setTokenURI(tokenId, uri);
        _safeMint(mappingRequestIdToAddressToMintTo[requestId], tokenId);

        if(!mappingIpfsIdToHasMinted[randomNum1]){
            mappingIpfsIdToHasMinted[randomNum1] = true;
            _setTokenURI(tokenId, mappingIpfsIdToIpfsHashStringForTokenUri[randomNum1]);
        }
        else if (!mappingIpfsIdToHasMinted[randomNum2]){
            mappingIpfsIdToHasMinted[randomNum2] = true;
            _setTokenURI(tokenId, mappingIpfsIdToIpfsHashStringForTokenUri[randomNum2]);
        }
        else if (!mappingIpfsIdToHasMinted[randomNum3]){
            mappingIpfsIdToHasMinted[randomNum3] = true;
            _setTokenURI(tokenId, mappingIpfsIdToIpfsHashStringForTokenUri[randomNum3]);
        }
        else {
            // if all 3 random numbers cannot be set, then just go to the nearest unfilled.
            uint256 mintToId = earliestUnmintedIpfsId();
            mappingIpfsIdToHasMinted[mintToId] = true;
            _setTokenURI(tokenId, mappingIpfsIdToIpfsHashStringForTokenUri[mintToId]);
            

            // // if all 3 randomly generated numbers have failed then we are going to do some combinations in order to increase randomness, before just giving someone their NFT
            // uint256 randomNum4 = ((randomNum1 + randomNum1) % 10000);
            // uint256 randomNum5 = ((randomNum1 + randomNum2) % 10000);
            // uint256 randomNum6 = ((randomNum1 + randomNum3) % 10000);
            // uint256 randomNum7 = ((randomNum2 + randomNum2) % 10000);
            // uint256 randomNum8 = ((randomNum2 + randomNum3) % 10000);
            // uint256 randomNum9 = ((randomNum3 + randomNum3) % 10000);
        }




        


        
    }

    
        




    



    


    




    


    

    








    






    



    // MATIC and Token Rescue Functions
    event MaticCollectedFromContract(address indexed collectorAddress, uint256 maticAmountCollected, uint256 timeCollected);
    function GetAllMaticInContract() external onlyOwner  {   
        emit MaticCollectedFromContract(_msgSender(), address(this).balance, block.timestamp);
        payable(_msgSender()).transfer(address(this).balance);
    }

    event TokenCollectedFromContract(address indexed tokenAddress, address indexed collectorAddress, uint256 tokenAmountCollected, uint256 timeCollected);
    function GetAllTokenInContract(address tokenAddress) external onlyOwner  {
        IERC20 tokenAddressAsTokenInterface = IERC20(tokenAddress);
        emit TokenCollectedFromContract(tokenAddress, _msgSender(), tokenAddressAsTokenInterface.balanceOf(address(this)), block.timestamp);
        tokenAddressAsTokenInterface.transfer(_msgSender(), tokenAddressAsTokenInterface.balanceOf(address(this))); 
    }




    receive() external payable {}








    // TODO - remove
    function selfDestructContract() public onlyOwner { 
        selfdestruct(payable(owner())); 
    }



}

