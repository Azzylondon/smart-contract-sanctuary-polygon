 */
abstract contract ChainLinkBase {
    VRFCoordinatorV2Interface internal coordinator;
    LinkTokenInterface internal linkToken;
    AggregatorV3Interface internal maticFeed;
    uint64 internal subscriptionId;

    /**
     * @dev Only used when deploying in local hardhat network to fund the mocked VRF subscription
     */
    function fund(uint96 amount) public {
        linkToken.transferAndCall(
            address(coordinator),
            amount,
