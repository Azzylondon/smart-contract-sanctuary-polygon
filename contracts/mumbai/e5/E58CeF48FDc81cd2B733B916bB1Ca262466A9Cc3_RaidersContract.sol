/// @notice The struct used to represent a staked worker
/// @dev `home` and `isPuzzle` are used to identify the CASTLE token that the worker is asociated with.
/// @dev `home` represents the tokenId, while `isPuzzle` shows if its a piece or puzzle token
/// @dev `landlord` and `taxesCollectedByLandlord` are used for the renting functionality.
struct Worker {
    uint256 tokenId;
    uint256 timestamp;
    address owner;
    uint256 stolen;
    uint256 taxesCollectedByLandlord;
    uint256 home;
    address landlord;
    bool isPuzzle;
    uint256 raided;
}

/**
 * @title WorkerStakingContractInterface
 */
interface WorkerStakingContractInterface {
    function claimableRaidersTaxes() external view returns (uint256);

    function calculateRewardsForManyWorkers(uint256[] memory tokenIds)
        external
        view
        returns (
            uint256[] memory rewards,
            uint256[] memory landlordTaxes,
            uint256[] memory raidersTaxes
        );

    function getStakedWorker(uint256 tokenId)
        external
        view
        returns (Worker memory worker);

    function getStakedWorkersLandlord(uint256 tokenId)
        external
        view
        returns (address landlord);

    function getStakedWorkersOwner(uint256 tokenId)
        external
        view
        returns (address owner);

    function updateStakedWorker(
        uint256 tokenId,
        uint256 _home,
        bool _isPuzzle,
        address _landlord
    ) external;

    function setTaxesCollectedByLandLordForWorker(
        uint256 tokenId,
        uint256 taxesCollectedByLandlord
    ) external;

    function setStolenForWorker(uint256 tokenId, uint256 stolen) external;
}

