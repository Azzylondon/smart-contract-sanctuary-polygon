import "./base/ChainLinkBase.sol";
import "./base/VRFConsumerBaseUpgradeableV2.sol";

import "./interfaces/CrownsContractInterface.sol";
import "./interfaces/WorkerStakingContractInterface.sol";

import "hardhat/console.sol";

/**
 * @title RaidersContract
 */
contract RaidersContract is
    ChainLinkBase,
    OwnableUpgradeable,
    VRFConsumerBaseUpgradeableV2
{
    modifier onlyCitizenContract() {
        require(_msgSender() == address(citizenContr), "INVALIDOPERATION");
        _;
    }

    struct Raider {
        uint256 timestamp;
        uint256 dailyCount;
        uint256 taxesClaimed;
    }

    struct RaidRequest {
        uint256 tokenId;
        address owner;
    }

    event RaidRequested(address raidersOwner, uint256 requestId);
    event RaidDone(address raidersOwner, uint256 amountStolen);

    IERC721EnumerableUpgradeable citizenContr;
    CrownsContractInterface crownsContr;
    WorkerStakingContractInterface workerStakingContr;

    uint256[6] public RAID_PRICES;
    uint8 public constant RAID_PERCENTAGE = 30;

    uint256 public totalTaxesClaimed;
    uint256[] public indexedRaiders;

    mapping(uint256 => bool) public tokenIdIsRaider;
    mapping(uint256 => Raider) public lurkingRaiders;
    mapping(uint256 => RaidRequest) public requestedRaids;

    function initialize(
        address _vrfCoordinator,
        address _link,
        uint64 _subscriptionId,
        address _maticFeed,
        CrownsContractInterface _crownsContr,
        IERC721EnumerableUpgradeable _citizenContr
    ) public initializer {
        __VRFConsumerBaseUpgradeableV2__init(_vrfCoordinator);
        __Ownable_init();

        coordinator = VRFCoordinatorV2Interface(_vrfCoordinator);
        maticFeed = AggregatorV3Interface(_maticFeed);

        if (_subscriptionId == 0) {
            linkToken = LinkTokenInterface(_link);
            subscriptionId = coordinator.createSubscription();

            coordinator.addConsumer(subscriptionId, address(this));
        } else {
            subscriptionId = _subscriptionId;
        }

        citizenContr = _citizenContr;
        crownsContr = _crownsContr;

        RAID_PRICES = [0, 2 * 10**8, 5 * 10**8, 8 * 10**8, 10 * 10**8];
    }

    /** @notice Withdraws funds from contract to owner address
     *
     * @param _amount Amount of ether to withdraw
     */
    function withdraw(uint256 _amount) external onlyOwner {
        payable(msg.sender).transfer(_amount);
    }

    /** @notice Initializes connection to the WorkerStakingContract
     *
     * @param _workerStakingContr Contract address of WorkerStakingContract
     */
    function setWorkerStakingContractAddress(address _workerStakingContr)
        external
        onlyOwner
    {
        require(address(workerStakingContr) == address(0), "ALREADYSET");
        workerStakingContr = WorkerStakingContractInterface(
            _workerStakingContr
        );
    }

    /** @notice Determines if a token is a raider
     *
     * @param _tokenId Id of the token
     * @param _randomWord Random number from the VRF request
     */
    function setIsRaiderByTokenId(uint256 _tokenId, uint256 _randomWord)
        external
        onlyCitizenContract
    {
        uint256 random = uint256(keccak256(abi.encode(_randomWord, 2))) % 10;

        if (random == 0) {
            tokenIdIsRaider[_tokenId] = true;
            indexedRaiders.push(_tokenId);

            lurkingRaiders[_tokenId].timestamp = block.timestamp;
            lurkingRaiders[_tokenId].taxesClaimed = workerStakingContr
                .claimableRaidersTaxes();
        }
    }

    /** @notice Returns information if a given tokenId is a raider
     *
     * @param _tokenId Id to be checked
     *
     * @return True if its a raider, false if not
     */
    function isRaider(uint256 _tokenId) external view returns (bool) {
        return tokenIdIsRaider[_tokenId];
    }

    /** @notice Used by CitizenContract to calculate the tokenId of the raider that gets a stolen mint
     *
     * @param _randomWord Random number generated by Chainlink VRF
     *
     * @return stealingRaider TokenId of the lucky raider that gets a free mint
     */
    function getStealingRaider(uint256 _randomWord)
        external
        view
        onlyCitizenContract
        returns (uint256 stealingRaider)
    {
        stealingRaider = indexedRaiders[
            uint256(keccak256(abi.encode(_randomWord, 4))) %
                indexedRaiders.length
        ];
    }

    /** @notice Returns current count of raiders
     *
     * @return Length of raiders array
     */
    function getRaidersCount() external view returns (uint256) {
        return indexedRaiders.length;
    }

    /** @notice Collects the rewards for a btach of raiders
     *
     * @dev Taxes are split equally, so each raider can claim the same amount
     * @dev It doesnt matter when he claims it tho
     *
     * @param _raiderTokenIds Identifiers of the raiders
     */
    function collectManyRaidersRewards(uint256[] calldata _raiderTokenIds)
        external
    {
        uint256 taxes;
        uint256 claimableRaidersTaxes = workerStakingContr
            .claimableRaidersTaxes();

        for (uint256 i; i < _raiderTokenIds.length; i++) {
            require(
                citizenContr.ownerOf(_raiderTokenIds[i]) == _msgSender(),
                "NOTYOURRAIDER"
            );
            require(tokenIdIsRaider[_raiderTokenIds[i]], "NOTARAIDER");

            Raider storage raider = lurkingRaiders[_raiderTokenIds[i]];

            if (raider.taxesClaimed < claimableRaidersTaxes) {
                taxes += claimableRaidersTaxes - raider.taxesClaimed;
                raider.taxesClaimed = claimableRaidersTaxes;
            }
        }
        if (taxes > 0) {
            totalTaxesClaimed += taxes;
            crownsContr.mint(_msgSender(), taxes);
        }
    }

    /** @dev Initiates a Raid for a given raider by requesting a random number that will choose the raided worker
     *
     * @param _raidersTokenId Id of the raider
     */
    function raidAWorker(uint256 _raidersTokenId) public payable {
        require(
            citizenContr.ownerOf(_raidersTokenId) == _msgSender(),
            "NOTYOURRAIDER"
        );
        require(tokenIdIsRaider[_raidersTokenId], "NOTARAIDER");

        Raider storage raider = lurkingRaiders[_raidersTokenId];

        if (block.timestamp - raider.timestamp > 1 days) {
            raider.dailyCount = 1;
        } else {
            raider.dailyCount++;
        }
        raider.timestamp = block.timestamp;
        uint256 raidCostMATIC = getPriceInMATIC(
            RAID_PRICES[raider.dailyCount - 1]
        );

        require(raider.dailyCount <= RAID_PRICES.length, "MAXRAIDSDONE");
        require(msg.value >= raidCostMATIC, "WRONGLYPRICED");

        uint256 requestId = coordinator.requestRandomWords(
            ChainLinkUtils.KEYHASH,
            subscriptionId,
            ChainLinkUtils.requestConfirmations,
            ChainLinkUtils.callbackGasLimit,
            1
        );

        requestedRaids[requestId] = RaidRequest(_raidersTokenId, _msgSender());
        emit RaidRequested(_msgSender(), requestId);
    }

    /** @notice Callback function which is executed after a VRF request was successful
     *
     * @dev In our case it uses the received random number to select a worker and steal from it
     *
     * @param _requestId Identifier of the previously sent request
     * @param _randomWords Result of the request containing the actual random numbers
     */
    function fulfillRandomWords(
        uint256 _requestId,
        uint256[] memory _randomWords
    ) internal override {
        address raidersOwner = citizenContr.ownerOf(
            requestedRaids[_requestId].tokenId
        );

        uint256 raidedWorkerId;
        uint256 raidedWorkerIndex;

        uint256 randomNr = uint256(keccak256(abi.encode(_randomWords[0], 1)));

        for (uint256 i; true; i++) {
            raidedWorkerIndex =
                randomNr %
                citizenContr.balanceOf(address(workerStakingContr));

            raidedWorkerId = citizenContr.tokenOfOwnerByIndex(
                address(workerStakingContr),
                raidedWorkerIndex
            );

            Worker memory worker = workerStakingContr.getStakedWorker(
                raidedWorkerId
            );

            if (worker.isPuzzle && worker.home == 1) {
                randomNr = uint256(keccak256(abi.encode(randomNr, 1)));
                continue;
            } else {
                break;
            }
        }

        uint256[] memory arr = new uint256[](1);
        arr[0] = raidedWorkerId;

        (uint256[] memory rewards, , ) = workerStakingContr
            .calculateRewardsForManyWorkers(arr);

        uint256 amountStolen = (((rewards[0]) * RAID_PERCENTAGE) / 100);
        workerStakingContr.setStolenForWorker(raidedWorkerId, amountStolen);

        crownsContr.mint(raidersOwner, amountStolen);
        emit RaidDone(raidersOwner, amountStolen);
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[49] private __gap;
}

