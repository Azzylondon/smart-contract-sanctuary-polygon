pragma solidity 0.8.0;

import '@openzeppelin/contracts/utils/Context.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "hardhat/console.sol";

interface ISafe {
    function deposit() external payable;
    function depositFromUser() external payable;
    function addReward(address user, uint amount) external;
    function getBalance(address user) external returns (uint256);
    function playFromBalance(address user, uint amount) external;
}

contract CryptosGame is Ownable, ReentrancyGuard, VRFConsumerBase {

    //vrf bsc testnet data
    bytes32 internal keyHash;
    uint256 internal fee;

    ISafe public safe;

    struct Round {
        uint64 epoch;
        uint64 betAmount;
        uint64 rewardAmount;
        uint8 numBets;
        uint8 resultType;
    }

    struct StopLossData {
        uint256 stopOnProfit;
        uint256 stopOnLoss;
        uint256 maxBet;
        uint256 increaseOnWin;
        uint256 increaseOnLoss;
        bool resetOnWin;
        bool resetOnLoss;
    }

    struct RequestData {
        address user;
        bool isStopLoss;
        uint8 numDraw;
        uint64 betAmount;
    }

    struct RoundData {
        uint256 currentEpoch;
        int batchProfit;
        uint batchReward;
        uint tempBet;
        uint additionalBet;
        uint userBalance;
    }

    mapping(bytes32 => StopLossData) public requestToStopLossData;
    mapping(bytes32 => RequestData) public requestData;
    mapping(bytes32 => bool) public requestToUseStopLoss;

    enum ResultType {NONE, PAIR, TWO_PAIR, THREE_KIND, FULL_HOUSE, FOUR_KIND, FIVE_KIND}
    uint256 public typesNum = 5;
    mapping(ResultType => uint256) public payoutAmounts;
    mapping(ResultType => uint256) public chances;
    mapping(address => mapping(uint256 => Round)) public userRounds;
    mapping(address => uint256) public userEpoch;

    uint256 nonce;

    constructor()         
        VRFConsumerBase(
            0x8C7382F9D8f56b33781fE506E897a4F1e2d17255,
            0x326C977E6efc84E512bB9C30f76E30c160eD06FB 
        )
    {
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4;
        fee = 0.0001 * 10 ** 18; 

        payoutAmounts[ResultType.NONE] = 0;
        payoutAmounts[ResultType.PAIR] = 10;
        payoutAmounts[ResultType.TWO_PAIR] = 250;
        payoutAmounts[ResultType.THREE_KIND] = 350;
        payoutAmounts[ResultType.FULL_HOUSE] = 600;
        payoutAmounts[ResultType.FOUR_KIND] = 700;
        payoutAmounts[ResultType.FIVE_KIND] = 6000;

        chances[ResultType.NONE] = 20508;
        chances[ResultType.PAIR] = 51270 + chances[ResultType.NONE];
        chances[ResultType.TWO_PAIR] = 15381 + chances[ResultType.PAIR];
        chances[ResultType.THREE_KIND] = 10254 + chances[ResultType.TWO_PAIR];
        chances[ResultType.FULL_HOUSE] = 1709 + chances[ResultType.THREE_KIND];
        chances[ResultType.FOUR_KIND] = 854 + chances[ResultType.FULL_HOUSE];
        chances[ResultType.FIVE_KIND] = 24 + chances[ResultType.FOUR_KIND];
    }

    function setSafe(address _safe) external onlyOwner {
        safe = ISafe(_safe);
    }

    function getRandom() private returns (uint) {
        nonce++;
        return uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce)));
    } 

    function playByBalance(uint8 numDraw, uint256 betAmount) external {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );

        safe.playFromBalance(msg.sender, betAmount * numDraw);
        //uint256 randomNumber = getRandom();
        //fullfillDraw(numDraw, randomNumber, betAmount);
        bytes32 requestId = requestRandomness(keyHash, fee);
        requestData[requestId] = RequestData(msg.sender, false, numDraw, uint64(betAmount));
    }

    function playByBalanceStopLoss(uint8 numDraw, uint256 betAmount, uint256 stopOnProfit, uint256 stopOnLoss, uint256 maxBet, uint256 increaseOnWin, uint256 increaseOnLoss, bool resetOnWin, bool resetOnLoss) external {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );

        safe.playFromBalance(msg.sender, betAmount * numDraw);
        //uint256 randomNumber = getRandom();
        //fullfillDraw(numDraw, randomNumber, betAmount);
        bytes32 requestId = requestRandomness(keyHash, fee);
        requestData[requestId] = RequestData(msg.sender, true, numDraw, uint64(betAmount));
        requestToStopLossData[requestId] = StopLossData(stopOnProfit, stopOnLoss, maxBet, increaseOnWin, increaseOnLoss, resetOnWin, resetOnLoss);
    }

    function playByPay(uint8 numDraw) external payable {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK - fill contract with faucet"
        );
        uint256 betAmount = msg.value;
        uint256 totalBet = betAmount * numDraw;
        safe.depositFromUser{value: totalBet}();
        //uint256 randomNumber = getRandom();
        //fulfillRandomness(requestId, randomNumber);
        bytes32 requestId = requestRandomness(keyHash, fee);
        requestData[requestId] = RequestData(msg.sender, false, numDraw, uint64(betAmount));
    }

    function recoverLink(uint256 _amount) external onlyOwner {
        require(
            LINK.balanceOf(address(this)) >= _amount,
            "Not enough LINK - fill contract with faucet"
        );

        LINK.transfer(msg.sender, _amount);
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomNumber) internal override {
        RequestData memory req = requestData[requestId];
        if(req.isStopLoss)
            fulfillDrawStopLoss(req.user, req.numDraw, randomNumber, req.betAmount, requestToStopLossData[requestId]);
        else
            fullfillDraw(req.user, req.numDraw, randomNumber, req.betAmount);
    }

    //     function checkToStop(address user, StopLossData memory stopLossData, int256 batchProfit, uint256 newBetAmount) internal pure returns (bool) {
    //     return (stopLossData.stopOnLoss > 0 && batchProfit < (int)(stopLossData.stopOnLoss) * -1) ||
    //         (stopLossData.stopOnProfit > 0 && batchProfit > (int)(stopLossData.stopOnProfit));
    //         //||
    //         //((stopLossData.increaseOnWin > 0 || stopLossData.increaseOnLoss > 0) && safe.getBalance(user) < newBetAmount));
    // }


    function checkToStop(StopLossData memory stopLossData, int256 batchProfit) internal pure returns (bool) {
        return (stopLossData.stopOnLoss > 0 && batchProfit < (int)(stopLossData.stopOnLoss) * -1) ||
            (stopLossData.stopOnProfit > 0 && batchProfit > (int)(stopLossData.stopOnProfit));
    }

    function fulfillDrawStopLoss(address user, uint8 numDraw, uint256 rnd, uint256 betAmount, StopLossData memory stopLossData) internal {
        RoundData memory roundData;
        roundData.currentEpoch = userEpoch[user];
        roundData.tempBet = betAmount;
        roundData.userBalance = safe.getBalance(user);
        
        uint i;
        //loop through each drawing
        for(i=0; i < numDraw; i++){

            //check if userBalance can afford the increased bet amounts
            if(roundData.tempBet > betAmount){
                if(roundData.userBalance < roundData.additionalBet + roundData.tempBet - betAmount)
                    break;
                roundData.additionalBet += roundData.tempBet - betAmount;
            }

            //expand chainlink VRF to get multiple random numbers for each draw
            uint randomNumber = uint256(keccak256(abi.encode(rnd, i)));

            //get the drawing result using the unique chainlink VRF
            ResultType result = drawOnce(randomNumber);
            uint256 reward = betAmount * payoutAmounts[result] / 100;
            
            Round storage round = userRounds[user][roundData.currentEpoch+i];
            round.epoch = uint64(roundData.currentEpoch+i);
            round.betAmount = uint64(roundData.tempBet);
            round.rewardAmount = uint64(reward);
            round.resultType = uint8(result);

            roundData.batchProfit += int(reward) - int(roundData.tempBet);
            roundData.batchReward += reward;

            if(reward > roundData.tempBet){
                if(stopLossData.resetOnWin)
                    roundData.tempBet = betAmount;
                else if(stopLossData.increaseOnWin > 0){
                    roundData.tempBet += roundData.tempBet * stopLossData.increaseOnWin / 100;
                    if(roundData.tempBet > stopLossData.maxBet) roundData.tempBet = stopLossData.maxBet;
                }
            }
            else{
                if(stopLossData.resetOnLoss)
                    roundData.tempBet = betAmount;
                else if(stopLossData.increaseOnLoss > 0){
                    roundData.tempBet += roundData.tempBet * stopLossData.increaseOnLoss / 100;
                    if(roundData.tempBet > stopLossData.maxBet) roundData.tempBet = stopLossData.maxBet;
                }
            }

            if(checkToStop(stopLossData, roundData.batchProfit))
                break;
        }

        if(roundData.additionalBet > 0){
            safe.playFromBalance(msg.sender, roundData.additionalBet);
        }
        if(roundData.batchReward > 0){
            safe.addReward(user, roundData.batchReward);
        }
        userEpoch[user] += i;
    }

    function fullfillDraw(address user, uint8 numDraw, uint256 rnd, uint256 betAmount) internal {
        uint256 currentEpoch = userEpoch[user];
        userRounds[user][currentEpoch].numBets = uint8(numDraw);
        uint256 batchReward;
        //loop through each drawing
        for(uint i=0; i < numDraw; i++){
            uint epoch = currentEpoch+i;
            //expand chainlink VRF to get multiple random numbers for each draw
            uint randomNumber = uint256(keccak256(abi.encode(rnd, i)));

            //get the drawing result using the unique chainlink VRF
            ResultType result = drawOnce(randomNumber);
            uint256 reward = betAmount * payoutAmounts[result] / 100;

            Round storage round = userRounds[user][epoch];
            round.epoch = uint64(epoch);
            round.betAmount = uint64(betAmount);
            round.rewardAmount = uint64(reward);
            round.resultType = uint8(result);
            
            batchReward += reward;
        }

        safe.addReward(user, batchReward);
        userEpoch[user] += numDraw;
    }

    function drawOnce(uint256 rnd) internal view returns (ResultType result) {
        uint256 randomNorm = rnd % 100000;
        result;

        if(randomNorm < chances[ResultType.NONE]){
            result = ResultType.NONE;
        }
        else if(randomNorm < chances[ResultType.PAIR]){
            result = ResultType.PAIR;
        }
        else if(randomNorm < chances[ResultType.TWO_PAIR]){
            result = ResultType.TWO_PAIR;
        }
        else if(randomNorm < chances[ResultType.THREE_KIND]){
            result = ResultType.THREE_KIND;
        }
        else if(randomNorm < chances[ResultType.FULL_HOUSE]){
            result = ResultType.FULL_HOUSE;
        }
        else if(randomNorm < chances[ResultType.FOUR_KIND]){
            result = ResultType.FOUR_KIND;
        }
        else if(randomNorm <= chances[ResultType.FIVE_KIND]){
            result = ResultType.FIVE_KIND;
        }
    }

}

