pragma solidity ^0.8.0;

//import "./Initializable.sol";
import "./VRFConsumerBaseUpgradable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

interface ISE {
    function updateReward(address _from, address _to) external;
}

contract DMintV31 is
    Initializable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    OwnableUpgradeable,
    UUPSUpgradeable,
    VRFConsumerBaseUpgradable
{
    using SafeMath for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    //Put variable in order of type https://www.youtube.com/watch?v=PYilP2bjtwc&t=30s

    struct Winner {
        uint256 tokenId;
        address token_holder;
        uint256 timestamp; // block.timestamp
    }

    uint256 internal constant RNG_Prime = 97;
    uint256 internal constant _RNG_Prime = 997;
    uint256 public constant MAX_D = 10000;
    uint256 internal constant START_AIRDROP = 2500;
    uint256 internal constant MAX_50P = 5000;
    uint256 internal constant MAX_75P = 7500;

    uint256 internal fee;
    uint256 public randomResult;
    uint256 public airDropRandomResult;
    uint256 public prizeWinnersRandomResult;
    uint256 public growthPrizeRandomResult;
    //uint256 internal nonce;
    uint256 public GiftMinted;
    uint256 public AidropGenerated;
    uint256 public GrowthPrizeGenerated;
    bool public GrandPrizeGenerated;
    uint256 public RunnerUpPrizeGenerated;

    string public constant TraitsFileHash =
        "ac38ffb9f1c6629680935a8084be99c91a8615d6d2b59b900d8624c6cb6cae2c";
    uint256 public constant UnitSalePrice = 0.06 ether;
    uint256 public constant TripleSalePrice = 0.15 ether;
    uint256 public constant TenSalePrice = 0.4 ether;

    uint256 public constant UnitpreSalePrice = 0.04 ether;
    uint256 public constant TriplepreSalePrice = 0.12 ether;
    uint256 public constant TenpreSalePrice = 0.4 ether;

    uint256 public constant GrandPrizeAmount = 10 ether;
    uint256 public constant RunnerUpPrizeAmount = 1 ether;
    uint256 public constant CharityDonationAmount = 10 ether;
    uint256 public constant TreasuryAmount = 20 ether;

    //uint256[MAX_D] internal indexTraits;

    bytes32 internal keyHash;
    bytes32 public requestId;
    bytes32 public init_requestId;

    //bytes32[MAX_D] internal MetadataAttributes;

    address public charityAddress;
    address public treasuryAddress;
    address public moneybagAddress;
    address public adminAddress;

    mapping(address => bool) public preSaleAddresses;

    /*
    mapping(uint256 => address) public AirdropList;
    mapping(uint256 => Winner) public m_AirdropWinners;
    */

    Winner[] public AirdropWinners;
    Winner[] public GrowthPrizeWinners;
    Winner[] public RunnerUpPrizeWinners;
    Winner public GrandPrizeWinner;

    mapping(uint256 => string) internal MetadataAttributes;
    mapping(uint256 => uint256) internal indexTraits;
    mapping(uint256 => string) internal IPFS_CIDs;
    string internal baseURI;

    IERC20Upgradeable public paymentToken;

    enum Stages {
        noSaleLive,
        preSaleLive,
        publicSaleLive,
        contractPaused
    }

    Stages public currentStage;

    event GenerateAirDrop(
        uint256 indexed tokenId,
        address indexed holder_address
    );
    event GotRandom(bytes32 indexed requestId, uint256 indexed result);

    function initialize() public initializer {
        __ERC721_init("DMintV31", "DMI");
        __ERC721Enumerable_init();
        __Ownable_init();
        __UUPSUpgradeable_init();

        VRFConsumerBaseUpgradable.initializeVRF(
            0x8C7382F9D8f56b33781fE506E897a4F1e2d17255, // VRF Coordinator
            0x326C977E6efc84E512bB9C30f76E30c160eD06FB // LINK Token
        );
        //nonce = 15;
        baseURI = "https://gateway.pinata.cloud/ipfs/";
        currentStage = Stages.noSaleLive;

        address _paymentToken = address(
            0xaf3dd790404f0ae9AF0142838031FF21cDd20846
        );
        paymentToken = IERC20Upgradeable(_paymentToken);
        moneybagAddress = address(0x4834e93aa9f9Fe6545d5caB63621D68ED9aa20E4); //Gnosis safe address for splitter 0x19c37f1006517E761280856864fE29fb6054132e
        treasuryAddress = address(0x4834e93aa9f9Fe6545d5caB63621D68ED9aa20E4); //Gnosis safe address for treasury 0x53a71e3455e6100b4c4261e93Ea85144210E1A29
        adminAddress = address(0xBb278630232D8CE1622B78483602a35B056B4968);
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4;
        //Gnosis safe address for charity in case we haven't set charity address by 75%
        //charityAddress = address(0x68045D654cB56178f9ED48401dEECA6286dAA301);
        fee = 100000000000000; // 0.0001 LINK
    }

    //For GAS Savings we need to have our most used functions at the top
    //https://medium.com/joyso/solidity-how-does-function-name-affect-gas-consumption-in-smart-contract-47d270d8ac92

    function _mintD(uint256 numberOfTokens, address sender) internal {
        require(randomResult > 0, "Missing Random");

        for (uint256 i = 0; i < numberOfTokens; i++) {
            uint256 mintIndex = totalSupply().add(1);

            _generateIndexTraits(mintIndex);

            _safeMint(sender, mintIndex);
        }

        if (totalSupply() == MAX_50P || totalSupply() == MAX_D) {
            _getRandomNumber();
        } else if (totalSupply() == MAX_75P) {
            _sendToCharity();
        }
    }

    function digiMint() external notPaused {
        uint256 price;
        if (currentStage == Stages.preSaleLive) {
            price = UnitpreSalePrice;
            require(
                preSaleAddresses[msg.sender],
                "Address is not whitelisted for preSale"
            );
        } else {
            price = UnitSalePrice;
            require(
                currentStage == Stages.publicSaleLive,
                "Sale must be active to mint"
            );
        }
        //Possible to check the amount approved for that address?
        //require(price <= msg.value, "ETH value sent is not correct");
        paymentToken.transferFrom(msg.sender, address(this), price);
        require(totalSupply().add(1) <= MAX_D, "Exceeds max supply of D");
        _mintD(1, msg.sender);
    }

    function digiMintThree() external notPaused {
        uint256 price;
        if (currentStage == Stages.preSaleLive) {
            price = TriplepreSalePrice;
            require(
                preSaleAddresses[msg.sender],
                "Address is not whitelisted for preSale"
            );
        } else {
            price = TripleSalePrice;
            require(
                currentStage == Stages.publicSaleLive,
                "Sale must be active to mint"
            );
        }
        //Possible to check the amount approved for that address?
        //require(price <= msg.value, "ETH value sent is not correct");
        paymentToken.transferFrom(msg.sender, address(this), price);
        require(totalSupply().add(3) <= MAX_D, "Exceeds max supply of D");
        _mintD(3, msg.sender);
    }

    function digiMintTen() external notPaused {
        uint256 price;
        if (currentStage == Stages.preSaleLive) {
            price = TenpreSalePrice;
            require(
                preSaleAddresses[msg.sender],
                "Address is not whitelisted for preSale"
            );
        } else {
            price = TenSalePrice;
            require(
                currentStage == Stages.publicSaleLive,
                "Sale must be active to mint"
            );
        }
        //Possible to check the amount approved for that address?
        //require(price <= msg.value, "ETH value sent is not correct");
        paymentToken.transferFrom(msg.sender, address(this), price);

        require(totalSupply().add(10) <= MAX_D, "Exceeds max supply of D");
        _mintD(10, msg.sender);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
        //SE.updateReward(from, to);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * Requests randomness
     */
    function _getRandomNumber() internal returns (bytes32 _requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Fund with LINK");
        init_requestId = requestRandomness(keyHash, fee);
        return init_requestId;
    }

    /**
     * Requests randomness
     */
    function getRandomNumber() external onlyOwner returns (bytes32 _requestId) {
        return _getRandomNumber();
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 _requestId, uint256 randomness)
        internal
        override
    {
        if (randomResult <= 0) {
            randomResult = randomness;
        }

        requestId = _requestId;

        if (
            totalSupply() >= START_AIRDROP &&
            AidropGenerated < 50 &&
            totalSupply() < MAX_50P
        ) {
            airDropRandomResult = randomness;
            _generateAirDrop(START_AIRDROP);
        } else if (totalSupply() >= MAX_50P && totalSupply() < MAX_D) {
            growthPrizeRandomResult = randomness;
            _getGrowthPrizeAddresses(MAX_50P);
        } else if (totalSupply() == MAX_D && RunnerUpPrizeGenerated < 5) {
            prizeWinnersRandomResult = randomness;
            _distributePrizes(MAX_D);
        }

        emit GotRandom(requestId, randomness);
    }

    function updateStage(Stages _stage) external onlyOwner {
        currentStage = _stage;
    }

    function _notPaused() private view {
        require(
            currentStage != Stages.contractPaused,
            "Contract is currently paused"
        );
    }

    modifier notPaused() {
        _notPaused();
        _;
    }

    function addToPresale(address[] calldata list_addresses)
        external
        onlyOwner
    {
        for (uint256 index = 0; index < list_addresses.length; index++) {
            address address_item = list_addresses[index];
            require(address_item != address(0), "Not valid Address");
            require(
                !preSaleAddresses[address_item],
                "This preSale address is duplicated"
            );

            preSaleAddresses[address_item] = true;
        }
    }

    function removeFromPresaleList(address[] calldata list_addresses)
        external
        onlyOwner
    {
        for (uint256 index = 0; index < list_addresses.length; index++) {
            address address_item = list_addresses[index];
            require(address_item != address(0), "Not valid Address");

            preSaleAddresses[address_item] = false;
        }
    }

    function getMetadataAttributes(uint256 tokenId)
        external
        view
        returns (string memory)
    {
        require(_exists(tokenId), "nonexistent token");

        return MetadataAttributes[tokenId];
    }

    function getIndexTraits(uint256 tokenId) external view returns (uint256) {
        require(_exists(tokenId), "nonexistent token");

        return indexTraits[tokenId];
    }

    // 1 .. MAX_D
    function _generateIndexTraits(uint256 tokenId) internal {
        indexTraits[tokenId] =
            (((RNG_Prime * tokenId) + randomResult) % MAX_D) +
            1;
    }

    function mintAdminGift(address _luckyAddress) external {
        require(msg.sender == adminAddress, "Not Allowed");
        require(_luckyAddress != address(0), "Not Valid Address");

        _mintD(1, _luckyAddress);
        GiftMinted = GiftMinted.add(1);
    }

    function mintGift(address _luckyAddress) external onlyOwner {
        require(_luckyAddress != address(0), "Not Valid Address");
        _mintD(1, _luckyAddress);
        GiftMinted = GiftMinted.add(1);
    }

    function _setCharityAddress(address _charityAddress) external onlyOwner {
        charityAddress = _charityAddress;
    }

    function _setBaseURI(string memory newBaseURI) external onlyOwner {
        baseURI = newBaseURI;
    }

    function setTokenCIDMetadata(
        uint256 tokenId,
        string memory tokenCID,
        string memory attributes
    ) external {
        require(msg.sender == adminAddress, "Not Allowed");
        IPFS_CIDs[tokenId] = tokenCID;
        MetadataAttributes[tokenId] = attributes;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(_exists(tokenId), "nonexistent token");

        return
            bytes(IPFS_CIDs[tokenId]).length > 0
                ? string(abi.encodePacked(baseURI, IPFS_CIDs[tokenId]))
                : "https://gateway.pinata.cloud/ipfs/QmbxSPVFfsZ5EfLHqnKqFUTkhukUMpyaSaFUKsntEdjayk";
    }

    function withdrawERC20(IERC20Upgradeable token) public onlyOwner {
        token.transfer(moneybagAddress, token.balanceOf(address(this)));
    }

    function _sendToCharity() internal {
        paymentToken.transfer(charityAddress, CharityDonationAmount);
    }

    function sendToTreasury() public onlyOwner {
        paymentToken.transfer(treasuryAddress, TreasuryAmount);
    }

    function _getRandomTokenID(uint256 seed, uint256 upper_limit)
        internal
        pure
        returns (uint256)
    {
        return (seed % upper_limit) + 1;
    }

    /* Not used , to be removed
    function _getRandomOwnerAddress(uint256 seed, uint256 upper_limit)
        internal
        view
        returns (address)
    {
        return ownerOf(_getRandomTokenID(seed, upper_limit));
    }
    */

    function _getGrowthPrizeAddresses(uint256 upper_limit) internal {
        uint256 tokenId = _getRandomTokenID(
            (_RNG_Prime * GrowthPrizeGenerated) + growthPrizeRandomResult,
            upper_limit
        );
        address holder_address = ownerOf(tokenId);
        GrowthPrizeWinners.push(
            Winner(tokenId, holder_address, block.timestamp)
        );
        GrowthPrizeGenerated = GrowthPrizeGenerated.add(1);
    }

    function generateGrowthPrizeAddresses() external {
        require(msg.sender == adminAddress, "Not Allowed");
        require(totalSupply() >= MAX_50P, "Wait for Growth Prize");
        require(GrowthPrizeGenerated < 5, "Max reached");
        _getRandomNumber();
    }

    function _generateAirDrop(uint256 upper_limit) internal {
        uint256 tokenId = _getRandomTokenID(
            //uint256(keccak256(abi.encode(airDropRandomResult, requestId))),
            airDropRandomResult,
            upper_limit
        );

        address holder_address = ownerOf(tokenId);

        //_mintD(1, holder_address);

        AirdropWinners.push(Winner(tokenId, holder_address, block.timestamp));

        AidropGenerated = AidropGenerated.add(1);
        emit GenerateAirDrop(tokenId, holder_address);
    }

    function generateAirDrop() external {
        require(msg.sender == adminAddress, "Not Allowed");
        require(totalSupply() >= START_AIRDROP, "Wait for AirDrops");
        require(AidropGenerated < 50, "Max reached");
        _getRandomNumber();
    }

    function _distributePrizes(uint256 _MAX_D) internal {
        if (GrandPrizeGenerated) {
            uint256 tokenId = _getRandomTokenID(
                prizeWinnersRandomResult,
                _MAX_D
            );
            address holder_address = ownerOf(tokenId);

            RunnerUpPrizeWinners.push(
                Winner(tokenId, holder_address, block.timestamp)
            );

            //paymentToken.transfer(holder_address, RunnerUpPrizeAmount);
            RunnerUpPrizeGenerated = RunnerUpPrizeGenerated.add(1);
        } else {
            uint256 tokenId = _getRandomTokenID(
                prizeWinnersRandomResult,
                _MAX_D
            );
            address holder_address = ownerOf(tokenId);

            GrandPrizeWinner = Winner(tokenId, holder_address, block.timestamp);

            //paymentToken.transfer(holder_address, GrandPrizeAmount);
            GrandPrizeGenerated = true;
        }
    }

    function generateRunnerUpPrize() external {
        require(msg.sender == adminAddress, "Not Allowed");
        require(totalSupply() == MAX_D, "Wait for RunnerUp");
        require(RunnerUpPrizeGenerated < 5, "Max reached");
        _getRandomNumber();
    }

    function transferAdminship(address newAdminAddress) external onlyOwner {
        adminAddress = newAdminAddress;
    }

    /**
     * Override isApprovedForAll to auto-approve OS's proxy contract
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override
        returns (bool isOperator)
    {
        // if OpenSea's ERC721 Proxy Address is detected, auto-return true
        if (_operator == address(0x58807baD0B376efc12F5AD86aAc70E78ed67deaE)) {
            return true;
        }

        // otherwise, use the default ERC721.isApprovedForAll()
        return ERC721Upgradeable.isApprovedForAll(_owner, _operator);
    }

    //USED for creating connection to a token
    ISE public SE;

    function setSE(address seAddress) external onlyOwner {
        SE = ISE(seAddress);
    }
}

