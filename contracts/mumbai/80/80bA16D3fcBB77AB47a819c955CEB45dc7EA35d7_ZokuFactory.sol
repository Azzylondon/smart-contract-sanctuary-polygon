pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./interfaces/IERC721Standard.sol";
import "./interfaces/IManager.sol";
import "./interfaces/IWhitelist.sol";
import "hardhat/console.sol";

contract ZokuFactory is Ownable {
  using EnumerableSet for EnumerableSet.AddressSet;

  struct Project {
    address token;
    address owner;
    address whitelist;
  }

  address public tokenImplementation;
  address public whitelistImplementation;

  /// mapping token address => project
  mapping(address => Project) private _projects;

  EnumerableSet.AddressSet private _admins;
  EnumerableSet.AddressSet private _projectTokens;

  event TokenCreated(
    address indexed owner,
    address indexed token
  );

  event ProjectCreated(
    address token,
    address owner,
    address whitelist
  );

  event ProjectTokenMinted(
    address token,
    address to,
    uint256 tokenId,
    string tokenURI
  );

  constructor(address _tokenImplementation, address _whitelistImplementation) {
    tokenImplementation = _tokenImplementation;
    whitelistImplementation = _whitelistImplementation;

    addAdmin(_msgSender());
  }

  modifier onlyAdmin() {
    require(_admins.contains(_msgSender()), "Not a whitelisted admin");
    _;
  }

  modifier onlyTokenOwner(address _token) {
    Project storage project = _projects[_token];
    require(project.owner == _msgSender(), "Not project owner");
    _;
  }

  function addAdmin(address admin) public onlyOwner {
    _admins.add(admin);
  }

  function removeAdmin(address admin) external onlyOwner {
    _admins.remove(admin);
  }

  function getProjects() public view returns (address[] memory) {
    uint256 length = _projectTokens.length();
    address[] memory projects = new address[](length);
    
    for (uint256 i = 0; i < length; i++) {
      projects[i] = _projectTokens.at(i);
    }

    return projects;
  }

  function getAdmins() public view returns (address[] memory) {
    uint256 length = _admins.length();
    address[] memory admins = new address[](length);
    
    for (uint256 i = 0; i < length; i++) {
      admins[i] = _admins.at(i);
    }

    return admins;
  }

  function setImplementation(
    address _tokenImplementation,
    address _whitelistImplementation
  ) external onlyOwner {
    tokenImplementation = _tokenImplementation;
    whitelistImplementation = _whitelistImplementation;
  }

  function _addProject(
    address _token,
    address _owner,
    address _whitelist
  ) internal {
    require(!_projectTokens.contains(_token), "Factory: Project exists");
    
    _projects[_token] = Project(_token, _owner, _whitelist);
    _projectTokens.add(_token);

    emit ProjectCreated(_token, _owner, _whitelist);
  }

  function create(
    address owner,
    string memory name,
    string memory symbol,
    string memory uri,
    uint256 maxSupply,
    uint256 royaltyFee
  ) external onlyAdmin {
    /// initialize token contract
    address token = Clones.clone(tokenImplementation);
    IERC721Standard(token).initialize(owner, name, symbol, uri, maxSupply, royaltyFee);

    /// initialize whitelist contract
    address whitelist = Clones.clone(whitelistImplementation);
    IWhitelist(whitelist).initialize(token);
    // callable by this contract
    IWhitelist(whitelist).addOperator(address(this));
    /// callable by token owner
    IWhitelist(whitelist).addOperator(owner);
    /// callable by admin
    IWhitelist(whitelist).addOperator(_msgSender());
    /// optional: transfer ownership of the whitelist contract to the owner
    IWhitelist(whitelist).transferOwnership(owner);

    _addProject(token, owner, whitelist);

    emit TokenCreated(_msgSender(), token);
  }

  function mint(
    address _token,
    uint8 _rarity,
    string memory _tokenURI
  ) external payable returns(uint256 tokenId) {
    Project storage project = _projects[_token];
    require(project.token != address(0), "Factory: Project not found");

    // validate whitelist
    if (project.whitelist != address(0)) {
      uint256 amount = 1;
      require(IWhitelist(project.whitelist).isWhitelisted(_rarity, _msgSender()), "Factory: You're not whitelisted");

      (uint256 price, uint256 qty, uint256 consumedQty,) = IWhitelist(project.whitelist).getWhitelist(_rarity);
      (bool _success, string memory _msgText) = IWhitelist(project.whitelist).canConsume(_rarity, _msgSender(), amount);
      
      require(_success, _msgText);
      require(uint256(consumedQty + amount) <= uint256(qty), "Factory: All item minted");
      require(amount <= qty, "Factory: Exceeds max amount per mint");

      uint256 ethValue = msg.value;
      require((price * amount) <= ethValue, "Factory: Insufficent Funds");

      // send amount to project owner or timelock contract
      // calculate currator fees (assumed 15%)
      uint256 adminFees = ethValue * 150 / 1000;
      (bool success, ) = project.owner.call{value: (ethValue - adminFees)}("");
      (bool success2, ) = owner().call{value: adminFees}("");
      require(success, "Factory: Could not send funds to project");
      require(success2, "Factory: Could not send funds to admin");
    }
    
    tokenId = IERC721Standard(_token).mint(_msgSender(), _tokenURI);
    emit ProjectTokenMinted(_token, _msgSender(), tokenId, _tokenURI);
  }
}

