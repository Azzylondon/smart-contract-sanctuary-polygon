interface IWhitelist {
  /// events
  event WhitelistMemberAdded(uint8 rarity, address member, address sender, uint256 qty);
  event WhitelistMemberRemoved(uint8 rarity, address member, address sender);
  event WhitelistPriceUpdated(uint8 rarity, uint256 price, address sender);
  event WhitelistQuantityUpdated(uint8 rarity, uint256 qty, address sender);

  /// functions
  function initialize(address tokenAddress) external;
  function setStatus(bool status) external;
  function setPeriod(uint256 start, uint256 end) external;
  function addOperator(address operator) external;
  function removeOperator(address operator) external;
  function getWhitelist(uint8 rarity) external view returns (uint256, uint256, uint256, uint256);
  function getWhitelistMemberData(uint8 rarity, address account) external view returns (uint256, uint256);
  function addWhitelist(uint8 rarity, uint256 price, uint256 qty) external;
  function removeWhitelist(uint8 rarity) external;
  function setPrice(uint8 rarity, uint256 price) external;
  function setQuantity(uint8 rarity, uint256 qty) external;
  function addMember(uint8 rarity, address member, uint256 multiplier) external;
  function addBatchMember(uint8 rarity, address[] memory members, uint256 multiplier) external;
  function removeMember(uint8 rarity, address member) external;
  function isWhitelisted(uint8 rarity, address member) external view returns (bool);
  function consume(uint8 rarity, address member, uint256 qty) external;
  function canConsume(uint8 rarity, address member, uint256 qty) external view returns (bool, string memory);
  function getConsumed(uint8 rarity, address member) external view returns (uint256);
  function totalQty() external view returns (uint256);
  function totalConsumedQty() external view returns (uint256);
  function getWhitelistCount() external view returns (uint256);
  function getWhitelistArray() external view returns (uint8[] memory);
  function transferOwnership(address newOwner) external;
}

