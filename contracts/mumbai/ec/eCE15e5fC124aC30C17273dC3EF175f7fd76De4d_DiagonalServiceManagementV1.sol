/**
 * @title DiagonalRegistryV1 contract interface
 * @author Diagonal Finance
 * @notice The DiagonalRegistry facilitates creation of Diagonal services, and has control over the protocol fees
 */
interface IDiagonalRegistryV1 {
    /**
     * @notice Emitted when service is created
     * @param serviceId The id of the created service
     * @param serviceAddress The address of the DiagonalServiceProxy contracts created
     * @param provider The address of the initial service owner
     * @param salt Service creation salt
     */
    event ServiceCreated(
        uint256 indexed serviceId,
        address indexed serviceAddress,
        address indexed provider,
        uint256 salt
    );

    /**
     * @notice Emitted when treasury address is updated
     * @param newTreasury The new treasury address
     */
    event TreasuryUpdated(address indexed newTreasury);

    /**
     * @notice Emitted when protocol fee percent is updated
     * @param superToken The super token whose associated fee has been updated
     * @param protocolFeePercent The new fee
     */
    event ProtocolFeePercentUpdated(ISuperToken indexed superToken, int96 indexed protocolFeePercent);
    /**
     * @notice Emitted when a super token is added to whitelist
     * @param superToken The newly accepted super token
     */
    event AcceptedSuperTokenAdded(ISuperToken indexed superToken);

    /**
     * @notice Emitted when a super token is removed from whitelist
     * @dev We do not clear storage, instead just toggle to false
     * @param superToken The removed super token
     */
    event AcceptedSuperTokenRemoved(ISuperToken indexed superToken);

    /**
     * @notice Emitted when new address for the DiagonalServiceBeacon is set
     * @param diagonalServiceBeacon The address of the new DiagonalServiceBeacon
     */
    event DiagonalServiceBeaconSet(address indexed diagonalServiceBeacon);

    /**
     * @notice Contract initialize function, should only be called once
     * @dev _acceptedTokens can be updated using `addAcceptedSuperToken` and `removeAcceptedSuperToken`
     * @param _treasury Diagonal treasury address
     * @param _deployer Diagonal deployer
     * @param _acceptedTokens Intial list of protocol whitelisted super tokens
     * @param _owner The owner of the contract
     */
    function initialize(
        address _treasury,
        IDiagonalDeployer _deployer,
        ISuperToken[] calldata _acceptedTokens,
        address _owner
    ) external;

    /**
     * @notice Update Diagonal treasury address
     * @param newTreasury New treasury address
     */
    function updateTreasury(address newTreasury) external;

    /**
     * @notice Add super token to protocol whitelist
     * @param superToken a super token
     */
    function addAcceptedSuperToken(ISuperToken superToken) external;

    /**
     * @notice Remove super token from protocol whitelist
     * @param superToken A super token
     */
    function removeAcceptedSuperToken(ISuperToken superToken) external;

    /**
     * @notice Update protocol fee for a super token
     * @dev Fee is charged as a percentage of a subscripton (incoming super app stream)
     * @param superToken The super token whose associated fee is updated
     * @param newProtocolFeePercent New protocol fee(%) for the super token
     */
    function updateProtocolFee(ISuperToken superToken, int96 newProtocolFeePercent) external;

    /**
     * @notice Set the address of the DiagonalServiceBeacon
     * @param _serviceBeacon The address of the new DiagonalServiceBeacon
     */
    function setServiceBeacon(address _serviceBeacon) external;

    /**
     * @notice Create service child contract
     * @param _primaryTokens Service primary tokens, optionally provided
     * @param defaultPackage The default package associated with the service
     * @return _serviceId _serviceAddress The NFT id of the service, and the address of the DiagonalServiceProxy
     */
    function createService(ISuperToken[] calldata _primaryTokens, PackageData calldata defaultPackage)
        external
        returns (uint256 _serviceId, address _serviceAddress);

    /**
     * @notice Create service child contract
     * @param _primaryTokens Service primary tokens, optionally provided
     * @return _serviceId _serviceAddress The NFT id of the service, and the address of the DiagonalServiceProxy
     */
    function createService(ISuperToken[] calldata _primaryTokens)
        external
        returns (uint256 _serviceId, address _serviceAddress);

    /**
     * @notice Create service child contract
     * @param _primaryTokens Service primary tokens, optionally provided
     * @param defaultPackage The default package associated with the service
     * @param saltParams Service creation salt params
     * @return _serviceId _serviceAddress The NFT id of the service, and the address of the DiagonalServiceProxy
     */
    function createServiceWithSalt(
        ISuperToken[] calldata _primaryTokens,
        PackageData calldata defaultPackage,
        SaltParams memory saltParams
    ) external returns (uint256 _serviceId, address _serviceAddress);

    /**
     * @notice Pause contract service creation, and service transfers.
     */
    function pause() external;

    /**
     * @notice Unpause contract service creation, and service transfers.
     */
    function unpause() external;

    /*******************************
     * Constant getters *
     *******************************/

    /**
     * @notice Returns the superfluid host address
     * @return The Superfluid host address
     */
    function SUPERFLUID_HOST() external view returns (ISuperfluid);

    /*******************************
     * Storage getters *
     *******************************/

    /**
     * @notice Returns the current address of Diagonal Treasury
     * @return The address of the treasury contract
     */
    function treasury() external view returns (address);

    /**
     * @notice Returns the id of the last service minted/created
     * @dev Starts at 0
     * @return The id of last created service
     */
    function serviceIds() external view returns (uint256);

    /**
     * @notice Returns the Diagonal service beacon address
     * @dev The beacon contract is used to store the implemntation address of each DiagonalServiceProxy
     * @return The address of the DiagonalServiceBeacon contract
     */
    function serviceBeacon() external view returns (address);

    /**
     * @notice Returns the Diagonal deployer address
     * @dev The diagonal deployer contract is used to deterministically deploy contracts using the create2 opcode
     * @return The address of the DiagonalDeployer contract
     */
    function deployer() external view returns (IDiagonalDeployer);

    /**
     * @notice Returns the protocol fee percentages for each SuperToken
     * @param token A Super Token
     * @return The fee per superToken
     */
    function protocolFees(ISuperToken token) external view returns (int96);

    /**
     * @notice Returns the address the DiagonalServiceProxy associated with the id
     * @param serviceId Id of service
     * @return The DiagonalServiceProxy address associated with the serviceId
     */
    function serviceIdToAddress(uint256 serviceId) external view returns (address);

    /**
     * @notice Returns if a superToken is on whitelisted
     * @param token A Super Token
     * @return The fee per superToken
     */
    function acceptedTokens(ISuperToken token) external view returns (bool);
}

