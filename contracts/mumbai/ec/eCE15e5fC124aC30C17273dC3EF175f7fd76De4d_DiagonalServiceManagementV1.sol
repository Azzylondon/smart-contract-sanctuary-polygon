pragma solidity 0.8.4;

import { DiagonalServiceStorage, ServiceState, PackageData, PackageState } from "../../utils/DataStructures.sol";
import { ISuperToken } from "@superfluid-finance/ethereum-contracts/contracts/interfaces/superfluid/ISuperToken.sol";

import { IDiagonalRegistryV1 } from "../../interfaces/V1/IDiagonalRegistryV1.sol";
import "hardhat/console.sol";

/**
 * @title DiagonalServiceManagementV1
 * @author Diagonal Finance
 * @notice This library contains service management functionality for a DigaonalServiceV1 instance
 */
library DiagonalServiceManagementV1 {
    /*******************************
     * Constants *
     *******************************/

    /*
        E_INVALID_TOKEN - Invalid token address. e.g. Zero address.
        E_FLOWRATE_INVALID- Invalid flow rate
        E_PACKAGE_INPUT- Invalid package data input
        E_TOKEN_NOT_SERVICE_WHITELIST - Token not service whitelisted
        E_TOKEN_NOT_REG_WHITELIST - Token not registry whitelisted
        E_PAYMENT_TERMS_EXIST - Payment terms already exist
        E_MAX_PRIMARY_TOKENS - Primary token list full
        E_PRIMARY_TOKEN_EXISTS - Already set as primary token
     */

    /// @dev DiagonalRegistry reference
    /// @dev Please check chain DiagonalRegisty address before deploying
    IDiagonalRegistryV1 public constant DIAGONAL_REGISTRY =
        IDiagonalRegistryV1(0x68CA69AFCBeE6a41320e4A799b6b16bE90b84e8A);

    /**
     * @notice The maximum number of primary tokens supported per package
     * @dev On service transfer, only outgoing streams in primary tokens (Diagonal --> merchant),
     * are automatically redirected to new service owner
     * @dev This constant can be updated by the protocol in the future
     */
    uint256 public constant MAX_PRIMARY_TOKENS = 5;

    /*******************************
     * Events *
     *******************************/

    /**
     * @notice Emitted when service is stopped
     */
    event ServiceStopped();

    /**
     * @notice Emitted when service is activated
     */
    event ServiceActivated();

    /**
     * @notice Emitted when a package is created
     * @param packageId The id of the created package
     * @param numPaymentTerms The number of payment terms added to the package
     */
    event PackageAdded(uint256 indexed packageId, uint256 indexed numPaymentTerms);

    /**
     * @notice Emitted when package is stopped
     * @param packageId The id of the created package
     */
    event PackageStopped(uint256 indexed packageId);

    /**
     * @notice Emitted when package is activated
     * @param packageId The id of the created package
     */
    event PackageActivated(uint256 indexed packageId);

    /**
     * @notice Emitted when primary token added
     * @param superToken The superToken primary token
     */
    event PrimaryTokenAdded(ISuperToken indexed superToken);

    /*******************************
     * Management logic *
     *******************************/

    /// @notice IDiagonalServiceV1.initializeService implementation
    /// @dev We do not revert in case of _primaryTokens.length > MAX_PRIMARY_TOKENS,
    /// as MAX_PRIMARY_TOKENS may change in future.
    function initializeService(DiagonalServiceStorage storage self, ISuperToken[] calldata _primaryTokens) public {
        // 1. Intilise tokens
        for (uint256 i = 0; i < _primaryTokens.length; i++) {
            if (i < MAX_PRIMARY_TOKENS) {
                ISuperToken _token = _primaryTokens[i];

                require(address(_token) != address(0), "SERV:E_INVALID_TOKEN");
                require(DIAGONAL_REGISTRY.acceptedTokens(_token), "SERV:E_TOKEN_NOT_REG_WHITELIST");

                self.primaryTokens[_token] = true;
                self.primaryAcceptedTokenList.push(_token);
                emit PrimaryTokenAdded(_token);
            } else {
                // Simply return when primary tokens exhausted
                break;
            }
        }
    }

    /// @notice IDiagonalServiceV1.initializeService implementation
    function initializeService(
        DiagonalServiceStorage storage self,
        ISuperToken[] calldata _primaryTokens,
        PackageData calldata defaultPackage
    ) external {
        // 1. Intilise tokens
        initializeService(self, _primaryTokens);

        // 2. Create package
        uint256 _packageId = self.packageIds;
        // Package IDs starting from 1
        _packageId++;
        _addPackage(self, defaultPackage, _packageId);
        self.packageIds = _packageId;
    }

    /// @notice IDiagonalServiceV1.stopService implementation
    function stopService(DiagonalServiceStorage storage self) external {
        self.state = ServiceState.STOPPED;
        emit ServiceStopped();
    }

    /// @notice IDiagonalServiceV1.activateService implementation
    function activateService(DiagonalServiceStorage storage self) external {
        self.state = ServiceState.ACTIVE;
        emit ServiceActivated();
    }

    /// @notice IDiagonalServiceV1.addPackages implementation
    function addPackages(DiagonalServiceStorage storage self, PackageData[] calldata packagesData) external {
        uint256 _packageId = self.packageIds;

        for (uint256 i = 0; i < packagesData.length; i++) {
            // Package IDs starting from 1
            _packageId++;
            _addPackage(self, packagesData[i], _packageId);
        }
        // Save on SSTORE, only write to storage once
        self.packageIds = _packageId;
    }

    function _addPackage(
        DiagonalServiceStorage storage self,
        PackageData calldata packageData,
        uint256 packageId
    ) private {
        require(
            packageData.tokens.length > 0 && (packageData.tokens.length == packageData.flowRates.length),
            "SERV:E_PACKAGE_INPUT"
        );

        for (uint256 i = 0; i < packageData.tokens.length; i++) {
            ISuperToken _token = packageData.tokens[i];

            require(packageData.flowRates[i] > 0, "SERV:E_FLOWRATE_INVALID");
            require(address(_token) != address(0), "SERV:E_INVALID_TOKEN");
            // Every loop we make an external call, this is annoying. Perhaps bulk verify?
            require(DIAGONAL_REGISTRY.acceptedTokens(_token), "SERV:E_TOKEN_NOT_REG_WHITELIST");
            require(self.packages[packageId].paymentTerms[_token] == 0, "SERV:E_PAYMENT_TERMS_EXIST");

            self.packages[packageId].paymentTerms[_token] = packageData.flowRates[i];
        }

        self.packages[packageId].id = packageId;
        self.packages[packageId].state = packageData.state;
        emit PackageAdded(packageId, packageData.tokens.length);
    }

    /// @notice IDiagonalServiceV1.stopPackage implementation
    function stopPackage(DiagonalServiceStorage storage self, uint256 id) external {
        self.packages[id].state = PackageState.STOPPED;
        emit PackageStopped(id);
    }

    /// @notice IDiagonalServiceV1.activatePackage implementation
    function activatePackage(DiagonalServiceStorage storage self, uint256 id) external {
        self.packages[id].state = PackageState.ACTIVE;
        emit PackageActivated(id);
    }

    /// @notice IDiagonalServiceV1.addPrimaryTokens implementation
    function addPrimaryTokens(DiagonalServiceStorage storage self, ISuperToken[] calldata superTokens) external {
        uint256 currLength = self.primaryAcceptedTokenList.length;
        require(currLength < MAX_PRIMARY_TOKENS, "SERV:E_MAX_PRIMARY_TOKENS");

        uint256 maxTokensToAdd = MAX_PRIMARY_TOKENS - currLength;

        for (uint256 i = 0; i < superTokens.length; i++) {
            if (i == maxTokensToAdd) break;

            ISuperToken _token = superTokens[i];
            require(address(_token) != address(0), "SERV:E_INVALID_TOKEN");
            require(self.primaryTokens[_token] == false, "SERV:E_PRIMARY_TOKEN_EXISTS");
            require(DIAGONAL_REGISTRY.acceptedTokens(_token), "SERV:E_TOKEN_NOT_REG_WHITELIST");

            self.primaryTokens[_token] = true;
            self.primaryAcceptedTokenList.push(_token);

            emit PrimaryTokenAdded(_token);
        }
    }
}

