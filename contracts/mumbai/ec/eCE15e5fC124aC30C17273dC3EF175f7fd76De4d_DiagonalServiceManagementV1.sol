pragma solidity 0.8.4;

import "@openzeppelin/contracts/utils/Counters.sol";
import { ISuperToken } from "@superfluid-finance/ethereum-contracts/contracts/interfaces/superfluid/ISuperToken.sol";

/// @dev If the service is STOPPED the already running subscriptions will continue to run.
enum ServiceState {
    STOPPED,
    ACTIVE
}

/// @dev If the package is stopped, users can no longer start subscriptions
enum PackageState {
    STOPPED,
    ACTIVE
}

/// @notice Package state information
struct Package {
    uint256 id;
    PackageState state;
    mapping(ISuperToken => int96) paymentTerms;
}

/// @notice Package return type
struct PackageReturnType {
    uint256 id;
    PackageState state;
}

/// @dev  Used for package input and output params
struct PackageData {
    ISuperToken[] tokens;
    PackageState state;
    int96[] flowRates;
}

/// @notice  The fee state per token
struct FeeState {
    int96 percent;
    address recipient;
}

struct SubscriberPackageState {
    int96 feeRate;
    bool isSubscribed;
}

/// @notice User flow state and subscription metadata per super token
struct SubscriberState {
    int96 totalInputFlowRate; // the total input flow rate
    int96 totalInputFeeRate; // the total input fee rate
    uint256 numSubscriptions; // For a service, the number of packages a user is subscribed to in a single token.
    mapping(uint256 => SubscriberPackageState) packageStates;
    uint256[] packageIds;
    bool terminated;
}

struct SubscriptionCallbackArgs {
    ISuperToken superToken;
    bytes32 agreementId;
    bytes agreementData;
    bytes cbdata;
    bytes ctx;
}

struct AfterAgreementTerminatedArgs {
    ISuperToken superToken;
    address subscriber;
    address receiver;
}

struct SubscriptionStateUpdateArgs {
    address subscriber;
    ISuperToken superToken;
    uint256 packageId;
    int96 totalFlowRate;
    int96 streamFlowRate; // the change in flowRate from the user
    int96 feeRate; // fee to be streamed to treasury, given (packageRate, protocolFee).
    int96 packageRate; // price of a package
    bytes ctx;
}

struct DiagonalServiceStorage {
    address owner;
    ServiceState state;
    uint256 serviceId;
    uint256 packageIds;
    mapping(ISuperToken => int96) ownerFlowRates;
    mapping(ISuperToken => int96) treasuryFeeRates;
    mapping(uint256 => Package) packages;
    mapping(address => mapping(ISuperToken => SubscriberState)) subscriberStates;
    mapping(ISuperToken => bool) primaryTokens;
    ISuperToken[] primaryAcceptedTokenList;
}

struct SaltParams {
    uint256 chainServiceId;
    uint256 chainId;
}

