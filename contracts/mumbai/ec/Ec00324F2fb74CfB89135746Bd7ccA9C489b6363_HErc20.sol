    if (_from == _to) {
      super._beforeTokenTransfer(_operator, _from, _to, _ids, _amounts, _data);
      return;
    }
    uint256 j;
    uint256 lengthToModify;
    for (j; j < _ids.length; j++) {
      // HTokens don't require coupon management
      if (_ids[j] > 0) {
        Collateral memory collateral = collateralPerBorrowCouponId[_ids[j]];
        if (!collateral.active) continue;
        borrowCoupons[collateral.collateralId].owner = _to;
        lengthToModify++;
      }
    }

    hivemind.transferAllowed(HTokenI(address(this)));

    if (_from != address(0)) {
      userToCoupons[_from] -= lengthToModify;
    }
    if (_to != address(0)) {
      userToCoupons[_to] += lengthToModify;
    }
    super._beforeTokenTransfer(_operator, _from, _to, _ids, _amounts, _data);
  }

