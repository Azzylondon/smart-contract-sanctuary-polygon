    if (currentBlockNumber > accrualBlockNumberPrior) {
      interestPerTokenStored = interestPerToken();
      accrualBlockNumber = currentBlockNumber;
    }

    uint256 earned = _accrueInterestToCoupon(_couponId);

    totalBorrows += earned;

    totalReserves += (reserveFactorMantissa * earned) / 10**decimals;
    totalHiveFees += (hiveFeeMantissa * earned) / 10**decimals;
    totalAdminFees += (adminFeeMantissa * earned) / 10**decimals;

