  function _accrueInterestToCoupon(uint256 _couponId) internal returns (uint256) {
    Coupon storage userCoupon = borrowCoupons[_couponId];
    uint256 principalPrior = userCoupon.borrowAmount;
    uint256 interestPerTokenPrior = userCoupon.interestPerToken;

    uint256 earned = ((principalPrior * (interestPerTokenStored - userCoupon.interestPerToken)) / 10**decimals);

    userCoupon.borrowAmount += earned;
    userCoupon.interestPerToken = interestPerTokenStored;

    borrowBalancePerUser[userCoupon.owner] += earned;

    /* We emit an AccrueInterest event */
    emit AccrueCouponInterest(borrowCoupons[_couponId], principalPrior, earned, interestPerTokenPrior, interestPerTokenStored);

    return earned;
