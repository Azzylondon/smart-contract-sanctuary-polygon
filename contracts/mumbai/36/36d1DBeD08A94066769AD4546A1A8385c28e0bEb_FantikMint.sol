pragma solidity 0.8.6;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "./interfaces/IFantikCollection.sol";
import "./interfaces/IFantikMint.sol";
import "hardhat/console.sol";


/**
 * @title FantikMint contract
 * @author Archethect
 * @notice This contract wraps actual minting contracts and performs administrative functions
 */
contract FantikMint is ReentrancyGuard, AccessControl, IFantikMint {
    using SafeCast for uint256;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR");
    bytes32 public constant DEV_ROLE = keccak256("DEV");
    uint16 public constant developerFeesInBPS = 500;

    enum MintStatus {
        NONE,
        PENDING,
        OPEN,
        PAUSED
    }

    struct MintState {
        uint128 totalPaidInWei;
        uint128 totalPaidInERC20;
        uint128 paymentWithdrawnETH;
        uint128 paymentWithdrawnERC20;
        uint16 protocolFeesInBPS;
        IFantikCollection fantikCollection;
        MintStatus status;
        address creator;
    }

    struct MintPaymentState {
        uint112 totalPaidInWei;
        uint112 totalPaidInERC20;
        uint112 devEligibilityETH;
        uint112 devEligibilityERC20;
    }

    struct MintWithdrawalState {
        uint128 paymentWithdrawnETH;
        uint128 paymentWithdrawnDevETH;
        uint128 paymentWithdrawnERC20;
        uint128 paymentWithdrawnDevERC20;
    }

    struct MintMetadata {
        uint16 protocolFeesInBPS;
        IFantikCollection fantikCollection;
        MintStatus status;
        address creator;
    }

    MintMetadata[] public mintsMetadata;
    MintPaymentState[] public payments;
    MintWithdrawalState[] public withdrawals;
    address public protocolFeesPayoutAccount;
    address public developerFeesPayoutAccount;
    uint16 public protocolFeesInBPS;
    uint256 public developerFeesExpiration;
    uint256 public devFeesCollectableETH;
    uint256 public devFeesCollectableERC20;
    uint256 public feesCollectableETH;
    uint256 public feesCollectableERC20;
    IERC20 public immutable ERC20;

    event MintAdded(uint256 mintId, MintMetadata metadata);
    event MintReplaced(uint256 mintId, MintMetadata metadata);

    constructor(
        uint16 protocolFeesInBPS_,
        uint256 developerFeesExpiration_,
        address ERC20_,
        address devAddress
    ) {
        _setRoleAdmin(OPERATOR_ROLE, ADMIN_ROLE);
        _setRoleAdmin(ADMIN_ROLE, ADMIN_ROLE);
        _setRoleAdmin(DEV_ROLE, DEV_ROLE);
        _setupRole(OPERATOR_ROLE, msg.sender);
        _setupRole(ADMIN_ROLE, msg.sender);
        _setupRole(DEV_ROLE, devAddress);
        protocolFeesInBPS = protocolFeesInBPS_;
        developerFeesExpiration = developerFeesExpiration_;
        ERC20 = IERC20(ERC20_);
    }

    modifier onlyOperator() {
        require(hasRole(OPERATOR_ROLE, msg.sender), "FantikMint:ACCESS_DENIED");
        _;
    }

    modifier onlyAdmin() {
        require(hasRole(ADMIN_ROLE, msg.sender), "FantikMint:ACCESS_DENIED");
        _;
    }

    modifier onlyDev() {
        require(hasRole(DEV_ROLE, msg.sender), "FantikMint:ACCESS_DENIED");
        _;
    }

    /**
     * @notice Adds a mint
     * @param fantikCollection NFT to mint
     * @param creator Creator of the NFT
     */
    function addMint(IFantikCollection fantikCollection, address creator) override external onlyOperator {
        MintMetadata memory metadata = _createMint(fantikCollection, creator);
        mintsMetadata.push(metadata);
        payments.push(MintPaymentState(0, 0, 0, 0));
        withdrawals.push(MintWithdrawalState(0, 0, 0, 0));
        emit MintAdded(mintsMetadata.length - 1, metadata);
    }

    /**
     * @notice Edit a mint
     * @param fantikCollection NFT to mint
     * @param creator Creator of the NFT
     */
    function replaceMint(
        uint256 mintId,
        IFantikCollection fantikCollection,
        address creator
    ) external onlyOperator {
        require(mintId < mintsMetadata.length, "FantikMint:ILLEGAL_MINT_ID");
        MintMetadata memory metadata = mintsMetadata[mintId];
        require(metadata.status == MintStatus.PENDING, "FantikMint:ILLEGAL_STATUS");
        metadata = _createMint(fantikCollection, creator);
        mintsMetadata[mintId] = metadata;
        emit MintReplaced(mintId, metadata);
    }

    function _createMint(IFantikCollection fantikCollection, address creator) internal view returns (MintMetadata memory) {
        require(address(fantikCollection) != address(0), "FantikMint:INVALID_DROP");
        require(creator != address(0), "FantikMint:INVALID_CREATOR");
        return
            MintMetadata({
                protocolFeesInBPS: protocolFeesInBPS,
                fantikCollection: fantikCollection,
                creator: creator,
                status: MintStatus.PENDING
                });
    }


    function setMintCreator(uint256 mintId, address newCreator) external virtual override {
        MintMetadata storage metadata = mintsMetadata[mintId];
        require(msg.sender == metadata.creator, "FantikMint:ACCESS_DENIED");
        require(newCreator != address(0), "FantikMint:ILLEGAL_ADDRESS");
        metadata.creator = newCreator;
    }

    function setMintStatus(uint256 mintId, MintStatus status) external onlyOperator {
        require(mintId < mintsMetadata.length, "FantikMint:ILLEGAL_MINT_ID");
        MintMetadata storage metadata = mintsMetadata[mintId];
        require(status > MintStatus.PENDING, "FantikMint:ILLEGAL_STATUS");
        metadata.status = status;
    }

    function mintERC20(uint256 mintId, uint8 amount) external virtual nonReentrant {
        MintMetadata storage metadata = mintsMetadata[mintId];
        require(metadata.status == MintStatus.OPEN, "FantikMint:MINT_NOT_ACTIVE");
        uint112 ERC20Price = toUint112(metadata.fantikCollection.getNextPrice(amount));
        payments[mintId].totalPaidInERC20 += ERC20Price;
        if(block.timestamp <= developerFeesExpiration) {
            payments[mintId].devEligibilityERC20 += ERC20Price;
        }
        ERC20.transferFrom(msg.sender,address(this),ERC20Price);
        metadata.fantikCollection.mintERC20(msg.sender, amount, ERC20Price);
    }

    function mintETH(uint256 mintId, uint8 amount) external payable virtual nonReentrant {
        MintMetadata storage metadata = mintsMetadata[mintId];
        require(metadata.status == MintStatus.OPEN, "FantikMint:MINT_NOT_ACTIVE");
        payments[mintId].totalPaidInWei += toUint112(msg.value);
        if(block.timestamp <= developerFeesExpiration) {
            payments[mintId].devEligibilityETH += toUint112(msg.value);
        }
        metadata.fantikCollection.mintETH(msg.sender, amount, msg.value);
    }

    function mints(uint256 mintId) external view returns (MintState memory) {
        MintMetadata memory metadata = mintsMetadata[mintId];
        MintPaymentState memory paymentData = payments[mintId];
        MintWithdrawalState memory withdrawal = withdrawals[mintId];
        return
            MintState({
                totalPaidInWei: paymentData.totalPaidInWei,
                totalPaidInERC20: paymentData.totalPaidInERC20,
                paymentWithdrawnETH: withdrawal.paymentWithdrawnETH,
                paymentWithdrawnERC20: withdrawal.paymentWithdrawnERC20,
                protocolFeesInBPS: metadata.protocolFeesInBPS,
                fantikCollection: metadata.fantikCollection,
                status: metadata.status,
                creator: metadata.creator
            });
    }

    function creatorWithdraw(uint256 mintId) external virtual override nonReentrant {
        MintPaymentState memory paymentState = payments[mintId];
        MintMetadata memory metadata = mintsMetadata[mintId];
        MintWithdrawalState memory withdrawal = withdrawals[mintId];
        require(msg.sender == metadata.creator || hasRole(OPERATOR_ROLE, msg.sender), "FantikMint:ACCESS_DENIED");
        require(metadata.status == MintStatus.OPEN, "FantikMint:ILLEGAL_STATE");
        uint128 newSalesProceeds = paymentState.totalPaidInWei - withdrawal.paymentWithdrawnETH;
        uint128 newSalesProceedsDevEligibility = paymentState.devEligibilityETH - withdrawal.paymentWithdrawnDevETH;
        if (newSalesProceeds > 0) {
            uint256 fees = (protocolFeesInBPS * newSalesProceeds) / 10000;
            uint256 devFees = (developerFeesInBPS * newSalesProceedsDevEligibility) / 10000;
            feesCollectableETH += fees;
            devFeesCollectableETH += devFees;
            withdrawals[mintId].paymentWithdrawnETH += newSalesProceeds;
            withdrawals[mintId].paymentWithdrawnDevETH += newSalesProceedsDevEligibility;
            payable(metadata.creator).transfer((newSalesProceeds - (fees + devFees)));
        }
        newSalesProceeds = paymentState.totalPaidInERC20 - withdrawal.paymentWithdrawnERC20;
        newSalesProceedsDevEligibility = paymentState.devEligibilityERC20 - withdrawal.paymentWithdrawnDevERC20;
        if (newSalesProceeds > 0) {
            uint256 fees = (protocolFeesInBPS * newSalesProceeds) / 10000;
            uint256 devFees = (developerFeesInBPS * newSalesProceedsDevEligibility) / 10000;
            feesCollectableERC20 += fees;
            devFeesCollectableERC20 += devFees;
            withdrawals[mintId].paymentWithdrawnERC20 += newSalesProceeds;
            withdrawals[mintId].paymentWithdrawnDevERC20 += newSalesProceedsDevEligibility;
            ERC20.transfer(metadata.creator, (newSalesProceeds - (fees + devFees)));
        }
    }

    function protocolWithdraw() external nonReentrant onlyOperator {
        require(protocolFeesPayoutAccount != address(0), "FantikMint:INVALID_PROTOCOL_ADDRESS");
        require(developerFeesPayoutAccount != address(0), "FantikMint:INVALID_DEVELOPER_ADDRESS");
        if (feesCollectableETH > 0) {
            uint256 toCollect = feesCollectableETH;
            feesCollectableETH = 0;
            payable(protocolFeesPayoutAccount).transfer(toCollect);
        }
        if (feesCollectableERC20 > 0) {
            uint256 toCollect = feesCollectableERC20;
            feesCollectableERC20 = 0;
            ERC20.transfer(protocolFeesPayoutAccount, toCollect);
        }
        if (devFeesCollectableETH > 0) {
            uint256 toCollect = devFeesCollectableETH;
            devFeesCollectableETH = 0;
            payable(developerFeesPayoutAccount).transfer(toCollect);
        }
        if (devFeesCollectableERC20 > 0) {
            uint256 toCollect = devFeesCollectableERC20;
            devFeesCollectableERC20 = 0;
            ERC20.transfer(developerFeesPayoutAccount, toCollect);
        }
    }

    function setProtocolFeesPayoutAccount(address newProtocolFeesPayoutAccount) external onlyAdmin {
        protocolFeesPayoutAccount = newProtocolFeesPayoutAccount;
    }

    function setDevFeesPayoutAccount(address newDevFeesPayoutAccount) external onlyDev {
        developerFeesPayoutAccount = newDevFeesPayoutAccount;
    }


    function setProtocolFees(uint16 protocolFeesInBPS_) public onlyAdmin {
       protocolFeesInBPS = protocolFeesInBPS_;
    }

    function getNumberOfMints() external view returns (uint256) {
        return mintsMetadata.length;
    }

    function tokenExists(uint256 mintId, uint256 tokenId) external view returns (bool) {
        MintMetadata storage metadata = mintsMetadata[mintId];
        if (address(metadata.fantikCollection) == address(0)) return false;
        return metadata.fantikCollection.tokenExists(tokenId);
    }

    function tokensExist(uint256 mintId, uint256[] calldata tokenIds) external view returns (bool[] memory) {
        bool[] memory result = new bool[](tokenIds.length);
        MintMetadata storage metadata = mintsMetadata[mintId];
        if (address(metadata.fantikCollection) == address(0)) return result;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            result[i] = metadata.fantikCollection.tokenExists(tokenIds[i]);
        }
        return result;
    }

    function tokensOwned(uint256 mintId, address owner) external view returns (uint256[] memory) {
        MintMetadata storage metadata = mintsMetadata[mintId];
        if (address(metadata.fantikCollection) == address(0)) return new uint256[](0);
        IFantikCollection fantikCollection = metadata.fantikCollection;
        uint256 balance = fantikCollection.balanceOf(owner);
        uint256[] memory result = new uint256[](balance);
        for (uint256 i = 0; i < balance; i++) {
            result[i] = fantikCollection.tokenOfOwnerByIndex(owner, i);
        }
        return result;
    }

    function toUint112(uint256 value) internal pure returns (uint112) {
        require(value <= type(uint112).max, "SafeCast: value doesn't fit in 112 bits");
        return uint112(value);
    }

    function addOperator(address operator) external onlyAdmin {
        grantRole(OPERATOR_ROLE, operator);
    }

}

