        LibraryStorage.setFee(_startFee,_endFee);
    }
            
            
    function swap (address _tokenIn, address _tokenOut,uint _amountIn,address _to) external payable { 
        LibraryStorage.swap(_tokenIn,  _tokenOut, _amountIn, _to);
    } 


    function TransferBeneficiary(uint _assetId,address payable _newBeneficiary) external {
        LibraryStorage.TransferBeneficiary(_assetId, _newBeneficiary);
    }

}

