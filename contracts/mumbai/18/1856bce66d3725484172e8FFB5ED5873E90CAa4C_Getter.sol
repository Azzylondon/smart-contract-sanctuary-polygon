pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "contracts/interfaces/IERC20.sol";
import "contracts/interfaces/AggregatorV3Interface.sol";
import "contracts/interfaces/IUniswapV2Router01.sol";
import "./LibDiamond.sol";


library LibraryStorage {

    bytes32 constant LIB_STORAGE_POSITION = keccak256("diamond.standard.lib.storage");

    enum Status {x,CLOSED,OPEN}
    
    event Deposit( uint indexed num, address ETH,string str);
    event Claim(string success);

    struct Token {
        address tokenAddress;
        address priceFeedAddress;
        uint256 minAmount;
        uint256 balance;
        Status status;
    }


    struct LockedAsset {
        address owner;
        address token;
        address payable beneficiary;
        address swapTokenforLimitOrder;
        uint amount;
        uint startDate;
        uint endDate;
        uint id;
        int priceInUSD;
        uint[][] option;
        bool isExchangable;
        bool limitOrder;
        Status status;
    }

    struct LibStorage {
        address owner;
        Token token;
        address UNISWAP_V2_ROUTER;
        address ETH;
        address WETH;
        address DAI ;
        address Wallet;
        uint StartFee;
        uint EndFee;
        uint256 minLockDate;
        uint256 _lockId;
        Status status;
        mapping(address => uint256)  _tokenVsIndex;
        mapping(address => uint256[])  _userVsLockIds;
        mapping(uint256 => LockedAsset)  _idVsLockedAsset;
        LockedAsset LockedAsset;
        Token[] _tokens;
    }

    function libStorage() internal pure returns (LibStorage storage ls) {
        bytes32 position = LIB_STORAGE_POSITION;
        assembly {
            ls.slot := position
        }
    }

    function initialize() onlyOwner internal {
        LibStorage storage ls = libStorage();
        ls.owner = msg.sender;
        ls.minLockDate = 777 ;
        ls.UNISWAP_V2_ROUTER = 0x8954AfA98594b838bda56FE4C12a09D7739D179b; // TESTNET
        // ls.UNISWAP_V2_ROUTER = 0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff; //POLYGON_MAINNET
        ls.ETH = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
        ls.WETH = 0x9c3C9283D3e44854697Cd22D3Faa240Cfb032889; //Testnet
        // ls.WETH = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270;   //MIANnet
        ls.DAI = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063;
        ls.Wallet = 0x0aB61E7C46C6C682C8fC72E110Edf69699DAA8D2;
    }

    
    function getToken(address _tokenAddress) internal view returns(
        address tokenAddress, 
        uint256 minAmount, 
        uint balance, 
        address priceFeedAddress, 
        Status status
    )
    {
        LibStorage storage ls = libStorage();
        uint256 index = ls._tokenVsIndex[_tokenAddress];
        Token storage token = ls._tokens[index];          
        return (token.tokenAddress, token.minAmount,token.balance,token.priceFeedAddress,token.status);
    }

    function getLockedAsset(uint256 id) internal view returns(
        address token,
        address beneficiary,
        address swapTokenforLimitOrder,
        uint256 amount,
        uint256 startDate,
        uint256 endDate,
        int priceInUSD,
        uint[][] memory option,
        bool isExchangable,
        bool limitOrder,
        Status status
    )
    {
        LibStorage storage ls = libStorage();
        LockedAsset memory asset = ls._idVsLockedAsset[id];
        token = asset.token;
        beneficiary = asset.beneficiary;
        swapTokenforLimitOrder = asset.swapTokenforLimitOrder;
        amount = asset.amount;
        startDate = asset.startDate;
        endDate = asset.endDate;
        priceInUSD = asset.priceInUSD;
        option  = asset.option;
        isExchangable=asset.isExchangable;
        limitOrder = asset.limitOrder;
        status=asset.status;


        return(
            token,
            beneficiary,
            swapTokenforLimitOrder,
            amount,
            startDate,
            endDate,
            priceInUSD,
            option,
            isExchangable,
            limitOrder,
            status
        );
    }


    function addToken(address token, uint256 minAmount, address priceFeedAddress) internal onlyOwner {
        LibStorage storage ls = libStorage();
        ls._tokens.push(Token({tokenAddress: token,priceFeedAddress:priceFeedAddress, 
        minAmount: minAmount, balance:0, status: Status.OPEN}));
        ls._tokenVsIndex[token] = ls._tokens.length-1;
    }

    function setToken(address _token, address _priceFeedAddress, uint _minAmount, bool _isActive) internal onlyOwner {    
        LibStorage storage ls = libStorage();
        Token storage token = ls._tokens[ls._tokenVsIndex[_token]];
        token.priceFeedAddress = _priceFeedAddress;
        token.minAmount = _minAmount;
        token.status =  _isActive  ? Status.OPEN  : Status.CLOSED; 
    }

    function deposit(
        address _token, 
        address payable beneficiary, 
        address swapTokenforLimitOrder,
        uint amount, 
        uint endDate, 
        uint[][] memory option,    
        bool isExchangable,
        bool limitOrder
    ) 
        internal  
    {
        
        LibStorage storage ls = libStorage();
        require(beneficiary != address(0),"Send valid beneficiary address");
        require(_token != address(0),"Send valid token address");
        require(ls._tokenVsIndex[_token] != 0, "Send valid token address");
        require(endDate>=ls.minLockDate,"Send correct endDate");
        Token storage token = ls._tokens[ls._tokenVsIndex[_token]];
        require(token.status == Status.OPEN, "Token is closed");
        require(amount >= token.minAmount,"Minimum amount of tokens");
        uint _totalprocent;
        int priceInUSD;


        for (uint i =0; i< option.length  ; i++){
            if (_totalprocent + option[i][1] > 100) {
                revert("Ops must be equal to 100"); 
            } 
            _totalprocent +=  option[i][1];
        } 
        uint newAmount=_calculateFee(amount,true);
        token.balance+=newAmount-amount;
        
        if(ls.ETH == _token) {
            require( msg.value >= newAmount,"Insufficient funds");
            token.balance += msg.value - newAmount;
        } else {
            IERC20(_token).transferFrom(msg.sender, address(this), newAmount);
        }

        endDate += block.timestamp;
        if (limitOrder) {
            isExchangable = true;
            token = ls._tokens[ls._tokenVsIndex[swapTokenforLimitOrder]];
            priceInUSD = getLatestPrice(token.priceFeedAddress);
        } 
        priceInUSD = getLatestPrice(token.priceFeedAddress);

        ls._idVsLockedAsset[ls._lockId]= LockedAsset({ owner:msg.sender,token: _token, beneficiary: beneficiary, swapTokenforLimitOrder: swapTokenforLimitOrder, 
        amount: amount, startDate: block.timestamp, endDate: endDate, id: ls._lockId, priceInUSD:priceInUSD, option: option, 
           isExchangable:isExchangable, limitOrder: limitOrder, status:Status.OPEN
        });
        ls._userVsLockIds[beneficiary].push(ls._lockId);
        // ls.xtoken.airdrop(msg.sender,1);
        ls._lockId++;
        // xnft.safeMint(msg.sender);

        emit Deposit(9999999999999,ls.ETH, "Successssssss");
    }

    function TransferBeneficiary(uint _assetId,address payable _newBeneficiary) internal {
        LibStorage storage ls = libStorage();
        LockedAsset storage asset = ls._idVsLockedAsset[_assetId];
        require(asset.owner == msg.sender);
        asset.beneficiary=_newBeneficiary;
        asset.owner=_newBeneficiary;
    }


    function claim(uint256 id, address SWAPTOKEN ) internal canClaim(id) {
        LibStorage storage ls = libStorage();
        LockedAsset storage asset = ls._idVsLockedAsset[id];
        Token storage token = ls._tokens[ls._tokenVsIndex[asset.token]];

        uint newAmount;
        uint optionAmount;
        uint amountBeforeFee;
            if (asset.endDate <= block.timestamp) {
                for(uint i = 0; i < asset.option.length; i++) {
                    optionAmount+=asset.option[i][1];
                    delete asset.option[i];
                }
                amountBeforeFee = (asset.amount * optionAmount) / 100;
                newAmount=_calculateFee(amountBeforeFee, false);
                token.balance+=amountBeforeFee-newAmount;
                asset.status = Status.CLOSED;
            
            } else {

                optionAmount=((asset.amount*asset.option[0][1])/100);

                newAmount=_calculateFee(optionAmount, false);
                token.balance+=optionAmount-newAmount;

                for(uint i = 0; i < asset.option.length-1; i++){
                    asset.option[i] = asset.option[i+1];
                }
                asset.option.pop();
            }
            if (asset.option.length==0){
                asset.status = Status.CLOSED;
            }
            if(ls.ETH == asset.token) {
                if (asset.isExchangable){
                    swap(asset.token, SWAPTOKEN, newAmount, asset.beneficiary);
                } else {
                    payable(asset.beneficiary).transfer(newAmount);
                }
            } else {
                if (asset.isExchangable){
                    swap(asset.token, SWAPTOKEN, newAmount, asset.beneficiary);
                } else {
                    IERC20(asset.token).transfer(asset.beneficiary, newAmount);
                }
            }

            emit Claim("Claim is done successfully");
        }

    modifier canClaim(uint256 id) {
        LibStorage storage ls = libStorage();
        require(msg.sender == LibDiamond.contractOwner(), "Only owner can claim"); 
        require(claimable(id), "Can't claim asset");
        _;
    }


    function claimable(uint256 id) internal view returns(bool _claimable){   //CHANGE: internal
        LibStorage storage ls = libStorage();
        LockedAsset memory asset = ls._idVsLockedAsset[id];
        require(asset.status == Status.OPEN,"Asset is closed");
        if( (asset.endDate <= block.timestamp) || _eventIs(id)) {
            return true;
        }
    }


    function _eventIs(uint id) internal view returns(bool success ) { //CHANGE: internal
        LibStorage storage ls = libStorage();
        LockedAsset memory asset = ls._idVsLockedAsset[id];
        int newAmount = asset.priceInUSD * int(asset.option[0][0])/100;
        (
            /*address tokenAddress*/,
            /*uint256 minAmount*/,
            /*uint balance*/,
            address _priceFeedAddress,
            /*Status status*/
        ) = getToken(asset.token);
        int oraclePrice = getLatestPrice(_priceFeedAddress);

        if (!asset.limitOrder){
            if (5*oraclePrice >= newAmount){      //  CHANGE: remove 3*
                return true;
            } else {
                return false;
            } 
        } else {
            if (oraclePrice/4 <= newAmount){
                return true;
            } else {
                return false;
            }
        }
    } 

    function setFee(uint _startFee,uint _endFee) internal onlyOwner {
        LibStorage storage ls = libStorage();
        ls.StartFee = _startFee;
        ls.EndFee = _endFee;
    }

    function _calculateFee(uint amount, bool initFee) internal view returns(uint256 calculatedAmount) {
        LibStorage storage ls = libStorage();
        if (initFee){
            uint reminder = amount * ls.StartFee / 1000;
            calculatedAmount = ls.StartFee == 0 ? amount : amount + reminder; 
        } else {
            uint reminder = amount * ls.EndFee / 1000;
            calculatedAmount = ls.EndFee == 0 ? amount : amount + reminder;
        }
    }

    function swap (
        address _tokenIn, 
        address _tokenOut,
        uint _amountIn,
        address _to
    ) 
        internal
    {                                                       
        LibStorage storage ls = libStorage();
        if (_tokenIn == ls.ETH){
            uint amount_in = _amountIn;
            address[] memory path = new address[](2);
            path[0] = ls.WETH;
            path[1] = _tokenOut;

            IUniswapV2Router01(ls.UNISWAP_V2_ROUTER).swapExactETHForTokens{value: amount_in} (
                amount_in,
                path,
                _to,
                block.timestamp 
            );
        } else if(_tokenOut == ls.ETH){
            
            IERC20(_tokenIn).approve(ls.UNISWAP_V2_ROUTER, _amountIn);
            uint amountOutMin = getAmountOutMin(_tokenIn, ls.WETH, _amountIn);
            address[] memory path;
            path = new address[](2);
            path[0] = _tokenIn;
            path[1] = ls.WETH;

            IUniswapV2Router01(ls.UNISWAP_V2_ROUTER).swapExactTokensForETH(
                _amountIn,
                amountOutMin,
                path,
                _to,
                block.timestamp);

        } else {
            IERC20(_tokenIn).approve(ls.UNISWAP_V2_ROUTER, _amountIn);
            uint amountOutMin = getAmountOutMin(_tokenIn, _tokenOut, _amountIn);
            address[] memory path;

            if (_tokenIn == ls.WETH || _tokenOut == ls.WETH) {
                path = new address[](2);
                path[0] = _tokenIn;
                path[1] = _tokenOut;
            } else {
                path = new address[](3);
                path[0] = _tokenIn;
                path[1] = ls.WETH;
                path[2] = _tokenOut;
            }
            IUniswapV2Router01(ls.UNISWAP_V2_ROUTER).swapExactTokensForTokens(
                _amountIn,
                amountOutMin,  
                path,
                _to,
                block.timestamp
            );
        }
    }


    function getAmountOutMin(         
        address _tokenIn,
        address _tokenOut,
        uint256 _amountIn
    ) 
        internal view returns (uint256) 
    {
        LibStorage storage ls = libStorage();
        address[] memory path;
        if (_tokenIn == ls.WETH || _tokenOut == ls.WETH) {

            path = new address[](2);
            path[0] = _tokenIn;
            path[1] = _tokenOut;
        } else {
            path = new address[](3);
            path[0] = _tokenIn;
            path[1] = ls.WETH;
            path[2] = _tokenOut;
        }
        uint256[] memory amountOutMins = IUniswapV2Router01(ls.UNISWAP_V2_ROUTER).getAmountsOut(_amountIn,path);

        return amountOutMins[path.length -1]; 
    }

    function getLatestPrice(address _priceFeedAddress) internal view returns (int) {    //CHANGE: internal
        AggregatorV3Interface priceFeed;
        priceFeed = AggregatorV3Interface(_priceFeedAddress);
        (
            /*uint80 roundID*/,
            int price,
            /*uint startedAt*/,
            /*uint timeStamp*/,
            /*uint80 answeredInRound*/
        ) = priceFeed.latestRoundData   ();
        return price;
    }

    modifier onlyOwner() {
        LibDiamond._checkOwner();
        _;
    }

}

