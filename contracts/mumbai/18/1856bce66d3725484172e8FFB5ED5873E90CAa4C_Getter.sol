pragma solidity ^0.8.0;

import "contracts/interfaces/IERC20.sol";
import "contracts/interfaces/IERC721.sol";
import "contracts/interfaces/Counters.sol";
import "./LibDiamond.sol";


library LibNft {

    bytes32 constant internal LIB_STORAGE_POSITION = keccak256("diamond.standard.lib.nft");
    
    enum StatusNft {x,CLOSED,OPEN}

    using Counters for Counters.Counter;

    struct LockedAssetNft {
        address contractAddress;
        address payable beneficiary;
        uint256 tokenId;
        uint256 startDate;
        uint256 endDate;
        StatusNft status;
    }


    struct StorageNft {
        uint minLockDate;
        address ETH;
        uint _lockId;
        uint _mintedNft;
        uint nftFeeBalance;
        mapping(address => uint256[])  _userVsLockNftIds;
        mapping(uint256 => LockedAssetNft) _idVsLockedNftAsset;
        LockedAssetNft LockedAssetNft;
        Counters.Counter  _tokenIdCounter;
        uint c;

    }


    function storageNft() internal pure returns (StorageNft storage ln) {
        bytes32 position = LIB_STORAGE_POSITION;
        assembly {
            ln.slot := position
        }
    }


    function storeNft() internal onlyOwner {
        StorageNft storage ln = storageNft();
        ln.ETH = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
        ln.minLockDate=1;
        ln.c=ln._tokenIdCounter.current();
    }


    function getLockedAssetNft(uint256 id) internal view returns(
        address contractAddress,
        address beneficiary,
        uint256 tokenId,
        uint256 startDate,
        uint256 endDate,
        StatusNft status
    )
    {
        StorageNft storage ln = storageNft();
        LockedAssetNft memory asset = ln._idVsLockedNftAsset[id];
        contractAddress = asset.contractAddress;
        beneficiary = asset.beneficiary;
        tokenId=asset.tokenId;
        startDate = asset.startDate;
        endDate = asset.endDate;
        status=asset.status;

        return(
            contractAddress,
            beneficiary,
            tokenId,
            startDate,
            endDate,
            status
        );
    }


    function depositNft(
        address contractAddress,
        address payable beneficiary,
        address feeContractAddress,///
        uint256 tokenId, 
        uint256 endDate 
    ) 
        internal 
    {
        StorageNft storage ln = storageNft();

        require(contractAddress != address(0),"Send valid contract address");
        require(beneficiary != address(0),"Send valid beneficiary address");
        require(endDate>=ln.minLockDate,"Send correct endDate");
        require(IERC721(contractAddress).ownerOf(tokenId) == msg.sender, "Wrong token ID");

        uint256 newAmount=_calculateFeeNft(endDate);

        if (feeContractAddress == ln.ETH){
            require(msg.value >= newAmount);
        }
        IERC721(contractAddress).transferFrom(msg.sender, address(this), tokenId);
        ln.nftFeeBalance += newAmount; 

        ln._idVsLockedNftAsset[ln._lockId]= LockedAssetNft({ contractAddress: contractAddress,beneficiary: beneficiary, 
        tokenId:tokenId,startDate: block.timestamp, endDate: endDate, status:StatusNft.OPEN});
        ln._userVsLockNftIds[beneficiary].push(ln._lockId);
        ln._lockId++;

    }

    function _calculateFeeNft(uint256 endDate) internal view returns(uint256) { 
        uint256 fee;
        if ((endDate - block.timestamp)/31536000<=1){
            fee=1;
        } else {
            fee=(endDate - block.timestamp)/31556926;
        }
        return fee;
    }


    function claimNft(uint256 id) internal canClaimNft(id) {
        StorageNft storage ln = storageNft();
        LockedAssetNft storage asset = ln._idVsLockedNftAsset[id];
        IERC721(asset.contractAddress).transferFrom(address(this), asset.beneficiary, asset.tokenId);
        asset.status = StatusNft.CLOSED;
    }

    modifier canClaimNft(uint256 id) {
        StorageNft storage ln = storageNft();
        require(msg.sender == LibDiamond.contractOwner() , "Only owner can claimNft"); //////////////////
        LockedAssetNft memory asset = ln._idVsLockedNftAsset[id];
        require(asset.status == StatusNft.OPEN,"Asset is closed");
        require(asset.endDate <= block.timestamp);
        _;
    }

    modifier onlyOwner() {
        LibDiamond._checkOwner();
        _;
    }

}

