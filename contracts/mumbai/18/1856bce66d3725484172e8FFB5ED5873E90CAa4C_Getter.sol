pragma solidity ^0.8.0;

import "../libraries/LibraryStorage.sol";
import "./Nft.sol";

contract Getter {  

    function initialize() external {
        return LibraryStorage.initialize();
    }

    function gett() external view returns (address) {
        return LibraryStorage.libStorage().ETH;
    }

    function getToken(address _tokenAddress) external view returns(address, uint256, uint, address , LibraryStorage.Status status){        
        return LibraryStorage.getToken(_tokenAddress);
    }

    function getLockedAsset(uint256 _id) external view returns(address, address, address, uint256, uint256, uint256, int, uint[][] memory, bool, bool, LibraryStorage.Status){
        return LibraryStorage.getLockedAsset(_id);    
    }

    function addToken(address token, uint256 minAmount, address priceFeedAddress) external  {
        LibraryStorage.addToken(token, minAmount, priceFeedAddress);
    }

    function getAmountOutMin(address _tokenIn, address _tokenOut, uint256 _amountIn) external view returns(uint256){
        return LibraryStorage.getAmountOutMin(_tokenIn, _tokenOut, _amountIn);
    }

    function setToken(address _token, address _priceFeedAddress, uint _minAmount, bool _isActive) external {    
        LibraryStorage.setToken(_token, _priceFeedAddress, _minAmount, _isActive);
    }

    function deposit(
        address _token, //contract
        address payable beneficiary, 
        address swapTokenforLimitOrder,
        uint amount, 
        uint endDate, 
        uint[][] memory option,    
        bool isExchangable,
        bool limitOrder
    ) 
        external payable
    {
        LibraryStorage.deposit(_token,beneficiary,swapTokenforLimitOrder,amount,endDate,option,isExchangable,limitOrder);
        Nft(address(this)).safeMintNft(msg.sender);
