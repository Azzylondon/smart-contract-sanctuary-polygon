pragma solidity 0.5.0;

import "hardhat/console.sol";
import "./MemeCoin.sol";

contract MegaMeme {
    TokenMintERC20Token memeCoin;

    event PostCreated(
        uint256 indexed postId,
        address indexed postOwner,
        uint256 contentId,
        uint256 categoryId
    );
    event ContentAdded(uint256 indexed contentId, string contentUri);
    event CategoryCreated(uint256 indexed categoryId, string category);
    event Tip(uint256 tipAmount, uint256 indexed postId);

    struct Post {
        address postOwner;
        uint256 contentId;
        uint256 categoryId;
        uint256 numOfTips;
        uint256 totalTipAmount;
    }

    address public owner;
    uint256 public fee;
    mapping(uint256 => string) public categoryRegistry;
    mapping(uint256 => string) public contentRegistry;
    mapping(uint256 => Post) public postRegistry;

    uint256[] public postIds;
    uint256[] public contentIds;
    uint256[] public categoryIds;

    modifier onlyOwner() {
        require(msg.sender == owner, "Only Owner");
        _;
    }

    constructor(address erc20Address) public {
        memeCoin = TokenMintERC20Token(erc20Address);
        owner = msg.sender;
        fee = 10;
    }

    function createPost(string calldata _contentUri, uint256 _categoryId)
        external
    {
        address postOwner = msg.sender;
        if (postOwner != owner) {
            require(
                memeCoin.allowance(msg.sender, address(this)) >= getFee(),
                "creator did not pre approve meme"
            );
            require(
                memeCoin.transferFrom(msg.sender, address(this), getFee()) ==
                    true,
                "fee error"
            );
        }
        uint256 _contentId = contentIds.length + 1;
        uint256 _postId = postIds.length + 1;
        require(postExists(_postId) == false, "Post already exists");
        contentRegistry[_contentId] = _contentUri;
        postRegistry[_postId].postOwner = postOwner;
        postRegistry[_postId].contentId = _contentId;
        postRegistry[_postId].categoryId = _categoryId;
        postRegistry[_postId].numOfTips = 0;
        postRegistry[_postId].totalTipAmount = 0;
        postIds.push(_postId);
        contentIds.push(_contentId);
        emit ContentAdded(_contentId, _contentUri);
        emit PostCreated(_postId, postOwner, _contentId, _categoryId);
    }

    function postExists(uint256 _postId) internal view returns (bool) {
        return postRegistry[_postId].postOwner != address(0);
    }

    function tipPostCreator(uint256 _postId, uint256 numOfMemeCoins) public {
        require(
            memeCoin.balanceOf(msg.sender) >= numOfMemeCoins,
            "balance is less than tip amount"
        );
        Post memory _post = postRegistry[_postId];
        memeCoin.transferFrom(msg.sender, _post.postOwner, numOfMemeCoins);
        _post.numOfTips = _post.numOfTips + 1;
        _post.totalTipAmount = _post.numOfTips + numOfMemeCoins;
        emit Tip(numOfMemeCoins, _postId);
    }

    function addCategory(string calldata _category) external {
        uint256 _categoryId = categoryIds.length + 1;
        categoryRegistry[_categoryId] = _category;
        emit CategoryCreated(_categoryId, _category);
    }

    function getFee() internal view returns (uint256) {
        return fee * (10**uint256(memeCoin.decimals()));
    }

    function getContractBalance() public view returns (uint256) {
        return memeCoin.balanceOf(address(this));
    }

    function getAllMemes() external view returns (uint256[] memory memes) {
        return postIds;
    }

    function setFee(uint256 _fee) public onlyOwner {
        require(_fee > 0, "fee should be greater than 0");
        fee = _fee;
    }

    function setToken(address _address) public onlyOwner {
        memeCoin = TokenMintERC20Token(_address);
    }

    function withDraw(address payable recipient) public onlyOwner {
        memeCoin.transfer(recipient, getContractBalance());
        recipient.transfer((address(this).balance));
    }

    //migration function
    function importMeme(
        string calldata _contentUri,
        uint256 _categoryId,
        address postOwner,
        uint256 numOfTips,
        uint256 totalTipAmount
    ) external onlyOwner {
        uint256 _contentId = contentIds.length + 1;
        uint256 _postId = postIds.length + 1;
        require(postExists(_postId) == false, "Post already exists");
        contentRegistry[_contentId] = _contentUri;
        postRegistry[_postId].postOwner = postOwner;
        postRegistry[_postId].contentId = _contentId;
        postRegistry[_postId].categoryId = _categoryId;
        postRegistry[_postId].numOfTips = numOfTips;
        postRegistry[_postId].totalTipAmount = totalTipAmount;
        postIds.push(_postId);
        contentIds.push(_contentId);
        emit ContentAdded(_contentId, _contentUri);
        emit PostCreated(_postId, postOwner, _contentId, _categoryId);
    }
}

