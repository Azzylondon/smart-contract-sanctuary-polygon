pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/access/IAccessControlEnumerableUpgradeable.sol";

interface IPentaCore is IAccessControlEnumerableUpgradeable {
    struct PlayDataParam {
        uint256 tokenId;
        string trackName;
        string artistName;
        string mode;
        string[] trackGenres;
        bytes32 nonce;
    }

    struct PlayData {
        address addr;
        uint256 timestamp;
        string trackName;
        string artistName;
        string mode;
        string[] trackGenres;
    }

    struct HealUserHPParam {
        uint256 newUserHp;
        uint256[] charge;
        bytes32 nonce;
    }

    struct MineStoneParam {
        uint256 tokenId;
        uint256 newUserHp;
        string newStatus;
        uint256 amount;
        bytes32 nonce;
    }

    struct GrowHeadphoneParam {
        uint256 tokenId;
        uint256 exp;
        uint256[] charge;
        bytes32 nonce;
    }

    struct RemodelHeadphoneParam {
        uint256 tokenId;
        uint256 newLevel;
        uint256 newRarity;
        string artworkUri;
        uint256[] charge;
        bytes32 nonce;
    }

    struct RepairHeadphoneParam {
        uint256 tokenId;
        string status;
        uint256[] charge;
        bytes32 nonce;
    }

    event HeadphoneStatusUpdated(uint256 indexed tokenId, address indexed user, string newStatus);

    event HeadphoneGrew(uint256 indexed tokenId, address indexed user, uint256 newExperience);

    event HeadphoneRemodeled(
        uint256 indexed tokenId,
        address indexed user,
        uint256 newLevel,
        uint256 newExperience,
        uint256 newRarity,
        string artworkUri
    );

    event HeadphoneRepaired(uint256 indexed tokenId, address indexed user, string newStatus);

    event PlayDataAdded(
        uint256 indexed tokenId,
        address indexed user,
        uint256 timestamp,
        string trackName,
        string artistName,
        string mode,
        string[] trackGenres
    );

    function getLastHPUsedTime(address user) external view returns (uint256);

    function getUserHp(address user) external view returns (uint256);

    function healUserHP(HealUserHPParam calldata healParam, bytes calldata signature) external;

    function mineStone(MineStoneParam calldata mineParam, bytes calldata signature) external;

    function growHeadphone(GrowHeadphoneParam calldata growParam, bytes calldata signature) external;

    function remodelHeadphone(RemodelHeadphoneParam calldata remodelParam, bytes calldata signature) external;

    function repairHeadphone(RepairHeadphoneParam calldata repairParam, bytes calldata signature) external;

    function addPlayData(PlayDataParam memory playDataParam, bytes calldata signature) external;

    function getPlayDataList(uint256 _tokenId) external view returns (PlayData[] memory);
}

