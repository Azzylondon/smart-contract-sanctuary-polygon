pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts/utils/Multicall.sol";
import "./IPentaCore.sol";
import "./VerifyCoreLogic.sol";
import "../nft/IHeadphoneNFT.sol";
import "../itemNFT/IPentaStone.sol";

contract PentaCoreWithMultiCall is EIP712Upgradeable, AccessControlEnumerableUpgradeable, IPentaCore, Multicall {
    using ECDSAUpgradeable for bytes32;

    IHeadphoneNFT private headphoneNFT;
    IPentaStone private pentaStone;

    mapping(bytes32 => bool) private usedNonces;
    mapping(uint256 => PlayData[]) private playDataListMapping;
    mapping(address => PlayData[]) private playDataListPerAddress;
    mapping(address => uint256) private userHpMapping;
    mapping(address => uint256) private userLastHpUsedTimeMapping;

    bytes32 public constant SIGNER_ROLE = keccak256("SIGNER_ROLE");

    function initialize(address _headphoneNFT, address _pentaStone) public initializer {
        headphoneNFT = IHeadphoneNFT(_headphoneNFT);
        pentaStone = IPentaStone(_pentaStone);

        __AccessControlEnumerable_init();
        __EIP712_init("Penta-Core", "1.0.0");
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(SIGNER_ROLE, _msgSender());
    }

    function _useStone(address from, uint256[] calldata ids) private {
        pentaStone.burn(from, ids);
    }

    modifier verify(
        bytes32 structHash,
        bytes32 nonce,
        bytes calldata signature
    ) {
        address signer = _hashTypedDataV4(structHash).recover(signature);

        require(!usedNonces[nonce], "PentaCore: nonce is already used.");
        require(hasRole(SIGNER_ROLE, signer), "PentaCore: invalid signature");
        usedNonces[nonce] = true;
        _;
    }

    modifier onlyNftOwner(uint256 tokenId) {
        require(headphoneNFT.ownerOf(tokenId) == _msgSender(), "PentaCore: not owner of the token");
        _;
    }

    function getLastHPUsedTime(address user) external view returns (uint256) {
        return userLastHpUsedTimeMapping[user];
    }

    function getUserHp(address user) public view returns (uint256) {
        uint256 autoHealHp = ((block.timestamp - userLastHpUsedTimeMapping[user]) / 1 hours) * 150;
        uint256 hp = userHpMapping[user] + autoHealHp;
        return hp > 3600 ? 3600 : hp;
    }

    function healUserHP(HealUserHPParam calldata healParam, bytes calldata signature)
        public
        verify(VerifyCoreLogic.getHealStructHash(healParam), healParam.nonce, signature)
    {
        _useStone(_msgSender(), healParam.charge);
        userHpMapping[_msgSender()] = healParam.newUserHp;
    }

    /**
     * @dev NFTを壊さない場合はnewStatsusはkeepにする
     */
    function mineStone(MineStoneParam calldata mineParam, bytes calldata signature)
        public
        onlyNftOwner(mineParam.tokenId)
        verify(VerifyCoreLogic.getMineStructHash(mineParam), mineParam.nonce, signature)
    {
        require(!headphoneNFT.isCoolDown(mineParam.tokenId), "PentaCore: cool down now");
        if (mineParam.amount > 0) {
            pentaStone.mint(_msgSender(), mineParam.amount);
        }
        userHpMapping[_msgSender()] = mineParam.newUserHp;
        userLastHpUsedTimeMapping[_msgSender()] = block.timestamp;

        if (keccak256(bytes(mineParam.newStatus)) != keccak256(bytes("keep"))) {
            headphoneNFT.setStatus(mineParam.tokenId, mineParam.newStatus);
            emit HeadphoneStatusUpdated(mineParam.tokenId, _msgSender(), mineParam.newStatus);
        }
    }

    function growHeadphone(GrowHeadphoneParam calldata growParam, bytes calldata signature)
        public
        onlyNftOwner(growParam.tokenId)
        verify(VerifyCoreLogic.getGrowStructHash(growParam), growParam.nonce, signature)
    {
        _useStone(_msgSender(), growParam.charge);
        uint256 experience = headphoneNFT.getHeadphoneNftData(growParam.tokenId).experience + growParam.exp;
        uint256 newExp = experience > 99 ? 99 : experience;
        headphoneNFT.setExperience(growParam.tokenId, newExp);

        emit HeadphoneGrew(growParam.tokenId, _msgSender(), newExp);
    }

    function remodelHeadphone(RemodelHeadphoneParam calldata remodelParam, bytes calldata signature)
        public
        onlyNftOwner(remodelParam.tokenId)
        verify(VerifyCoreLogic.getRemodelStructHash(remodelParam), remodelParam.nonce, signature)
    {
        _useStone(_msgSender(), remodelParam.charge);
        headphoneNFT.setLevel(remodelParam.tokenId, remodelParam.newLevel);
        headphoneNFT.setExperience(remodelParam.tokenId, 0);
        headphoneNFT.setRarity(remodelParam.tokenId, remodelParam.newRarity);
        headphoneNFT.setArtworkUrl(remodelParam.tokenId, remodelParam.artworkUri);

        emit HeadphoneRemodeled(
            remodelParam.tokenId,
            _msgSender(),
            remodelParam.newLevel,
            0,
            remodelParam.newRarity,
            remodelParam.artworkUri
        );
    }

    function repairHeadphone(RepairHeadphoneParam calldata repairParam, bytes calldata signature)
        public
        onlyNftOwner(repairParam.tokenId)
        verify(VerifyCoreLogic.getRepairStructHash(repairParam), repairParam.nonce, signature)
    {
        _useStone(_msgSender(), repairParam.charge);
        string memory nftStatus = headphoneNFT.getHeadphoneNftData(repairParam.tokenId).status;
        require(keccak256(bytes(nftStatus)) != keccak256("normal"), "status should not be normal before repairing.");
        headphoneNFT.setStatus(repairParam.tokenId, repairParam.status);

        emit HeadphoneRepaired(repairParam.tokenId, _msgSender(), repairParam.status);
    }

    function addPlayData(PlayDataParam memory playDataParam, bytes calldata signature)
        public
        onlyNftOwner(playDataParam.tokenId)
        verify(VerifyCoreLogic.getPlayDataStructHash(playDataParam), playDataParam.nonce, signature)
    {
        PlayData memory playData = PlayData(
            _msgSender(),
            block.timestamp,
            playDataParam.trackName,
            playDataParam.artistName,
            playDataParam.mode,
            playDataParam.trackGenres
        );

        playDataListMapping[playDataParam.tokenId].push(playData);
        playDataListPerAddress[_msgSender()].push(playData);

        emit PlayDataAdded(
            playDataParam.tokenId,
            _msgSender(),
            block.timestamp,
            playData.trackName,
            playData.artistName,
            playData.mode,
            playData.trackGenres
        );
    }

    function getPlayDataList(uint256 tokenId) public view returns (PlayData[] memory) {
        return playDataListMapping[tokenId];
    }

    function getPlayDataListPerAddress(address _user) public view returns (PlayData[] memory) {
        return playDataListPerAddress[_user];
    }
}

