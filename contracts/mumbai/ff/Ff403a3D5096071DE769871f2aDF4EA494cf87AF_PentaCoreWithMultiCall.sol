pragma solidity 0.8.9;

import "./IPentaCore.sol";

library VerifyCoreLogic {
    bytes32 private constant HEAL_USER_HP_TYPEHASH =
        keccak256("HealUserHPParam(uint256 newUserHp,uint256[] charge,bytes32 nonce)");
    bytes32 private constant MINE_STONE_TYPEHASH =
        keccak256("MineStoneParam(uint256 tokenId,uint256 newUserHp,string newStatus,uint256 amount,bytes32 nonce)");
    bytes32 private constant GROW_HEADPHONE_TYPEHASH =
        keccak256("GrowHeadphoneParam(uint256 tokenId,uint256 exp,uint256[] charge,bytes32 nonce)");
    bytes32 private constant REMODEL_HEADPHONE_TYPEHASH =
        keccak256(
            abi.encodePacked(
                "RemodelHeadphoneParam(",
                "uint256 tokenId,",
                "uint256 newLevel,",
                "uint256 newRarity,",
                "string artworkUri,",
                "uint256[] charge,",
                "bytes32 nonce",
                ")"
            )
        );
    bytes32 private constant REPAIR_HEADPHONE_TYPEHASH =
        keccak256("RepairHeadphoneParam(uint256 tokenId,string status,uint256[] charge,bytes32 nonce)");
    bytes32 private constant PLAY_DATA_TYPEHASH =
        keccak256(
            abi.encodePacked(
                "PlayDataParam(",
                "uint256 tokenId,",
                "string trackName,",
                "string artistName,",
                "string mode,",
                "string[] trackGenres,",
                "bytes32 nonce",
                ")"
            )
        );

    function stringArrayHash(string[] memory _array) internal pure returns (bytes32) {
        bytes32[] memory arrayHashes = new bytes32[](_array.length);
        for (uint256 i = 0; i < _array.length; i++) {
            arrayHashes[i] = keccak256(bytes(_array[i]));
        }
        return keccak256(abi.encodePacked(arrayHashes));
    }

    function getHealStructHash(IPentaCore.HealUserHPParam calldata param) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    HEAL_USER_HP_TYPEHASH,
                    param.newUserHp,
                    keccak256(abi.encodePacked(param.charge)),
                    param.nonce
                )
            );
    }

    function getMineStructHash(IPentaCore.MineStoneParam calldata param) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    MINE_STONE_TYPEHASH,
                    param.tokenId,
                    param.newUserHp,
                    keccak256(bytes(param.newStatus)),
                    param.amount,
                    param.nonce
                )
            );
    }

    function getGrowStructHash(IPentaCore.GrowHeadphoneParam calldata param) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    GROW_HEADPHONE_TYPEHASH,
                    param.tokenId,
                    param.exp,
                    keccak256(abi.encodePacked(param.charge)),
                    param.nonce
                )
            );
    }

    function getRemodelStructHash(IPentaCore.RemodelHeadphoneParam calldata param) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    REMODEL_HEADPHONE_TYPEHASH,
                    param.tokenId,
                    param.newLevel,
                    param.newRarity,
                    keccak256(bytes(param.artworkUri)),
                    keccak256(abi.encodePacked(param.charge)),
                    param.nonce
                )
            );
    }

    function getRepairStructHash(IPentaCore.RepairHeadphoneParam calldata param) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    REPAIR_HEADPHONE_TYPEHASH,
                    param.tokenId,
                    keccak256(bytes(param.status)),
                    keccak256(abi.encodePacked(param.charge)),
                    param.nonce
                )
            );
    }

    function getPlayDataStructHash(IPentaCore.PlayDataParam memory param) internal pure returns (bytes32) {
        return
            keccak256(
                abi.encode(
                    PLAY_DATA_TYPEHASH,
                    param.tokenId,
                    keccak256(bytes(param.trackName)),
                    keccak256(bytes(param.artistName)),
                    keccak256(bytes(param.mode)),
                    stringArrayHash(param.trackGenres),
                    param.nonce
                )
            );
    }
}

