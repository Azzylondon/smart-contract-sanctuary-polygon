pragma solidity 0.8.12;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

// Staking contract for Monzter Minions OG NFT 0x688D311B30D6121Fc7354E7b5d8F5C83bb4fe92a
contract mmGenesisStakeSC is IERC721Receiver, ERC165, ERC721Holder, Ownable{
    using SafeMath for uint256;
    
    IERC721 public parentNFT;
    IERC20 public rewardTokenFire;
    IERC20 public rewardTokenWater;
    IERC20 public rewardTokenEarth;
    IERC20 public rewardTokenStorm;
    IERC20 public rewardTokenLegendary;

    uint256 public rewardPerSecond = SafeMath.div(1e18,(24*60*60));
    uint256 public lastRewardTimestamp;
    bool public paused = true;

    uint256 maxStaked = 30;
    
    struct Stake {
        address staker;
        uint256 timestamp;
    }

    //array of Stake
    Stake[1251] stakes; //ignore array element 0


    //maping of tokenId to element type, load in after contract deployment
    mapping(uint256 => uint256) public elements;


    constructor(address _parentNFT, address _rewardTokenFire, address _rewardTokenWater, address _rewardTokenEarth, address _rewardTokenStorm, address _rewardTokenLegendary) {
        supportsInterface(IERC721Receiver.onERC721Received.selector);
        parentNFT = IERC721(_parentNFT);
        rewardTokenFire = IERC20(_rewardTokenFire);
        rewardTokenWater = IERC20(_rewardTokenWater);
        rewardTokenEarth = IERC20(_rewardTokenEarth);
        rewardTokenStorm = IERC20(_rewardTokenStorm);
        rewardTokenLegendary = IERC20(_rewardTokenLegendary);
    }


    // Stake a NFT by transferring it from the sender to the contract address
    function stake(uint256 _tokenId) public {
        _stake(_tokenId);  
    }

    //stake all NFTs at once
    function stakeAll(uint256[] memory _tokenIDs) public {
        for (uint256 _tokenId=0;_tokenId<_tokenIDs.length;_tokenId++) {
            _stake(_tokenIDs[_tokenId]);  
        }
    }

    //internal function to implement common unstaking functionality
    function _stake(uint256 _tokenId) private {
        require(!paused, "_stake: Cant stake when the contract is paused!");
        stakes[_tokenId] = Stake(msg.sender, block.timestamp);
        parentNFT.safeTransferFrom(msg.sender, address(this), _tokenId, "0x00");
    }


    // return the staked NFT to the staker
    function unstake(uint256 _tokenId) external {
         require(msg.sender == stakes[_tokenId].staker || msg.sender == owner() , "unstake: Cant unstake someone elses NFT!");
        _unstake(_tokenId);
    }

    //unstake all NFTs at once
    function unstakeAll() public {
        if (msg.sender == owner()) {
            for (uint256 _tokenId = 1; _tokenId < stakes.length; _tokenId++) {
            if ( stakes[_tokenId].staker != address(0))
                _unstake(_tokenId);  
            }
        } else {
            for (uint256 _tokenId = 1; _tokenId < stakes.length; _tokenId++) {
                if ( stakes[_tokenId].staker == msg.sender)
                    _unstake(_tokenId);  
            }
        }
    }

    //internal function to implement common unstaking functionality
    function _unstake(uint256 _tokenId) private {
          address staker = stakes[_tokenId].staker;
          require(msg.sender == staker || msg.sender == owner() , "_unstake: Cant unstake someone elses NFT!");
          parentNFT.safeTransferFrom(address(this), staker, _tokenId,  "0x00");
          applyRewardsFromToken(_tokenId);
          delete stakes[_tokenId];
    }

    //internal function to implement rewards transfer functionality
    function applyRewardsFromToken(uint256 _tokenId) private {
        uint256 reward = (block.timestamp - stakes[_tokenId].timestamp)*rewardPerSecond;
        if (elements[_tokenId] == 1) //fire`
            require(rewardTokenFire.transfer(stakes[_tokenId].staker, reward), "applyRewardsFromToken: Fire Token transfer failed");
        else if (elements[_tokenId] == 2) //water
            require(rewardTokenWater.transfer(stakes[_tokenId].staker, reward), "applyRewardsFromToken: Water Token transfer failed");
        else if (elements[_tokenId] == 3) //earth
            require(rewardTokenEarth.transfer(stakes[_tokenId].staker, reward), "applyRewardsFromToken: Earth Token transfer failed");
        else if (elements[_tokenId] == 4) //storm
            require(rewardTokenStorm.transfer(stakes[_tokenId].staker, reward), "applyRewardsFromToken: Storm Token transfer failed");
        else if (elements[_tokenId] == 5) //legendary
            require(rewardTokenLegendary.transfer(stakes[_tokenId].staker, reward), "applyRewardsFromToken: Legendary Token transfer failed");
    }

    //internal function to implement crewards claiming functionality
    function claimRewardsFromToken(uint256 _tokenId) private {
        address staker = stakes[_tokenId].staker;
        require(staker == msg.sender,"claimRewardsFromToken - ERROR: staker and sender do not match");
        applyRewardsFromToken(_tokenId);
        stakes[_tokenId].timestamp = block.timestamp;
    }

    // external function to claim rewards
    function claimRewards() external {
        for (uint256 i = 1; i < stakes.length; i++) {
            if (stakes[i].staker == msg.sender) {
                claimRewardsFromToken(i);
            }
        }
    }

    // external function to list the senders staked NFTs
    function listStaked() external view returns (uint256[] memory ownedStakes) {
        ownedStakes = new uint256[](maxStaked); 
        uint256 count=0;
        for (uint256 i = 1; i < stakes.length; i++) {
            if (stakes[i].staker == msg.sender)
                ownedStakes[count++] = i;
        }
        return ownedStakes;
    }

    // external admin function to list all staked NFTs
    function listAllStaked() external view onlyOwner returns (uint256[] memory allStakes) {
        allStakes = new uint256[](1251); 
        uint256 count=0;
        for (uint256 i = 1; i < stakes.length; i++) {
          if ( stakes[i].staker != address(0))
            allStakes[count++] = i;
        }
        return allStakes;
    }

    // external function to list the senders pending rewards
    function listPending() external view returns (uint256 firePending, uint256 waterPending, uint256 earthPending, uint256 stormPending, uint256 legendaryPending ) {
      //check stakes array for all staked tokens
      for (uint256 i = 1; i < stakes.length; i++) {
          if (stakes[i].staker == msg.sender) {
            uint256 rewardThisToken = (block.timestamp - stakes[i].timestamp)*rewardPerSecond;
            if (elements[i] == 1)
              firePending+=rewardThisToken;
            if (elements[i] == 2)
              waterPending+=rewardThisToken;
            if (elements[i] == 3)
              earthPending+=rewardThisToken;
            if (elements[i] == 4)
              stormPending+=rewardThisToken;
            if (elements[i] == 5)
              legendaryPending+=rewardThisToken;
          }
      }

    }

    // external function to list the balance of the senders rewards
    function listBalance() external view returns (uint256 fireBalance, uint256 waterBalance, uint256 earthBalance, uint256 stormBalance, uint256 legendaryBalance ) {
        if (msg.sender == owner()) {
            fireBalance= rewardTokenFire.balanceOf(address(this));
            waterBalance= rewardTokenWater.balanceOf(address(this));
            earthBalance= rewardTokenEarth.balanceOf(address(this));
            stormBalance= rewardTokenStorm.balanceOf(address(this));
            legendaryBalance= rewardTokenLegendary.balanceOf(address(this));
        } else {
            fireBalance= rewardTokenFire.balanceOf(msg.sender);
            waterBalance= rewardTokenWater.balanceOf(msg.sender);
            earthBalance= rewardTokenEarth.balanceOf(msg.sender);
            stormBalance= rewardTokenStorm.balanceOf(msg.sender);
            legendaryBalance= rewardTokenLegendary.balanceOf(msg.sender);
        }
    }


    function setparentNFT(address _parentNFT) external onlyOwner {
        parentNFT = IERC721(_parentNFT);
    }

    function setrewardTokenFire(address _rewardTokenFire) external onlyOwner {
        rewardTokenFire = IERC20(_rewardTokenFire);
    }

    function setrewardTokenWater(address _rewardTokenWater) external onlyOwner {
        rewardTokenWater = IERC20(_rewardTokenWater);
    }

    function setrewardTokenEarth(address _rewardTokenEarth) external onlyOwner {
        rewardTokenEarth = IERC20(_rewardTokenEarth);
    }

    function setrewardTokenStorm(address _rewardTokenStorm) external onlyOwner {
        rewardTokenStorm = IERC20(_rewardTokenStorm);
    }

    function setrewardTokenLegendary(address _rewardTokenLegendary) external onlyOwner {
        rewardTokenLegendary = IERC20(_rewardTokenLegendary);
    }

    function setMaxStaked(uint256 _maxstaked) external onlyOwner {
        maxStaked = _maxstaked;
    }

    // external admin function to add ERC20 reward tokens from the owner to the contract for distribution to stakers as rewards
    // Owner should have approved ERC20 before.
    function addRewards(uint256 _rewardsAmount, uint256 _element) external onlyOwner {
        if (_element == 1) //fire
          require(rewardTokenFire.transferFrom(msg.sender, address(this), _rewardsAmount), "Staker: Fire Token transfer failed");
        else  if (_element == 2) //water
          require(rewardTokenWater.transferFrom(msg.sender, address(this), _rewardsAmount), "Staker: Water Token transfer failed");
        else  if (_element == 3) //earth
          require(rewardTokenEarth.transferFrom(msg.sender, address(this), _rewardsAmount), "Staker: Earth Token transfer failed");
        else  if (_element == 4) //storm
          require(rewardTokenStorm.transferFrom(msg.sender, address(this), _rewardsAmount), "Staker: Storm Token transfer failed");
        else  if (_element == 5) //legendary
          require(rewardTokenLegendary.transferFrom(msg.sender, address(this), _rewardsAmount), "Staker: Legendary Token transfer failed");
    }

    function setPaused(bool _state) public onlyOwner  {
      paused = _state;
    }

    // external admin function to add the NFT elements to the array, it is too big too declare statically and may need updating after contract deployment if there are errors in the data
    // has a starting point so admin caller can break up the array as required
    function addArrayToElements(uint256 start, uint256[] memory _elementmappings) external onlyOwner  {
        for (uint256 i=0;i<_elementmappings.length;i++)
            elements[i+start] = _elementmappings[i]; 
    }

    //collab.land bot roles
    function balanceOf(address staker) public view returns (uint256) {
        uint256 count=0;
        for (uint256 i = 1; i < stakes.length; i++) {
            if (stakes[i].staker == staker)
                count++;
        }
        return count;
    }

    // ** WARNING ** this wil pause the contract and return any NFTs and rewards
    function suspendContract() external  onlyOwner () {  
        paused=true;
        unstakeAll();
        uint256 fireBalance= rewardTokenFire.balanceOf(address(this));
        uint256 waterBalance= rewardTokenWater.balanceOf(address(this));
        uint256 earthBalance= rewardTokenEarth.balanceOf(address(this));
        uint256 stormBalance= rewardTokenStorm.balanceOf(address(this));
        uint256 legendaryBalance= rewardTokenLegendary.balanceOf(address(this));
        require(rewardTokenFire.transfer(owner(), fireBalance), "suspendContract: Fire Token transfer failed");
        require(rewardTokenWater.transfer(owner(), waterBalance), "suspendContract: Fire Token transfer failed");
        require(rewardTokenEarth.transfer(owner(), earthBalance), "suspendContract: Fire Token transfer failed");
        require(rewardTokenStorm.transfer(owner(), stormBalance), "suspendContract: Fire Token transfer failed");
        require(rewardTokenLegendary.transfer(owner(), legendaryBalance), "suspendContract: Fire Token transfer failed");
    }

    // name says it all, this contract is no longer required (probably to be replaced)
    function terminateContract() external  onlyOwner () {  
        require(paused, "terminateContract: Contract not paused");
        require(balanceOf(address(this)) == 0, "terminateContract: Contract must not own any NFTs");
        require(rewardTokenFire.balanceOf(address(this)) == 0, "terminateContract: Contract must not own any Fire Tokens");
        require(rewardTokenWater.balanceOf(address(this)) == 0, "terminateContract: Contract must not own any Water Tokens");
        require(rewardTokenEarth.balanceOf(address(this)) == 0, "terminateContract: Contract must not own any Earth Tokens");
        require(rewardTokenStorm.balanceOf(address(this)) == 0, "terminateContract: Contract must not own any Storm Tokens");
        require(rewardTokenLegendary.balanceOf(address(this)) == 0, "terminateContract: Contract must not own any Legendary Tokens");        
        for (uint256 i = 1; i < stakes.length; i++) 
            if (stakes[i].staker != address(0))
                require(true, "terminateContract: Contract must not have any stored NFTs");
        renounceOwnership();
    }

}

