import "../interfaces/INFTMintable.sol";

import "hardhat/console.sol";

contract OpenseaFactoryMinter is 
    FactoryERC721, 
    Initializable,
    OwnableUpgradeable
{
    using StringsUpgradeable for string;
    /**
     * constants
     */
     /**
     * Four different options
     * 1 : mint single nft in private sale
     * 2 : mint 4 nft in private sale
     * 3 : mint single nft in public sale
     * 4 : mint 4 nft in public sale
     */
    uint256 constant NUM_OPTIONS = 4;
    uint256 constant  SINGLE_PRIV_SALE = 0;
    uint256 constant MULTIPLE_PRIV_SALE = 1;
    uint256 constant SINGLE_PUB_SALE = 2;
    uint256 constant MULTIPLE_PUB_SALE = 3;
    // can mint 4 together in the mulitple_xx option
    uint256 constant NUM_PASSES_IN_MULTIPLE_OPTION = 4;
    struct Config {
        /*uint256 startTime;
        uint256 endTime;
        uint256 supply;*/
        uint256 maxNFTAllowed;
    }
    
    /**********************************************
     * public state
     ***********************************************/
    string public name;
    string public symbol;
    mapping(address => bool) public blacklist;
    uint256 public privateSaleCount;
    uint256 public publicSaleCount;
    Config public privateSaleConfig;
    Config public publicSaleConfig;
    mapping(address => uint256) public accountPrivateSaleCount;
    mapping(address => uint256) public accountPublicSaleCount;
    
    /**********************************************
     * internal state
     ***********************************************/
    INFTMintable internal mintableNft;
    address internal proxyRegistryAddress;
    string internal baseUri;
    mapping(address => mapping(address => bool)) private _operatorApprovals;
    /**
     * @dev This empty reserved space is put in palce to allow future version to add new
     * variables without shifting down storage in the inheritance chain,
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeablestorage_gaps
     */
    uint256[38] private __gap;

    /**********************************************
     * events
     ***********************************************/
    event SetBaseURI(string uri);
    event SetSaleConfig(uint256 maxWalletLimit);
    event UpdateBlacklist(address indexed account, bool status);
    event PrivateSaleMint(address indexed sender, address indexed to, uint256 amount);
    event PublicSaleMint(address indexed sender, address indexed to, uint256 amount);
    event Transfer(address indexed from, address indexed to, uint256 indexed tokenId);
    /**********************************************
     * initializers & constructors
     ***********************************************/
    /* solhint-disable-next-line  func-name-mixedcase*/
    function __OpenseaFactoryMinter_init(
        string memory name_,
        string memory symbol_,
        address proxyRegistry_,
        address payable nftAddress_,
        string memory baseUri_
    ) external initializer {
        __Ownable_init_unchained();

        __OpenseaFactoryMinter_init_unchained(name_, symbol_, proxyRegistry_, nftAddress_, baseUri_);
    }

    /* solhint-disable-next-line  func-name-mixedcase*/
    function __OpenseaFactoryMinter_init_unchained(
        string memory name_,
        string memory symbol_,
        address proxyRegistry_,
        address payable nftAddress_,
        string memory baseUri_
    ) internal onlyInitializing {
        name = name_;
        symbol = symbol_;
        proxyRegistryAddress = proxyRegistry_;
        mintableNft = INFTMintable(nftAddress_);
        baseUri = baseUri_;        
        // fire Transfer events for all tokenids/optionids
        fireTransferEvents(address(0), owner());
    }

    /**********************************************
     * public or external functions (IFactoryERC721)
     ***********************************************/
    /**
     * @notice Indicates that this is a factory contract. 
     * @dev Ideally would use EIP 165 supportsInterface(), but for now is a custom method
     */
    function supportsFactoryInterface() override public pure returns (bool) {
        return true;
    }
    function supportsInterface(bytes4 interfaceId) public view virtual returns (bool) {
        bytes4 _ERC165_ = 0x01ffc9a7;
        bytes4 _ERC721_ = 0x80ac58cd;
        bytes4 _ERC721Metadata_ = 0x5b5e139f;
        return interfaceId == _ERC165_ 
            || interfaceId == _ERC721_
            || interfaceId == _ERC721Metadata_
            || interfaceId == type(FactoryERC721).interfaceId;
    }
    /**
     * @dev Returns a URL specifying some metadata about the option. This metadata can be of the
     * same structure as the ERC721 metadata.
     */
    function tokenURI(uint256 _optionId) override public view returns (string memory) {
        return string(abi.encodePacked(baseUri, StringsUpgradeable.toString(_optionId)));
    } 
    /**
     * @notice Number of options the factory supports.
     */
    function numOptions() override public pure returns (uint256) {
        return NUM_OPTIONS;
    }

    /**
     * @dev Returns whether the option ID can be minted. Can return false if the developer wishes to
     * restrict a total supply per option ID (or overall).
     * @param _optionId the optionid 
     * 
     * @dev Returns false under the following conditions:
     *  - The sale period has expired or 
     *  - allocated supply for the NFT sale is exhausted or 
     *  - the optionid does not match the type of sale
     */
    function canMint(uint256 _optionId) override public view returns (bool flag) {
        flag = false;
        if(_optionId < NUM_OPTIONS) {
            uint256 maxMintLimit = mintableNft.maxMintLimit();
            uint256 mintAmountRequested = 0;

            if(_optionId == SINGLE_PRIV_SALE)  {
                mintAmountRequested = 1;
                // private sale count should not exceed the configured supply or max mint limit
                // invariant validation
                assert((privateSaleCount + mintAmountRequested) > privateSaleCount);
                return ((privateSaleCount + mintAmountRequested) <= maxMintLimit);               
            }
            else if(_optionId == MULTIPLE_PRIV_SALE) {
                mintAmountRequested = NUM_PASSES_IN_MULTIPLE_OPTION;
                // private sale count should not exceed the configured supply or max mint limit
                // invariant validation
                assert ((privateSaleCount + mintAmountRequested) > privateSaleCount);
                return ((privateSaleCount + mintAmountRequested) <= maxMintLimit);
            }
            else if(_optionId == SINGLE_PUB_SALE)  {
                mintAmountRequested = 1;
                // public sale count should not exceed the configured supply or max mint limit
                // mint per wallet shoudl not exceed the wallet limit
                assert((publicSaleCount + mintAmountRequested) > publicSaleCount);
                return ((publicSaleCount + mintAmountRequested) <= maxMintLimit);
            }
            else if(_optionId == MULTIPLE_PUB_SALE) {
                mintAmountRequested = NUM_PASSES_IN_MULTIPLE_OPTION;
                // public sale count should not exceed the configured supply or max mint limit
                assert ((publicSaleCount + mintAmountRequested) > publicSaleCount);
                return ((publicSaleCount + mintAmountRequested) <= maxMintLimit);
            }
        } 
        return flag;
    }

    /**
     * @dev Mints asset(s) in accordance to a specific address with a particular "option". This should be
     * callable only by the contract owner or the owner's Wyvern Proxy (later universal login will solve this).
     * Options should also be delineated 0 - (numOptions() - 1) for convenient indexing.
     * @param _optionId the option id
     * @param _toAddress address of the future owner of the asset(s)
     * 
     * @dev Error details
     * - 0x1: must be sent from owner proxy or owner
     * - 0x2: Sender address in black list
     * - 0x3: Cannot mint due to following conditions
     *      - allocated supply for the NFT sale is exhausted
     * - 0x4: per-wallet mint limit exceeded
     */
    function mint(uint256 _optionId, address _toAddress) override public {
        address sender = _msgSender();
        // must be sent from owner proxy or owner

        //////// console.log /////////////////
        //console.log("owner:sender", owner(), sender);
        require((owner() == sender || address(proxyRegistryAddress) == sender), "0x1");
        require(blacklist[_toAddress] == false, "0x2");        
        require(canMint(_optionId), "0x3");

        // calculate the amount to be minted
        //((accountPublicSaleCount[sender] + mintAmountRequested) <= publicSaleConfig.maxNFTAllowed)
        uint256 mintAmount = 0;  
        if(_optionId == SINGLE_PRIV_SALE || _optionId == MULTIPLE_PRIV_SALE) {
            mintAmount = (_optionId == MULTIPLE_PRIV_SALE) ? NUM_PASSES_IN_MULTIPLE_OPTION : 1;
            // check for per wallet limit
            require((accountPrivateSaleCount[_toAddress] + mintAmount) <= privateSaleConfig.maxNFTAllowed, "0x4");
            // mint the required tokens to the specifed address
            /**
             * Opensea somehow doesn't handle the batch mint properly and only first mint of batch
             * is reflected properly with owner
             * 
             * So we loop and do a single mint - not gas-efficient
             */
            //mintableNft.batchMint(_toAddress, mintAmount);
            for(uint256 i = 0; i < mintAmount; i++) {
                mintableNft.mint(_toAddress);
            }
            accountPrivateSaleCount[_toAddress] = accountPrivateSaleCount[_toAddress] + mintAmount;
            privateSaleCount = privateSaleCount + mintAmount;
            // emits the event
            //emit PrivateSaleMint(sender, amount);
            emit PrivateSaleMint(sender,_toAddress, mintAmount);
            // call the transfer for this contract & optionid
            address owneraddr = owner();
            emit Transfer(owneraddr, owneraddr, _optionId);
        } 
        else if(_optionId == SINGLE_PUB_SALE || _optionId == MULTIPLE_PUB_SALE) {
            mintAmount = (_optionId == MULTIPLE_PUB_SALE) ? NUM_PASSES_IN_MULTIPLE_OPTION : 1;
            // check for the per-wallet limit
            require((accountPublicSaleCount[_toAddress] + mintAmount) <= publicSaleConfig.maxNFTAllowed, "0x4");
            // mint the required tokens to the specied address
            //mintableNft.batchMint(_toAddress, mintAmount);
            /**
             * Opensea somehow doesn't handle the batch mint properly and only first mint of batch
             * is reflected properly with owner
             * 
             * So we loop and do a single mint - not gas-efficient
             */
            //mintableNft.batchMint(_toAddress, mintAmount);
            for(uint256 i = 0; i < mintAmount; i++) {
                mintableNft.mint(_toAddress);                
            }
            accountPublicSaleCount[_toAddress] = accountPublicSaleCount[_toAddress] + mintAmount;
            publicSaleCount = publicSaleCount + mintAmount;
            // emits the event
            //emit PublicSaleMint(sender, amount);
            emit PublicSaleMint(sender, _toAddress, mintAmount);
            // call the transfer for this contract & optionid
            address owneraddr = owner();
            emit Transfer(owneraddr, owneraddr, _optionId);
        }
    } 

