     * Hack to get things to work automatically on OpenSea.
     * Use isApprovedForAll so the frontend doesn't have to worry about different method names.
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        returns (bool)
    {
        if (owner() == _owner && _owner == _operator) {
            return true;
        }

        // if OpenSea's ERC721 Proxy Address is detected, auto-return true
        // for OpenSea product use 0x58807baD0B376efc12F5AD86aAc70E78ed67deaE
        // here we check for OS proxy registry for Polygon Mumbai testnet
        // if(operator_ == address(0xff7Ca10aF37178BdD056628eF42fD7F799fAc77c)) {
        //     return true;
        // }
        // otherwise check if the
        /* solhint-disable-next-line mark-callable-contracts */
        /* 
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        if (address(proxyRegistry.proxies(owner_)) == operator_) {
            return true;
        }  */
        //////////////////// DEBUG ///////////////////////
        //console.log("ProxyRegistry:", proxyRegistryAddress);
        if (owner() == _owner && address(proxyRegistryAddress) == _operator) {
            return true;
        }

        return _operatorApprovals[_owner][_operator];
    }
    
    /**
