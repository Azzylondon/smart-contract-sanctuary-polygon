     * @param maxNFTAllowed_ max allowed NFT per user
     */
    function setPrivateSaleConfig(
        uint256 maxNFTAllowed_
    ) external onlyOwner {
        privateSaleConfig = Config(maxNFTAllowed_);
        emit SetSaleConfig(maxNFTAllowed_);
    }

    /**
     * @notice Set parameters for the public sale
     * @dev Only calleable by owner
     * @param maxNFTAllowed_ max allowed NFT per user
     */
    function setPublicSaleConfig(
        uint256 maxNFTAllowed_
    ) external onlyOwner {
        publicSaleConfig = Config(maxNFTAllowed_);
        emit SetSaleConfig(maxNFTAllowed_);
    }

    /**
     * @notice Update blacklist status of an account address
     * @dev Only callable by owner
     * @param account_ Address of user account
     * @param status_ Status of address in blacklist
     */
    function updateBlacklist(address account_, bool status_) external onlyOwner {
        blacklist[account_] = status_;
        emit UpdateBlacklist(account_, status_);
    }
   
    /**********************************************
     * overrides
     ***********************************************/
    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) override public onlyOwner {
        address prevOwner = owner();
        super.transferOwnership(newOwner);
        // fire the transfer event for all options
        fireTransferEvents(prevOwner, newOwner);
    }
    /**********************************************
     * interal or private functions
     ***********************************************/
    function fireTransferEvents(address from_, address to_) private {
        for (uint256 i = 0; i < NUM_OPTIONS; i++) {
            emit Transfer(from_, to_, i);
        }
    }

