contract rigelNFTLaunchPadClaim is Ownable, ERC1155Holder {
 using SafeMath for uint256;

    struct PairTokens {
        address rgp;
        address token2;
        address token3;
        address rgpLaunchPad;
        address busdLaunchPad;
        address usdtLauncPad;
    }

    struct PairDetails {
        uint256 rgpInvestmentRequirement;
        uint256 busdInvestmentRequirement;
        uint256 usdtInvestmentRequirement;
        uint256 numberOfProjectRequire;
    }

    struct Price {
        uint256 RGPPrice;
        uint256 USDCPrice;
        uint256 USDTPrice;
    }

  struct Claimed {    
    bool InvestedLaunchPad;
    bool NumberOfProjectInvest;
  }
  address[] tk;
  
  address payable public _owner;
  IERC20 public rigelToken;
  IERC1155 public rigelNFT;
  
  mapping (uint256 => PairTokens) public getListedTokens;
  mapping (uint256 => Price) public pricePerToken;
  mapping (uint256 => PairDetails) public pairDetails;
  mapping (address => Claimed) public claimed;
  mapping (uint256 => bool) public sold;
  
  
  event Purchase(address indexed recipient, uint price, uint id);
  event claims(address indexed recipient, uint256 tokenID, uint256 price);
  event access(address indexed user, uint256 id, uint256 _event);

 constructor(
     uint256 _id,
     address _rgp,
     address _rigelNFT,
     address _buyWithOne,
     address _buyWithTwo,
     address _rgpLaunchPad,
     address _usdcLaunchPad,
     address _usdtLauncPad
    ) {
        _owner = payable(_msgSender());
        PairTokens storage _token = getListedTokens[_id];
        rigelToken = IERC20(_rgp);
        rigelNFT = IERC1155(_rigelNFT);
        _token.rgpLaunchPad = _rgpLaunchPad;
        _token.busdLaunchPad = _usdcLaunchPad;
        _token.usdtLauncPad = _usdtLauncPad; 
        _token.rgp = _rgp;
        _token.token2 = _buyWithOne;
        _token.token3 = _buyWithTwo;  
        tk.push(_rgpLaunchPad);
        tk.push(_usdcLaunchPad);
        tk.push(_usdtLauncPad);
        
    }

     function Pairs(
         uint256 _id,
         uint256 _investmentOnRGPPair,
         uint256 _investmentOnBUSDPair,
         uint256 _investmentOnUSDTPair,
         uint256 _numberOfProjInvestmentRequire,
         uint256 rgpPrice, 
         uint256 USDCPrice,
         uint256 usdtPrice
        ) external onlyOwner {
        PairDetails storage __pairs = pairDetails[_id];
        __pairs.rgpInvestmentRequirement = _investmentOnRGPPair;
        __pairs.busdInvestmentRequirement = _investmentOnBUSDPair;
        __pairs.usdtInvestmentRequirement = _investmentOnUSDTPair;
        __pairs.numberOfProjectRequire = _numberOfProjInvestmentRequire;        
        setTokenPrice(_id, rgpPrice, USDCPrice, usdtPrice);
    }

   function setTokenPrice(uint256 _id, uint256 rgpPrice, uint256 USDCPrice, uint256 usdtPrice) internal {    
        Price storage _price = pricePerToken[_id];
        _price.RGPPrice = rgpPrice;
        _price.USDCPrice = USDCPrice;
        _price.USDTPrice = usdtPrice;
    }

    function claimLaunchPad(uint256 _tokenId, address _pairAddress) external {
        PairTokens memory _token = getListedTokens[_tokenId];
        PairDetails memory __pairs = pairDetails[_tokenId];
        Claimed storage clm = claimed[_msgSender()];
        uint256 userAmt = getLaunchPadData(_tokenId, _msgSender(), _pairAddress);
        if (_pairAddress == _token.rgpLaunchPad && userAmt >= __pairs.rgpInvestmentRequirement) {
            require(!(clm.InvestedLaunchPad), "Can't Claim twice");
            clm.InvestedLaunchPad = true;
            claim(_tokenId);
        }

        if (_pairAddress == _token.busdLaunchPad && userAmt >= __pairs.busdInvestmentRequirement) {
            require(!(clm.InvestedLaunchPad), "Can't Claim twice");
            clm.InvestedLaunchPad = true;
            claim(_tokenId);
        }

        if (_pairAddress == _token.usdtLauncPad && userAmt >= __pairs.usdtInvestmentRequirement) {
            require(!(clm.InvestedLaunchPad), "Can't Claim twice");
            clm.InvestedLaunchPad = true;
            claim(_tokenId);
        }

    }

    function numberOfProjInvestedIn(uint256 _tokenId) external {
        Claimed storage clm = claimed[_msgSender()];      
        PairDetails memory __pairs = pairDetails[_tokenId];
        uint256 num = getNumbersOfProjInvestedIn( _msgSender());
        if (num >= __pairs.numberOfProjectRequire) {
            require(!(clm.NumberOfProjectInvest), "Can't Claim twice");
            clm.NumberOfProjectInvest = true;
            claim(_tokenId);
        }
    }

    function claim(uint256 _tokenId) internal { 
        Price memory _price = pricePerToken[_tokenId]; 
        rigelNFT.safeTransferFrom(address(this), _msgSender(), _tokenId, _price.RGPPrice, " "); //nft to user 
        emit claims(_msgSender(), _tokenId, _price.RGPPrice);
    }

    function getLaunchPadData(uint256 _id, address _user, address __pair) public view returns(uint256 _userAmount) {  
        
        PairTokens storage _token = getListedTokens[_id];
        require(__pair == _token.rgpLaunchPad || __pair == _token.busdLaunchPad || __pair == _token.usdtLauncPad, "Rigel: Token address Not accepted");
  
        _userAmount = launchPadInterface(__pair).userFunds(_user);
        return (_userAmount);    
    }

    function getNumbersOfProjInvestedIn(address _user) public view returns(uint256 _num) {
        
        uint256 tNumber;
        for (uint256 i = 0; i <= tk.length; i++) {
            address t = tk[i];
            uint256 _userAmount = launchPadInterface(t).userFunds(_user);  
            if(_userAmount > 0) {
                tNumber = tNumber + 1;
            }
        }
        return tNumber; 
    }

    function buy(uint _id, uint256 _amount, address buyWith) external {
        PairTokens memory _token = getListedTokens[_id];
        Price memory _price = pricePerToken[_id];
        require(buyWith == _token.rgp || buyWith == _token.token2 || buyWith == _token.token3, "Rigel: Token address Not accepted");
        if(_amount == _price.RGPPrice && buyWith == _token.rgp) {
            require(IERC20(buyWith).transferFrom(_msgSender(), address(this), _price.RGPPrice));    
            _trade(_id, _price.RGPPrice); //swap nft for eth
        }
        if(_amount == _price.USDCPrice && buyWith == _token.token2) {
            require(IERC20(buyWith).transferFrom(_msgSender(), address(this), _price.USDCPrice));    
            _trade(_id, _price.USDCPrice); //swap nft for eth
        }
        if(_amount == _price.USDTPrice && buyWith == _token.token3) {
            require(IERC20(buyWith).transferFrom(_msgSender(), address(this), _price.USDTPrice));    
            _trade(_id, _price.USDTPrice); //swap nft for eth
        }
        
    }

    function _trade(uint _id, uint256 amount) internal {
        rigelNFT.safeTransferFrom(address(this), msg.sender, _id, amount, " ");
        emit Purchase(_msgSender(), amount, _id);
    }

    function EmmergencyWithdraw(address _token, address recipient, uint256 _amt) external onlyOwner {
        IERC20(_token).transfer(recipient, _amt);
    }
}