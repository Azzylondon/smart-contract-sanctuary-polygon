contract TradingFactory is Initializable, BasicMetaTransaction {
    //--------------------Other Contracts--------------------//

    address public masterTrading;
    address public owner;

    IWhitelisting public whitelisting;
    IAdmin public admin;

    bytes32 public constant OPERATOR = keccak256("OPERATOR");

    //---------------------Events--------------------//

    event TradingCreated(address instance, string tradingID, uint amount);

    //---------------------------modifiers------------------------//

    modifier validation(address _address) {
        require(_address != address(0), "Zero address");
        _;
    }

    //------------------------Initialize------------------------//

    function initialize(
        address _admin,
        address _tradingContract,
        address _owner,
        address _whitelisting
    ) external initializer {
        admin = IAdmin(_admin);
        masterTrading = _tradingContract;
        owner = _owner;
        whitelisting = IWhitelisting(_whitelisting);
    }

    //---------------------------Admin-setters------------------------//

    function setAdmin(address _address) external {
        require(admin.hasRole(OPERATOR, _msgSender()), "OnlyOperator");
        admin = IAdmin(_address);
    }

    function setMasterTrading(address _address) external {
        require(admin.hasRole(OPERATOR, _msgSender()), "OnlyOperator");
        masterTrading = _address;
    }

    function setWhitelisting(address _address) external {
        require(admin.hasRole(OPERATOR, _msgSender()), "OnlyOperator");
        whitelisting = IWhitelisting(_address);
    }

    // NOTE: call this function with bytecode from getCreationByteCode and a random salt
    function createClone() internal returns (address) {
        address addr;
        bytes memory bytecode = getCreationBytecode();
        uint _salt = 10;
        assembly {
            addr := create2(0, add(bytecode, 0x20), mload(bytecode), _salt)

            if iszero(extcodesize(addr)) {
                revert(0, 0)
            }
        }
        return (addr);
    }

    function getCreationBytecode() internal pure returns (bytes memory) {
        bytes memory bytecode = type(OwnedUpgradeabilityProxy).creationCode;

        return abi.encodePacked(bytecode, abi.encode());
    }

    function createTrading(
        address _admin,
        string memory tradingID,
        address _usdc,
        address _token,
        address _whitelisting,
        address _priceFeed,
        uint _amount
    ) external {
        require(admin.hasRole(OPERATOR, _msgSender()), "OnlyOperator");
        require(whitelisting.isKYC(_msgSender()), "Not KYC");
        // address Clone = createClone();
        address instance = Clones.clone(masterTrading);
        // IOwnedUpgradeability(Clone).upgradeTo(masterTrading);
        ITrading(instance).initialize(_admin, _usdc, _token,_whitelisting,_priceFeed);
        console.log("Trading Created ",instance);
        // IToken(_msgSender()).approve(instance,_amount);
        // console.log("check");
        // IToken(_token).transferFrom(_msgSender(), instance , _amount);
        emit TradingCreated(instance, tradingID, _amount);
    }

    function _msgSender()
        internal
        view
        override(BasicMetaTransaction)
        returns (address sender)
    {
        if (msg.sender == address(this)) {
            bytes memory array = msg.data;
            uint256 index = msg.data.length;
            assembly {
                // Load the 32 bytes word from memory with the address on the lower 20 bytes, and mask those.
                sender := and(
                    mload(add(array, index)),
                    0xffffffffffffffffffffffffffffffffffffffff
                )
            }
        } else {
            return msg.sender;
        }
    }
}

