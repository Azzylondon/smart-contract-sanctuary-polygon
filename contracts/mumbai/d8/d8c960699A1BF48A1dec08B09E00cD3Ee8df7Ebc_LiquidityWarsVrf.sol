        }
        for (uint256 i = 0; i < _initialAbilities.length; i++) {
            buildingsParams.push(
                BuildingParams(_initialAbilities[i], _bonuses[i])
            );
        }
        buildingsParams.push(BuildingParams(0, 0));
        calibrationFlag |= BUILDING_PARAMS_DEFINED;
    }

