pragma solidity ^0.8.0;

interface IBoringCollection {
  function collectionSize() external view returns (uint8);

  function getOutfits(uint8 _from, uint8 _to) external view returns (string[] memory);

  function getOutfit(uint256 id) external view returns (string memory);
}


abstract contract BoringCollection is IBoringCollection {
  uint8 public immutable collectionSize;

  constructor(uint8 _collectionSize) {
    collectionSize = _collectionSize;
  }

  function getOutfits(uint8 _from, uint8 _to) external view override returns (string[] memory) {
    uint8 to = _to < collectionSize ? _to : collectionSize;
    uint8 from = _from >= to ? to - 1 : _from;

    string[] memory collection = new string[](collectionSize);
    for (uint8 i = from; i < to; i++) {
      collection[i] = getOutfit(i);
    }
    return collection;
  }

  function getOutfit(uint256 id) public view virtual override returns (string memory);
}


