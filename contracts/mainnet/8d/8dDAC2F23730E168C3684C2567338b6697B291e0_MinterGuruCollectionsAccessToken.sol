        address to,
        string memory metaUri,
        bytes memory _data
    ) external onlyOwner returns (uint256) {
        uint256 id = tokensCount;
        require(id < tokensLimit, "MinterGuruPrivateCollection: limit reached");
        _mint(to, id, metaUri, _data);
        return id;
    }

