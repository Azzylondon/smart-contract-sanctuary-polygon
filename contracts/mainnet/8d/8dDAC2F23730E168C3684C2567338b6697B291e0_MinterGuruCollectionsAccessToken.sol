        address prevOwner = owner();
        uint256 ownerTokensCount = balanceOf(prevOwner);
        for (uint256 i = 0; i < ownerTokensCount; i++) {
            uint256 id = tokenOfOwnerByIndex(prevOwner, 0);
            _transfer(prevOwner, to, id);
        }
        _transferOwnership(to);
    }

