/// @title Hodler Pay main contract
/// @author haipro287 from Hodler Pay team
/// @notice Using Aave lending protocol to borrow reserve.
abstract contract HodlerPayBase is
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    UUPSUpgradeable
{
    event SupplyAndBorrow(
        address indexed collateral,
        address indexed reserve,
        uint256 depositAmount,
        uint256 borrowAmount
    );
    event Supply(address indexed collateral, uint256 amount);
    event Repay(address indexed reserve, uint256 amount);

    IAccessControlManager accessControlManager;

    function __HodlerPayBase_init(address acm) internal onlyInitializing {
        __HodlerPayBase_init_unchained(acm);
        __Pausable_init();
        __ReentrancyGuard_init();
        __UUPSUpgradeable_init();
    }

    function __HodlerPayBase_init_unchained(address acm)
        internal
        onlyInitializing
    {
        accessControlManager = IAccessControlManager(acm);
    }

    modifier pauserGuard() {
        require(
            accessControlManager.isPauser(msg.sender),
            Errors.CALLER_NOT_PAUSER
        );
        _;
    }

    modifier adminGuard() {
        require(
            accessControlManager.isAdmin(msg.sender),
            Errors.CALLER_NOT_ADMIN
        );
        _;
    }

    modifier whenNotPause() {
        require(!paused(), Errors.CONTRACT_PAUSED);
        _;
    }

    /**
     * @dev Pauses all token transfers.
     *
     * See {ERC721Pausable} and {Pausable-_pause}.
     *
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function pause() public virtual {
        require(
            accessControlManager.isAdmin(msg.sender),
            Errors.CALLER_NOT_PAUSER
        );

        _pause();
    }

    /**
     * @dev Unpauses all token transfers.
     *
     * See {ERC721Pausable} and {Pausable-_unpause}.
     *
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function unpause() public virtual pauserGuard {
        _unpause();
    }

    /**
