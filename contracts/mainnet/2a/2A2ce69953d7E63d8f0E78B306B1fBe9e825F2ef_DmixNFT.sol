import "ERC1155.sol";
import "AccessControl.sol";
import "Pausable.sol";
import "ERC1155Burnable.sol";
import "ERC1155Supply.sol";
import "Strings.sol";
import "SafeMath.sol";
import "SignedSafeMath.sol";

/// @custom:security-contact [email protected]
contract DmixNFT is
    ERC1155,
    AccessControl,
    Pausable,
    ERC1155Burnable,
    ERC1155Supply
{
    using SafeMath for uint256;
    using SafeMath for uint16;
    using SignedSafeMath for int256;
    using SignedSafeMath for int16;
    using Strings for uint256;

    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    address private constant _dead = 0x000000000000000000000000000000000000dEaD;
    mapping(address => bool) private _blacklistAddress;
    mapping(uint256 => string) private _tokenUris;
    mapping(uint256 => uint256) private _tokenMintPrice;
    mapping(uint256 => uint256) private _tokenLimit;
    mapping(uint256 => uint256) private _tokenSoldAmount;

    mapping(uint256 => mapping(address => uint256)) private _whitelistAddress;

    address public _receiver;

    address public _lootBoxProxy;

    event mintInfoEvent(address sender, uint256 tokenId, address account);

    //IDynamicMetadata public _metadataGenerator;

    constructor(address receiver_, string memory baseUri_) ERC1155(baseUri_) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);

        //_metadataGenerator = IDynamicMetadata(metadataGenerator_);
        _receiver = receiver_;
        _blacklistAddress[address(this)] = true;
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public payable whenNotPaused {
        emit mintInfoEvent(_msgSender(), id, account);

        require(_tokenLimit[id] > 0);
        require(!isBlacklisted(_msgSender()));
        require(amount > 0);
        require(
            _tokenSoldAmount[id] + amount <= _tokenLimit[id],
            "Not enough token"
        );

        if (!hasRole(DEFAULT_ADMIN_ROLE, _msgSender())) {
            uint256 mintlimit = 0;

            if (_whitelistAddress[id][account] > 0) {
                mintlimit = _whitelistAddress[id][account];
            } else if (
                hasRole(MINTER_ROLE, _msgSender()) ||
                _msgSender() == _lootBoxProxy
            ) {
                mintlimit = 5;
            }

            bool freeMint = false;
            if (mintlimit > 0) {
                if (balanceOf(account, id) + amount <= mintlimit) {
                    freeMint = true;
                }
            }

            if (!freeMint) {
                require(
                    msg.value >= _tokenMintPrice[id].mul(amount),
                    "insuficient payment"
                );
            }
        }

        _mint(account, id, amount, data);
        _tokenSoldAmount[id] += amount;
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) whenNotPaused {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setBlacklistAddress(address address_, bool status)
        external
        onlyRole(URI_SETTER_ROLE)
    {
        require(
            !hasRole(DEFAULT_ADMIN_ROLE, address_),
            "Dmix: Illegal argument address_ is _owner."
        );
        _blacklistAddress[address_] = status;
    }

    function setWhitelistAddress(
        uint256 tokenId_,
        address[] calldata addresses_,
        uint256 mintLimit
    ) external onlyRole(URI_SETTER_ROLE) {
        for (uint256 i = 0; i < addresses_.length; i++) {
            _whitelistAddress[tokenId_][addresses_[i]] = mintLimit;
        }
    }

    function getWhitelistAddress(uint256 tokenId_, address address_)
        public
        view
        returns (uint256)
    {
        return _whitelistAddress[tokenId_][address_];
    }

    function uri(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        return tokenURI(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        returns (string memory)
    {
        require(
            bytes(_tokenUris[tokenId]).length > 0,
            "DmixNFTMetadata: URI query for nonexistent token"
        );

        return
            string(
                abi.encodePacked(_tokenUris[tokenId], Strings.toString(tokenId))
            );
    }

    function setRoyaltyReceiver(address receiver_)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _receiver = receiver_;
    }

    function isBlacklisted(address address_) public view returns (bool) {
        return _blacklistAddress[address_];
    }

    function updateDmixNFTSetting(
        uint256 tokenId_,
        string memory tokenUri,
        uint256 limit,
        uint256 price
    ) public onlyRole(URI_SETTER_ROLE) {
        setTokenUri(tokenId_, tokenUri);
        setTokenLimit(tokenId_, limit);
        setTokenMintPrice(tokenId_, price);
    }

    /** token value set and get */
    function setTokenUri(uint256 tokenId_, string memory uri)
        public
        onlyRole(URI_SETTER_ROLE)
    {
        _tokenUris[tokenId_] = uri;
    }

    function getTokenUri(uint256 tokenId_) public view returns (string memory) {
        return _tokenUris[tokenId_];
    }

    /** Token amount limit */
    function setTokenLimit(uint256 tokenId_, uint256 limit)
        public
        onlyRole(URI_SETTER_ROLE)
    {
        _tokenLimit[tokenId_] = limit;
    }

    function getTokenLimit(uint256 tokenId_) public view returns (uint256) {
        require(_tokenLimit[tokenId_] > 0);
        return _tokenLimit[tokenId_];
    }

    /** sold amount */
    function getSoldAmount(uint256 tokenId_) public view returns (uint256) {
        return _tokenSoldAmount[tokenId_];
    }

    /** Token mint price */
    function setTokenMintPrice(uint256 tokenId_, uint256 price)
        public
        onlyRole(URI_SETTER_ROLE)
    {
        _tokenMintPrice[tokenId_] = price;
    }

    function setLootBoxProxy(address lootBoxProxy_)
        public
        onlyRole(URI_SETTER_ROLE)
    {
        _lootBoxProxy = lootBoxProxy_;
    }

    function getTokenMintPrice(uint256 tokenId_) public view returns (uint256) {
        require(_tokenMintPrice[tokenId_] > 0);
        return _tokenMintPrice[tokenId_];
    }

    function withdraw(address targetAddress)
        public
        payable
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(targetAddress == payable(targetAddress), "Not payable address");

        (bool success, ) = payable(targetAddress).call{
            value: address(this).balance
        }("");
        require(success);
    }
}

