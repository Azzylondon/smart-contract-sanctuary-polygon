pragma solidity 0.8.9;
import "./dependencies/openzeppelin/ERC165.sol";
import "./dependencies/openzeppelin/Address.sol";
import "./dependencies/openzeppelin/Strings.sol";
import "./dependencies/openzeppelin/interfaces/IERC1155.sol";
import "./dependencies/openzeppelin/interfaces/IERC1155MetadataURI.sol";

import {ISocialGraphNTT} from "./interfaces/ISocialGraph.sol";
import "./dependencies/openzeppelin/OwnableUpgradeable.sol";
import "./dependencies/openzeppelin/structs/EnumerableMap.sol";
import "./libraries/utils/EnumerableMap.util.sol";
import "./libraries/utils/Address.util.sol";
contract SocialGraphNTT is ERC165, ISocialGraphNTT, OwnableUpgradeable {
    using Address for address;
    using Strings for uint256;
    using AddressUtils for address;
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    using EnumerableMapUtils for EnumerableMap.AddressToUintMap;

    mapping(uint256 => EnumerableMap.AddressToUintMap) private _balances;
    string private _uri;
    string public name;
    string public symbol;
    address private _socialGraph;
    uint256 private _limit;

    modifier onlySocialGraph() {
        require(msg.sender == _socialGraph, "invalid_caller");
        _;
    }

    function initialize(string memory name_, string memory symbol_, string memory uri_, uint256 limit_, address owner_) external initializer {
        name = name_;
        symbol = symbol_;
        _limit = limit_;
        _socialGraph = msg.sender;
        __Ownable_init();
        setURI(uri_);
        _transferOwnership(owner_);
    }

    //
    // VIEW FUNCTIONS
    //
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC165, IERC165) returns (bool) {
        return
            interfaceId == type(IERC1155).interfaceId ||
            interfaceId == type(IERC1155MetadataURI).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function uri(uint256 id) public view virtual returns (string memory) {
        return string(abi.encodePacked(_uri, uint256(uint160(address(this))).toHexString(), '/', id.toString()));
    }

    function balanceOf(address account, uint256 id) public view virtual returns (uint256) {
        require(account != address(0), "ERC1155: balance query for the zero address");
        return _balances[id]._tryGetUint(account);
    }

    function total(address account_) external view returns(uint256, uint256) {
        return(_balances[account_.toUint()].length(), _limit);
    }

    //
    // EXTERNAL FUNCTION
    //
    function mint(address to,uint256 id, uint256 amount) external onlySocialGraph {
        _mint(to, id, amount);
    }

    function burn(address from, uint256 id, uint256 amount) external onlySocialGraph {
        _burn(from, id, amount);
    }

    function setURI(string memory newUri) public onlyOwner {
        _uri = newUri;
    }

    function setLimit(uint256 newLimit_) external onlyOwner {
        _limit = newLimit_;
    }

    //
    // INTERNAL FUNCTIONS
    //

    function _mint(
        address to,
        uint256 id,
        uint256 amount
    ) internal virtual {
        require(to != address(0), "ERC1155: mint to the zero address");
        require(_limit == 0 || _balances[id].length() + amount <= _limit, "over_limit");

        address operator = _msgSender();
        _balances[id].set(to, _balances[id]._tryGetUint(to) + amount);
        emit TransferSingle(operator, address(0), to, id, amount);
    }

    function _burn(
        address from,
        uint256 id,
        uint256 amount
    ) internal virtual {
        require(from != address(0), "ERC1155: burn from the zero address");

        address operator = _msgSender();

        uint256 fromBalance = _balances[id]._tryGetUint(from);
        require(fromBalance >= amount, "ERC1155: burn amount exceeds balance");
        unchecked {
            if (fromBalance - amount == 0) {
                _balances[id].remove(from);
            } else _balances[id].set(from, fromBalance - amount);
        }
        emit TransferSingle(operator, from, address(0), id, amount);
    }
}

