import "../../dependencies/openzeppelin/structs/EnumerableMap.sol";
library EnumerableMapUtils  {
  using EnumerableMap for EnumerableMap.AddressToUintMap;
  function _tryGetUint(EnumerableMap.AddressToUintMap storage map_, address user_) internal view returns(uint256) {
    (bool success, uint256 amount) = map_.tryGet(user_);
    if (!success) return 0;
    return amount;
  }

  function _tryGetBool(EnumerableMap.AddressToUintMap storage map_, address user_) internal view returns(bool) {
    (bool success, uint256 amount) = map_.tryGet(user_);
    if (!success) return success;
    return amount > 0;
  }
}

