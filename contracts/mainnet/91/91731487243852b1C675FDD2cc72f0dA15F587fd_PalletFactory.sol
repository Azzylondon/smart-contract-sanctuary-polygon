pragma solidity ^0.8.0;

import "./IReputationalNote.sol";
import "./ICollateralizationNote.sol";

interface IBondCouncil {

    /* ========== EVENTS ========== */
    
    event PriceUpdated(
        uint256 priceStakeToken
    );

    event FactorySet(
        address palletFactoryAddress
    );

    event CouncilPaused(
        address requester
    );

    event CouncilUnPaused(
        address requester
    );

    event WhitelistedEdit(
        address importerAddress,
        bool editType,
        address requestor
    );

    event LiquidationApproved(
        uint256 _loanId
    );

    event ColletaralApproved(
        uint256 _loanId
    );

    event LiquidationRegistered(
        uint256 _loanId
    );

    event RedemptionMarked(
        uint256 _noteId, 
        address _importerAddress
    );

    event CollaterizationMarked(
        uint256 _noteId, 
        address _eventAddress
    );

    event DeleverageMarked(
        uint256 _noteId, 
        address _eventAddress
    );

    event DestructionMarked(
        uint256 _noteId, 
        address _eventAddress
    );

    event BondCoreSet(
        address loanCoreAddress
    );

    event LoanRegistered(
        uint256 loanId,
        address importerAddress
    );

    event LoanRepayed(
        uint256 loanId
    );

    event LoanLiquidated(
        uint256 loanId
    );

    event PaymentTokenSet(
        address paymentTokenAddress,
        uint8 decimals
    );


    /* ========== MUTATIVE FUNCTIONS =========== */

    function setPaymentToken(address _paymentTokenAddress, uint8 _decimals) external;

    function whitelistImporterEdit(address _importerAddress, bool _setTo) external;

    function setDepositTokenPrice(uint256 _newPriceDepositToken) external;

    function pauseBondCouncil() external;

    function unpauseBondCouncil() external;

    function emergencyWithdraw() external;

    function approvePalletLiquidation(uint256 _palletIndex) external;

    function approvePalletCollateral(uint256 _palletIndex, bool _setting) external;

    function setBondCore(address _loanCoreAddress) external;

    function setPalletFactory(address _palletFactoryAddress) external;

    /* ========== ROUTING FUNCTIONS =========== */

    function registerInventoryCollaterization(uint256 _palletIndex, address _eventAddress) external;

    function registerInventoryDeleveraging(uint256 _palletIndex, address _eventAddress) external;

    function registerInventoryDestruction(uint256 _palletIndex, address _eventAddress) external;

    function registerLoan(uint256 _loanId, address _importerAddress) external;

    function registerRepayment(uint256 _loanId) external;

    function registerLiquidation(uint256 _loanId) external;



    /* ========== VIEW FUNCTIONS ========== */

    function getPriceDepositToken() external view returns(uint256);

    function isImporterWhitelisted(address _importerAddress) external view returns(bool);

    function isCouncilPaused() external view returns(bool);

    function isLiquidationAllowed(uint256 _palletIndex) external view returns(bool);

    function isPalletApproved(uint256 _palletIndex) external view returns(bool);

    function isCollateralInUse(uint256 _palletIndex) external view returns (bool);

    function returnPalletEvent(uint256 _palletIndex) external view returns(address _eventAddress);

    function returnPalletImporter(uint256 _palletIndex) external view returns(address _importerAddress);

    function governanceMultiSig() external returns (address);

    /* ========== STATE VARIABLES ========== */

    function reputationalNote() external returns (IReputationalNote);

    function collateralizationNote() external returns (ICollateralizationNote);

    function paymentToken() external returns (address);

    function decimalsPaymentToken() external returns (uint8);

    event UpdateOriginationFee(uint256 _newFee);

    function setOriginationFee(uint256 _originationFee) external;

    function getOriginationFee() external view returns (uint256);

}

