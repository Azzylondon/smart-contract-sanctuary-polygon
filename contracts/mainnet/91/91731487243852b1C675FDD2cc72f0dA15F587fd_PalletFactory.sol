pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "./ERC721Permit.sol";
import "hardhat/console.sol";

import "./get_interfaces/IBondCouncil.sol";
import "./get_interfaces/IPalletFactory.sol";

/// @title GET Protocol Event Financing - PalletFactory
/// @author KasperK

contract PalletFactory is 
    Context,
    IPalletFactory,
    ERC721Enumerable,
    ERC721Burnable,
    ERC721Holder,
    ERC721Permit {

    /* ========== DEPENDENCIES ========== */
    using Counters for Counters.Counter;

    /* ========== STATE VARIABLES ========== */

    Counters.Counter public palletIndexTracker;

    address private immutable lockingToken;     
    address private immutable bondCouncil;

    // storage to check if a certain bundle is of the correct composition
    mapping(uint256 => PalletLibrary.PalletStruct) private pallets;

    // palletIndex => list of registered [nftIndexes] on a pallet/bundle
    mapping(uint256 => uint256[][]) private stackedAssets;


    /* ========== CONSTRUCTOR ========== */
    constructor(
        string memory name, 
        string memory symbol,
        address _lockingToken,
        address _bondCouncil
        ) ERC721(name, symbol) ERC721Permit(name) {
        lockingToken = _lockingToken; 
        bondCouncil = _bondCouncil;
    }

    /* ========== MUTATIVE FUNCTIONS =========== */

    function _initializePallet(address to) internal returns (uint256) {
        palletIndexTracker.increment();
        _mint(to, palletIndexTracker.current());
        return palletIndexTracker.current();
    }

    /**  
     * Locks up GET and mints a pallet/bundle NFT, needed for the collaterization of the ticket inventory.
     * @param _tokenAmount amount of GET tokens sent by the importer for creation of the bundle/pallet
     * @dev function can only be called by approved importer
     */
    function createPallet(
        uint256 _tokenAmount
        ) external override checkCouncil checkCaller {

        // mint pallet NFT to the importer
        uint256 _palletIndex = _initializePallet(_msgSender());

        // transfers GET from function caller to the PalletFactory contract
        bool _transferState = IERC20(lockingToken).transferFrom(_msgSender(), address(this), _tokenAmount);
        require(_transferState, "PalletFactory:create::Token transfer failed");

        // calculate market value of the deposited GET 
        uint64 _palletValue = uint64((IBondCouncil(bondCouncil).getPriceDepositToken() * _tokenAmount) / 1e18);

        PalletLibrary.PalletStruct storage _newpallet = pallets[_palletIndex];

        _newpallet.lockedTokenAmount = _tokenAmount;
        _newpallet.priceTokenAtLock = uint64(IBondCouncil(bondCouncil).getPriceDepositToken());
        _newpallet.totalValueDeposit = _palletValue;
        _newpallet.palletstate = PalletLibrary.PalletState.AVAILABLE;
        _newpallet.importerAddress = _msgSender();

        emit PalletCreated(_palletIndex, _msgSender());
    }
    
    /** 
    * Stacks a list of tickets onto a pallet
    * @dev this requires an approvalForAll 
    * @param _nftIndexes list with indexes of the assets that will be moved and registered to the pallet/bundle
    * @param _palletIndex index of the pallet/bundle the tickets are being registered to
    * @param _relayerAddress address of the relayer that has issued the tickts (marker of the ticketeer)
    * @param _eventAddress address of the EventImplementation proxt of the event
    * @param _transfer address of the EventImplementation proxt of the event
     */
    function transferAndRegisterTickets(
        uint256[] memory _nftIndexes,
        uint256 _palletIndex,
        address _relayerAddress,
        address _eventAddress,
        bool _transfer
        ) external override checkCouncil checkCaller {
            require(ownerOf(_palletIndex) == _msgSender(), "PalletFactory:register:Pallet not owned by importer.");

            // check that the importer is mapped to the importerAdmin (in the PalletLockStruct)
            require(pallets[_palletIndex].importerAddress == _msgSender(), "PalletFactory::register:Invalid importerRelayer");

            //  pallet must be marked as available
            require(pallets[_palletIndex].palletstate == PalletLibrary.PalletState.AVAILABLE, "PalletFactory::register:Pallet invalid state");

            if (_transfer == true) {
                _transferLoop(_nftIndexes, _relayerAddress);
            } else {
                _registerLoop(_nftIndexes, _eventAddress);
            }

            PalletLibrary.PalletStruct storage _newpallet = pallets[_palletIndex];
            _newpallet.stackedAssetCount += uint64(_nftIndexes.length);
            _newpallet.relayerAddress = _relayerAddress;

            // all nfts stacked on a pallet need to be for/from the same event
            _newpallet.eventAddress = _eventAddress; 
            _newpallet.containsTickets = true;
            
            stackedAssets[_palletIndex].push(_nftIndexes);

            emit RegistrationComplete(
                _palletIndex,
                _nftIndexes.length
            );
    }

    // transfers the tickets from the importer address to the PalletFactory
    function _transferLoop(
        uint256[] memory _nftIndexes,
        address _relayerAddress
    ) internal {
        for (uint256 i = 0; i < _nftIndexes.length; i++) {
            IERC721(_relayerAddress).transferFrom(
                _msgSender(),
                address(this),
                _nftIndexes[i]
            );
        }       
    }

    function _registerLoop(
        uint256[] memory _nftIndexes,
        address _eventAddress
    ) internal {
        for (uint256 i = 0; i < _nftIndexes.length; i++) {
            require(IERC721(_eventAddress).ownerOf(
                _nftIndexes[i]
            ) == address(this), "NFT not owned by palletFactory");
        }
    }

    /**
    * @dev Seals the bundle/pallet NFT, no more assets can be added or removed after this.
    * @dev wrapping of a pallet will result in an cNOTE NFT to be sent to the eventAddress, marking that the inventory has been collaterized
    * @param _palletIndex index of the pallet/bundle that needs to be sealed
     */
    function wrapPallet(
        uint256 _palletIndex) external override checkCouncil checkCaller {

        require(ownerOf(_palletIndex) == _msgSender(), "PalletFactory:wrap:Pallet not owned by importer.");

        PalletLibrary.PalletStruct storage _pallet = pallets[_palletIndex];
        
        // only creator of the pallet (entity that depisted GET) can wrap the pallet
        require(_pallet.importerAddress == _msgSender(), "PalletFactory::wrap:Invalid importerRelayer");

        require(_pallet.palletstate == PalletLibrary.PalletState.AVAILABLE, "PalletFactory::wrap:Pallet invalid state");

        _pallet.palletstate = PalletLibrary.PalletState.WRAPPED;

        // mints a inventorytCollaterizationNote (an NFT) to the eventAddress, marking to observers in the public that the inventory of the evendAddress is 'involved' in a bond/loan
        IBondCouncil(bondCouncil).registerInventoryCollaterization(_palletIndex, _pallet.eventAddress);

        emit PalletWrapped(_palletIndex);

    }

    /** 
    * @dev Is called (and can only be called) by the importer when the bond/loan is repaid.
    * @dev unlocks the tickets (approval for all). Also returns the locked GET from the pallet
    * @param _palletIndex index of the relayerAddresse pallet/bundle being redeemed
    * @dev can only be called by the importer that created the pallet
     */
    function unwrapPallet(uint256 _palletIndex) checkCouncil checkCaller external override  {
        require(ownerOf(_palletIndex) == _msgSender(), "PalletFactory::unwrap:Pallet not owned by importer.");
        
        burn(_palletIndex);

        PalletLibrary.PalletStruct storage _pallet = pallets[_palletIndex];

        require(_pallet.palletstate == PalletLibrary.PalletState.WRAPPED, "PalletFactory::unwrap:Wrong pallet state.");

        // only the address registered as the importer (original locker/creator of the pallet can unwrap a plalet
        require(_pallet.importerAddress == _msgSender(), "PalletFactory::unwrap:Unathorized unwrapper.");

        require(!IBondCouncil(bondCouncil).isCollateralInUse(_palletIndex),"PalletFactory::unwrap:Collateral in use");
        
        require(IERC20(lockingToken).transfer( _msgSender(), _pallet.lockedTokenAmount), "PalletFactory::unwrap:Deposit token transfer failed");

        _pallet.palletstate = PalletLibrary.PalletState.REDEEMED;
        
        // by approvaling the relayerAddress the importer will be able to move/transfer the tickets to the ticket buyer on the secondary market
        IERC721(_pallet.eventAddress).setApprovalForAll(_pallet.relayerAddress, true); 

        // marks the cNOTE to DELEVERAGED
        IBondCouncil(bondCouncil).registerInventoryDeleveraging(_palletIndex, _pallet.eventAddress);

        emit PalletUnwrapped(
            _palletIndex,
            _pallet.lockedTokenAmount
        );
    }


    /** 
    * Redeems a pallet for the locked/deposited GET, destroys ACCESS to the tickets that are stacked on the pallet
    * @param _palletIndex uint256 index of the pallet/bundle (NOT the loanId)
    * @dev function can only be successfully called by the owner of the pallet-NFT
     */
    function liquidatePallet(uint256 _palletIndex) external checkCouncil override {
        require(ownerOf(_palletIndex) == _msgSender(), "PalletFactory::liquidate:Pallet not owned by liquidator.");

        /** Important to know:
        If an EOA owns a palletNFT and calls this function, this means due to the design that the loan/bond has defaulted (and the collateral was claimed in the RepaymentController and with that BondCore transferred the palletNFT to the bond holder)
        */

        // burn the pallet as it can only be redeemed once
        burn(_palletIndex);

        PalletLibrary.PalletStruct storage _pallet = pallets[_palletIndex];

        // pallet must be in wrapped state, only valid state for liquidation
        require(_pallet.palletstate == PalletLibrary.PalletState.WRAPPED, "PalletFactory::liquidate:Wrong pallet state.");

        // check if the pallet isn't in use as collateral according to BondCore
        require(!IBondCouncil(bondCouncil).isCollateralInUse(_palletIndex), "PalletFactory::liquidate:Pallet in use as collateral");

        // check if council has contested the liquidation of a pallet
        require(IBondCouncil(bondCouncil).isLiquidationAllowed(_palletIndex), "PalletFactory::liquidate:Liquidation of pallet contested");

        // transfer the GET deposited on the pallet to the liquidator (tx caller) 
        IERC20(lockingToken).transfer(_msgSender(), _pallet.lockedTokenAmount);

        _pallet.palletstate = PalletLibrary.PalletState.LIQUIDATED;

        emit PalletLiquidated(
            _palletIndex,
            _pallet.lockedTokenAmount,
            _pallet.importerAddress
        );

        IBondCouncil(bondCouncil).registerInventoryDestruction(_palletIndex, _pallet.eventAddress);

    }

    /* ========== EMERGENCY FUNCTIONS =========== */

    /** 
    * Emergency function that approves stacked assets owned by this contract to be transferred by a relayer 
    * @param _handlingContract EOA or contract addrss that is handeling the primary sale of the assets on the pallet/bundle
    * @param _eventAddress contract address of the asset being approved
    * @dev this only works if the PalletFactory contracts owns these assets
     */
    function approveRelayerManual(
        address _handlingContract,
        address _eventAddress
    ) external override {
        require(_msgSender() == address(bondCouncil), "PalletFactory::emergencyApproval:Caller not council");
        IERC721(_eventAddress).setApprovalForAll(_handlingContract, true);

        emit EmergencyApproval(_eventAddress);
    }

    /**
    * Emergency function that withdraws all the locked tokens from the pallets to a pre-determined DAO address.
    @dev only should be used when the system is compromised 
     */
    function emergencyWithdrawl() external override {
        require(_msgSender() == address(bondCouncil), "PalletFactory::emergencywithdrawl:Caller not council");
        IERC20(lockingToken).transferFrom(address(this), IBondCouncil(bondCouncil).governanceMultiSig(), IERC20(lockingToken).balanceOf(address(this)));

        emit EmergencyWithdrawl(_msgSender());
    }

    /* ========== VIEW FUNCTIONS ========== */

    function doesPalletExist(uint256 _palletIndex) external override view returns(bool) {
        return _exists(_palletIndex);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @dev Hook that is called before any token transfer
     */
    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId
    ) internal virtual override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(_from, _to, _tokenId);
    }

    function getPalletData(uint256 _palletIndex) external view override returns (PalletLibrary.PalletStruct memory palletData) {
        return pallets[_palletIndex];
    }

    function getstackedAssets(uint256 _palletIndex) external override view returns(uint256[][] memory) {
        return stackedAssets[_palletIndex];
    }
    

    /* ========== INTERNAL FUNCTIONS ========== */


    /**
     * @dev Throws if called by any account other than the GET Protocol admin account.
     */
    modifier checkCaller() {
        require(IBondCouncil(bondCouncil).isImporterWhitelisted(msg.sender),"PalletFactory::create:Importer not whitelisted");
        _;
    }

    /**
     * @dev Throws if called by any account other than the GET Protocol admin account.
     */
    modifier checkCouncil() {
        require(!IBondCouncil(bondCouncil).isCouncilPaused(), "PalletFactory::checkCouncil:Council is paused");
        _;
    }

}

