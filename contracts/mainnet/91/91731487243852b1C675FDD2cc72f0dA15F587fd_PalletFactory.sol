pragma solidity ^0.8.0;

library PalletLibrary {

    enum PalletState {
        NON_EXISTANT,
        AVAILABLE,
        WRAPPED,
        REDEEMED,
        LIQUIDATED
    }

    struct PalletStruct {
        uint256 lockedTokenAmount;
        uint64 priceTokenAtLock;
        uint64 totalValueDeposit;
        uint64 stackedAssetCount; 
        PalletState palletstate;
        address importerAddress;
        address relayerAddress; 
        address eventAddress;
        bool containsTickets;
    }

}