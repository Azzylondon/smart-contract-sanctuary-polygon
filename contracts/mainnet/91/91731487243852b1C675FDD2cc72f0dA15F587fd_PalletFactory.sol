    function mintCollaterizationNote(uint256 _palletIndex, address _eventAddress) external;

    function deleverageInventory(uint256 _palletIndex) external;

    function markLiquidation(uint256 _palletIndex) external;

    function burn(uint256 _tokenId) external;

    function destroyColNote(uint256 _palletIndex) external;

}

