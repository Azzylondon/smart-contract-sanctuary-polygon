import "@openzeppelin/contracts/token/ERC721/ERC721.sol";

interface IReputationalNote is IERC721 {

    event TransferAttempt(
        address from, 
        address to, 
        uint256 tokenId
    );

    event LoanREDEEMED(
        uint256 loanId
    );

    event LoanLiquidated(
        uint256 loanId
    );

    event LoanCancelled(
        uint256 loanId
    );

    function mintReputationalNote(uint256 _loanId, address _importerAddress) external;

    function repayLoan(uint256 _loanId) external;

    function liquidateLoan(uint256 _loanId) external;

    function cancelLoan(uint256 _loanId) external;

    function burn(uint256 tokenId) external;

    function destroyRepNote(uint256 _loanId) external;
}

