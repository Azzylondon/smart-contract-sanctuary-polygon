pragma solidity ^0.7.3;
pragma abicoder v2;

import "v3-periphery/interfaces/INonfungiblePositionManager.sol";
import "./interfaces/IExchangeIssuanceZeroEx.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "v3-periphery/libraries/TransferHelper.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../src/interfaces/ISetToken.sol";
import "v3-periphery/interfaces/external/IWETH9.sol";

contract Butler is Ownable, IERC721Receiver {
    INonfungiblePositionManager public immutable nonfungiblePositionManager;
    IExchangeIssuanceZeroEx public immutable exchange;
    IWETH9 public immutable WETH;

    mapping(uint256 => Position) public positions;

    struct Position {
        uint128 liquidity;
        address token0;
        address token1;
    }

    struct NewPositionParams {
        address token0;
        address token1;
        uint24 poolFee;
        int24 tickLower;
        int24 tickUpper;
        uint256 amount0Min;
        uint256 amount1Min;
        uint256 amount0Desired;
        uint256 amount1Desired;
    }

    struct ExchangeIssuanceParams {
        ISetToken token;
        uint256 amountToExchange;
        bool isMinting;
        bytes[] quotes;
        uint256 ethAmount;
        address issuanceModule;
        bool isDebtIssuance;
    }

    constructor(address _nonfungiblePositionManager, address payable _exchange, address _weth)
    {
        nonfungiblePositionManager = INonfungiblePositionManager(
            _nonfungiblePositionManager
        );
        exchange = IExchangeIssuanceZeroEx(_exchange);
        WETH = IWETH9(_weth);
    }

    receive() external payable {

    }

    function onERC721Received(
        address,
        address,
        uint256 tokenId,
        bytes calldata
    ) external override returns (bytes4) {
        _addPosition(tokenId);

        return this.onERC721Received.selector;
    }

    function rebalancePosition(
        uint256 tokenId,
        ExchangeIssuanceParams calldata exchangeParams,
        NewPositionParams calldata newPosParams
    ) external onlyOwner returns (uint256 newTokenId){
        // DecreaseLiquidity
        removeLiquidityAndFees(tokenId);

        // Mint/Redeem missing/spare setTokens
        getMissingAmounts(exchangeParams);

        bool isToken0 = false;
        if (newPosParams.token0 != address(WETH)) {
          isToken0 = true;
        } 

        uint256 wethAmount;
        if (isToken0) {
          wethAmount = newPosParams.amount1Desired;
        } else {
          wethAmount = newPosParams.amount0Desired;
        }

        uint256 wethAvailable = WETH.balanceOf(address(this));
        if (wethAvailable < wethAmount) {
          uint256 wethToMint = wethAmount - wethAvailable;
          WETH.deposit{value: wethToMint}();
        } 

        // Mint new position with new position params
        newTokenId = mintNewPosition(newPosParams);
    }

    function getMissingAmounts(ExchangeIssuanceParams calldata params)
        internal
    {
        if (params.isMinting) {
            WETH.approve(address(WETH), params.ethAmount);
            WETH.withdraw(params.ethAmount);

            exchange.issueExactSetFromETH{value: params.ethAmount}(
                params.token,
                params.amountToExchange,
                params.quotes,
                params.issuanceModule,
                params.isDebtIssuance
            );
        } else {
            params.token.approve(address(exchange), params.amountToExchange);

            exchange.redeemExactSetForETH(
                params.token,
                params.amountToExchange,
                params.ethAmount,
                params.quotes,
                params.issuanceModule,
                params.isDebtIssuance
            );
        }
    }

    /// @notice A function that removes the current liquidity + collected fees.
    /// @param tokenId The tokenId of the position we want to remove liquidity from
    /// @return amount0 The amount received back in token0
    /// @return amount1 The amount returned back in token1
    function removeLiquidityAndFees(uint256 tokenId)
        internal
        returns (uint256 amount0, uint256 amount1)
    {
        // get liquidity data for tokenId
        uint128 liquidity = positions[tokenId].liquidity;

        // amount0Min and amount1Min are price slippage checks
        // if the amount received after burning is not greater than these minimums, transaction will fail
        INonfungiblePositionManager.DecreaseLiquidityParams
            memory decreaseParams = INonfungiblePositionManager
                .DecreaseLiquidityParams({
                    tokenId: tokenId,
                    liquidity: liquidity,
                    amount0Min: 0,
                    amount1Min: 0,
                    deadline: block.timestamp
                });

        nonfungiblePositionManager.decreaseLiquidity(
            decreaseParams
        );

        INonfungiblePositionManager.CollectParams
            memory collectParams = INonfungiblePositionManager.CollectParams({
                tokenId: tokenId,
                recipient: address(this),
                amount0Max: type(uint128).max,
                amount1Max: type(uint128).max
            });

        (amount0, amount1) = nonfungiblePositionManager.collect(collectParams);
    }

    /// @notice Calls the mint function defined in periphery.
    /// @param newPos Params of the new position to mint
    /// @return tokenId The id of the newly minted ERC721
    function mintNewPosition(NewPositionParams memory newPos)
        internal
        returns (
            uint256 tokenId
        )
    {
        TransferHelper.safeApprove(
            newPos.token0,
            address(nonfungiblePositionManager),
            newPos.amount0Desired
        );
        TransferHelper.safeApprove(
            newPos.token1,
            address(nonfungiblePositionManager),
            newPos.amount1Desired
        );

        INonfungiblePositionManager.MintParams
            memory params = INonfungiblePositionManager.MintParams({
                token0: newPos.token0,
                token1: newPos.token1,
                fee: newPos.poolFee,
                tickLower: newPos.tickLower,
                tickUpper: newPos.tickUpper,
                amount0Desired: newPos.amount0Desired,
                amount1Desired: newPos.amount1Desired,
                amount0Min: newPos.amount0Min,
                amount1Min: newPos.amount1Min,
                recipient: address(this),
                deadline: 1799217365
            });

        (tokenId,,,) = nonfungiblePositionManager
            .mint(params);

        _addPosition(tokenId);
    }

    function _addPosition(uint256 tokenId) internal {
        (
            ,
            ,
            address token0,
            address token1,
            ,
            ,
            ,
            uint128 liquidity,
            ,
            ,
            ,

        ) = nonfungiblePositionManager.positions(tokenId);

        positions[tokenId] = Position({
            liquidity: liquidity,
            token0: token0,
            token1: token1
        });
    }

    /// @notice Transfers remaining funds in contract to owner
    /// @param token0 The address of token0
    /// @param token1 The address of token1
    function withdrawRemainingBalance(address token0, address token1) external onlyOwner {
        TransferHelper.safeTransfer(
            token0,
            owner(),
            IERC20(token0).balanceOf(address(this))
        );
        TransferHelper.safeTransfer(
            token1,
            owner(),
            IERC20(token1).balanceOf(address(this))
        );
    }

    /// @notice Transfers the NFT to the owner
    /// @param tokenId The id of the erc721
    function retrieveNFT(uint256 tokenId) external onlyOwner {
        // transfer ownership
        nonfungiblePositionManager.safeTransferFrom(
            address(this),
            owner(),
            tokenId
        );
        //remove information related to tokenId
        delete positions[tokenId];
    }
}

