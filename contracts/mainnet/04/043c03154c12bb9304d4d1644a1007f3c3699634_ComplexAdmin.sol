pragma solidity ^0.8.10;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/ClonesUpgradeable.sol";

import "../core/Community.sol";
import "../interfaces/ICommunity.sol";
import "../interfaces/IComplexAdmin.sol";

contract CommunityFactory is Initializable, OwnableUpgradeable {
    ///@notice a variable to track the version of the contract
    string public version;
    //@notice mapping of all communities by name
    mapping(string => address) public communitiesByName;
    ///@notice community base contract address
    address public communityBase;

    /**
     @notice initializer
     @param complexAdminAddress_ the address for complex admin contract
     @dev makes the complex admin contract the owner of this contract
     */
    function initialize(address complexAdminAddress_, address communityBase_)
        public
        initializer
    {
        _transferOwnership(complexAdminAddress_);
        version = "1.0.0";
        communityBase = communityBase_;
    }

    function updateCommunityInstance(address newBase_) external onlyOwner {
        communityBase = newBase_;
    }

    /**
     @notice _createCommunity Allows complex admin to create a new community
     @param communityName_ String representation of the new community name
     @param complexAdmin_ The complex admin contract address
     @param admin_ The initial admin of the community
     @param badgeContractBase_ The address of the base badge contract
     @param passportAddress_ The address of the passport contract
     @dev restricted to owner (complexAdmin contract)
     */
    function _createCommunity(
        string memory communityName_,
        address complexAdmin_,
        address admin_,
        address badgeContractBase_,
        address passportAddress_,
        uint256 tokenId_,
        bool private_,
        string memory badgeURL_
    ) external onlyOwner returns (address) {
        require(
            communitiesByName[communityName_] == address(0x0),
            "ERR: Must use unique name"
        );
        address communityClone = ClonesUpgradeable.clone(communityBase);
        ICommunity community = ICommunity(communityClone);
        IComplexAdmin admin = IComplexAdmin(complexAdmin_);
        admin.setIsCommunity(communityClone);
        community.initialize(
            complexAdmin_,
            admin_,
            communityName_,
            badgeContractBase_,
            passportAddress_,
            tokenId_,
            private_,
            badgeURL_
        );
        communitiesByName[communityName_] = communityClone;
        return communityClone;
    }

    /**
     @notice returnCommunityByName returns the address of a community when given a name
     @param communityName_ the string name of a community for lookup
     @return address of the community
     */
    function returnCommunityByName(string memory communityName_)
        public
        view
        returns (address)
    {
        return communitiesByName[communityName_];
    }

    /**
     @notice updateVersion allows the complex admin contract to update the contract version number
     @param newVersion_ The string representation of the new contract version
     */
    function updateVersion(string memory newVersion_) public onlyOwner {
        version = newVersion_;
    }
}

