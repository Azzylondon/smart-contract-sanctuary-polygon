pragma solidity ^0.8.10;

import {StringsUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "./PassportBase.sol";

import "hardhat/console.sol";

contract Passport is PassportBase, OwnableUpgradeable, ERC1155Holder {
    string public urlBase;
    // ms Representation of time required before a transfer can be executed
    uint256 cooldownPeriod;

    /// Maps a token id to an array of transfer requests (to show transfer history easily)
    mapping(uint256 => TransferRequest[]) public tokenIdToTransferRequests;

    /// Maps a token id to an array of communities to enable display of communities the passport holder belongs to
    mapping(uint256 => Community[]) public tokenIdToCommunities;

    /// Maps a token id to communities by community implementation address
    //EX: tokenIdToCommunityByImplementation[community.implementationAddress]
    mapping(uint256 => mapping(address => Community))
        public tokenIdToCommunityByImplementation;

    // Token id => community address => Badge array
    mapping(uint256 => mapping(address => Badge[]))
        public tokenIdToCommunityBadges;

    // EVENTS
    event PassportMinted(address to, uint256 id);
    event TransferRequested(uint256 tokenId, TransferRequest request);
    event TransferExecuted(uint256 tokenId, TransferRequest request);
    event ForceTransfer(uint256 tokenId, address to);
    event PassportBurned(uint256 tokenId, uint256 timestamp);
    event CommunityAdded(uint256 tokenId, Community community);
    event CommunityRemoved(uint256 tokenId, Community community);
    event BadgeAdded(uint256 tokenId, Badge badge);
    event BadgeRemoved(uint256 tokenId, Badge badge);

    /**
        @notice Initializer
        @param _urlBase initial URL base for passport
        @dev Initializes ownable and transfers ownership to the complex admin contract
        @dev sets cooldown period to one week
     */
    function initialize(string memory _urlBase) public initializer {
        cooldownPeriod = 7 days;
        __Ownable_init();
        urlBase = _urlBase;
    }

    /**
     @notice Allows complex Admin contract to mint a passport
     @param to_ The address passport is to be minted to
     @return uint256 token ID of new passport
     */
    function adminMint(address to_) external onlyOwner returns (uint256) {
        _safeMint(to_);
        emit PassportMinted(to_, addressToId[to_]);
        return addressToId[to_];
    }

    /**
     @notice adminBatchMint allows complex admin contract to batch mint passports
     @param recipients_ An array of recipients
     */
    function adminBatchMint(address[] calldata recipients_) external onlyOwner {
        for (uint256 i = 0; i < recipients_.length; i++) {
            _safeMint(recipients_[i]);
            emit PassportMinted(recipients_[i], addressToId[recipients_[i]]);
        }
    }

    /**
        @notice Updates the url base
        @dev Only owner
        @param newUrlBase_ The new url base (string)
     */
    function updateUrlBase(string memory newUrlBase_) external onlyOwner {
        urlBase = newUrlBase_;
    }

    /**
        @notice A function that allows contract owner to update cooldown period for transfers
        @dev onlyOwner
        @param time_ The time period in ms to set cooldown to (1 week = 7 * 24 * 60 * 60 * 1000)
     */
    function updateCooldownPeriod(uint256 time_) external onlyOwner {
        cooldownPeriod = time_;
    }

    /**
        @notice Returns a marketplace readable URI
        @param tokenId_ the id of the token you want to retrieve
        @return string token URI
     */
    function tokenURI(uint256 tokenId_) external view returns (string memory) {
        require(_exists(tokenId_), "ERR: does not exist");
        return
            string(
                abi.encodePacked(urlBase, StringsUpgradeable.toString(tokenId_))
            );
    }

    /**
        @notice initializeTransfer: Allows a passport owner to initialize a transfer of the passport
        @param tokenId_ the token id of the passport being transferred
        @param to_ The address the passport is to be transferred to
        @dev Creates a struct for TransferRequest and starts the timer
        @dev The caller must be the token owner
     */
    function initializeTransfer(uint256 tokenId_, address to_) external {
        require(_msgSender() == ownerOf(tokenId_), "ERR: not owner");
        TransferRequest memory request = TransferRequest({
            requestId: block.number,
            from: _msgSender(),
            to: to_,
            tokenId: tokenId_,
            timestamp: block.timestamp,
            cooldownComplete: block.timestamp + cooldownPeriod,
            executed: false
        });
        tokenIdToTransferRequests[tokenId_].push(request);
        emit TransferRequested(tokenId_, request);
    }

    /**
        @notice adminInitTransfer: Allows a complex Admin to initialize a transfer of the passport
        @param tokenId_ the token id of the passport being transferred
        @param from_ The owner of the passport
        @param to_ The address the passport is to be transferred to
        @dev Creates a struct for TransferRequest and starts the timer
        @dev The caller must be complex admin
     */
    function adminInitTransfer(
        uint256 tokenId_,
        address from_,
        address to_
    ) external onlyOwner {
        TransferRequest memory request = TransferRequest({
            requestId: block.number,
            from: from_,
            to: to_,
            tokenId: tokenId_,
            timestamp: block.timestamp,
            cooldownComplete: block.timestamp + cooldownPeriod,
            executed: false
        });
        tokenIdToTransferRequests[tokenId_].push(request);
        emit TransferRequested(tokenId_, request);
    }

    /**
        @notice Finalizes transfer of token if params (cooldown) are met
        @param tokenId_ The id of the token that is to be transferred
        @dev Cooldown period MUST be met or will revert
     */
    function finalizeTransfer(uint256 tokenId_) external {
        require(_msgSender() == ownerOf(tokenId_), "ERR: Caller is not owner");
        TransferRequest memory request = tokenIdToTransferRequests[tokenId_][
            tokenIdToTransferRequests[tokenId_].length - 1
        ];
        require(request.cooldownComplete <= block.timestamp, "ERR: Cooldown");
        require(request.executed != true, "ERR: already executed");
        _transferFrom(_msgSender(), request.to, request.tokenId);
        request.executed = true;
        emit TransferExecuted(tokenId_, request);
    }

    /**
        @notice Allows complex admin to override cooldown period and force a transfer
        @param tokenId_ The passport token ID
        @param to_ The address passport to be transferred to
        @dev onlyOwner. Can be used to block malicious transfer, or execute transfer in emergent situations
     */
    function forceTransfer(uint256 tokenId_, address to_) external onlyOwner {
        address from = idToAddress[tokenId_];
        TransferRequest memory request = tokenIdToTransferRequests[tokenId_][
            tokenIdToTransferRequests[tokenId_].length - 1
        ];
        _transferFrom(from, to_, tokenId_);
        request.executed = true;
        emit ForceTransfer(tokenId_, to_);
    }

    /**
     @notice burn allows complexAdmin contract to burn passport
     @param tokenId_ The token ID to be burned
     @dev Restricted to complex admin contract
     */
    function burn(uint256 tokenId_) external onlyOwner {
        _burn(tokenId_);
        emit PassportBurned(tokenId_, block.timestamp);
    }

    /**
     @notice addComunity Allows complexAdmin to add comm unity to a users passport
     @param tokenId_ The id of the passport to have community assigned to it
     @param communityName_ The name of the community to be added
     @param communityImplementation_ The address of the community to be added
     @param badgeImplementation_ The badge implementation address for the community to be added
     @param public_ Whether or not the community is public
     @dev Restricted to complexAdmin contract
     */
    function addCommunity(
        uint256 tokenId_,
        string memory communityName_,
        address communityImplementation_,
        address badgeImplementation_,
        bool public_
    ) external onlyOwner {
        Community memory community = Community({
            communityName: communityName_,
            implementationAddress: communityImplementation_,
            badgeImplementationAddress: badgeImplementation_,
            public_: public_,
            valid: true
        });
        tokenIdToCommunities[tokenId_].push(community);
        tokenIdToCommunityByImplementation[tokenId_][
            communityImplementation_
        ] = community;
        emit CommunityAdded(tokenId_, community);
    }

    /**
     @notice removeCommunity allows complexAdmin contract to remove community from user passport
     @param tokenId_ The token ID to be altered
     @param communityImplementation_ The community address to be removed
     @dev restricted to complex admin contract
     */
    function removeCommunity(uint256 tokenId_, address communityImplementation_)
        external
        onlyOwner
    {
        delete tokenIdToCommunityByImplementation[tokenId_][
            communityImplementation_
        ];
        Community[] memory communities = tokenIdToCommunities[tokenId_];

        for (uint256 i; i < communities.length; i++) {
            if (
                communities[i].implementationAddress == communityImplementation_
            ) {
                delete communities[i];
                delete tokenIdToCommunities[tokenId_][i];
            }
        }
    }

    /**
     @notice removeBadge allows complex admin contract to remove badge from user passport
     @param tokenId_ The id of the user passport to be altered
     @param communityImplementation_ The address of the community that issued the badge
     @param badgeName_ The name of the badge to be removed
     @dev restricted to complexAdmin contract
     */
    function removeBadge(
        uint256 tokenId_,
        address communityImplementation_,
        string memory badgeName_
    ) external onlyOwner {
        Badge[] storage badges = tokenIdToCommunityBadges[tokenId_][
            communityImplementation_
        ];
        for (uint i = 0; i < badges.length; i++) {
            if (
                keccak256(abi.encode(badges[i].badgeName)) ==
                keccak256(abi.encode(badgeName_))
            ) {
                emit BadgeRemoved(tokenId_, badges[i]);
                delete badges[i];
            }
        }
    }

    // Getters==========================================================
    ///@notice returns all assigned communities to a given passport
    function getAssignedCommunities(uint256 tokenId_)
        external
        view
        returns (Community[] memory)
    {
        return tokenIdToCommunities[tokenId_];
    }

    ///@notice returns all transfer requests for a given passport
    function getTransferRequests(uint256 tokenId_)
        external
        view
        returns (TransferRequest[] memory)
    {
        return tokenIdToTransferRequests[tokenId_];
    }

    ///@notice returns all badges held by a passport
    function getBadgesByPassportId(uint256 tokenId_, address communityAddress_)
        external
        view
        returns (Badge[] memory)
    {
        return tokenIdToCommunityBadges[tokenId_][communityAddress_];
    }

    // ERC1155 holder functions
    /**
     @notice override function for ERC1155Holder
     @dev Adds badge that is received to the passport assigned to
     */
    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes memory data
    ) public override returns (bytes4) {
        (
            uint256 passportTokenId_,
            address badgeAddress,
            string memory badgeName,
            string memory communityName,
            address communityAddress,
            bool shared
        ) = abi.decode(data, (uint256, address, string, string, address, bool));
        Community memory community = tokenIdToCommunityByImplementation[
            passportTokenId_
        ][communityAddress];
        require(
            community.implementationAddress != address(0x0),
            "ERR: Community doesnt exist"
        );
        Badge memory badge = Badge({
            community: Community({
                communityName: communityName,
                implementationAddress: communityAddress,
                badgeImplementationAddress: badgeAddress,
                public_: true,
                valid: true
            }),
            tokenId: passportTokenId_,
            badgeName: badgeName,
            shared: shared
        });
        tokenIdToCommunityBadges[passportTokenId_][communityAddress].push(
            badge
        );
        return this.onERC1155Received.selector;
    }

    //Supports interface override
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155Receiver, PassportBase)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

