pragma solidity ^0.8.9;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";

import "./Passport.sol";
import "../factories/CommunityFactory.sol";
import "./Community.sol";
import "../interfaces/ICommunity.sol";

/**
 @notice Complex admin contract
 This contract serves as the command and control hub for the suite of core contracts in the
 complex ecosystem.
 
 Complex admin serves as owner of passport contract (for access control reasons)
 Maintains mappings of active communities
 Maintains mappings of authorized blocks to be implemented in communities
 
 Access control is through accessControlUpgradeable contract
 */

contract ComplexAdmin is Initializable, AccessControlUpgradeable {
    Passport passport;
    CommunityFactory communityFactory;
    /**
     @notice access control variables
     @dev 2 layers of access control: Admin, Founder admin and community
     @dev OZ Contracts also introduces a 3rd layer of access control "DEFAULT_ADMIN_ROLE"
     @dev This role is reseved for a complex admin and serves as a safety catch for overall function
     @dev access
     @dev Founder admin is akin to "super admin" in traditional role systems
     @dev admin is equal to traditional admin roles
     */
    bytes32 public constant ADMIN = keccak256("ADMIN");
    bytes32 public constant FOUNDER_ADMIN = keccak256("FOUNDER_ADMIN");

    /**
     @notice Community struct
     @param communityName the string name of the community
     @param implementationAddress The address of the particular community
     @param badgeImplementationAddress the address of the badge contract deployed for the community
     */
    struct CommunityStruct {
        string communityName;
        address implementationAddress;
    }

    /**
     @notice Badge struct
     @param community a community struct for the issuing community
     @param tokenId the passport token ID that the badge is assigned to
     @param badgename The string name of the badge
     */
    struct Badge {
        Community community;
        uint256 tokenId;
        string badgeName;
    }

    ///@notice An array of all communities
    CommunityStruct[] public communities;

    ///@notice _isCommunity: A mapping to track whether an an address is a registered community (used for access control)
    mapping(address => bool) private _isCommunity;
    ///@notice blockAddressToName: A mapping of all authorized block addresses to string names of the block
    mapping(address => string) public blockAddressToName;
    ///@notice blockNameToAddress: Allows lookup of authorized blocks by name
    mapping(string => address) public blockNameToAddress;

    //Events
    ///@notice BlockApproved: Emits when a new authorized block is added
    event BlockApproved(string blockName, address blockImplementation);
    ///@notice CommunityCreated: Emits when a community is created
    event CommunityCreated(address communityAddress, string communityName);
    event CommunityFactoryUpdated(address communityFactory);
    event BlockRemoved(string blockName, address blockImplementation);
    event CommunityDisabled(address communityAddress, string communityName);
    event CommunityEnabled(address communityAddress, string communityName);

    //modifiers
    ///@notice adminOrCommunity: Checks that msgSender is either an admin or a registered community
    modifier adminOrCommunity() {
        require(
            hasRole(ADMIN, _msgSender()) ||
                _isCommunity[_msgSender()] ||
                _msgSender() == address(this),
            "ERR: Inadequate permissions"
        );
        _;
    }

    /**
     @notice Initializer
     @param passportAddress_ the address of the deployed passport contract
     @dev grants appropriate initial roles and sets the passport instance to the correct address
     */
    function initialize(address passportAddress_, address communityFactory_)
        public
    {
        _grantRole(FOUNDER_ADMIN, _msgSender());
        _grantRole(ADMIN, _msgSender());
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());
        passport = Passport(passportAddress_);
        communityFactory = CommunityFactory(communityFactory_);
    }

    /**
     @notice function that allows admin to change the base community factory instance
     @param newFactory_ The address of the new base community Factory
     @dev restricted to admin role
     */
    function changeCommunityFactoryBase(address newFactory_)
        external
        onlyRole(ADMIN)
    {
        communityFactory = CommunityFactory(newFactory_);
        emit CommunityFactoryUpdated(newFactory_);
    }

    //BLOCK MANAGEMENT===========================
    /**
     @notice addAuthorizedBlock Allows admin to add authorized block to ecosystem
     @param blockImplementationAddress_ The address of the deployed block
     @param blockName_ The string name of the block to be added
     @dev Only callable by admin
     @dev Cannot add block that already exists (must be unique name)
     */
    function addAuthorizedBlock(
        address blockImplementationAddress_,
        string memory blockName_
    ) external onlyRole(ADMIN) {
        require(
            blockNameToAddress[blockName_] == address(0x0),
            "ERR: Block name already exists"
        );
        blockAddressToName[blockImplementationAddress_] = blockName_;
        blockNameToAddress[blockName_] = blockImplementationAddress_;
        emit BlockApproved(blockName_, blockImplementationAddress_);
    }

    /**
     @notice removeAuthorizedBlock Allows admin to remove block from the ecosystem
     @param blockImplementationAddress_ The address of the blocks implementation
     @param blockName_ The string name of the block to be removed
     @dev Only callable by admin
     @dev reverts if block doesn't exist
     */
    function removeAuthorizedBlock(
        address blockImplementationAddress_,
        string memory blockName_
    ) external onlyRole(ADMIN) {
        require(
            blockNameToAddress[blockName_] != address(0x0),
            "ERR: block doesn not exist"
        );
        delete blockAddressToName[blockImplementationAddress_];
        delete blockNameToAddress[blockName_];
        emit BlockRemoved(blockName_, blockImplementationAddress_);
    }

    //PASSPORT MANAGEMENT========================
    /**
     @notice getOrIssuePassport issues passport to new user
     @param to_ The address passport is to be issued to
     @dev If the user already has a passport, will return passport token id
     @dev limited to admin or existing community
     @return uint256 passport id
     */
    function getOrIssuePassport(address to_)
        public
        adminOrCommunity
        returns (uint256)
    {
        if (passport.balanceOf(to_) == 0) {
            return passport.adminMint(to_);
        } else {
            return passport.balanceOf(to_);
        }
    }

    /**
     @notice badgeMintPassport Allows admin to mint passports to an array of recipients
     @param recipients_ an array of recipients
     @dev limited to admin
     */
    function batchMintPassport(address[] memory recipients_)
        external
        onlyRole(ADMIN)
    {
        passport.adminBatchMint(recipients_);
    }

    /**
     @notice forceTransferPassport allows an admin to bypass all time requirements and force transfer a passport
     @notice Useful for emergent transfers (in the case of wallet compromise) or preventing a bad actor from selling
     a passport
     @param to_ the address the passport is to be transferred to
     @param tokenId_ the tokenId of the passport to be transferred
     @dev Only callable by admin
     */
    function forceTransferPassport(address to_, uint256 tokenId_)
        external
        onlyRole(ADMIN)
    {
        passport.forceTransfer(tokenId_, to_);
    }

    /**
     @notice adminInitializeTransfer allows admin to initialize a standard transfer for a token
     @param tokenId_ The token ID of the passport to be transferred
     @param from_ The current owner of the passport
     @param to_ The address passport is to be transferred to
     @dev only callable by admin
     */
    function adminInitializeTransfer(
        uint256 tokenId_,
        address from_,
        address to_
    ) external onlyRole(ADMIN) {
        passport.adminInitTransfer(tokenId_, from_, to_);
    }

    /**
     @notice burnPassport Allows an admin to burn an individual passport
     @param tokenId_ The id of the passport to be burned
     @dev only callable by admin
     */
    function burnPassport(uint256 tokenId_) external onlyRole(ADMIN) {
        passport.burn(tokenId_);
    }

    /**
     @notice updateCooldownPeriod allows the admin to change the time requirement for passport transfers
     @param time_ The amount of time in ms that a user must wait to execute a transfer
     @dev only callable by admin
     */
    function updateCooldownPeriod(uint256 time_) external onlyRole(ADMIN) {
        passport.updateCooldownPeriod(time_);
    }

    /**
     @notice addCommunityToPassport adds a community to a users passport
     @param tokenId_ The passport token ID to be updated
     @param communityName_ The string name of the community to be added
     @param communityImplementation_ The address of the community to be added
     @param public_ Whether a community is public or not
     @dev Should be limited to adminOrCommunity
     */
    function addCommunityToPassport(
        uint256 tokenId_,
        string memory communityName_,
        address communityImplementation_,
        address badgeImplementation_,
        bool public_
    ) external adminOrCommunity {
        passport.addCommunity(
            tokenId_,
            communityName_,
            communityImplementation_,
            badgeImplementation_,
            public_
        );
    }

    /**
     @notice removeCommunityFromPassport removes a community from a given users passport
     @param tokenId_ the passport id of the member having community removed
     @param implementation_ The address of the community being removed
     @dev limited to adminOrCommunity
     */
    function removeCommunityFromPassport(
        uint256 tokenId_,
        address implementation_
    ) external adminOrCommunity {
        passport.removeCommunity(tokenId_, implementation_);
    }

    /**
     @notice updatePassportURI allows founder to update passport URI
     @param newUri_ the new uri for the passport
     @dev limited to founder admin
     */
    function updatePassportURI(string memory newUri_)
        external
        onlyRole(FOUNDER_ADMIN)
    {
        passport.updateUrlBase(newUri_);
    }

    /**
     @notice removeBadgeFromPassportId removes a badge from members passport
     @param tokenId_ the passport id to have badge removed from
     @param communityImplementation_ The address of the community that issued badge
     @param badgeName_ the string name of the badge to be deleted
     @dev restricted to admin or community
     */
    function removeBadgeFromPassportId(
        uint256 tokenId_,
        address communityImplementation_,
        string memory badgeName_
    ) external adminOrCommunity {
        passport.removeBadge(tokenId_, communityImplementation_, badgeName_);
    }

    //END PASSPORT MANAGEMENT===============================

    //COMMUNITY MANAGEMENT==================================

    /**
     @notice createNewCommunity allows admin to create a new community
     @param communityName_ the string name of community to be created
     @param admin_ the address of the original admin
     @param badgeContractBase_ The address of the base badge contract (for cloning)
     @dev restricted to admin role
     */
    function createNewCommunity(
        string memory communityName_,
        address admin_,
        address badgeContractBase_,
        bool private_,
        string memory badgeURL_
    ) external onlyRole(ADMIN) {
        uint256 tokenId = getOrIssuePassport(admin_);
        address community = communityFactory._createCommunity(
            communityName_,
            address(this),
            admin_,
            badgeContractBase_,
            address(passport),
            tokenId,
            private_,
            badgeURL_
        );
        communities.push(
            CommunityStruct({
                communityName: communityName_,
                implementationAddress: community
            })
        );
        _isCommunity[community] = true;
        emit CommunityCreated(community, communityName_);
    }

    function setIsCommunity(address communityAddress_) external {
        require(_msgSender() == address(communityFactory));
        _isCommunity[communityAddress_] = true;
    }

    /**
     @notice Allows admin to disable a community
     @param communityImplementation_ The address of the commuity to be disabled
     @dev Restricted to Admin
     */
    function disableCommunity(address communityImplementation_)
        external
        onlyRole(ADMIN)
    {
        ICommunity community = ICommunity(communityImplementation_);
        community.disableCommunity();
        _isCommunity[communityImplementation_] = false;
        emit CommunityDisabled(
            communityImplementation_,
            community.returnCommunityName()
        );
    }

    /**
     @notice enableCommunity allows an admin to re-enable a community that was previously disabled
     @param communityImplementation_ The address of the community to be re-enabled
     @dev restricted to Admin role
     */
    function enableCommunity(address communityImplementation_)
        external
        onlyRole(ADMIN)
    {
        ICommunity community = ICommunity(communityImplementation_);
        community.enableCommunity();
        _isCommunity[communityImplementation_] = true;
        emit CommunityEnabled(
            communityImplementation_,
            community.returnCommunityName()
        );
    }

    //END COMMUNITY MANAGEMENT==============================

    //GETTERS ==============================================
    /**
     @notice getModuleAddressByName returns a block name string when given an address
     @param blockName_ The string representation of the block to be looked up
     @return address of the block contract
     */
    function getModuleAddressByName(string memory blockName_)
        external
        view
        returns (address)
    {
        return blockNameToAddress[blockName_];
    }

    /**
     @notice supportsInterface returns the interface ID
     @dev Required per imports
     @param interfaceId_ The interface id to be checked
     */
    function supportsInterface(bytes4 interfaceId_)
        public
        view
        virtual
        override(AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId_);
    }
}

