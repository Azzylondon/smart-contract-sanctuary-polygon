pragma solidity ^0.8.10;

interface ICommunity {
    function initialize(
        address adminContract_,
        address admin_,
        string memory communityName_,
        address badge_,
        address passportAddress_,
        uint256 adminPassportId_,
        bool private_,
        string memory badgeURL_
    ) external;

    function disableCommunity() external;

    function enableCommunity() external;

    function returnCommunityName() external returns (string memory);
}

