pragma solidity ^0.8.10;

import {ERC165Upgradeable} from "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import {IERC165Upgradeable} from "@openzeppelin/contracts-upgradeable/utils/introspection/IERC165Upgradeable.sol";
import {IERC721Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import {CountersUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import {IERC721MetadataUpgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/IERC721MetadataUpgradeable.sol";

abstract contract PassportBase is
    ERC165Upgradeable,
    IERC721Upgradeable,
    IERC721MetadataUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter numberTokens;
    CountersUpgradeable.Counter tokenId;

    struct TransferRequest {
        uint256 requestId;
        address from;
        address to;
        uint256 tokenId;
        uint256 timestamp;
        uint256 cooldownComplete; // the timestamp for block.timestamp + cooldownPeriod
        bool executed;
    }

    /// @notice basic struct to hold required community info
    struct Community {
        string communityName;
        address implementationAddress;
        address badgeImplementationAddress;
        bool public_;
        bool valid;
    }

    /// @notice basic struct to hold required badge info
    struct Badge {
        Community community;
        uint256 tokenId;
        string badgeName;
        bool shared;
    }

    /// @notice Stores address to passport id
    mapping(address => uint256) public addressToId;

    /// @notice Stores passport id to address
    mapping(uint256 => address) public idToAddress;

    /// @notice modifier to prevent transferring.
    modifier notSupported() {
        revert("Nontransferrable NFT");
        _;
    }

    string public constant name = "Complex Community Passport";

    string public constant symbol = "CCP";

    function balanceOf(address user) public view returns (uint256) {
        return addressToId[user]; //Changed
    }

    /// @notice ownerOf getter, checks if token exists
    function ownerOf(uint256 id) public view returns (address) {
        require(idToAddress[id] != address(0x0), "does not exist");
        return idToAddress[id];
    }

    /// @notice approvals not supported
    // function getApproved(uint256) public pure returns (address) {
    //     // No implementation
    //     return address(0);
    // }

    function getApproved(uint256) public pure notSupported returns (address) {
        return address(0x0);
    }

    /// @notice approvals not supported
    function isApprovedForAll(address, address) public pure returns (bool) {
        return false;
    }

    /// @notice approvals not supported
    function approve(address, uint256) public notSupported {}

    /// @notice approvals not supported
    function setApprovalForAll(address, bool) public notSupported {
        //
    }

    /// @notice internal safemint function
    function _safeMint(address to) internal returns (uint256) {
        require(to != address(0x0), "cannot mint");
        tokenId.increment();
        numberTokens.increment();
        _transferFrom(address(0x0), to, tokenId.current());
        return tokenId.current();
    }

    /// @notice transfer function to be overridden
    function transferFrom(
        address from,
        address to,
        uint256 checkTokenId
    ) external virtual {}

    /// @notice not supported
    function safeTransferFrom(
        address,
        address,
        uint256
    ) public notSupported {
        //No implementation
    }

    /// @notice not supported
    function safeTransferFrom(
        address,
        address,
        uint256,
        bytes memory
    ) public notSupported {}

    function currentTokenId() public view returns (uint256) {
        return tokenId.current();
    }

    function totalSupply() public view returns (uint256) {
        return numberTokens.current();
    }

    /// @notice Supports ERC721, ERC165
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC165Upgradeable, IERC165Upgradeable)
        returns (bool)
    {
        return
            ERC165Upgradeable.supportsInterface(interfaceId) ||
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC721MetadataUpgradeable).interfaceId;
    }

    function _burn(uint256 id) internal {
        address from = ownerOf(id);
        numberTokens.decrement();
        delete idToAddress[id];
        delete addressToId[from];
        emit Transfer(from, address(0x0), id);
    }

    function _exists(uint256 id) internal view returns (bool) {
        return idToAddress[id] != address(0x0);
    }

    function _transferFrom(
        address from,
        address to,
        uint256 id
    ) internal {
        addressToId[from] = 0x0;
        idToAddress[id] = to;
        addressToId[to] = id;
        emit Transfer(from, to, id);
    }
}

