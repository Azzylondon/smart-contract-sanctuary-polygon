// File: token.sol



contract ERC20TOKEN is ERC20,Ownable {  

    using SafeMath for uint256;
    using SafeMathInt for int256;

    address internal _feeReciever1; 
    address internal _feeReciever2;

    address internal _pairAddress;
    address internal _dexRouterAddress=0x546C79662E028B661dFB4767664d0273184E4dD1;

    uint256 internal _buyFee=5;
    uint256 internal _sellFee=10;
    uint256 internal _feeDenominator=100;
    uint256 internal _sellLimit=0;
    uint256 internal _minFeeDistribution=10**3 * 10**18;

    IDMMPool internal _pairContract;
    IDMMRouter02 internal _dexRouterContract;
 
    bool inSwap = false;

    modifier swapping() {
        inSwap = true;
        _;
        inSwap = false;
    }  
    
    mapping(address => bool) _isFeeExempt;

    constructor() ERC20("World of Ragnarok Zenny", "wZeny") { 
        _mint(msg.sender, 10000000 * 10 ** decimals());
        
        _dexRouterContract = IDMMRouter02(_dexRouterAddress);  

        _pairAddress = IDMMFactory(_dexRouterContract.factory()).createPool(
            _dexRouterContract.weth(),
            IERC20(address(this)),
            10000
        ); 

        _pairContract = IDMMPool(_pairAddress); 

        _isFeeExempt[address(this)]=true;
        _isFeeExempt[owner()]=true; 
    }


    function transfer(address to, uint256 value)
        public override  returns (bool){
            _transferFrom(msg.sender, to, value);
            return true;
    }

    function transferFrom(
        address from_,
        address to_,
        uint256 amount_
    ) public virtual override returns (bool) {    
        _transferFrom(from_, to_, amount_); 
        return true;
    } 

    function _transferFrom(address from_,address to_,uint256 amount_) internal {
        uint256 feeRate;
        uint256 totalFee; 
 

        if( !inSwap && !_isFeeExempt[from_]){
            if (to_==_pairAddress) {
                feeRate=_sellFee;
            }

            if(from_==_pairAddress){
                feeRate=_buyFee;
            }

            if(feeRate>0){ 
                totalFee = amount_.div(_feeDenominator).mul(feeRate);
                amount_=amount_.sub(totalFee);    
                _transfer(from_,address(this),totalFee);  
            }
        } 
        
        _transfer(from_,to_,amount_); 
        emit TakeFeesAndTransfer(from_,to_,feeRate,totalFee);
    } 

    function _beforeTokenTransfer(
        address from_,
        address to_,
        uint256 amount_
    ) internal override{   
        if (to_==_pairAddress && !_isFeeExempt[from_] && (_sellLimit>0) ) {
            require(amount_<=_sellLimit,"Exceeds Sell Limit");
        }  
        super._beforeTokenTransfer(from_,to_,amount_);
    }

    function _afterTokenTransfer(
        address from_,
        address to_,
        uint256 amount_
    ) internal override{
        if ( !inSwap && (msg.sender != _pairAddress) ) {
            _sendFees();
        }  
        super._afterTokenTransfer(from_,to_,amount_);
    }

    function _sendFees() internal{
        uint256 amountToSwap = balanceOf(address(this));
        
        if( amountToSwap < _minFeeDistribution) { return; }
         
        swapTokenBalanceTokensForCoins(); 
        sendTokenCoinsToFeeReceivers();
    } 

    function swapTokenBalanceTokensForCoins() public swapping {
        uint256 amountToSwap = balanceOf(address(this));

        address[] memory poolsPath = new address[](1);
        poolsPath[0] = _pairAddress;

        IERC20[] memory path = new IERC20[](2);
        path[0] = IERC20(address(this));
        path[1] = IERC20(address(_dexRouterContract.weth())); 


        
        _dexRouterContract.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amountToSwap,
            0,
            poolsPath,
            path,
            address(this),
            block.timestamp
        ); 
    }

    function sendTokenCoinsToFeeReceivers() public {  
        uint256 balanceToSend= address(this).balance;
        uint256 feeReceiverShares=balanceToSend.div(2);

        bool success;

        (success, ) = payable(_feeReciever1).call{
            value:feeReceiverShares,
            gas: 30000
        }("");

        (success, ) = payable(_feeReciever2).call{
            value:feeReceiverShares,
            gas: 30000
        }("");
    }

    function setSellLimit(uint256 amount_) public onlyOwner{
        _sellLimit=amount_.mul(10**decimals());
    }

    function setFeeReceiver1(address address_) public onlyOwner{
        _feeReciever1=address_;
    }

    function setFeeReceiver2(address address_) public onlyOwner{
        _feeReciever2=address_;
    }

    function setSellFee(uint256 rate_) public onlyOwner{
        _sellFee=rate_;
    }

    function setBuyFee(uint256 rate_) public onlyOwner{
        _buyFee=rate_;
    }

    function setMinFeeDistribution(uint256 amount_) public onlyOwner{
        _minFeeDistribution=amount_ * 10**decimals();
    }

    receive() external payable {}

    function setFeeDenominator(uint256 amount_) public onlyOwner{
        _feeDenominator=amount_;
    }

    event TakeFeesAndTransfer(address from_, address to_, uint256 feeRate,uint256 totalFee);
}