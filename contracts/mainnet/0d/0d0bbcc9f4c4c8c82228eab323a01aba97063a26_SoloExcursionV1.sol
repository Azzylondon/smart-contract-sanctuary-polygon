contract CompassRentalV1 is AccessControl {
    using Counters for Counters.Counter;

    Counters.Counter private _rentalIdCounter;    
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    
    address public vault;
    IERC721 public compassNFT;
    IToken public gold;
    IToken public wise;   


    mapping(uint256 => Rental) public rentals;
    
    mapping(uint256 => address) public renter;
    mapping(uint256 => address) public owner;

    event AvailableForRent(uint256 indexed tokenId, address indexed owner, uint256 indexed timeout, uint256 timestamp);
    event Renting(uint256 indexed tokenId, address indexed renter, uint256 timeout, uint256 timestamp);
    event EndRental(uint256 indexed tokenId, address indexed owner, address indexed renter, uint256 rentalId, uint256 timestamp);
    event ForcedEndRental(uint256 indexed tokenId, address indexed owner, address indexed renter, uint256 rentalId, uint256 timestamp);

    struct Rental {
        uint256 compassId;
        address cRenter;
        address cOwner;
        bool ended;
        uint256 rentEnd;
    }

    constructor(address _vault, address _compass, address _gold, address _wise) {
        vault = _vault;
        compassNFT = IERC721(_compass);
        gold = IToken(_gold);
        wise = IToken(_wise);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);                
    }

    function rentOut(uint256 tokenId, uint256 rentTimeout) public {
        address caller = msg.sender;
        address operator = address(this);

        require(compassNFT.isApprovedForAll(caller, operator), "");
        require(compassNFT.ownerOf(tokenId) == caller, "");
        require(rentTimeout > block.timestamp + 86400, "Duration is at least 1d");

        uint256 goldFee = viewRentOutCost(rentTimeout);

        require(gold.allowance(caller, operator) > goldFee, "Insufficient allowance");
        require(gold.balanceOf(caller) > goldFee, "Insufficient balance");
        
        gold.burnFrom(caller, goldFee);
        compassNFT.transferFrom(caller, operator, tokenId);
        
        uint256 rentalId = _rentalIdCounter.current();
        _rentalIdCounter.increment();      

        Rental storage rental = rentals[rentalId]; 
        rental.compassId = tokenId;   
        rental.cOwner = caller;
        rental.rentEnd = rentTimeout;
        owner[tokenId] = caller;

        emit AvailableForRent(tokenId, caller, rentTimeout, block.timestamp);
    }

    function viewRentOutCost(uint256 rentTimeout) public view returns (uint256 goldFee) {
        uint256 duration = rentTimeout - block.timestamp;
        goldFee = duration * 1e15;        
    }


    function toRent(uint256 rentalId) public {
        address caller = msg.sender;
        Rental storage rental = rentals[rentalId]; 

        rental.cRenter = caller;         
        renter[rental.compassId] = caller;

        emit Renting(rental.compassId, caller, rental.rentEnd, block.timestamp);
    }

    function endRental(uint256 rentalId) public {
        address caller = msg.sender;
        address operator = address(this);

        Rental storage rental = rentals[rentalId];  
        uint256 tokenId = rental.compassId;

        require(block.timestamp >= rental.rentEnd, "Can only end rental after timeout");

        compassNFT.transferFrom(operator, caller, tokenId); 
        
        renter[tokenId] = address(0);
        owner[tokenId] = address(0);

        emit EndRental(tokenId, rental.cOwner, rental. cRenter, rentalId, block.timestamp);
    } 

    function forceEndRental(uint256 rentalId) public {
        address caller = msg.sender;
        address operator = address(this);

        Rental storage rental = rentals[rentalId];  
        uint256 tokenId = rental.compassId;

        uint256 goldFee = 100 * 1e18;

        require(gold.allowance(caller, operator) > goldFee, "Insufficient allowance");
        require(gold.balanceOf(caller) > goldFee, "Insufficient balance");    

        gold.burnFrom(caller, goldFee);

        compassNFT.transferFrom(operator, caller, tokenId); 
        
        renter[tokenId] = address(0);
        owner[tokenId] = address(0);

        emit ForcedEndRental(tokenId, rental.cOwner, rental. cRenter, rentalId, block.timestamp);
    }  

    function viewRenting(uint256 tokenId) public view returns (address owner_, address renter_) {
        owner_ = owner[tokenId];
        renter_ = renter[tokenId];
    }   

    function viewBalance() public view returns (uint coinBalance, uint goldBalance, uint wiseBalance) {
        coinBalance = address(this).balance;
        goldBalance = gold.balanceOf(address(this));
        wiseBalance = wise.balanceOf(address(this));
    }

    function withdrawToken(uint coinAmount, uint goldAmount, uint wiseAmount) public onlyRole(MINTER_ROLE) {
        (uint coinBalance, uint goldBalance, uint wiseBalance) = viewBalance();
        require(coinAmount <= coinBalance, "insufficient balance");
        require(goldAmount <= goldBalance, "insufficient balance");
        require(wiseAmount <= wiseBalance, "insufficient balance");
        (bool sent, ) = vault.call{value: coinAmount}("");
        require(sent, "Failed to send Ether to Vault");           
        gold.transfer(vault, goldAmount);
        wise.transfer(vault, wiseAmount);
    }          

}


