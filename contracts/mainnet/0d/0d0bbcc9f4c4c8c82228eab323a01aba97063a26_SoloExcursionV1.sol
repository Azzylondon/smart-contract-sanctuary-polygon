    mapping(uint256 => uint256) compassBatteryAdded;

    event BatteryInitiated(uint256 indexed tokenId);
    event BatteryAdded(uint256 indexed tokenId, uint256 powerAdded);
    event BatteryUsed(uint256 indexed tokenId, uint256 timestamp);
    event BatteryLevelUpgraded(uint256 indexed tokenId, uint256 compassBatteryLevelLevel);

    event Withdrawal(uint256 goldAmount, uint256 wiseAmount);

    constructor(address _compass, address _compassDetail, address _gold, address _wise) {
        compassNFT = IERC721(_compass);
        compassNFTDetail = ICompassNFTDetail(_compassDetail);
        gold = IToken(_gold);
        wise = IToken(_wise);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);                
    }

    function initiateCompass(uint256 tokenId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == false, "Compass must not initiated prior");

        ( , , , uint256 battery) = compassNFTDetail.viewCompassAttributes(tokenId);

        compassInitiated[tokenId] = true;
        compassBattery[tokenId] = battery;
        compassBatteryCapacity[tokenId] = battery;
        compassBatteryLevel[tokenId] = 0;

        emit BatteryInitiated(tokenId);
    }

    function addBattery(uint256 tokenId) public {
        address caller = msg.sender;

        uint256 batteryNeeded = compassBatteryCapacity[tokenId] - compassBattery[tokenId];

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");
        require(batteryNeeded > 0, "Battery < Capacity");
        require(compassBatteryAdded[tokenId] + 86400 > block.timestamp, "Updated only once per day");
        
        (, , , uint256 battery) = compassNFTDetail.viewCompassAttributes(tokenId);
        uint256 goldCost = batteryNeeded * battery * 1e18;      

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        gold.burnFrom(caller, goldCost);

        compassBatteryAdded[tokenId] = block.timestamp;
        compassBatteryCapacity[tokenId] = batteryNeeded;

        emit BatteryAdded(tokenId, batteryNeeded);
    }

    function updateBatteryCasing(uint256 tokenId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");

        ( , , , uint256 battery) = compassNFTDetail.viewCompassAttributes(tokenId);
        
        uint256 goldCost = 100e18;

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        gold.burnFrom(caller, goldCost);

        uint256 newCompassBatteryLevel = compassBatteryLevel[tokenId] + 1;

        compassBatteryCapacity[tokenId] = battery;
        compassBattery[tokenId] = battery;
        compassBatteryLevel[tokenId] = newCompassBatteryLevel;

        emit BatteryLevelUpgraded(tokenId, newCompassBatteryLevel);
    }    

    function reduceBattery(uint256 tokenId, uint256 amount) public {
        require(compassBattery[tokenId] > 0, "");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");
        address signer = msg.sender;
        bool signerCanRemove = false;
        for (uint i; i< removers.length;i++){
            if (removers[i] == signer) {
                signerCanRemove = true;
            }            
        }        
        require(signerCanRemove == true, "Not valid signer");
        compassBattery[tokenId] -= amount;

        emit BatteryUsed(tokenId, block.timestamp);
    }    

    function viewBatteryLeft(uint256 tokenId) public view returns (uint256 compassBattery_, uint256 compassBatteryLevel_) {
            require(compassInitiated[tokenId] == true, "Compass must be initiated prior");
            compassBattery_ = compassBattery[tokenId];
            compassBatteryLevel_ = compassBatteryLevel[tokenId];        
    }

    function viewCompass(uint256 tokenId) public view returns (bool compassInitiated_, uint256 compassBatteryCapacity_, 
        uint256 compassBattery_, uint256 compassBatteryLevel_) {
            compassInitiated_ = compassInitiated[tokenId];
            compassBatteryCapacity_ = compassBatteryCapacity[tokenId];
            compassBattery_ = compassBattery[tokenId];
            compassBatteryLevel_ = compassBatteryLevel[tokenId];
    }


    function isCompassInitiated(uint256 tokenId) public view returns (bool ready) {
        ready = compassInitiated[tokenId];
    }

    function howManyCompassBattery(uint256 tokenId) public view returns (uint256 batteryAmount) {
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");
        batteryAmount = compassBattery[tokenId];
    }

    function viewBalance() public view returns (uint256 coinBalance, uint256 goldBalance, uint256 wiseBalance) {
        address operator = address(this);
        coinBalance = operator.balance;
        goldBalance = gold.balanceOf(operator);
        wiseBalance = wise.balanceOf(operator);
    }

    function withdrawTokens(uint256 coinAmount, uint256 goldAmount, uint256 wiseAmount) public onlyRole(MINTER_ROLE) {
        address caller = msg.sender;

        (uint coinBalance, uint goldBalance, uint wiseBalance) = viewBalance();    

        require(coinBalance >= coinAmount, "insuffiencient fund");
        require(goldBalance >= goldAmount, "insuffiencient fund");
        require(wiseBalance >= wiseAmount, "insuffiencient fund");

        gold.transfer(caller, goldAmount);
        wise.transfer(caller, wiseAmount);
    }   

    function addRemover(address remover) public onlyRole(MINTER_ROLE) {
        removers.push(remover);
    }

}


// File contracts/SoloExcursionV1.sol

pragma solidity ^0.8.0;

contract SoloExcursionV1 is AccessControl {

    using Counters for Counters.Counter;

    Counters.Counter private _excursionIdCounter; 
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    
    uint256 public spanner;
    uint256 public intelligence;
    uint256 public renterPercent;
    uint256 public ownerPercent;
    IERC20 public gold;
    IERC20 public wise;  
    IERC721 public compassNFT;
    ICompassNFTDetail public compassNFTDetail;
    ICompassBatteryV1 public batteryContract;

    struct Excursion {
        address user;
        uint tokenId;
        bool completed;
        uint goldReward;
        uint wiseReward;
        uint startTimestamp;
        uint endTimestamp;
    }
    
    mapping(uint=> Excursion) public excursions;

    event ExcursionStarted(uint256 indexed tokenId, uint256 indexed excursionId, address indexed user);
    event ExcursionEnded(uint256 indexed tokenId, address indexed user, uint256 goldReward, uint256 wiseReward);

    constructor(address _compass, address _compassDetail, address _battery, address _gold, address _wise) {
        compassNFT = IERC721(_compass);
        compassNFTDetail = ICompassNFTDetail(_compassDetail);
        batteryContract = ICompassBatteryV1(_battery);
        gold = IERC20(_gold);
        wise = IERC20(_wise);

        spanner = 1e9;
        intelligence = 1e7;
        renterPercent = 4000;
        ownerPercent = 6000;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);        
    }

    function startExcursion(uint256 tokenId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be owner");            
        require(batteryContract.isCompassInitiated(tokenId) == true, "Compass must be initiated prior");
        require(batteryContract.howManyCompassBattery(tokenId) > 0, "Battery >0");

        uint256 excursionId = _excursionIdCounter.current();
        _excursionIdCounter.increment();      
        batteryContract.reduceBattery(tokenId, 1);

        (uint goldReward, uint wiseReward) = viewExcursionRewards(tokenId);

        Excursion storage excursion = excursions[excursionId];
        excursion.user = caller;
        excursion.tokenId = tokenId;
        excursion.goldReward = goldReward;
        excursion.wiseReward = wiseReward;
        excursion.startTimestamp = block.timestamp;
    
        emit ExcursionStarted(tokenId, excursionId, caller);  
    }


    function endExcursion(uint256 excursionId) public {
        address caller = msg.sender;

        Excursion storage excursion = excursions[excursionId];

        require(excursion.completed == false, "Must not be able to edit excursion");
        require(excursion.user == caller, "Must be original caller");

        excursion.completed = true;
        excursion.endTimestamp = block.timestamp;
        gold.transfer(caller, excursion.goldReward);
        wise.transfer(caller, excursion.wiseReward);         

        emit ExcursionEnded(excursion.tokenId, caller, excursion.goldReward, excursion.wiseReward);
    }



    function viewExcursionRewards(uint256 tokenId) public view returns (uint goldReward, uint wiseReward) {
        (uint256 accuracy, , uint256 connectivity,) = compassNFTDetail.viewCompassAttributes(tokenId);
        
        goldReward = 1e18 * spanner / 1e9 * accuracy;
        wiseReward = 1e18 * intelligence / 1e9 * connectivity; 

        (uint256 compassQuality, uint256 compassLevel, uint256 compassType) = compassNFTDetail.viewCompassBasic(tokenId);

        if (compassQuality > 0) {
            uint256 compassQualityMultiplier = compassQuality ** compassQuality;        

            if (compassType == 2) {
                goldReward = goldReward * compassQualityMultiplier / 100;
                wiseReward = wiseReward * compassQualityMultiplier;
            } else if (compassType == 3) {
                goldReward = goldReward * 6 / 10 * compassQualityMultiplier;
                wiseReward = wiseReward * compassQualityMultiplier / 100;
            } else {
                goldReward = goldReward * compassQualityMultiplier;
                wiseReward = wiseReward * compassQualityMultiplier / 100;            
            }     
        }   

        if (compassLevel > 0) {
            uint levelMultiplier = ((compassLevel * 1000) + 10000) / 10000;
            goldReward =  goldReward * levelMultiplier;
            wiseReward =  wiseReward * levelMultiplier;
        } 
    }


    function viewExcursion(uint excursionId) public view returns (
        address user, bool completed, uint goldReward, uint wiseReward, 
        uint startTimestamp, uint endTimestamp
    ) {
        Excursion memory excursion = excursions[excursionId];
        user = excursion.user;
        completed = excursion.completed;
        goldReward = excursion.goldReward;
        wiseReward = excursion.wiseReward;
        startTimestamp = excursion.startTimestamp;
        endTimestamp = excursion.endTimestamp;
    }


    function updateValue(uint256 _spanner, uint256 _intelligence, uint256 _renterPercent, uint256 _ownerPercent) public onlyRole(MINTER_ROLE) {
        spanner = _spanner;
        intelligence = _intelligence;
        renterPercent = _renterPercent;
        ownerPercent = _ownerPercent;
    }

    function viewBalance() public view returns (uint coinBalance, uint goldBalance, uint wiseBalance) {
        coinBalance = address(this).balance;
        goldBalance = gold.balanceOf(address(this));
        wiseBalance = wise.balanceOf(address(this));
    }

    function withdrawToken(uint coinAmount, uint goldAmount, uint wiseAmount) public onlyRole(MINTER_ROLE) {
        address caller = msg.sender;
        (uint coinBalance, uint goldBalance, uint wiseBalance) = viewBalance();
        require(coinAmount <= coinBalance, "insufficient balance");
        require(goldAmount <= goldBalance, "insufficient balance");
        require(wiseAmount <= wiseBalance, "insufficient balance");
        (bool sent, ) = caller.call{value: coinAmount}("");
        require(sent, "Failed to send Ether to caller");           
        gold.transfer(caller, goldAmount);
        wise.transfer(caller, wiseAmount);
    }    


}