interface IToken is IERC20 {
    function burnFrom(address, uint) external;
}

interface ICompassNFT is IERC721 {
    function safeMint(address) external returns (uint);
}

interface ICompassNFTDetail {

    function viewCompass(uint) external view returns(
        uint ,uint ,uint ,uint ,
        uint ,uint ,uint ,uint ,
        uint ,uint ,uint ,uint ,
        uint ,uint);
    function viewCompassAttributes(uint) external view returns(uint ,uint ,uint ,uint);
    function viewCompassCountInfo(uint) external view returns(uint ,uint ,uint);
    function viewCompassBasic(uint) external view returns(uint ,uint ,uint);
}

contract CompassNFTDetail is AccessControl {
    
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    struct Compass {
        uint256 attribute01;
        uint256 attribute02;
        uint256 attribute03;
        uint256 attribute04;  

        uint256 quality;
        uint256 level;
        uint256 compassType;
        uint256 mergedCount;
        uint256 mergedTimestamp;
        uint256 upgradeTimestamp;        
    }

    address public vault;
    IToken public gold;
    IToken public wise;
    ICompassNFT public compassContract;
    uint256 public baseMint;
    uint256 public maxQuality;
    uint256 public maxType;

    mapping (uint256 => Compass) public compasses;

    event CompassMerged(address indexed to, uint256 indexed fatherId, uint256 indexed motherId, uint256 tokenId, uint256 timestamp);
    event CompassUpgraded(uint256 indexed tokenId, uint256 level, uint256 timestamp, uint256 goldCost, uint256 wiseCost);

    constructor(address _compass, address _vault, address _gold, address _wise) {
        compassContract = ICompassNFT(_compass);
        vault = _vault;
        gold = IToken(_gold);
        wise = IToken(_wise);
        baseMint = 500e18;
        maxQuality = 2;
        maxType = 4;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);           
    }

    function setBaseMintFee(uint256 _fee) public onlyRole(MINTER_ROLE) {
        baseMint = _fee;
    }  

    function setMax(uint _maxQuality, uint _maxType) public onlyRole(MINTER_ROLE) {
        maxQuality = _maxQuality;
        maxType = _maxType;
    }      

    function initialiseCompass(uint256 tokenId, uint compassQuality, uint compassType) public {
        address caller = msg.sender;
        address operator = address(this);
        uint goldCost;

        if (compassQuality == 0) {
            goldCost = baseMint;
        } else {
            goldCost = baseMint * 2;
        }

        if (compassQuality == 0) {
            goldCost = baseMint;
        } else if (compassQuality == 1) {
            goldCost = baseMint * 2;
        } else {
            uint256 multiplier = compassQuality + 1;
            goldCost = baseMint * (multiplier ** multiplier);
        }      
    
        require(compassQuality < maxQuality, "");
        require(compassType < maxType, "");
        require(gold.allowance(caller, operator) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");        
        require(caller == compassContract.ownerOf(tokenId), ""); 

        gold.transferFrom(caller, vault, goldCost); 

        _setCompass(tokenId, compassQuality, compassType);
    }

    function initialiseSpecialCompass(uint256 tokenId, uint compassQuality, uint compassType) public onlyRole(MINTER_ROLE) {
        _setCompass(tokenId, compassQuality, compassType);
    }    

    function _setCompass(uint256 tokenId, uint compassQuality, uint compassType) internal {

        Compass storage compass = compasses[tokenId];
        compass.level += 1;
        compass.upgradeTimestamp = block.timestamp;
        compass.quality = compassQuality;
        compass.compassType = compassType;        

        uint256 maxRange = 5; uint256 cQualityMultiplier = 1;
        uint256 attr01; uint256 attr02; uint256 attr03; uint256 attr04;

        if (compass.quality > 0) {
            uint256 multiplier = compass.quality + 1;
            cQualityMultiplier = multiplier ** multiplier;
        } else if (compass.quality == 1) {
            cQualityMultiplier = 1;
        }

        attr01 = _getPRandom(0, maxRange) * cQualityMultiplier;
        attr02 = _getPRandom(1, maxRange) * cQualityMultiplier;
        attr03 = _getPRandom(2, maxRange) * cQualityMultiplier;
        attr04 = _getPRandom(3, maxRange) * cQualityMultiplier;

        if (compass.compassType == 0) {
            attr01 = attr01 * 2;
        } else if (compass.compassType == 1) {
            attr02 = attr02 * 2;
        } else if (compass.compassType == 2) {
            attr03 = attr03 * 2;
        } else if (compass.compassType == 3) {
            attr04 = attr04 * 2;
        }

        compass.attribute01 += attr01;
        compass.attribute02 += attr02;
        compass.attribute03 += attr03;
        compass.attribute04 += attr04;  
    }

    function _getPRandom(uint inputNumber, uint _maxRange) internal view returns (uint) {        
        uint randomHash = uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp + inputNumber, _maxRange)));
        uint num_ = randomHash % _maxRange;
        if (num_ == 0) {
            num_ = 1;
        }
        return num_;
    }

    function upgradeCompass(uint256 tokenId) public {

        address caller = msg.sender;
        (uint256 goldCost, uint256 wiseCost) = viewUpgradeCost(tokenId);

        Compass storage token = compasses[tokenId];

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        require(wise.allowance(caller, address(this)) >= wiseCost, "Not enough WISE");
        require(wise.balanceOf(caller) >= wiseCost, "Not enough WISE");      

        require(token.upgradeTimestamp > block.timestamp + (86400 * token.level), "More than 1d per level"); 

        gold.burnFrom(caller, goldCost);
        wise.burnFrom(caller, wiseCost);

        _setCompass(tokenId, token.quality, token.compassType);

        token.upgradeTimestamp = block.timestamp;

        emit CompassUpgraded(tokenId, token.level, block.timestamp, goldCost, wiseCost);
    }

    function mergeCompass(uint256 fatherId, uint256 motherId) public {

        address caller = msg.sender;    

        (uint goldCost, uint wiseCost) = viewMergeCost(fatherId, motherId);

        (uint fatherMergedCount, uint fatherMergedTimestamp) = viewCompassCountInfo(fatherId);
        (uint motherMergedCount, uint motherMergedTimestamp) = viewCompassCountInfo(motherId);

        require(fatherMergedCount < 5, "Father merged >5");
        require(motherMergedCount < 5, "Mother merged >5");

        require(fatherMergedTimestamp < block.timestamp, "Merge father ts > now");
        require(motherMergedTimestamp < block.timestamp, "Merge mother ts > now");

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        require(wise.allowance(caller, address(this)) >= wiseCost, "Not enough WISE");
        require(wise.balanceOf(caller) >= wiseCost, "Not enough WISE");   

        gold.burnFrom(caller, goldCost);
        wise.burnFrom(caller, wiseCost);

        _updateCompassCountInfo(fatherId);
        _updateCompassCountInfo(motherId);

        (uint fatherQuality, , ) = viewCompassBasic(fatherId);
        (, , uint motherType) = viewCompassBasic(motherId);

        uint256 tokenId = compassContract.safeMint(caller);
        _setCompass(tokenId, fatherQuality, motherType);
        emit CompassMerged(caller, fatherId, motherId, tokenId, block.timestamp);
    }

    function viewMergeCost(uint256 fatherId, uint256 motherId) public view returns (uint256 goldCost, uint256 wiseCost){
        
        (uint fatherQuality, , ) = viewCompassBasic(fatherId);
        (uint motherQuality, , ) = viewCompassBasic(motherId);

        uint256 cQualityMultiplier;       

        if (fatherQuality == 0) {
            cQualityMultiplier = 1;
        } else if (fatherQuality == 1) {
            cQualityMultiplier = 2;
        }  else {
            uint256 multip = fatherQuality + 1;
            cQualityMultiplier = multip ** multip;
        }

        goldCost = 400e18 * cQualityMultiplier; 
        wiseCost = 1e18 * motherQuality; 
    }        

    function viewUpgradeCost(uint256 tokenId) public view returns (uint256 goldCost, uint256 wiseCost){

        Compass storage token = compasses[tokenId];
        if (token.quality == 0) {
            goldCost = baseMint;
        } else if (token.quality > 1) {
            goldCost = baseMint + (token.quality * baseMint);
        } else {
            goldCost = baseMint * 1000e24;
        }        
    
        if (token.level >= 30) {
            wiseCost = goldCost / 100;
        }
    }

    function viewCompass(uint256 tokenId) public view returns (
        uint256 attribute01_,uint256 attribute02_,uint256 attribute03_,uint256 attribute04_,
        uint256 cQuality_,uint256 cLevel_,uint256 cType_,uint256 cMergedCount_,
        uint256 cMergedTimestamp_,uint256 cUpgradeTimestamp_) {

        Compass memory compass = compasses[tokenId];

        attribute01_ = compass.attribute01;
        attribute02_ = compass.attribute02;
        attribute03_ = compass.attribute03;
        attribute04_ = compass.attribute04;
        cQuality_ = compass.quality;
        cLevel_ = compass.level;
        cType_ = compass.compassType;
        cMergedCount_ = compass.mergedCount;
        cMergedTimestamp_ = compass.mergedTimestamp;
        cUpgradeTimestamp_ = compass.upgradeTimestamp;
    }

    function viewCompassCountInfo(uint tokenId) public view returns (
        uint256 cMergedCount_, uint256 cMergedTimestamp_) {
        
        Compass memory compass = compasses[tokenId];
        cMergedCount_ = compass.mergedCount;
        cMergedTimestamp_ = compass.mergedTimestamp; 
    }

    function _updateCompassCountInfo(uint tokenId) internal {
        Compass storage compass = compasses[tokenId];
        uint256 newTimestamp = compass.mergedCount * 86400;
        compass.mergedCount += 1;
        compass.mergedTimestamp = block.timestamp + newTimestamp; 
    }    

    function viewCompassAttributes(uint256 tokenId) public view returns (
        uint256 attribute01_,uint256 attribute02_,uint256 attribute03_,uint256 attribute04_
    ) {
        Compass memory compass = compasses[tokenId];
        attribute01_ = compass.attribute01;
        attribute02_ = compass.attribute02;
        attribute03_ = compass.attribute03;
        attribute04_ = compass.attribute04;        
    }  

    function viewCompassBasic(uint256 tokenId) public view returns (
        uint256 cQuality_,uint256 cLevel_,uint256 cType_
    ) {
        Compass memory compass = compasses[tokenId];
        cQuality_ = compass.quality;
        cLevel_ = compass.level;
        cType_ = compass.compassType;     
    }    

    function viewBalance() public view returns (uint coinBalance, uint goldBalance, uint wiseBalance) {
        coinBalance = address(this).balance;
        goldBalance = gold.balanceOf(address(this));
        wiseBalance = wise.balanceOf(address(this));
    }

    function withdrawToken(uint coinAmount, uint goldAmount, uint wiseAmount) public onlyRole(MINTER_ROLE) {
        address caller = msg.sender;
        (uint coinBalance, uint goldBalance, uint wiseBalance) = viewBalance();
        require(coinAmount <= coinBalance, "insufficient balance");
        require(goldAmount <= goldBalance, "insufficient balance");
        require(wiseAmount <= wiseBalance, "insufficient balance");
        (bool sent, ) = caller.call{value: coinAmount}("");
        require(sent, "Failed to send Ether to caller");           
        gold.transfer(caller, goldAmount);
        wise.transfer(caller, wiseAmount);
    } 


 
}


