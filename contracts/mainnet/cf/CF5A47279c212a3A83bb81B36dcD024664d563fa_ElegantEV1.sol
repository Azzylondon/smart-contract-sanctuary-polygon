pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

interface OrganiserI{
    function returnRace(uint _race) external view returns (uint[] memory);
    function setPrize(uint _token, uint _matic, uint _nuts) external;
    function buildRace() external;
    function nftByID(uint _token) external view returns (uint, address , uint, uint, uint);
    function currentlyStaked(uint _token) external view returns (uint);
}

contract ElegantEV1 is Ownable, AccessControl {
    using SafeMath for uint256;
    address organs = 0x5FD77E43f7BF27e8E7Cb4056ec2a2773930828d6;

    uint deployT;

    //data//
    uint public raceNum = 1;

    struct performance{
        uint id;
        uint firstLeg;
        uint secondLeg;
        uint finishPos;
    }
    //mapping each race id to the racers
        mapping(uint => uint[]) public racersByRace; 
    //mapping each racer id to array of racer numbers given
        mapping(uint => uint[]) public numsByRacer; 
    //mapping each racer number to their performance
        mapping(uint => performance) public perfByRacer;
    
    //data//
    struct stakedNFT{
        uint id; 
        address user;
        uint prize;
        uint nutPrize;
        uint raceIn;
    }
    mapping(uint => stakedNFT)  public nftByID;
    mapping(address => uint[])  public nftByOwner;
    mapping(address => uint)  public pointsByOwner;
    mapping(uint => uint[])  public nftByRace;
    uint[] public currentlyStaked;

    bytes32 public constant TEAM_ROLE = keccak256("TEAM_ROLE");

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        grantRole(TEAM_ROLE, msg.sender);
        deployT = block.timestamp;
    }

    // ADMIN //
    function makeTeamMember(address to) public {
        require(hasRole(TEAM_ROLE, msg.sender), "Caller is not a member of the team.");
        grantRole(TEAM_ROLE, to);
    }

    //  //

    function copy() public {
        require(hasRole(TEAM_ROLE, msg.sender), "Caller is not a member of the team.");
        for (uint i=0; i<1340;i++){
            (uint _id, address _user, uint _prize, uint _nutPrize, uint _raceIn) = OrganiserI(organs).nftByID(i);
            nftByID[i].id = _id;
            nftByID[i].user = _user;
            nftByID[i].prize = _prize;
            nftByID[i].nutPrize = _nutPrize;
            nftByID[i].raceIn = _raceIn;
            currentlyStaked.push(OrganiserI(organs).currentlyStaked(i));
        }
    }


    function build(uint _amount) public {
        require(hasRole(TEAM_ROLE, msg.sender), "Caller is not a member of the team.");
        for (uint i=0; i<_amount; i++){
            buildRace();
        }
    }

    function buildRace() internal {
        require(hasRole(TEAM_ROLE, msg.sender), "Caller is not a member of the team.");
        
        uint i = 0;
        uint total = currentlyStaked.length;
        uint racing = 0;
        if (total >= 10){
            racing = 10;
        } else {
            racing = total;
        }
        for(i; i<racing; i++){ //go through every racer
            uint rNonce = block.timestamp.sub(deployT);
            uint _r = randomNumber(rNonce, 0, (currentlyStaked.length));
            nftByRace[raceNum].push(currentlyStaked[_r]);
            nftByID[currentlyStaked[_r]].raceIn = raceNum;
            currentlyStaked[_r] = currentlyStaked[(currentlyStaked.length - 1)];
            currentlyStaked.pop();
        }
        raceNum = raceNum.add(1);
    }

    function returnRace(uint _race) public view returns (uint[] memory){
        return nftByRace[_race];
    }

    function nowRacing() public {
        require(hasRole(TEAM_ROLE, msg.sender), "Caller is not a member of the team.");
        uint[] memory _racers = returnRace(raceNum);
        uint[] memory progress = _racers;
        uint[] memory finishOrder = _racers;
        uint i = 0;
        uint nowRunning;
        for(i; i < _racers.length; i ++){
            uint _racer = raceCheck(raceNum,i);
            progress[i] = 0;
            finishOrder[i] = 0;
            uint _nonce = i;
            nowRunning = i.add(10*raceNum);
            performance memory _pp;
            _pp.id = _racer;
            _pp.firstLeg = randomNumber(_nonce, 4, 11);
            _nonce = _nonce + 1;
            _pp.secondLeg = randomNumber(_nonce, 6, 13);
            perfByRacer[nowRunning] = _pp;
        }
        uint finished = 0;
        uint run = (_racers.length);
        while (finished < run){
            uint j = 0;
            for(j; j < _racers.length; j ++){
                nowRunning = j.add(10*raceNum);
                if (progress[j] < 300){
                    if (progress[j] > 150){
                        progress[j] = progress[j] + perfByRacer[nowRunning].secondLeg;
                    } else {
                        progress[j] = progress[j] + perfByRacer[nowRunning].firstLeg;
                    }
                } else if (perfByRacer[nowRunning].finishPos == 0){
                    progress[j] = 300;
                    perfByRacer[nowRunning].finishPos = finished + 1;
                    finishOrder[finished] = nowRunning;
                    finished = finished + 1;
                    if (perfByRacer[nowRunning].finishPos == 1){
                        OrganiserI(organs).setPrize(perfByRacer[nowRunning].id,500000000000000000,1600000000000000000000);
                    }
                }
            }
            //finished = finished + 1;
            raceNum = raceNum + 1;
        }

        
    }

    function raceCheck(uint _race, uint _slot) public view returns(uint){
        uint[] memory _racers = OrganiserI(organs).returnRace(_race);
        return _racers[_slot];
    }
    
    function randomNumber(uint _nonce, uint _start, uint _end) private view returns (uint){
        uint _far = _end.sub(_start);
        uint random = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, _nonce))).mod(_far);
        random = random.add(_start);
        return random;
    }
    
}

