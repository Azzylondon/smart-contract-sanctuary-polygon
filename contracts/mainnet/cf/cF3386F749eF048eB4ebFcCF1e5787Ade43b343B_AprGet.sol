pragma solidity 0.6.12;
interface PairCont {
    function balanceOf(address owner) external view returns (uint);
    function token0() external view returns (address);
    function token1() external view returns (address);
    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
    function price0CumulativeLast() external view returns (uint);
    function price1CumulativeLast() external view returns (uint);
    function totalSupply() external view returns(uint);
}
interface Factory {
  function getPair(address token0,address token1) external view returns(address);
}
contract AprGet {
  
  using SafeMath for uint256;
  using SafeBEP20 for IBEP20;
  Factory factory=Factory(0xCf083Be4164828f00cAE704EC15a36D711491284);
  PairCont bananapair=PairCont(0xd7b3132b9aedc86e6cd28bA3b29Da6C30C049327);
  IChef master=IChef(0x54aff400858Dcac39797a81894D9920f16972D1D);

  address _usdc=0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
  address _banana=0x5d47bAbA0d66083C52009271faF3F50DCc01023C;
  uint _usdcdecimal=6;
  uint _bananadecimal=18;
  //token0=USDC,1=banana

  function tokenPrice(address _token) public view returns(uint256){
    if(_token==_usdc){
      return 1;
    }
    PairCont pair=PairCont(factory.getPair(_usdc, _token));
    uint usdc;//amount
    uint token;
    uint price;
    (usdc,token,)=pair.getReserves();
    if(pair.token1()==_usdc){
      (usdc,token)=(token,usdc);
    }
    uint decimal=IBEP20(_token).decimals();
    price=usdc.mul(10**(decimal-_usdcdecimal+18)).div(token);//18 decimal point under 0
    return price;
  }
  function getLPvalue(address _lp) public view returns(uint256){
    PairCont pair=PairCont(_lp);
    uint totalsupply=pair.totalSupply();
    IBEP20 token0=IBEP20(pair.token0());
    uint bal0=token0.balanceOf(_lp);
    uint price0=tokenPrice(pair.token0());
    return 2*price0.mul(bal0).div(totalsupply).div(10**(18-token0.decimals()));//18 decimal point under 0
  }
  function getTVL(address _lp) public view returns(uint256){
    return IBEP20(_lp).balanceOf(address(master)).mul(getLPvalue(_lp)).div(10**18);//18 decimal point under 0
  }
  function getAPR(uint _pid) public view returns (uint256){
    //per sec basis on apeswap.
    uint secondsinYear=31536000;
    uint alloc;
    (,,alloc)=master.poolInfo(_pid);
    address lp=master.lpToken(_pid);
    uint TVL=getTVL(lp);
    //returns % * 10000
    return 10000*100*secondsinYear.mul(master.bananaPerSecond()).mul(tokenPrice(_banana)).mul(alloc).div(master.totalAllocPoint()).div(TVL).div(10**(_bananadecimal));
  }
}