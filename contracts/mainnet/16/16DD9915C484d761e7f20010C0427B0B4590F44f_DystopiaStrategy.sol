    );

    event SwapPlaceRemoved(
        string swapPlaceType
    );


    // ---  structures

    struct SwapPlaceInfo {
        address pool;
        string swapPlaceType;
    }

    struct SwapParams {
        address tokenIn;
        address tokenOut;
        uint256 amountIn;
        uint256 amountOutMin;
        uint256 partsAmount; // if zero - then would be used pools amount for pair
    }

    struct SwapParamsExact {
        address tokenIn;
        address tokenOut;
        uint256 amountIn;
        uint256 amountOutMin;
