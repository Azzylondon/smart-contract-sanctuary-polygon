pragma solidity ^0.8.0;

/**
 * @title MeshSwap Strategy
 * @notice Investment strategy for investing assets via MeshSwap Strategy
 */
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { SafeMath } from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import { StableMath } from "../utils/StableMath.sol";
import { OvnMath } from "../utils/OvnMath.sol";

import { IERC20, InitializableAbstractStrategy } from "../utils/InitializableAbstractStrategy.sol";
import { DystopiaExchange } from "../exchanges/DystopiaExchange.sol";
import "../interfaces/IDystopiaLP.sol";
import "../connectors/IUserProxy.sol";
import "../connectors/IPenLens.sol";
import "../swapper/ISwapper.sol";
import "../utils/Helpers.sol";
import "hardhat/console.sol";
 

contract DystopiaStrategy is InitializableAbstractStrategy, DystopiaExchange   {
    using StableMath for uint256;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using OvnMath for uint256;


    IERC20 public token0;
    IERC20 public token1;
    
    
    IERC20 public dystToken; // Reward Token
    IERC20 public middleToken; // Will be used to collect rewards
    IDystopiaLP public  dystGauge;
    IDystopiaLP public dystPair;

    IERC20 public penroseToken;
    IUserProxy public penroseRouter;
    IPenLens public penroseLens;
    ISwapper internal swapper;


    uint256[] decimals;
    uint256[] minThresholds;

    /**
     * Initializer for setting up strategy internal state. This overrides the
     * InitializableAbstractStrategy initializer as Dystopia strategies don't fit
     * well within that abstraction.
     @param _vaultAddress Address of Vault
     @param _rewardTokenAddresses Addresses of Reward Tokens
     @param _assets Addresses of supported asset tokens
     @param _pToken Address of the LP token of the strategy
     @param _dystopiaContracts Addresses of Dystopia Router & Guage
     @param _penroseContracts Addresses of Penrose Router & Lens
     @param _swapper Addresses of XStabl Swapper
     */
    function initialize(
        address _vaultAddress,    // VaultProxy address
        address[] calldata _rewardTokenAddresses, // DystToken, Penrose, Middle address
        address[] calldata _assets, // Ex: MATIC + USDC
        address _pToken, // Dystopia Pair
        address[] calldata _dystopiaContracts,  // Dystopia Router & Guage
        address[] calldata _penroseContracts,   // Penrose  Router & Lens
        address _swapper // XStabl Swapper
    ) external onlyGovernor initializer {
        token0 = IERC20(_assets[0]);
        token1 = IERC20(_assets[1]);

        dystToken = IERC20(_rewardTokenAddresses[0]);
        penroseToken = IERC20(_rewardTokenAddresses[1]);
        middleToken = IERC20(_rewardTokenAddresses[2]);

        dystPair = IDystopiaLP(_pToken);
        _setDystopiaRouter(_dystopiaContracts[0]);
        dystGauge = IDystopiaLP(_dystopiaContracts[1]);

        penroseRouter = IUserProxy(_penroseContracts[0]);
        penroseLens = IPenLens(_penroseContracts[1]);

        swapper = ISwapper(_swapper);

        decimals.push(Helpers.getDecimals(_assets[0]));
        decimals.push(Helpers.getDecimals(_assets[1]));

        super._initialize(
            _vaultAddress,
            _rewardTokenAddresses,
            _assets
        );
        // Loop 6 times
        for (uint256 i = 0; i < 6; i++) {
            minThresholds.push(0);
        }

    }
    /**
    @dev Function to return the current reserve ratio in form 100:X
    @return (uint256,uint256) - ratios of reserves
     */
    function getRatios() public view override returns (uint256,uint256) {
        (uint256 reserve0, uint256  reserve1,) = dystPair.getReserves();
        return (10**20, reserve1.scaleBy(18,  decimals[1]).mul(10**20).div(reserve0.scaleBy(18, decimals[0])) );
    }
    /**
    @dev Function to return the actual reserves of the sPool
    @return (uint256,uint256) - actual reserves ratios of the sPool
     */
    function getReserves() internal view returns (uint256,uint256,uint256) {
        (uint256 reserve0, uint256  reserve1,) = dystPair.getReserves();
        uint256[2] memory decimals = [Helpers.getDecimals(assetsMapped[0]), Helpers.getDecimals(assetsMapped[1])];
        require(reserve0 > (10 ** (decimals[0] - 3)) && reserve1 > (10 ** (decimals[1] - 3)), "R LOW");
        return (reserve0, reserve1, dystPair.totalSupply()) ;

    }
    /**
    @dev Internal - function to deposit the assets to the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-USDC
    @param _amounts Corresponding amounts of each asset
    @param _useUnderlying There may be a case where not all the asset get staked. Make sure these stray assets would get staked too.
     */
    function _deposit(
        address[] memory _assets,
        uint256[] memory _amounts,
        bool _useUnderlying
    )  internal {
        require(_useUnderlying == false, "NS");
        uint256[2] memory amounts = [uint256(_amounts[0]), uint256(_amounts[1])];
        (uint256 reserve0, uint256 reserve1, ) = getReserves();
        // console.log("Reserves: %s, %s", reserve0, reserve1);

        if (amounts[0].scaleBy(18, decimals[0]) < amounts[1].scaleBy(18, decimals[1])) {
            // MATIC is less, USDC is high
            amounts[1] = quoteLiquidity(amounts[0], reserve0, reserve1);
            if (amounts[1] > _amounts[1]) {
                amounts[1] = _amounts[1];
                amounts[0] = quoteLiquidity(amounts[1], reserve1, reserve0);
            }
        } else if (amounts[0].scaleBy(18, decimals[0]) > amounts[1].scaleBy(18, decimals[1])) {
            // USDC is less, MATIC is high
            amounts[0] = quoteLiquidity(amounts[1], reserve1, reserve0);
            if (amounts[0] > _amounts[0]) {
                amounts[0] = _amounts[0];
                amounts[1] = quoteLiquidity(amounts[0], reserve0, reserve1);
            }
        } else {
            // Both are equal
            amounts[1] = quoteLiquidity(amounts[0], reserve0, reserve1);
        }
        // console.log("Amounts: %s, %s", amounts[0]/1e18, amounts[1]/1e6);


        _addLiquidity(
            address(token0),
            address(token1),
            dystPair.stable(),
            amounts[0],
            amounts[1],
            OvnMath.subBasisPoints(amounts[0], BASIS_POINTS_FOR_SLIPPAGE),
            OvnMath.subBasisPoints(amounts[1], BASIS_POINTS_FOR_SLIPPAGE),
            address(this)
        );
        // console.log("Liquidity Added Amounts: %s, %s", token0.balanceOf(address(this)), token1.balanceOf(address(this)));
        // console.log("DystPair Balance: %s", dystPair.balanceOf(address(this)));
        
        _stakeToPenrose();
    }
    function _stakeToPenrose() internal {
        // TODO: Put a threshold condition here
        if (dystPair.balanceOf(address(this)) > 0) {
            dystPair.approve(address(penroseRouter),  dystPair.balanceOf(address(this)));
            penroseRouter.depositLpAndStake(address(dystPair));
        }
       
    }
    function _unstakeFromPenrose() internal {
        uint256 lpTokenBalance = lpBalance();
        // TODO: Put a threshold condition here
        if (lpTokenBalance > 0) {
            penroseRouter.unstakeLpAndWithdraw(address(dystPair), lpTokenBalance);
        }
    }
    
    /**
    @dev Function to deposit the assets to the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-USDC
    @param _amounts Corresponding amounts of each asset
     */
    function deposit(
        address[] calldata _assets,
        uint256[] calldata _amounts
    )   external
        override
        onlyVault
        nonReentrant {
        require((_assets.length == 2) && (_assets.length == _amounts.length), "2 TKN S");
        require(supportsAsset(_assets), "TKN NS");
        _deposit(_assets, _amounts, false);
    }
    /**
    @dev Function to deposit the assets to the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-USDC
    @param _amounts Corresponding amounts of each asset
    @param _useUnderlying There may be a case where not all the asset get staked. Make sure these stray assets would get staked too.
     */
    function deposit(
        address[] calldata _assets,
        uint256[] calldata _amounts,
        bool _useUnderlying
    )   external
        onlyVault
        nonReentrant {
        require((_assets.length == 2) && (_assets.length == _amounts.length), "2 TKN S");
        require(supportsAsset(_assets), "TKN NS");
        _deposit(_assets, _amounts, _useUnderlying);
    }
    /**
    @dev Function to deposit all the assets to the sPools
     */
    function depositAll() external override onlyVault nonReentrant {
        uint256[] memory _amounts = new uint256[](2);
        _amounts[0] = token0.balanceOf(address(this));
        _amounts[1] = token1.balanceOf(address(this));
        _deposit(assetsMapped, _amounts, true);
    }
    /**
    @dev Function to deposit all the assets to the sPools
    @param _useUnderlying There may be a case where not all the asset get staked. Make sure these stray assets would get staked too.
     */
    function depositAll(bool _useUnderlying) external onlyVault nonReentrant {
        uint256[] memory _amounts = new uint256[](2);
        _amounts[0] = token0.balanceOf(address(this));
        _amounts[1] = token1.balanceOf(address(this));
        _deposit(assetsMapped, _amounts, _useUnderlying);
    }
    /**
    @dev Function to withdraw the assets from the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-USDC
    @param _amounts Corresponding amounts of each asset
     */
    function withdraw(
        address _beneficiary,
        address[] calldata _assets,
        uint256[] calldata _amounts
    ) external override onlyVaultOrGovernor nonReentrant  {
        _withdraw(_beneficiary, _assets, _amounts);
    }
    /**
    @dev Internal - Function to withdraw the assets from the sPools
    @param _assets Assets which are to be supported by the strategy. Ex: MATIC-USDC
    @param _amounts Corresponding amounts of each asset
     */
    function _withdraw(
        address _beneficiary,
        address[] memory _assets,
        uint256[] memory _amounts
    ) internal  {
        require((_assets.length == 2) && (_assets.length == _amounts.length), "2 TKN");
        require(supportsAsset(_assets), "TKN NS.");
        (uint256 reserve0, uint256 reserve1, ) = getReserves();
        _unstakeFromPenrose();
        // console.log("Token Amounts: %s, %s", token0.balanceOf(address(this)), token1.balanceOf(address(this)));
        uint256 lpTokenBalance = dystPair.balanceOf(address(this));
        // console.log("LP Token Balance: %s", lpTokenBalance);
        // console.log("Amounts: %s, %s", _amounts[0], _amounts[1]);
        uint256[2] memory _amountsWithdrawable = [ (_amounts[0] > token0.balanceOf(address(this))) ? _amounts[0].sub(token0.balanceOf(address(this))) : 0, (_amounts[1] > token1.balanceOf(address(this))) ? _amounts[1].sub(token1.balanceOf(address(this))) : 0];
        if (lpTokenBalance > 0 && (_amountsWithdrawable[0] > 0 || _amountsWithdrawable[1] > 0)) {
            // count amount to unstake
            uint256 totalLpBalance = dystPair.totalSupply();
            uint256 LPToWithdraw = lpTokenBalance;
            // E18
            uint256 maxOfAssets = (_amountsWithdrawable[0].scaleBy(18, decimals[0]) > _amountsWithdrawable[1].scaleBy(18, decimals[1])) ? _amountsWithdrawable[0].scaleBy(18,  decimals[0]) : _amountsWithdrawable[1].scaleBy(18,  decimals[1]);
            // Check where the reserves are titling
            if (reserve0.scaleBy(18, decimals[0]) < reserve1.scaleBy(18, decimals[1])) {
                // MATIC is less, USDC is high in reserves
                // console.log("MATIC is less, USDC is high in reserves");
                LPToWithdraw = totalLpBalance.mul(maxOfAssets.scaleBy(decimals[0], 18)).div(reserve0);
            } else if (reserve0.scaleBy(18, decimals[0]) > reserve1.scaleBy(18, decimals[1])) {
                // USDC is less, MATIC is high in reserves
                // console.log("USDC is less, MATIC is high in reserves");
                LPToWithdraw = totalLpBalance.mul(maxOfAssets.scaleBy(decimals[1], 18)).div(reserve1);
            } else {
                // Both are equal
                // console.log("Both are equal");
                LPToWithdraw = totalLpBalance.mul(_amounts[0]).div(reserve0);
            }


            if (LPToWithdraw > lpTokenBalance) {
                LPToWithdraw = lpTokenBalance;
            }
            // console.log("LP to withdraw: ", (LPToWithdraw == lpTokenBalance) ?  LPToWithdraw : OvnMath.addBasisPoints(LPToWithdraw, BASIS_POINTS_FOR_SLIPPAGE));
            // console.log("Token Amounts: %s, %s", token0.balanceOf(address(this)), token1.balanceOf(address(this)));
            // console.log("Min Amounts: %s, %s", _amountsWithdrawable[0], _amountsWithdrawable[1]);
            // console.log("Min Amounts: %s, %s", _amountsWithdrawable[0]/10**decimals[0], _amountsWithdrawable[1]/10**decimals[1]);
            if (LPToWithdraw == 0) {
                //  We need force withdraw all, in hope that it will satisfy the withdraw request
                // console.log("Force Withdraw");
                _withdrawAll();
                uint256[] memory _redepositAmounts = new uint256[](2);
                (_redepositAmounts[0], _redepositAmounts[1] ) = _checkBalance();
                // console.log("Re-deposit Amounts: %s, %s", _redepositAmounts[0], _redepositAmounts[1]);
                // console.log("Token Amounts: %s, %s", token0.balanceOf(address(this)), token1.balanceOf(address(this)));
                if (_amounts[0] > 0 && _redepositAmounts[0] > _amounts[0]) {
                    _redepositAmounts[0] -= _amounts[0];
                }
                if (_amounts[1] > 0 && _redepositAmounts[1] > _amounts[1]) {
                    _redepositAmounts[1] -= _amounts[1];
                }
                if (_redepositAmounts[0] > 0 || _redepositAmounts[1] > 0) {
                    _deposit(_assets, _redepositAmounts, false);
                }

            } else {
                // console.log("LP Token Balance: %s", lpTokenBalance);
                // console.log("LP To withdraw:",   LPToWithdraw);
                // console.log("Actual LP withdraw:", (LPToWithdraw == lpTokenBalance) ?  LPToWithdraw : OvnMath.addBasisPoints(LPToWithdraw, 100));
                // remove liquidity
                (uint256 token0Withdrawn, uint256 token1Withdrawn) = _removeLiquidity(
                    address(token0),
                    address(token1),
                    address(dystPair),
                    (LPToWithdraw == lpTokenBalance) ?  LPToWithdraw : OvnMath.addBasisPoints(LPToWithdraw, BASIS_POINTS_FOR_SLIPPAGE),
                    0,//_amountsWithdrawable[0],
                    0,//_amountsWithdrawable[1],
                    address(this)
                );
                // console.log("Token Withdrawn:", token0Withdrawn, token1Withdrawn);
                // console.log("Token Withdrawn:", token0Withdrawn/10**decimals[0], token1Withdrawn/10**decimals[1]);
            }

        }
        token0.safeTransfer(_beneficiary, _amounts[0]);
        token1.safeTransfer(_beneficiary, _amounts[1]);
    }
    /**
    @dev Internal - Function to withdraw all the assets from the sPools to the Vault
     */
    function _withdrawAll() internal {
        address[] memory _assets = new address[](2);
        _assets[0] = address(token0);
        _assets[1] = address(token1);

        uint256[] memory _amounts = new uint256[](2);
        (_amounts[0], _amounts[1] ) = _checkBalance();
        _withdraw(address(this), _assets, _amounts);

    }
    /**
    @dev Function to withdraw all the assets from the sPools to the Vault
     */
    function withdrawAll() external override onlyVaultOrGovernor nonReentrant  {
        _withdrawAll();
        token0.safeTransfer(vaultAddress, token0.balanceOf(address(this)));
        token1.safeTransfer(vaultAddress, token1.balanceOf(address(this)));
    }
    /**
    @dev Internal - Function to check the current balance of assets in this contract
                    and staked in the sPool.
    @return (uint256,uint256) - (Balance of token0, Balance of token1)
     */
    function _checkBalance()
        internal
        view
        returns (uint256, uint256)
    {
        uint256 token0Balance = token0.balanceOf(address(this));
        uint256 token1Balance = token1.balanceOf(address(this));
        uint256 lpTokenBalance = lpBalance();
        lpTokenBalance += dystGauge.balanceOf(address(this)); // Would always 0 as all LP is going to Penrose
        if (lpTokenBalance > 0) {
            uint256 totalLpBalance = dystPair.totalSupply();
            (uint256 reserve0, uint256 reserve1,) = dystPair.getReserves();
            token0Balance += reserve0 * lpTokenBalance / totalLpBalance;
            token1Balance += reserve1 * lpTokenBalance / totalLpBalance;
        }
        return (token0Balance,token1Balance);
    }
    /**
    @dev Function to check the current balance of assets in this contract
         and staked in the sPool.
    @return (uint256,uint256) - (Balance of token0, Balance of token1)
     */
    function checkBalance()
        external
        view
        override
        returns (uint256, uint256)
    {
        return _checkBalance();
    }

    /**
    @dev Internal - Function to collect rewards from the sPool and send it to Harvestor when invoked.
    */
    function _collectRewards() internal returns (uint256, uint256) {

        uint256 _swappedT0amount = 0;
        uint256 _swappedT1amount = 0;
        // _unstakeFromPenrose();
        penroseRouter.claimStakingRewards();
        uint256 _dystBalance = dystToken.balanceOf(address(this));
        // console.log("RewardCollection - DYST Balance: ", _dystBalance);
        uint256 _penBalance = penroseToken.balanceOf(address(this));
        // console.log("RewardCollection - PEN Balance: ", _penBalance);


        if (_dystBalance > minThresholds[4]) {
            dystToken.approve(address(swapper), _dystBalance);
            (uint256 _swapPart0, uint256 _swapPart1) = _rewardSwapDistribution(_dystBalance, address(dystToken));
            // console.log("RewardCollection - DYST - Swap Part: ", _swapPart0, _swapPart1);
            if (_swapPart0 > minThresholds[0]) {
                _swappedT0amount += swapper.swapCommon(address(dystToken), address(token0), _swapPart0);
            }
            if (_swapPart1 > minThresholds[0]) {
                _swappedT1amount += swapper.swapCommon(address(dystToken), address(token1), _swapPart1);
            }
            // Print Token balances
            // console.log("RewardCollection - DYST - Token Balance: ", token0.balanceOf(address(this))/10**decimals[0], token1.balanceOf(address(this))/10**decimals[1]);
        }
        if (_penBalance > minThresholds[5]) {
            penroseToken.approve(address(swapper), _penBalance);
            (uint256 _swapPart0, uint256 _swapPart1) = _rewardSwapDistribution(_penBalance, address(penroseToken));
            // console.log("RewardCollection - PEN - Swap Part: ", _swapPart0, _swapPart1);
            if (_swapPart0 > minThresholds[0]) {
                _swappedT0amount += swapper.swapCommon(address(penroseToken), address(token0), _swapPart0);
            }
            if (_swapPart1 > minThresholds[0]) {
                _swappedT1amount += swapper.swapCommon(address(penroseToken), address(token1), _swapPart1);
            }
            // Print Token balances
            // console.log("RewardCollection - PEN - Token Balance: ", token0.balanceOf(address(this))/10**decimals[0], token1.balanceOf(address(this))/10**decimals[1]);
        }

        if (token0.balanceOf(address(this)) > minThresholds[1] && token1.balanceOf(address(this)) > minThresholds[2]) {
            _deposit();
        } else {
            // console.log("Not enough tokens to deposit");
        }
        return (_swappedT0amount, _swappedT1amount);
    }
    /**
    @dev Function to collect rewards from the sPool and send it to Harvestor when invoked.
    */
    function collectRewardTokens()
        external
        override
        onlyVault
        nonReentrant
        returns (uint256, uint256)
    {
        return _collectRewards();
    }
    /**
    @dev Internal function to the calculate the ratio in which the reward needs to be swapped
    @return (uint256,uint256) - (Part for token0, Part for  token1)
     */
    function _rewardSwapDistribution(uint256 _reward, address __rewardToken) internal view returns (uint256, uint256) {
        address _usdToken = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
        uint256 rewardValueInUSD = swapper.getAmountOut(__rewardToken, _usdToken, 10**Helpers.getDecimals(__rewardToken));
        // console.log("RewardCollection - Reward Value in USD: ", rewardValueInUSD);

        uint256 _leftover0 = token0.balanceOf(address(this)).scaleBy(18, decimals[0]);
        uint256 _leftover1 = token1.balanceOf(address(this)).scaleBy(18, decimals[1]);
        // console.log("RewardCollection - Leftover: ", _leftover0, _leftover1);

        uint256 _LDH = uint256(Helpers.abs(int256(_leftover0) - int256(_leftover1)))/2;
        // console.log("RewardCollection - LDH: ", _LDH);

        uint256 _part0 = 0;
        uint256 _part1 = 0;
        if (_LDH < (_reward/2)) {
            _part0 = _reward/2 - _LDH;
            _part1 = _reward/2 + _LDH;
            // console.log("LDH < Reward - LDH: %s - Reward: %s", _LDH, _reward);
            // console.log("RewardCollection - Part: ", _part0, _part1);
            return (_part0, _part1);
        }

        if (_leftover0 < _leftover1) {
            _part0 = _reward;
        } else {
            _part1 = _reward;
        }
        // console.log("Parts", _part0, _part1);
        return (_part0, _part1);
    }
    /**
    @dev Function to set the thresholds for various conditions used in the Strategy
    @param _minThresholds array containing the thresholds vaules
    */
    function setThresholds(uint256[] calldata _minThresholds) external onlyVaultOrGovernor nonReentrant {
        require(_minThresholds.length == 6, "6NEED");
        // 0: Stake from Penrose
        // 1: Unstake from Penrose
        // 2: Withdraw LP from Dystopia
        // 3: Check Balance from Dystopia
        // 4: Rewards - Dyst > 0
        // 5: Rewards - PEN > 0
        minThresholds = _minThresholds;
    }
    /**
    @dev Function to check whether an asset it is supported or not by the Strategy
    @param _assets Assets to check whether they are supported or not.
    @return bool whether the asset is supported or not
    */
    function supportsAsset(address[] memory _assets)
        internal
        view
        returns (bool)
    {
        require(_assets.length == 2, "NS");
        return _assets[0] == address(token0) && _assets[1] == address(token1);
    }
    /**
    @dev Function to get the equivalent LP tokens for the given assets
    @return uint256 - LP tokens
     */
    function lpBalance() public view  returns (uint256) {
        return IERC20(penroseLens.stakingRewardsByDystPool(address(dystPair))).balanceOf(penroseLens.userProxyByAccount(address(this)));
    }

    /**
    @dev Return the health factor of the Strategy
    @return uint256 Average health factor of the Strategy
    */
    function health() external view returns (uint256, uint256) {
        (uint256 balance0, uint256 balance1) = _checkBalance();
        if (balance0 == 0 && balance1 == 0) {
            return (0, 0);
        }
        return (100 - ((token0.balanceOf(address(this)) * 100) / balance0), 100 - ((token1.balanceOf(address(this)) * 100) / balance1));
    }
    /**
    * @dev Deposit whatever present in the contract to the sPool
    */
    function _deposit() internal {
        uint256[] memory _amounts = new uint256[](2);
        _amounts[0] = token0.balanceOf(address(this));
        _amounts[1] = token1.balanceOf(address(this));
        uint256 lpTokenBalance = dystPair.balanceOf(address(this));
        // console.log("Dystopia Strategy - Before LP Token Balance: %s", lpBalance());
        _deposit(assetsMapped, _amounts, false);
        lpTokenBalance = dystPair.balanceOf(address(this));
        // console.log("Dystopia Strategy - After LP Token Balance: %s", lpBalance());
    }
}

