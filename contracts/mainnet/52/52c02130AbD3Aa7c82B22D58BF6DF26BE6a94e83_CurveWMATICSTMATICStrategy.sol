pragma solidity ^0.8.0;

import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { SafeMath } from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import { Helpers } from "../../utils/Helpers.sol";
import { OvnMath } from "../../utils/OvnMath.sol";

import "../../swapper/ISwapper.sol";
import "../../interfaces/ICurvePool.sol";
import "../../interfaces/ICurveGauge.sol";

import { IERC20, InitializableAbstractStrategy } from "../../utils/InitializableAbstractStrategy.sol";
import { IVault } from "../../interfaces/balancer/interfaces/IVault.sol";
import { IAsset } from "../../interfaces/balancer/interfaces/IAsset.sol";
import { StableMath } from "../../utils/StableMath.sol";

import "hardhat/console.sol";

contract CurveWMATICSTMATICStrategy is InitializableAbstractStrategy {
    using StableMath for uint256;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using OvnMath for uint256;

    IERC20 public token0;
    IERC20 public token1;
    IERC20 public crvToken;
    address public pTokenAddress;
    ISwapper internal swapper;
    ICurveGauge crvGauge;
    ICurvePool curvePool;

    uint256[] decimals;
    uint256[] minThresholds;
    uint256 internal constant maxSlippage = 1e16; // 1%, same as the Curve UI

    function initialize(
        address _vaultAddress, // VaultProxy address
        address[] calldata _rewardTokenAddresses, // Leave this for low
        address[] calldata _assets, // : STMATIC - wMATIC
        address _pToken, // It is the token we get from the sPool inexchange of liquidity provided. Which is in our case: 0xe7CEA2F6d7b120174BF3A9Bc98efaF1fF72C997d - Check this txn id: 0x1c5453690cee84fdf57d65294618d4c330e39164add2d8cf3cebdc6eaa941cdb on polyscan
        // Here goes the Startegy specific parameters
        address _curveRouter, // Router contract for this case: 0xFb6FE7802bA9290ef8b00CA16Af4Bc26eb663a28 - Check this txn id: 0x1c5453690cee84fdf57d65294618d4c330e39164add2d8cf3cebdc6eaa941cdb on polyscan
        address _curveGauge, // Guage contract are the contract which we put our pToken (LP token) in to get rewards. Which in this case is 0x9633E0749faa6eC6d992265368B88698d6a93Ac0
        address _lidoToken, // reward token
        address _swapper
    ) external onlyGovernor initializer {
        token0 = IERC20(_assets[0]); // Address of stMATIC
        token1 = IERC20(_assets[1]); // Address of wMATIC
        pTokenAddress = _pToken;

        // Set the above params to contracts properties here
        swapper = ISwapper(_swapper);
        curvePool = ICurvePool(_curveRouter);
        crvGauge = ICurveGauge(_curveGauge);
        crvToken = IERC20(_lidoToken);

        decimals.push(Helpers.getDecimals(_assets[0]));
        decimals.push(Helpers.getDecimals(_assets[1]));

        super._initialize(_vaultAddress, _rewardTokenAddresses, _assets);

        // Loop 6 times
        for (uint256 i = 0; i < 6; i++) {
            minThresholds.push(0);
        }
    }

    function _deposit(
        address[] memory _assets,
        uint256[] memory _amounts,
        bool _useUnderlying
    ) internal {
        require(
            (_assets.length == 2) && (_assets.length == _amounts.length),
            "2 TKN S"
        );
        require(supportsAsset(_assets), "TKN NS");
        // in wMATIC-stMATIC
        uint256[2] memory amounts = [uint256(_amounts[0]), uint256(_amounts[1])];

        // console.log("\ncheck for balance %s %s", amounts[0], amounts[1]);
        // console.log("token0: ", token0.balanceOf(address(this)));
        // console.log("token1: ", token1.balanceOf(address(this)));

        (uint256 reserve1, uint256 reserve0) = getReserves(); // swapping as it returns wMATIC-stMATIC
        console.log("Curve - Reserves: %s, %s", reserve0, reserve1);
        // console.log("Curve - Amounts: %s, %s", _amounts[0], _amounts[1]);

        if (_amounts[0].scaleBy(18, decimals[0]) < _amounts[1].scaleBy(18, decimals[1])) {
            // wMATIC is less, stMATIC is high
            amounts[1] = curvePool.calc_token_amount([uint256(0), _amounts[0]]); // in order stMATIC-wMATIC
            if (amounts[1] > _amounts[1]) {
                amounts[1] = _amounts[1];
                // in order stMATIC-wMATIC
                amounts[0] = curvePool.calc_token_amount([_amounts[1], uint256(0)]);
            }
        } else if (_amounts[0].scaleBy(18, decimals[0]) > _amounts[1].scaleBy(18, decimals[1])) {
            // stMATIC is less, wMATIC is high
            amounts[0] = curvePool.calc_token_amount([_amounts[1], uint256(0)]);
            if (amounts[0] > _amounts[0]) {
                amounts[0] = _amounts[0];
                amounts[1] = curvePool.calc_token_amount([uint256(0), _amounts[0]]); // in order stMATIC-wMATIC
            }
        } else {
            // Both are equal
            amounts[1] = curvePool.calc_token_amount([_amounts[1], uint256(0)]); // in order stMATIC-wMATIC
        }

        // swapped as sPool is stMATIC-wMATIC
        amounts = [
            uint256(_amounts[1]),
            uint256(_amounts[0])
        ];

        uint256 minMintAmount = curvePool.calc_token_amount(amounts);
        // set approval
        IERC20(token0).approve(address(curvePool), amounts[0]); // stMATIC
        IERC20(token1).approve(address(curvePool), amounts[1]); // wMATIC

        // add liquidity
        curvePool.add_liquidity(amounts, minMintAmount, _useUnderlying);

        // deposit all
        _lpDepositAll();
    }

    /**
     * @dev Deposit an amount of asset into the platform
     * @param _assets               Addresses for the asset
     * @param _amounts              Units of asset to deposit
     */
    function deposit(address[] calldata _assets, uint256[] calldata _amounts)
        external
        override
        onlyVault
        nonReentrant
    {
        // Use this link and replicate it here
        // Approve the usage of assets: https://polygonscan.com/tx/0xcc78e14e7317dc7d25ec2ebfe644409e36c56183de5efa6fc0e052d0d32327d9
        // Invoke Add Liquidty: https://polygonscan.com/tx/0x7246df2fc69f82472ce4ced2cad45b31210ff1d297563cb5746410ca8885d7b1
        _deposit(_assets, _amounts, false);
    }

    /**
        @dev Function to check whether an asset it is supported or not by the Strategy
        @param _assets Assets to check whether they are supported or not.
        @return bool whether the asset is supported or not
    */
    function supportsAsset(address[] memory _assets)
        internal
        view
        returns (bool)
    {
        require(_assets.length == 2, "NS");
        // swapped tokens to (vault tokens wMATIC-stMATIC)
        return _assets[0] == address(token1) && _assets[1] == address(token0);
    }

    /**
     * @dev Collect accumulated reward token and send to Vault.
     */
    function collectRewardTokens()
        external
        override
        onlyVaultOrGovernor
        nonReentrant
        returns (uint256, uint256)
    {
        return _collectRewards();
    }

    function _collectRewards() internal returns (uint256, uint256) {
        uint256 swappedT0amount = 0;
        uint256 swappedT1amount = 0;

        crvGauge.claim_rewards(address(this));
        uint256 crvBalance = crvToken.balanceOf(address(this));
        // console.log("after claimRewards - CRV Balance: %s", crvBalance);

        console.log("before swapping token balances: token0: %s token1: %s", token0.balanceOf(address(this)), token1.balanceOf(address(this)));

        if (crvBalance > minThresholds[0]) {
            crvToken.approve(address(swapper), crvBalance);
            (uint256 _swapPart0, uint256 _swapPart1) = _rewardSwapDistribution(
                crvBalance
            );
            // console.log("RewardCollection - Swap Part: ", _swapPart0, _swapPart1);
            // console.log("RewardCollection - Before Token Balance: ", token0.balanceOf(address(this)), token1.balanceOf(address(this)));

            // console.log("stMATIC balance before", token1.balanceOf(address(this)));
            if (_swapPart0 > minThresholds[0]) {
                swapper.swapCommon(
                    address(crvToken),
                    address(token1),
                    _swapPart0
                ); // swap LDO -> wMATIC
                uint256 halfMatic = token1.balanceOf(address(this)).div(2); // half token1 from balance, as swapped matic can be low
                token1.approve(address(swapper), halfMatic); // approve half token1
                swappedT0amount += swapper.swapCommon(
                    address(token1),
                    address(token0),
                    halfMatic
                ); // half wMATIC -> stMATIC
            }
            // console.log("after stMATIC balance before", token1.balanceOf(address(this)));

            if (_swapPart1 > minThresholds[0]) {
                // swap LDO -> token1
                swappedT1amount += swapper.swapCommon(
                    address(crvToken),
                    address(token1),
                    _swapPart1
                );
            }

            console.log(
                "\nCRV balance after everything",
                crvToken.balanceOf(address(this))
            );
            console.log(
                "RewardCollection after - CURVE - Token Balance: stMATIC: %s wMATIC: %s",
                token0.balanceOf(address(this)),
                token1.balanceOf(address(this))
            );
        }

        // Print Token balances
        console.log(
            "RewardCollection - After Token Balance: ",
            token0.balanceOf(address(this)),
            token1.balanceOf(address(this))
        );
        if (
            token0.balanceOf(address(this)) > minThresholds[1] &&
            token1.balanceOf(address(this)) > minThresholds[2]
        ) {
            _deposit();
        } else {
            console.log("Not enough tokens to deposit");
        }
        console.log("after deposit fn, token balances: token0: %s token1: %s", token0.balanceOf(address(this)), token1.balanceOf(address(this)));
        return (swappedT0amount, swappedT1amount);
    }

    /**
    @dev Internal function to the calculate the ratio in which the reward needs to be swapped
    @return (uint256,uint256) - (Part for token0, Part for token1)
     */
    function _rewardSwapDistribution(uint256 _reward)
        internal
        view
        returns (uint256, uint256)
    {
        // address _usdToken = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
        // uint256 rewardValueInUSD = swapper.getAmountOut(address(crvToken), _balancerToken, 10**Helpers.getDecimals(address(crvToken)));
        // console.log("RewardCollection - Reward Value in USD: ", rewardValueInUSD);

        uint256 _leftover0 = token0.balanceOf(address(this)).scaleBy(
            18,
            decimals[0]
        ); // token0
        uint256 _leftover1 = token1.balanceOf(address(this)).scaleBy(
            18,
            decimals[1]
        ); // token1
        console.log("RewardCollection - Leftover: ", _leftover0, _leftover1);

        uint256 _LDH = uint256(
            Helpers.abs(int256(_leftover0) - int256(_leftover1))
        ) / 2;
        console.log("RewardCollection - LDH: ", _LDH);

        uint256 _part0 = 0;
        uint256 _part1 = 0;
        if (_LDH < _reward / 2) {
            _part0 = _reward / 2 - _LDH;
            _part1 = _reward / 2 + _LDH;
            console.log("LDH < Reward - LDH: %s - Reward: %s", _LDH, _reward);
            console.log("RewardCollection - Part: ", _part0, _part1);
            return (_part0, _part1);
        }

        if (_leftover0 < _leftover1) {
            _part0 = _reward;
        } else {
            _part1 = _reward;
        }
        console.log("Parts", _part0, _part1);
        return (_part0, _part1);
    }

    /**
     * @dev Deposit balance of all supported assets into the platform
     */
    function depositAll() public override onlyVault nonReentrant {
        uint256[] memory _amounts = new uint256[](2);
        // swapping to vault tokens
        _amounts[0] = token1.balanceOf(address(this));
        _amounts[1] = token0.balanceOf(address(this));

        address[] memory _address = new address[](2);
        _address[0] = address(token1);
        _address[1] = address(token0);

        _deposit(_address, _amounts, false);
    }

    function _withdraw(
        address _beneficiary,
        address[] memory _assets,
        uint256[] memory _amounts
    ) internal {
        // _assets in as wMATIC-stMATIC, _amounts in as wMATIC-stMATIC
        require((_assets.length == 2) && (_amounts.length == 2), "2 TKN S");
        require(supportsAsset(_assets), "TKN NS");
        (uint256 reserve0, uint256 reserve1) = getReserves(); // re-swapped (stMATIC-wMATIC)

        // withdraw from gauge
        (, uint256 gaugePTokens, ) = _getTotalPTokens();
        // console.log("gauge balance before", crvGauge.balanceOf(address(this)));
        _lpWithdraw(gaugePTokens);

        uint256 lpTokenBalance = IERC20(pTokenAddress).balanceOf(address(this));
        // console.log("pToken balance after withdraw liquidity:", lpTokenBalance);

        uint256[2] memory amounts;
        amounts[0] = _amounts[1]; // stMATIC
        amounts[1] = _amounts[0]; // wMATIC

        // swap amounts
        uint256[2] memory _amountsWithdrawable = [
            (amounts[0] > token0.balanceOf(address(this)))
                ? amounts[0].sub(token0.balanceOf(address(this)))
                : 0,
            (amounts[1] > token1.balanceOf(address(this)))
                ? amounts[1].sub(token1.balanceOf(address(this)))
                : 0
        ];

        // console.log("amounts after swapping %s %s", amounts[0], amounts[1]);

        if (
            lpTokenBalance > 0 &&
            (_amountsWithdrawable[0] > 0 || _amountsWithdrawable[1] > 0)
        ) {
            // count amount to unstake
            uint256 totalLpBalance = IERC20(pTokenAddress).totalSupply();
            uint256 LPToWithdraw = lpTokenBalance;
            // E18
            uint256 maxOfAssets = (_amountsWithdrawable[0].scaleBy(
                18,
                decimals[0]
            ) > _amountsWithdrawable[1].scaleBy(18, decimals[1]))
                ? _amountsWithdrawable[0].scaleBy(18, decimals[0])
                : _amountsWithdrawable[1].scaleBy(18, decimals[1]);

            // Check where the reserves are titling
            if (
                reserve0.scaleBy(18, decimals[0]) <
                reserve1.scaleBy(18, decimals[1])
            ) {
                // MATIC is less, stMATIC is high in reserves
                // console.log("MATIC is less, stMATIC is high in reserves");
                LPToWithdraw = totalLpBalance
                    .mul(maxOfAssets.scaleBy(decimals[0], 18))
                    .div(reserve0);
            } else if (
                reserve0.scaleBy(18, decimals[0]) >
                reserve1.scaleBy(18, decimals[1])
            ) {
                // stMATIC is less, MATIC is high in reserves
                console.log("stMATIC is less, MATIC is high in reserves");
                LPToWithdraw = totalLpBalance
                    .mul(maxOfAssets.scaleBy(decimals[1], 18))
                    .div(reserve1);
            } else {
                // Both are equal
                console.log("Both are equal");
                LPToWithdraw = totalLpBalance.mul(amounts[0]).div(reserve0);
            }

            if (LPToWithdraw > lpTokenBalance) {
                LPToWithdraw = lpTokenBalance;
            }

            // console.log("Token Amounts: %s, %s",token1.balanceOf(address(this)),token0.balanceOf(address(this)));
            // console.log("Min Amounts: %s, %s",_amountsWithdrawable[0],_amountsWithdrawable[1]);
            // console.log("Min Amounts: %s, %s",_amountsWithdrawable[0] / 10**decimals[0],_amountsWithdrawable[1] / 10**decimals[1]);
            if (LPToWithdraw == 0) {
                //  We need force withdraw all, in hope that it will satisfy the withdraw request
                // console.log("Force Withdraw");
                _withdrawAll();
                uint256[] memory _redepositAmounts = new uint256[](2);
                (_redepositAmounts[0], _redepositAmounts[1]) = checkBalance(); // wMATIC-stMATIC
                (_redepositAmounts[0], _redepositAmounts[1]) = checkBalance();
                // console.log("Re-deposit Amounts: %s, %s",_redepositAmounts[0],_redepositAmounts[1]);
                // console.log("Token Amounts: %s, %s",token1.balanceOf(address(this)),token0.balanceOf(address(this)));
                if (_amounts[0] > 0 && _redepositAmounts[0] > _amounts[0]) {
                    // wMATIC > 0 && _redepositWMATIC > wMATIC
                    _redepositAmounts[0] -= _amounts[0];
                }
                if (_amounts[1] > 0 && _redepositAmounts[1] > _amounts[1]) {
                    // stMATIC > 0 && _redepositSTMATIC > stMATIC
                    _redepositAmounts[1] -= _amounts[1];
                }
                if (_redepositAmounts[0] > 0 || _redepositAmounts[1] > 0) {
                    // WMATIC > 0 || STMATIC > 0; assets: WMATIC-STMATIC; _redepositAmounts: WMATIC-STMATIC
                    _deposit(_assets, _redepositAmounts, false);
                }
            } else {
                uint256 lpWithdrawAble = OvnMath.addBasisPoints(LPToWithdraw,100);
                if (lpWithdrawAble > lpTokenBalance) {
                    lpWithdrawAble = lpTokenBalance;
                }

                // console.log("LP Token Balance: %s", lpTokenBalance);
                // console.log("LP To withdraw:", lpWithdrawAble);
                // console.log("liquidity greater than balance %s",lpWithdrawAble > lpTokenBalance);
                // console.log("lp withdraw", lpWithdrawAble);

                // remove liquidity
                curvePool.remove_liquidity(
                    lpWithdrawAble,
                    [uint256(0), uint256(0)],
                    false,
                    address(this)
                );
                // console.log("removed liquidity");
                // console.log("lpbalance", lpBalance()); // 0
                // console.log("p_token", IERC20(pTokenAddress).balanceOf(address(this))); // 0
                // console.log("token0 %s %s", token0.balanceOf(address(this)));
                // console.log((_amountsWithdrawable[0] >token0.balanceOf(address(this))? token0.balanceOf(address(this)): _amountsWithdrawable[0]));
                // console.log("token1 %s %s",token1.balanceOf(address(this)),_amounts[1]);
                // console.log((_amountsWithdrawable[1] >token1.balanceOf(address(this))? token1.balanceOf(address(this)): _amountsWithdrawable[1]));
            }
        }

        // _amountsWithdrawable -> STMATIC-wMATIC
        token0.safeTransfer(_beneficiary,(_amountsWithdrawable[0] > token0.balanceOf(address(this))? token0.balanceOf(address(this)): _amountsWithdrawable[0]));
        token1.safeTransfer(_beneficiary,(_amountsWithdrawable[1] > token1.balanceOf(address(this))? token1.balanceOf(address(this)): _amountsWithdrawable[1]));

        // console.log("stMATIC", token0.balanceOf(address(this)));
        // console.log("wMATIC", token1.balanceOf(address(this)));
        _lpDepositAll();
    }

    /**
     * @dev Withdraw an amount of asset from the platform.
     * @param _recipient         Address to which the asset should be sent
     * @param _assets             Address of the assets
     * @param _amounts            Units of assets to withdraw
     */
    function withdraw(
        address _recipient,
        address[] calldata _assets,
        uint256[] calldata _amounts
    ) external override onlyVaultOrGovernor nonReentrant {
        _withdraw(_recipient, _assets, _amounts);
    }

    /**
    @dev Internal - Function to withdraw all the assets from the sPools to the Vault
     */
    function _withdrawAll() internal {
        address[] memory _assets = new address[](2);
        _assets[0] = address(token1);
        _assets[1] = address(token0);

        uint256[] memory _amounts = new uint256[](2);
        (_amounts[0], _amounts[1]) = checkBalance(); // wMATIC stMATIC
        _withdraw(address(this), _assets, _amounts);
    }

    /**
     * @dev Withdraw all assets from strategy sending assets to Vault.
     */
    function withdrawAll() external override onlyVaultOrGovernor nonReentrant {
        _withdrawAll();
        // transfer to vault
        token1.safeTransfer(vaultAddress, token1.balanceOf(address(this)));
        token0.safeTransfer(vaultAddress, token0.balanceOf(address(this)));
    }

    /**
    @dev Internal - Function to check the current balance of assets in this contract
                    and staked in the sPool.
    @return (uint256,uint256) - (Balance of wMATIC, Balance of stMATIC)
     */
    function checkBalance() public view override returns (uint256, uint256) {
        uint256 token1Balance = token0.balanceOf(address(this)); // stMATIC
        uint256 token0Balance = token1.balanceOf(address(this)); // wMATIC
        (, , uint256 totalPTokens) = _getTotalPTokens(); // contract + gauge

        if (totalPTokens > 0) {
            uint256 totalSupply = IERC20(pTokenAddress).totalSupply();
            // get reserves
            (uint256 reserve0, uint256 reserve1) = getReserves();

            token0Balance += (reserve0 * totalPTokens) / totalSupply;
            token1Balance += (reserve1 * totalPTokens) / totalSupply;
        }

        // swapped to wMATIC-stMATIC
        return (token1Balance, token0Balance);
    }

    function _lpWithdraw(uint256 numPTokens) internal {
        crvGauge.withdraw(numPTokens, address(this), true);
    }

    function _lpDepositAll() internal {
        uint256 crvBalance = IERC20(pTokenAddress).balanceOf(address(this));

        if (crvBalance > 0) {
            IERC20(pTokenAddress).safeApprove(address(crvGauge), crvBalance);
            crvGauge.deposit(crvBalance, address(this), false);
        }
    }

    function _getTotalPTokens()
        internal
        view
        returns (
            uint256 contractPTokens,
            uint256 gaugePTokens, // gauge is a misnomer here, need a better name
            uint256 totalPTokens
        )
    {
        contractPTokens = IERC20(pTokenAddress).balanceOf(address(this));
        gaugePTokens = crvGauge.balanceOf(address(this));
        totalPTokens = contractPTokens + gaugePTokens;
    }

    function lpBalance() public view returns (uint256) {
        return crvGauge.balanceOf(address(this));
    }

    function getRatios() external view override returns (uint256, uint256) {
        // swapped tokens; 1: wMATIC, 2: stMATIC
        (uint256 reserve0, uint256 reserve1) = getReserves();

        return (
            10**20,
            reserve1.scaleBy(18, decimals[1]).mul(10**20).div(
                reserve0.scaleBy(18, decimals[0])
            )
        );
    }

    function getReserves() internal view returns (uint256, uint256) {
        // swapped -> wMATIC / stMATIC
        uint256 reserve0 = curvePool.balances(1); // wMATIC
        uint256 reserve1 = curvePool.balances(0); // stMATIC
        require(
            reserve0 > (10**(decimals[0] - 3)) &&
                reserve1 > (10**(decimals[1] - 3)),
            "R LOW"
        );
        return (reserve0, reserve1); // wMATIC-stMATIC
    }

    /**
    @dev Return the health factor of the Strategy
    @return uint256 Average health factor of the Strategy
    */
    function health() external view returns (uint256, uint256) {
        (uint256 balance0, uint256 balance1) = checkBalance(); // wMATIC-stMATIC
        if (balance0 == 0 && balance1 == 0) {
            return (0, 0);
        }

        return (
            100 - ((token1.balanceOf(address(this)) * 100) / balance0),
            100 - ((token0.balanceOf(address(this)) * 100) / balance1)
        );
    }

    /**
     * @dev Deposit whatever present in the contract to the xPool
     */
    function _deposit() internal {
        uint256[] memory _amounts = new uint256[](2);
        // swapped bcz token swapping is done in deposit fn too.
        _amounts[0] = token1.balanceOf(address(this));
        _amounts[1] = token0.balanceOf(address(this));

        // wMATIC-stMATIC
        address[] memory _assets = new address[](2);
        _assets[0] = assetsMapped[1];
        _assets[1] = assetsMapped[0];

        // if balance -> deposit
        if (_amounts[0] > 0 || _amounts[1] > 0) {
            _deposit(_assets, _amounts, false);
        }
    }
}

