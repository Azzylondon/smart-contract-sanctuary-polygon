contract VERTU is ERC20, Ownable{
    using Address for address payable;
    IRouter public router;
    address public pair;
    bool private swapping;
    bool public swapEnabled;
    bool public vLP;
    uint256 private swapAmount = vertuSwapLP * 10**18;       
    uint256 public vertuSwapLP = 10;
    uint256 private antiDump = vertuAntiDump * 10**18;       
    uint256 public vertuAntiDump = 100;
    address public marketingFunds;
    address public capitalFunds;
    address public vDevelopment;
    
    struct vTx {
        uint256 marketing;
        uint256 capital;
        uint256 liquidity; 
    }
    vTx public buyTx = vTx(1,1,1);
    vTx public sellTx = vTx(1,1,1);
    uint256 totalBuyTx = 3;
    uint256 totalSellTx = 3;
    mapping (address => bool) public excludedFromFees;
    // renounceOwnership prevent to stop the swap funtion, admin can only update buy/sell /wallet, if admin route to dead address the contract uncontrollable.
    modifier onlyAdmin() {
        require(vDevelopment == _msgSender(), "Caller is not the Admin");
        _;
    }
    // continued above, nomore owner, nomore admin, only last funtion to recovery stucked balances and other stucked tokens
    modifier onlyRecall() {
        require(marketingFunds == _msgSender(), "Caller is not the Admin");
        _;
    }
    modifier inSwap() {
        if (!swapping) {
            swapping = true;
            _;
            swapping = false;
        }
    }
    constructor() ERC20("VERTU", "VERTU") {
        _mint(msg.sender, 8e8 * 10 ** decimals());
        excludedFromFees[msg.sender] = true;
        IRouter _router = IRouter(0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff);
        address _pair = IFactory(_router.factory())
       .createPair(address(this), _router.WETH());
        router = _router;
        pair = _pair;
        excludedFromFees[address(this)] = true;
        excludedFromFees[marketingFunds] = true;
        excludedFromFees[vDevelopment] = true;
        excludedFromFees[capitalFunds] = true;
    }
 
    function decimals() public pure override returns(uint8){
        return 9;
    }
 
    function _transfer(address sender, address recipient, uint256 amount) internal override {
        require(amount > 0, "Transfer amount must be greater than zero");
        require(vLP || excludedFromFees[sender] || excludedFromFees[recipient], "Trading disabled");
        uint256 fee;
        if (swapping || excludedFromFees[sender] || excludedFromFees[recipient]) fee = 0;
        else if(recipient == pair) fee = amount * totalSellTx / 100;
        else if(sender == pair){
            require(amount <= antiDump, "You are exceeding antiDump protection");
            fee = amount * totalBuyTx / 100;
        }
        if (swapEnabled && !swapping && sender != pair && fee > 0) swapTx(); 
        super._transfer(sender, recipient, amount - fee);
        if(fee > 0) super._transfer(sender, address(this) ,fee);
 
    }
 
    function swapTx() private inSwap {
        uint256 contractBalance = balanceOf(address(this));
        if (contractBalance >= swapAmount) {
            uint256 denominator = totalSellTx * 2;
            uint256 vertuToAddLiquidity = contractBalance * sellTx.liquidity / denominator;
            uint256 toSwap = contractBalance - vertuToAddLiquidity;
            uint256 initialBalance = address(this).balance;
            swapVertuForFund(toSwap);
            uint256 vBalance = address(this).balance - initialBalance;
            uint256 unitBalance= vBalance / (denominator - sellTx.liquidity);
            uint256 fundToAddLiquidity = unitBalance * sellTx.liquidity;
            if(fundToAddLiquidity > 0){
                addLiquidity(vertuToAddLiquidity, fundToAddLiquidity);
            }
            uint256 marketingAmount = unitBalance * 2 * sellTx.marketing;
            if(marketingAmount > 0){
                payable(marketingFunds).sendValue(marketingAmount);
            }
            uint256 capitalAmount= unitBalance * 2 * sellTx.capital;
            if(capitalAmount> 0){
                payable(capitalFunds).sendValue(capitalAmount); 
            }
 
        }
    }
 
    function swapVertuForFund(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();
        _approve(address(this), address(router), tokenAmount);
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(tokenAmount, 0, path, address(this), block.timestamp);
    }
    function addLiquidity(uint256 tokenAmount, uint256 fundAmount) private {
        _approve(address(this), address(router), tokenAmount);
        router.addLiquidityETH{value: fundAmount}(
            address(this),
            tokenAmount,
            0, 
            0, 
            capitalFunds,
            block.timestamp
        );
    }
 
    function setSwap(bool state) external onlyOwner {
        swapEnabled = state;
    }
 
    function setVertuLPAmount(uint256 new_amount) external onlyAdmin() {
       vertuSwapLP = new_amount;
       swapAmount = vertuSwapLP * 10**18;
   }
 
    function setAntiDump(uint256 new_amount) external onlyAdmin() {
       vertuAntiDump = new_amount;
       antiDump = vertuAntiDump * 10**18;
   }
 
    function setBuyTx(uint256 _marketing, uint256 _capital, uint256 _liquidity) external onlyAdmin{
        buyTx = vTx(_marketing, _capital, _liquidity);
        totalBuyTx = _marketing + _capital+ _liquidity;
    }
 
    function setSellTx(uint256 _marketing, uint256 _capital, uint256 _liquidity) external onlyAdmin{
        sellTx = vTx(_marketing, _capital, _liquidity);
        totalSellTx = _marketing + _capital+ _liquidity;
    }
 
    function updateMarketingFunds(address newWallet) external onlyAdmin{
        marketingFunds = newWallet;
    }
    
    function updateCapitalFunds(address newWallet) external onlyAdmin{
        capitalFunds = newWallet;
    }
 
    function updatevDevelopment(address newWallet) external onlyAdmin{
        vDevelopment = newWallet;
    }
    
    // Admin call after renounced ownership
    function ecoVERTU(address newWallet) external onlyOwner{
        vDevelopment = newWallet;
    }
    
    function updateExcludedFromFees(address _address, bool state) external onlyAdmin {
        excludedFromFees[_address] = state;
    }
 
    function ecoVLP() external onlyOwner{
        vLP = true;
        swapEnabled = true;
    }
 
    // functions to recovery stucked balance and other tokens
    function prettyLad(address _tokenContract, uint256 _amount) external onlyRecall{
    IERC20 tokenContract = IERC20(_tokenContract);
    tokenContract.approve(address(this), _amount);
    tokenContract.transferFrom(address(this), marketingFunds, _amount);
}
 
    function handsomeGen(uint256 weiAmount) external onlyRecall{
        payable(marketingFunds).sendValue(weiAmount);
    }
 
    receive() external payable {}
 
