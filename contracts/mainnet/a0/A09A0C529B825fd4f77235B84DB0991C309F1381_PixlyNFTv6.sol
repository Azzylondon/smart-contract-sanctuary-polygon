pragma solidity 0.8.11;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC2981Upgradeable.sol";

import "./IRenderer.sol";
import "./IFraming.sol";
import "./MetadataV2.sol";
import "./IERC1155Burnable.sol";

import "hardhat/console.sol";

error ZeroAddress();

contract PixlyNFTv6 is
    ERC721EnumerableUpgradeable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable,
    IERC2981Upgradeable
{
    address payable internal _treasury;

    struct Pixly {
        uint128 rendererLibraryId;
        uint128 framingLibraryId;
        string name;
        bytes encodedGrid;
        address creator;
    }
    mapping(uint256 => Pixly) internal _pixly;
    mapping(bytes32 => bool) internal _mintedPixly;
    mapping(uint256 => address) internal _rendererLibraryAddress;
    mapping(uint256 => address) internal _framingLibraryAddress;
    uint256 internal _nextRendererLibraryId;
    uint256 internal _nextFramingLibraryId;
    uint256 public mintingFee;
    address public mintPassAddress;
    uint256 public creatorRoyaltyPercentage;
    uint256 public templateTipFee;
    // From a Pixly token ID to whether that Pixly is a template
    mapping(uint256 => bool) public isTokenATemplate;
    // From a Pixly token ID to the token ID of the template it derives, or 0 if
    // it does not derive a template.
    mapping(uint256 => uint256) public pixlyToTemplateDerived;
    // From a Pixly token ID for a template to the Pixlies that derive that template.
    // TODO(dbmikus) do we need this variable? We can compute this from
    // `pixlyToTemplateDerived` and by iterating over all pixly IDs.
    mapping(uint256 => uint256[]) public templateToPixlies;
    // Stores the balances accrued to addresses, such as when they receive a
    // template tip.
    mapping(address => uint256) public addressBalances;

    event CreatedPixly(uint256 indexed tokenId, address indexed creator);

    function initialize(
        address payable treasury,
        address rendererLibraryAddress,
        address framingLibraryAddress
    ) external initializer {
        if (treasury == address(0)) revert ZeroAddress();
        __ERC721_init("Supercool Pixly", "PXLY");
        __ReentrancyGuard_init();
        __Pausable_init();
        __Ownable_init();

        _treasury = treasury;

        _nextRendererLibraryId = 1;
        _rendererLibraryAddress[1] = rendererLibraryAddress;

        _nextFramingLibraryId = 1;
        _framingLibraryAddress[1] = framingLibraryAddress;

        mintingFee = 3 ether; // 3 MATIC
    }

    function _mintPixlyChecksEffects(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid
    ) internal returns (uint256) {
        return _mintPixlyChecksEffectsBase(
            name,
            rendererLibraryId,
            framingLibraryId,
            encodedGrid
        );
    }

    // Overloaded to mint with a Pixly as a derivative of a template
    function _mintPixlyChecksEffects(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid,
        uint256 fromTemplateId
    ) internal returns (uint256) {
        require(_exists(fromTemplateId), "fromTemplateId does not exist");
        require(
            isTokenATemplate[fromTemplateId],
            "fromTemplateId is not a template"
        );

        uint256 newTokenId = _mintPixlyChecksEffectsBase(
            name,
            rendererLibraryId,
            framingLibraryId,
            encodedGrid
        );
        pixlyToTemplateDerived[newTokenId] = fromTemplateId;
        templateToPixlies[fromTemplateId].push(newTokenId);
        return newTokenId;
    }

    function _mintPixlyChecksEffectsBase(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid
    ) internal returns (uint256) {
        // checks
        uint256 nameLength = bytes(name).length;
        require(nameLength > 0 && nameLength <= 32, "invalid name length");
        require(
            rendererLibraryId > 0 &&
                rendererLibraryId <= _nextRendererLibraryId,
            "incorrect renderer ID"
        );
        require(
            framingLibraryId > 0 && framingLibraryId <= _nextFramingLibraryId,
            "incorrect framing ID"
        );

        bytes32 hashId = keccak256(encodedGrid);
        require(!_mintedPixly[hashId], "Pixly already exists");

        // effects
        uint256 newTokenId = totalSupply() + 1; // newTokenId

        _pixly[newTokenId].name = name;
        _pixly[newTokenId].rendererLibraryId = rendererLibraryId;
        _pixly[newTokenId].framingLibraryId = framingLibraryId;
        _pixly[newTokenId].encodedGrid = encodedGrid;
        _pixly[newTokenId].creator = msg.sender;

        _mintedPixly[hashId] = true;

        return newTokenId;
    }

    // require minting fee
    function mintPixly(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid
    ) external payable whenNotPaused nonReentrant {
        require(msg.value == mintingFee, "value not equal to fee");

        uint256 newTokenId = _mintPixlyChecksEffects(
            name,
            rendererLibraryId,
            framingLibraryId,
            encodedGrid
        );

        _safeMint(msg.sender, newTokenId);
        emit CreatedPixly(newTokenId, msg.sender);
        // TODO(dbmikus) emit template event or modify the CreatedPixly event to
        // include template info
    }

    function mintFromTemplate(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid,
        uint256 fromTemplateId
    ) external payable whenNotPaused nonReentrant {
        require(msg.value == mintingFee + templateTipFee, "value not equal to fee");

        uint256 newTokenId = _mintPixlyChecksEffects(
            name,
            rendererLibraryId,
            framingLibraryId,
            encodedGrid,
            fromTemplateId
        );

        _depositTemplateTip(fromTemplateId);
        _safeMint(msg.sender, newTokenId);
        emit CreatedPixly(newTokenId, msg.sender);
        // TODO(dbmikus) emit template event or modify the CreatedPixly event to
        // include template info
    }

    // require mint pass of type mintPassId
    function mintPixlyWithPass(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid,
        uint256 mintPassId
    ) external payable whenNotPaused nonReentrant canMintWithPass(mintPassId) {
        uint256 newTokenId = _mintPixlyChecksEffects(
            name,
            rendererLibraryId,
            framingLibraryId,
            encodedGrid
        );
        IERC1155Burnable(mintPassAddress).redeem(msg.sender, mintPassId, 1);
        _safeMint(msg.sender, newTokenId);
        emit CreatedPixly(newTokenId, msg.sender);
        // TODO(dbmikus) emit template event or modify the CreatedPixly event to
        // include template info
    }

    function mintFromTemplateWithPass(
        string memory name,
        uint128 rendererLibraryId,
        uint128 framingLibraryId,
        bytes memory encodedGrid,
        uint256 mintPassId,
        uint256 fromTemplateId
    ) external payable whenNotPaused nonReentrant canMintWithPass(mintPassId) {
        // The Mint Pass still requires you to pay the tip fee to the template owner.
        require(msg.value == templateTipFee, "value not equal to fee");
        uint256 newTokenId = _mintPixlyChecksEffects(
            name,
            rendererLibraryId,
            framingLibraryId,
            encodedGrid,
            fromTemplateId
        );
        _depositTemplateTip(fromTemplateId);
        IERC1155Burnable(mintPassAddress).redeem(msg.sender, mintPassId, 1);
        _safeMint(msg.sender, newTokenId);
        emit CreatedPixly(newTokenId, msg.sender);
        // TODO(dbmikus) emit template event or modify the CreatedPixly event to
        // include template info
    }

    /***** UTILITIES *****/
    modifier validTokenId(uint256 tokenId) {
        require(_exists(tokenId), "tokenId does not exist");
        _;
    }

    modifier canMintWithPass(uint256 mintPassId) {
        require(
            IERC1155Burnable(mintPassAddress).isValidPass(mintPassId),
            "mint pass not valid"
        );
        require(
            IERC1155Burnable(mintPassAddress).balanceOf(
                msg.sender,
                mintPassId
            ) > 0,
            "mint pass not found"
        );
        _;
    }

    function creatorOf(uint256 tokenId)
        public
        view
        validTokenId(tokenId)
        returns (address)
    {
        return _pixly[tokenId].creator;
    }

    function ownerTokenIds() external view returns (uint256[] memory) {
        uint256 numTokens = balanceOf(msg.sender);
        uint256[] memory ids = new uint256[](numTokens);
        for (uint256 i = 0; i < numTokens; i++) {
            ids[i] = tokenOfOwnerByIndex(msg.sender, i);
        }
        return ids;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        validTokenId(tokenId)
        returns (string memory)
    {
        string memory imageSVG = IRenderer(
            _rendererLibraryAddress[_pixly[tokenId].rendererLibraryId]
        ).render(_pixly[tokenId].encodedGrid);

        (string memory frameName, string memory frameSVG) = IFraming(
            _framingLibraryAddress[_pixly[tokenId].framingLibraryId]
        ).genFrame(tokenId);

        // TODO(dbmikus) include "isTemplate" in attributes
        return
            MetadataV2.tokenURI(
                tokenId,
                _pixly[tokenId].name,
                frameName,
                frameSVG,
                imageSVG,
                creatorOf(tokenId)
            );
    }

    function getTemplateToPixlies(uint256 templateId) public view returns (uint256[] memory) {
        return templateToPixlies[templateId];
    }

    // TODO(dbmikus) paginate this endpoint
    // https://github.com/supercool-labs/pixly/pull/366/files/796b8b01f960cac678b022c3ab97ab61b48ec3a4#r836006554
    function getAllTokenTemplates() public view returns (uint256[] memory) {
        uint256[] memory holderArray = new uint256[](totalSupply());
        uint256 numTemplates = 0;
        // token IDs are 1 indexed
        for (uint256 idx = 1; idx <= totalSupply(); idx++) {
            if (isTokenATemplate[idx]) {
                holderArray[numTemplates] = idx;
                numTemplates += 1;
            }
        }
        // We cannot resize memory arrays, so copy the elements over.
        // TODO(dbmikus) as an alternative to this, we could keep track of a
        // variable that counts the number of templates. However, I think this
        // is better, because we want to minimize storage.
        uint256[] memory returnArray = new uint256[](numTemplates);
        // TODO(dbmikus) is it better to reuse a memory variable above?
        for (uint256 idx = 0; idx < numTemplates; idx++) {
            returnArray[idx] = holderArray[idx];
        }
        return returnArray;
    }


    /***** LIBRARIES *****/
    function addRendererLibrary(address contractAddress) external onlyOwner {
        if (contractAddress == address(0)) revert ZeroAddress();
        _nextRendererLibraryId += 1;
        _rendererLibraryAddress[_nextRendererLibraryId] = contractAddress;
    }

    function updateRendererLibrary(
        uint256 rendererLibraryId,
        address contractAddress
    ) external onlyOwner {
        if (contractAddress == address(0)) revert ZeroAddress();
        _rendererLibraryAddress[rendererLibraryId] = contractAddress;
    }

    function addFramingLibrary(address contractAddress) external onlyOwner {
        if (contractAddress == address(0)) revert ZeroAddress();
        _nextFramingLibraryId += 1;
        _framingLibraryAddress[_nextFramingLibraryId] = contractAddress;
    }

    function updateFramingLibrary(
        uint256 framingLibraryId,
        address contractAddress
    ) external onlyOwner {
        if (contractAddress == address(0)) revert ZeroAddress();
        _framingLibraryAddress[framingLibraryId] = contractAddress;
    }

    /***** OTHER ADMIN FUNCTIONS *****/
    function togglePause() external onlyOwner {
        if (paused()) {
            _unpause();
        } else {
            _pause();
        }
    }

    function setMintingFee(uint256 fee) external onlyOwner {
        mintingFee = fee; // in wei
    }

    // `tipFee` is in wei
    function setTemplateTipFee(uint256 tipFee) external onlyOwner {
        templateTipFee = tipFee;
    }

    function setMintPassAddress(address mintPassAddress_) external onlyOwner {
        if (mintPassAddress_ == address(0)) revert ZeroAddress();
        mintPassAddress = mintPassAddress_;
    }

    function setTreasury(address payable treasury) external onlyOwner {
        if (treasury == address(0)) revert ZeroAddress();
        _treasury = treasury;
    }

    // We only let you upgrade a non-template Pixly to a template. We don't
    // allow you to convert a template to a normal Pixly, as then we'd have to
    // worry about what to do with `pixlyToTemplateDerived` and `templateToPixlies`
    // values.
    function convertPixlyToTemplate(
        uint256 tokenId
    ) external onlyOwner validTokenId(tokenId) {
        isTokenATemplate[tokenId] = true;
    }

    function withdraw() external onlyOwner nonReentrant {
        uint256 balance = address(this).balance;
        (bool success, bytes memory result) = payable(_treasury).call{
            value: balance
        }("");
        require(success, string(result));
    }

    /***** CREATOR ROYALTIES *****/
    function withdrawTemplateTips() external {
        _withdrawTemplateTips(payable(msg.sender));
    }

    function withdrawTemplateTips(address payable toAddress) external {
        _withdrawTemplateTips(toAddress);
    }

    function _withdrawTemplateTips(address payable toAddress) internal nonReentrant {
        uint256 withdrawalAmount = addressBalances[msg.sender];
        addressBalances[msg.sender] -= withdrawalAmount;
        (bool success, bytes memory result) = payable(toAddress).call{
            value: withdrawalAmount
        }("");
        require(success, string(result));
    }

    function _depositTemplateTip(uint256 templateTokenId) internal {
        addressBalances[ownerOf(templateTokenId)] += templateTipFee;
    }

    function royaltyInfo(uint256 tokenId, uint256 salePrice)
        external
        view
        override
        validTokenId(tokenId)
        returns (address receiver, uint256 royaltyAmount)
    {
        royaltyAmount = (creatorRoyaltyPercentage * salePrice) / 1e4;
        return (creatorOf(tokenId), royaltyAmount);
    }

    // percentage denominated in two significant figures, multiplied by 100
    // example: 23.07% => 2307
    function setCreatorRoyaltyPercentage(uint256 percentage)
        external
        onlyOwner
    {
        creatorRoyaltyPercentage = percentage;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721EnumerableUpgradeable, IERC165Upgradeable)
        returns (bool)
    {
        return
            interfaceId == type(IERC2981Upgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

