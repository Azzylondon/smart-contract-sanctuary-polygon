pragma solidity 0.8.11;

import "@openzeppelin/contracts/utils/Strings.sol";
import "base64-sol/base64.sol";

library MetadataV2 {
    using Strings for uint256;

    function _wrapImage(string memory frame, string memory image)
        internal
        pure
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    "<svg height='320' width='260' xmlns='http://www.w3.org/2000/svg' shape-rendering='crispEdges'>",
                    frame,
                    "<g transform='translate(10, 10)'>",
                    image,
                    "</g></svg>"
                )
            );
    }

    function tokenURI(
        uint256 tokenId,
        string memory name,
        string memory frameName,
        string memory frame,
        string memory image,
        address creator
    ) external pure returns (string memory) {
        string memory tokenIdString = tokenId.toString();
        string memory attributes = string(
            abi.encodePacked(
                '[{"trait_type":"Frame","value":"',
                frameName,
                '"},{"trait_type":"Artist","value":"',
                _addressToString(creator),
                '"}]'
            )
        );
        string memory imageCard = _wrapImage(frame, image);
        string memory dataURI = string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(
                    bytes(
                        abi.encodePacked(
                            '{"name":"',
                            name,
                            " (Pixly #",
                            tokenIdString,
                            ")",
                            '","description":"A 100% on-chain Pixly creation"',
                            ',"image":"',
                            imageCard,
                            '","external_url":"https://app.supercool.xyz/pixlies/',
                            tokenIdString,
                            '","attributes":',
                            attributes,
                            "}"
                        )
                    )
                )
            )
        );
        return dataURI;
    }

    function _addressToString(address _address)
        internal
        pure
        returns (string memory)
    {
        bytes32 _bytes = bytes32(uint256(uint160(_address)));
        bytes memory HEX = "0123456789abcdef";
        bytes memory _string = new bytes(42);
        _string[0] = "0";
        _string[1] = "x";
        for (uint256 i = 0; i < 20; i++) {
            _string[2 + i * 2] = HEX[uint8(_bytes[i + 12] >> 4)];
            _string[3 + i * 2] = HEX[uint8(_bytes[i + 12] & 0x0f)];
        }
        return string(_string);
    }
}

