  function getCultName(uint16 gawdId) public view returns (string memory) {
    return cultName(getCultId(gawdId));
  }

  function cultForPower(Power id) public pure returns (Cult) {
    if(id == Power.None) { return Cult.None; }
    else if(id == Power.Cosmos) { return Cult.Astral; }
    else if(id == Power.Divine) { return Cult.Arcane; }
    else if(id == Power.Chaos) { return Cult.Arcane; }
    else if(id == Power.Corporeal) { return Cult.Terrene; }
    else if(id == Power.Mystic) { return Cult.Arcane; }
    else if(id == Power.Creature) { return Cult.Terrene; }
    else if(id == Power.Verdure) { return Cult.Terrene; }
    else if(id == Power.Toxic) { return Cult.Terrene; }
    else if(id == Power.Dark) { return Cult.Arcane; }
    else if(id == Power.Inferno) { return Cult.Astral; }
    else if(id == Power.Mundane) { return Cult.Terrene; }
    else if(id == Power.Spirit) { return Cult.Arcane; }
    else if(id == Power.Aqueous) { return Cult.Terrene; }
    else if(id == Power.Geological) { return Cult.Astral; }
    else if(id == Power.Oblivion) { return Cult.Arcane; }
    else if(id == Power.Automaton) { return Cult.Astral; }
    else if(id == Power.Numerary) { return Cult.Astral; }
    else if(id == Power.Alchemy) { return Cult.Astral; }
    else { revert("Unknown power ID"); } // unreachable, but satisfies linter
  }

