    gawds[gawd.id] = gawd;
    gawdsCount++;
  }

  function removeGawd(uint16 id) public onlyOwner validGawdId(id) {
    delete gawds[id];
    delete gawdPowers[id];

    delete gawdAccents[id];
    delete gawdOculuses[id];
    delete gawdAccents[id];
    delete gawdIrises[id];
    delete gawdCompositions[id];
    delete gawdAuras[id];
    delete gawdRealms[id];
    delete gawdConveyences[id];

    gawdsCount--;
  }

  // NEW -- let's use some friggin structs, geeze
  function addPartStruct(uint16 gawdID, GawdPart memory part) public onlyOwner {
    addPart(gawdID, part.kind, part.value, part.power, part.set);
  }

  function bulkAddPartStructs(uint16 gawdID, GawdPart[] memory parts) public onlyOwner {
    for(uint i; i < parts.length; i++) {
      addPartStruct(gawdID, parts[i]);
    }
  }

  function addGawdStructWithPartStructs(Gawd memory gawdData, GawdPart[] memory parts) public onlyOwner {
    addGawdStruct(gawdData);
    bulkAddPartStructs(gawdData.id, parts);
  }

  function bulkAddGawdStructs(Gawd[] memory gawdData) public onlyOwner {
    for(uint i; i < gawdData.length; i++) {
      addGawdStruct(gawdData[i]);
    }
  }

  function bulkAddGawdStructsWithPartStructs(Gawd[] memory gawdData, GawdPart[][] memory parts) public onlyOwner {
    for(uint i; i < gawdData.length; i++) {
      addGawdStructWithPartStructs(gawdData[i], parts[i]);
    }
  }

  // TODO structify this too
  function addPart(
    uint16 gawdID,
    // uint16 partID,
    PartType kind,
    uint8 value,
    Power power,
    Set set
  )
  public onlyOwner
  validGawdId(gawdID)
  {
    if (value == 0) {
      revert ValueCannotBeZero();
    }

    // unused
    // parts[partID] = GawdPart(partID, kind, value, power, set);
    // partIDs.push(partID);
    // gawdParts[gawdID].push(partID);

    // build indexes of powers
    // all parts have powers, so need to check for None
    gawdPowers[gawdID].push(power);

    // same for sets, but don't add blank sets to the list
    if(set != Set.None) {
      gawdSets[gawdID].push(set);
      if(!allSets[set]) {
        allSets[set] = true;
      }
    }

    // per-trait indexes were more efficient than mass-storage with clever sorting
    // more write cost, less read cost. we're on Polygon so nbd!
    if(kind == PartType.Accent) {
      gawdAccents[gawdID] = Accent(value);
    }
    else if(kind == PartType.Iris) {
      gawdIrises[gawdID] = Iris(value);
    }
    else if(kind == PartType.Aura) {
      // our only attribute that can have either 0, 1, or 2 elements
      gawdAuras[gawdID].push(Aura(value));
    }
    else if(kind == PartType.Composition) {
      gawdCompositions[gawdID] = Composition(value);
    }
    else if(kind == PartType.Oculus) {
      gawdOculuses[gawdID] = Oculus(value);
    }
    else if(kind == PartType.Realm) {
      gawdRealms[gawdID] = Realm(value);
    }
    else if(kind == PartType.Conveyence) {
      gawdConveyences[gawdID] = Conveyence(value);
    }
    else {
      revert("unknown partType");
    }
  }

  // ** UTILS ** //
  function concatenateStrings(string[] memory arr) internal pure returns(string memory) {
    if(arr.length == 0) {
      return "";
    }
    string memory output = arr[0];
    if(arr.length > 1) {
      for(uint i = 1; i < arr.length; i++) {
        output = string(abi.encodePacked(output, ", " , arr[i]));
      }
    }
    return output;
  }

