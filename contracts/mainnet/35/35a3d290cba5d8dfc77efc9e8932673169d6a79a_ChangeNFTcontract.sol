pragma solidity ^0.8.12;
contract ChangeNFTcontract {
    
    address public owner;
    IERC721 public oldNftContract;
    IERC721 public newNftContract;
    address address_Receiving_Old_NFTs;
    address address_Sending_New_NFTs;
    bool public paused;

    constructor() {
        oldNftContract = IERC721(0xCf463Fe8DF9c7e6b623d24AB02bDDEA74fdE7d06); // old NFT contract addr
        newNftContract = IERC721(0xC1e0e77EE2093c1f286f6Dc11902c851732E8339); // new NFT contract addr

        address_Receiving_Old_NFTs = 0xae2cc6F2A7a6Fd4Bf318B68795a2757cE294c277; 
        address_Sending_New_NFTs = 0xae2cc6F2A7a6Fd4Bf318B68795a2757cE294c277;

        owner = msg.sender;
        paused = true;
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    modifier notPaused() {
        require(paused == false);
        _;
    }

    function setPause(bool pause) public onlyOwner {
        paused = pause;
    }

    function changeContract(uint256 _tokenId) public notPaused {
        // receive old NFT
        oldNftContract.safeTransferFrom(msg.sender, address_Receiving_Old_NFTs, _tokenId);

        // send back new NFT
        newNftContract.safeTransferFrom(address_Sending_New_NFTs, msg.sender, _tokenId);
    }

}