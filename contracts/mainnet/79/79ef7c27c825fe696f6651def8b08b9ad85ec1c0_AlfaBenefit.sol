pragma solidity ^0.8.7;








// import './IterableMapping.sol';
// import './Strings.sol';
// import './Console.sol';

contract AlfaBenefit is Ownable, KeeperCompatibleInterface, ReentrancyGuard {
    using SafeMath for uint256;
    using DateTime for uint256;
    // using IterableMapping for IterableMapping.Map;

    event AirdropData(bytes performData, uint256 blockNumber, uint256 month, uint256 amount);

    enum SalePhase {
        Private,
        Public
    }

    struct TokenBeneficiary {
        uint256 fromTokenId;
        uint256 toTokenId;
        uint256 perAmount;
        uint256 totalCycleTimes;
        uint256 historyCycleTimes;
    }

    struct DropRecord {
        uint256 total;
        uint256 Airdropped;
        uint256 lastDropBlock;
        uint256 lastMonth;
        uint256 nextTime;
        address owner;
    }

    mapping(uint256 => TokenBeneficiary) public beneficiarys;
    mapping(uint256 => DropRecord) public airdrDropRecords;

    address public ERC20Token = 0x84b9B910527Ad5C03A9Ca831909E21e236EA7b06;
    address private ERC721Token = 0x9289fBE1B5E8149ED66FdB43A534F249Fd6FE0fF;
    uint16 public releaseDay = 15;
    uint256 public totalCycles = 12;
    uint256 public lastMonthClaim = 0;

    constructor() {}

    function checkUpkeep(bytes calldata checkData) external view override returns (bool upkeepNeeded, bytes memory performData) {
        uint256 curTime = block.timestamp;
        uint256 curDay = DateTime.getDay(curTime);
        uint256 curMonth = DateTime.getMonth(curTime);
        upkeepNeeded = (releaseDay == curDay && lastMonthClaim != curMonth);
        performData = checkData;
        return (upkeepNeeded, performData);
    }

    function performUpkeep(bytes calldata performData) external override {
        uint256 month;
        uint256 amount_pri;
        uint256 amount_pub;
        (month, amount_pri) = releaseToBeneficiary(uint256(SalePhase.Private));
        (month, amount_pub) = releaseToBeneficiary(uint256(SalePhase.Public));
        emit AirdropData(performData, block.number, month, amount_pri + amount_pub);
    }

    function releaseToBeneficiary(uint256 phase) public nonReentrant returns (uint256, uint256) {
        uint256 curTime = block.timestamp;
        uint256 curDay = DateTime.getDay(curTime);
        uint256 curMonth = DateTime.getMonth(curTime);
        uint256 total_amount = 0;
        if (releaseDay == curDay && lastMonthClaim != curMonth) {
            TokenBeneficiary storage moBene = beneficiarys[phase];
            if (moBene.historyCycleTimes < moBene.totalCycleTimes || moBene.historyCycleTimes == 0) {
                for (uint256 i = moBene.fromTokenId; i <= moBene.toTokenId; i += 1) {
                    uint256 tokenId = i;
                    DropRecord storage dropRecord = airdrDropRecords[tokenId];
                    if (dropRecord.lastDropBlock != block.number && dropRecord.lastMonth != curMonth) {
                        address owner = IERC721(ERC721Token).ownerOf(tokenId);
                        if (dropRecord.Airdropped < dropRecord.total || dropRecord.Airdropped == 0) {
                            uint256 amount = moBene.perAmount;
                            total_amount = total_amount + amount;
                            IERC20(ERC20Token).transfer(owner, amount);
                            dropRecord.total = moBene.perAmount * moBene.totalCycleTimes;
                            dropRecord.Airdropped = dropRecord.Airdropped + moBene.perAmount;
                            dropRecord.lastDropBlock = block.number;
                            dropRecord.nextTime = DateTime.addDays(curTime, 1);
                            dropRecord.owner = owner;
                            dropRecord.lastMonth = curMonth;
                        }
                    }
                }
                moBene.historyCycleTimes = moBene.historyCycleTimes + 1;
            }
        }
        lastMonthClaim = curMonth;
        return (lastMonthClaim, total_amount);
    }

    function addTokenBeneficiary(
        uint256 phase,
        uint256 fromTokenId,
        uint256 toTokenId,
        uint256 perAmount,
        uint256 totalCycleTimes
    ) public onlyOwner {
        TokenBeneficiary storage moBene = beneficiarys[phase];
        moBene.fromTokenId = fromTokenId;
        moBene.toTokenId = toTokenId;
        moBene.perAmount = perAmount;
        moBene.totalCycleTimes = totalCycleTimes;
    }

    function setReleaseDayCycles(uint16 _day, uint256 _totalCycles) public onlyOwner {
        releaseDay = _day;
        totalCycles = _totalCycles;
    }

    function setERC20Token(address addr) public onlyOwner {
        ERC20Token = addr;
    }

    function setERC721Token(address addr) public onlyOwner {
        ERC721Token = addr;
    }

    function toWei(uint256 amount, uint8 _decimals) private pure returns (uint256) {
        return amount * (uint256(10)**_decimals);
    }
}