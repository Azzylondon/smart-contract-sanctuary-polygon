abstract contract OnlyBank is Ownable {
    address public bank;

    event BankUpdated(address indexed bank);

    modifier onlyBank() {
        require(msg.sender == bank, "OnlyBank: onlyBank");
        _;
    }

    modifier onlyBankOrOwner() {
        require(
            msg.sender == bank || msg.sender == owner(),
            "OnlyBank: onlyBankOrOwner"
        );
        _;
