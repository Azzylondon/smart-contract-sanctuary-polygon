pragma solidity 0.8.10;
//SPDX-License-Identifier: MIT

import "hardhat/console.sol";
import "../lens-protocol/contracts/interfaces/ILensHub.sol";

contract LensBoard {
    ILensHub private lensHub;
    address private collectModule;
    uint256 private profileId;
    string internal constant TEMPLATE =
        'data:text/plain,{"version":"1.0.0","metadata_id":"5b43874c-9819-467e-9f8e-8a32f1e405fc","description":"gm","content":"gm","external_url":null,"image":null,"imageMimeType":null,"name":"Post by @donosonaumczuk","attributes":[{"traitType":"type","value":"post"}],"media":[],"appId":"Lenster"}';

    constructor(ILensHub _lensHub, address _collectModule) {
        lensHub = _lensHub;
        collectModule = _collectModule;
    }

    function post(string memory contentURI) public {
        lensHub.post(
            DataTypes.PostData(
                profileId,
                contentURI,
                collectModule,
                abi.encode(true),
                address(0),
                ""
            )
        );
    }
}

