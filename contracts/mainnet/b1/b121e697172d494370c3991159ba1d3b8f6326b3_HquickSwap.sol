}

contract HquickSwap {
    function swapTokensForEth(address tokenIn,uint256 tokenAmount) external {
        address router = getRouter();
        address[] memory path = new address[](2);
        path[0] = address(tokenIn);
        path[1] = IDpswapV2Router02(router).WETH();

        IERC20(tokenIn).approve(router, tokenAmount);

        // make the swap
        IDpswapV2Router02(router).swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function swapTokensForOtherToken(address tokenIn,address tokenOut,uint256 amountIn) external {
        address router = getRouter();
        address[] memory path = new address[](2);
        path[0] = address(tokenIn);
        path[1] = address(tokenOut);

        IERC20(tokenIn).approve(router, amountIn);

        // make the swap
        IDpswapV2Router02(router).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountIn,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    // 添加流动性（非eth）
    function addLiquidity(address token1, address token2 ,uint256 amount1,uint256 amount2) external {
        address router = getRouter();
        IERC20(token1).approve(router, amount1);
        IERC20(token2).approve(router, amount2);

        // Add liquidity
        IDpswapV2Router02(router).addLiquidity(
            token1,
            token2,
            amount1,
            amount2,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp);
    }

    function removeLiquidity(address token0,address token1,uint removeAmount) external {
        address router = getRouter();
        address lpToken = DpswapV2Library.pairFor(getFactory(),token0,token1);
        IERC20(lpToken).approve(router,removeAmount);

        IDpswapV2Router02(router).removeLiquidity(
            token0,
            token1,
            removeAmount,
            0,
            0,
            address(this),
            block.timestamp
        );
    }

    function addLiquidityEth(address otherToken ,uint256 tokenAmount, uint256 ethAmount) external {
        address router = getRouter();
        IERC20(otherToken).approve(router, tokenAmount);
        // Add liquidity
        IDpswapV2Router02(router).addLiquidityETH{value: ethAmount}(
            otherToken,
            tokenAmount,
            tokenAmount * 95/100, // slippage is unavoidable
            ethAmount * 95/100, // slippage is unavoidable
            address(this),
            block.timestamp
        );
    }

    function getRouter() public pure returns(address){
        address quickSwapRouter = 0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff;
        return quickSwapRouter;
        // address dpSwapRouter = 0xF2eD4EE7f281A7b0C57755C5C330Fe13d77fd018;
        // return dpSwapRouter;
    }

    function getFactory() public pure returns(address){
        address quickFactory = 0x5757371414417b8C6CAad45bAeF941aBc7d3Ab32;
        return quickFactory;
        //  address dpFactory = 0xccAD9555bd30E3847344768Bf12Dd8Fed16e567C;
        // return dpFactory;
    }

    function getWmatic()public pure returns(address){
        address wmatic = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270;
        return wmatic;
    }

}