pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
//import "@openzeppelin/contracts/utils/Base64.sol";

import {Base64} from "./libraries/Base64.sol";
import {ISecretRoseNFTMeta} from "./SecretRoseNFTMeta.sol";

contract SecretRoseNFT is ERC721URIStorage, Ownable, ReentrancyGuard {

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    uint256 private _maxSupply;
    uint256 private _balance;    
    address public metadataAddress;

    struct HP {
        uint256 lastHP;
        uint256 lastBlock;
        uint256 totalTurnover;
    }

    mapping (uint256 => HP) public hpMap;
 
    event NewNFTMinted(address sender, uint256 tokenId);

    constructor() ERC721 ("Secret Rose", "ROSE") {
        _maxSupply = 4096;
    }
    
    function setMetadataAddress(address addr) public onlyOwner {
        metadataAddress = addr;
    }

    function checkHP(uint256 tokenId) public view returns (uint256) {        
        uint256 lastBlock = hpMap[tokenId].lastBlock;
        if (lastBlock == 0) {
            return 0;
        }

        uint256 newHP = hpMap[tokenId].lastHP;
        uint256 diff = (block.number - lastBlock) / 44000;

        if (diff > (hpMap[tokenId].totalTurnover -1) * 10) {
            newHP = diff - (hpMap[tokenId].totalTurnover -1) * 10;
        }
        return newHP;
    }

    function tokenURI(uint256 tokenId) override public view returns (string memory) {
        require(metadataAddress != address(0), "Metadata address is not set yet!");
        require(tokenId < _maxSupply, "Token ID does not exist!");
        return ISecretRoseNFTMeta(metadataAddress).getTokenURI(tokenId, checkHP(tokenId));
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override {
            super._beforeTokenTransfer(from, to, tokenId);
            hpMap[tokenId].lastHP = checkHP(tokenId);
            hpMap[tokenId].lastBlock = block.number;
            hpMap[tokenId].totalTurnover = hpMap[tokenId].totalTurnover + 1;
    }

    function mintNFT(address to) payable public {
        require(metadataAddress != address(0), "Metadata address is not set yet!");
        require(_msgSender() == owner() || msg.value > 2 ether, "Not enough fund for mint!");
        
        uint256 newItemId = _tokenIds.current();
        require(newItemId < _maxSupply, "Total supply has been reached!");

        _safeMint(to, newItemId);
        _tokenIds.increment();
        
        emit NewNFTMinted(to, newItemId);
    }

    function batchMint(uint256 amount) public onlyOwner {
        uint256 totalMinted = getTotalNFTsMinted();
        uint256 numberToMint = amount;
        if (totalMinted + amount > _maxSupply) {
            numberToMint = _maxSupply - totalMinted;
        }
        for(uint i = 0; i < numberToMint; i++) {
            mintNFT(_msgSender());
        }
    }

    function getTotalNFTsMinted() public view returns (uint256) {
        return _tokenIds.current();
    }

    function withdrawBalance() public onlyOwner nonReentrant {
        uint256 amount = address(this).balance;
        payable(_msgSender()).transfer(amount);
    }

}

