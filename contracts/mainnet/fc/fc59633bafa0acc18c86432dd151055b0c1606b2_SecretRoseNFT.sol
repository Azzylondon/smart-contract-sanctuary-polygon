        }

        return canvas;
    }

    function _createInitialPattern(string[41] memory canvas, uint256 tokenId, uint256 hp) public pure returns (string[41] memory) {
        
        for(uint256 i = 0; i < 19; i++) {
            _fillLine(canvas, i+1, 24-i, 24+i+1, i, i, tokenId, hp);
        }
            
        for(uint256 i = 8; i >0; i--) {
            _fillLine(canvas, 27-i, 24-i*2-2, 23+i*2+3, i*2, i*2, tokenId, hp);
