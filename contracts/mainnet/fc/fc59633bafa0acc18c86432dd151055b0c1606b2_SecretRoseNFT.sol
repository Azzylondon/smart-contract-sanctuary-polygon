        for(uint256 i = 0; i < 7; i++) {
            _fillLine(canvas, 21+i, 6+i, 6+i*2+1, i*3, 0, tokenId, hp);
            _fillLine(canvas, 21+i, 43-i*2-1, 43-i, i*3, 0, tokenId, hp);
