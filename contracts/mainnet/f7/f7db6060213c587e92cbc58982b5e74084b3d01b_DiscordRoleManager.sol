pragma solidity 0.8.17;

import "../abstract/AccessControlledAndUpgradeable.sol";

/**
 @title Discord Role Manager
 @notice The Marketting team can use this to have fine grained access over discord roles.
 @dev The Marketting team can be given 'minor admin role' and have more autonomy to add/remove roles on discord.
 */
contract DiscordRoleManager is AccessControlledAndUpgradeableModifiers {
  mapping(address => mapping(uint256 => bool)) public hasRole;

  constructor() initializer {} // Prevent the implementation of upgradeable contract from being initialized by initializing the constructor.

  event RolesGranted(address[] indexed user, uint256 indexed role);
  event RolesRevoked(address[] indexed user, uint256 indexed role);

  function initialize() external initializer {
    _AccessControlledAndUpgradeable_init(msg.sender);
  }

  function grantDiscordRoles(address[] calldata users, uint256 role) external minorAdminOnly {
    for (uint256 i = 0; i < users.length; i++) {
      hasRole[users[i]][role] = true;
    }
    emit RolesGranted(users, role);
  }

  function revokeDiscordRoles(address[] calldata users, uint256 role) external minorAdminOnly {
    for (uint256 i = 0; i < users.length; i++) {
      hasRole[users[i]][role] = false;
    }
    emit RolesRevoked(users, role);
  }
}

/// Below is the deployment script for this contract.
import "forge-std/Script.sol";
import "forge-std/console2.sol";
import "@openzeppelin/contracts/proxy/ERC1967/ERC1967Proxy.sol";

contract DiscordRoleManagerDeploy is Script {
  function run() external {
    vm.startBroadcast();

    DiscordRoleManager discordRoleManagerImplementation = new DiscordRoleManager();
    ERC1967Proxy discordRoleManagerProxy = new ERC1967Proxy(address(discordRoleManagerImplementation), abi.encodeWithSignature("initialize()"));

    console2.log("The discord role manager is deployed at", address(discordRoleManagerProxy));

    vm.stopBroadcast();
  }
}

