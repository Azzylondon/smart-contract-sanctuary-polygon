  function ADMIN_ROLE() external returns (bytes32);

  function MINOR_ADMIN_ROLE() external returns (bytes32);

  function EMERGENCY_ROLE() external returns (bytes32);

  function UPGRADER_ROLE() external returns (bytes32);
}

