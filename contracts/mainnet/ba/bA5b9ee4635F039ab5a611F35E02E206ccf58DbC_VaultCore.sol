    // TODO: Emit deposit()/withdraw() events
    function deposit(
        address[] calldata _assets,
        uint256[] calldata _amounts
    ) external  whenNotCapitalPaused nonReentrant {
        _deposit(_assets, _amounts);
      
    }
    function _deposit(
        address[] calldata _assets,
        uint256[] calldata _amounts
    ) internal {
        require(assetCombinations[ keccak256(abi.encodePacked(_assets)) ].isSupported, "Unsupported Assets");
        require(_assets.length == _amounts.length, "Mismatch");
        require(_assets.length == 2, "Only pair assets supported");
        require(token0!=address(0) && token1!=address(0), "Tokens not set! Contact Admin!");
        console.log("VaultCore.Deposit", reserve0, reserve1);
        _sync();
        console.log("VaultCore.Deposit", reserve0, reserve1);


        // Swapping and replacing DAI/USDT to USDC
        (address[] memory poolAssets, uint256[] memory poolAmounts) = _swapToTokens(_assets, _amounts);
        (uint256 r0, uint256 r1) = avgStrategyRatio();
        console.log("poolAssets ", poolAssets[0], poolAssets[1]);
        console.log("poolAmounts ", poolAmounts[0], poolAmounts[1]);
        console.log("Min Strategy Reserves ", r0, r1);
        console.log("Min Strategy Reserves ", r0.scaleBy(tokenDecimals[0], 20), r1.scaleBy(tokenDecimals[1], 20));
        console.log("VaultCore.Deposit Reserves ", reserve0, reserve1);
        // TODO: Make sure the funds are in accordanace with the reserves, if extra return extra
        if (r0 > 0) {
            uint256 requiredToken1Amount = r1.scaleBy(tokenDecimals[1],20).mul(poolAmounts[0]).div(r0.scaleBy(tokenDecimals[0],20));
            console.log("requiredToken1 ", requiredToken1Amount);
        
            if (requiredToken1Amount > poolAmounts[1]) {
                revert("Not enough token1 for token0");
            } else if (requiredToken1Amount < poolAmounts[1]) {
                // Return extra to the user
                uint256 _extra =  poolAmounts[1].sub(requiredToken1Amount);
                console.log("Returning extra token1 to user", _extra/10**tokenDecimals[1]);
                IERC20(poolAssets[1]).safeTransfer(msg.sender, _extra);
                poolAmounts[1] = poolAmounts[1].sub(_extra);
            }
        }
        console.log("VaultCore.Deposit Reserves ", reserve0, reserve1);

        // Minting Pool LP
        _mint(msg.sender);

        // Quick Allocate
        _quickAllocate();

        (uint256 balance0, uint256 balance1) = checkBalance();
        console.log("VaultCore.Deposit Balance: %s %s", balance0, balance1);
        console.log("VaultCore.Deposit Reserves ", reserve0, reserve1);

        lastMints[msg.sender] = block.number;
        lastMintTime[msg.sender] = block.timestamp;
    }

    function _swapToTokens(address[] calldata _assets, uint256[] calldata _amounts) internal returns (address[] memory, uint256[] memory) {
        address[] memory poolAssets = new address[](_assets.length);
        uint256[] memory poolAmounts = new uint256[](_amounts.length);
        for(uint8 i = 0; i < _assets.length; i++) {
            address subjectAsset = _assets[i];
            console.log("Pulling money from:", subjectAsset, _amounts[i]);
            IERC20 asset = IERC20(subjectAsset);
            asset.safeTransferFrom(msg.sender, address(this), _amounts[i]);
            
            poolAssets[i] = subjectAsset;
            poolAmounts[i] = _amounts[i];
            console.log("Pool Assets:", poolAssets[i], poolAmounts[i], primaryOfKinds[assets[subjectAsset].kind]);
            if (subjectAsset != primaryOfKinds[assets[subjectAsset].kind]) {
                poolAssets[i] = primaryOfKinds[assets[subjectAsset].kind];
                console.log("Swapping using %s ", address(swapper));
                IERC20(subjectAsset).safeApprove(address(swapper), poolAmounts[i]);
                poolAmounts[i]  = swapper.swapCommon(_assets[i], primaryOfKinds[assets[subjectAsset].kind], poolAmounts[i]);
                console.log("Swapper to value %s", poolAmounts[i]);
            } 
        }
        console.log("Swapped to 0 pool assets:", poolAssets[0], poolAmounts[0]);
        console.log("Swapped to 1 pool assets:", poolAssets[1], poolAmounts[1]);
        return (poolAssets, poolAmounts);
    }


    /**
     * @dev Withdraw a supported asset and burn CASH.
     * @param _amount Amount of CASH to burn
     */
    function withdraw(uint256 _amount)
        external
        
        whenNotCapitalPaused
        nonReentrant
    {
        _withdraw(_amount);
    }
    
    


    /**
     * @dev Withdraw a supported asset and burn CASH.
     * @param _amount Amount of LP to burn
     */
    function _withdraw(uint256 _amount) internal {
        require(_amount > 0, "Amount must be greater than 0");
        require(block.number > lastMints[msg.sender], "Wait after mint - 1");
        require(block.timestamp > lastMintTime[msg.sender], "Wait after mint - 2");
        require(block.timestamp - lastMintTime[msg.sender] > withdrawLockup, "WAIT_LOCKED");
        console.log("Withdrawing %s | Max: %s", _amount, balanceOf[msg.sender]);
        require(_amount <= balanceOf[msg.sender], "Not enough LP");
        
        _sync();
        uint256[] memory required = new uint256[](2);
        (required[0], required[1]) = burn(msg.sender, _amount);
        
        require(required[0] > 0 || required[1] > 0, "Nothing to withdraw");
        console.log("Tokens: %s %s", address(token0), address(token1));
        console.log("Tokens Balance to withdraw: %s %s", required[0], required[1]);

        // Getting tokes 
        IERC20 _token0 = IERC20(token0);
        IERC20 _token1 = IERC20(token1);
        address[] memory tokens = new address[](2);
        tokens[0] = address(_token0);
        tokens[1] = address(_token1);
        
        (uint256 available0, uint256 available1) = available();
        uint256[] memory _withdrawables = new uint256[](2);
        console.log("Initial available: %s %s", available0, available1);


        uint8 i = 0;
        while((available0 <= required[0] || available1 <= required[1]) && (i < strategyWithWeights.length)) {
            console.log("Loop start: %s with initialToken0: %s  & available0: %s", i, available0, available1);
            IStrategy strategy = IStrategy(strategyWithWeights[i].strategy);
            (uint256 strategyBal0, uint256 strategyBal1) = strategy.checkBalance();
            console.log("Strategy: ", strategyWithWeights[i].strategy);
            console.log("Current Balances: %s %s", strategyBal0, strategyBal1);
            
            // If the strategy have any kind of balance
            if ((strategyBal0 > 0) || (strategyBal1 > 0)) {
                // Check if this strategy will be sufficient for withdrawl
                if (((available0 + strategyBal0) >= required[0]) && ((available1 + strategyBal1) >= required[1])) {

                    if (required[0] > available0) {
                        _withdrawables[0] = required[0] - available0;
                    }
                    if (required[1] > available1) {
                        _withdrawables[1] = required[1] - available1;
                    }
                    // if we have something to withdraw, condition should never go false ideally
                    if (_withdrawables[0] > 0 || _withdrawables[1] > 0) {
                        strategy.withdraw(address(this), tokens, _withdrawables);
                    }
                } else {
                    // If this strategy alone would not be able to complete withdrawl, 
                    // we withdraw all from this and move to next. 
                    strategy.withdrawAll();
                }
                (available0, available1) = available();
                
            }
            console.log("Loop end: %s with initialToken0: %s  & available0: %s", i, available0, available1);
            i++;
        }
        console.log("Withdraw target", required[0]/ 10**18, required[1] / 10**6);
        console.log("Withdrawed from strategies", _token0.balanceOf(address(this)) / 10**18, _token1.balanceOf(address(this)) / 10**6);
        require((_token0.balanceOf(address(this)) >= required[0]) && (_token1.balanceOf(address(this)) >= required[1]), "Not enough funds after withdrawl.");

        // Transferring tokens to user
        if (required[0] > 0) {
            _token0.safeTransfer(msg.sender, required[0] );
        }
        if (required[1] > 0) {
            _token1.safeTransfer(msg.sender, required[1] );
        }

    }

    /**
     * @notice Allocate unallocated funds on Vault to quick deposit strategies.
     * @dev Allocate unallocated funds on Vault to quick deposit strategies.
     **/

    function quickAllocate() external whenNotCapitalPaused nonReentrant {
        _quickAllocate();
    }
    function _quickAllocate() internal {
        // require(quickDepositStrategies.length > 0, "No quick deposit strategy present");
        if (quickDepositStrategies.length == 0) {
            return;
        }
        // console.log("quickAllocate -  primaryStableBalance: ", IERC20(primaryStableAddress).balanceOf(address(this)));
        uint256 index =  block.number  % quickDepositStrategies.length;
        address quickDepositStrategyAddr = quickDepositStrategies[index];

        // Getting tokes from XPool
        IERC20 _token0 = IERC20(token0);
        IERC20 _token1 = IERC20(token1);
        uint256 _token0Bal = _token0.balanceOf(address(this));
        uint256 _token1Bal = _token1.balanceOf(address(this));
        console.log("quickAllocate -  forUser0Bal: ", _token0Bal);
        console.log("quickAllocate -  forUser1Bal: ", _token1Bal);

        address[] memory _tokens = new address[](2);
        _tokens[0] = address(_token0);
        _tokens[1] = address(_token1);
        uint256[] memory _amounts = new uint256[](2);

        IStrategy strategy = IStrategy(quickDepositStrategyAddr);
        (uint256 _r0, uint256 _r1) = strategy.getRatios();
        console.log("quickAllocate -  ratios: ", _r0, _r1);
        (uint256 _s0, uint256 _s1) = _selectAmount(_token0Bal.scaleBy(18, tokenDecimals[0]), _token1Bal.scaleBy(18, tokenDecimals[1]), _r0, _r1);
        _amounts[0] = _s0.scaleBy(tokenDecimals[0], 18);
        _amounts[1] = _s1.scaleBy(tokenDecimals[1], 18);
        
        console.log("quickAllocate -  _amounts0: ", _amounts[0]);
        console.log("quickAllocate -  _amounts1: ", _amounts[1]);


        if (quickDepositStrategyAddr != address(0)   && _token0Bal > 0 && _token1Bal > 0) {
            _token0.safeTransfer(address(strategy), _amounts[0]);
            _token1.safeTransfer(address(strategy), _amounts[1]);
            strategy.deposit(_tokens, _amounts);
            emit AssetAllocated(_tokens,quickDepositStrategyAddr,_amounts);
        }

    }
    function _selectAmount(uint256 _a0, uint256 _a1, uint256 _r0, uint256 _r1) internal pure returns (uint256, uint256) {
        uint256 __a0 = _a0;
        uint256 __a1 = _a1;
        if (_a0 <= _a1) {
            __a1 = _a0.mul(_r1).div(_r0);
            if (__a1 > _a1) {
                __a1 = _a1;
                __a0 = _a1.mul(_r0).div(_r1);
            }
        } else if (_a0 > _a1) {
            __a0 = _a1.mul(_r0).div(_r1);
            if (__a0 > _a0) {
                __a0 = _a0;
                __a1 = _a0.mul(_r1).div(_r0);
            }
        }
        return (__a0, __a1);
    }
    function checkBalance() public view returns (uint256, uint256) {
        uint256 balance1 = IERC20(token0).balanceOf(address(this));
        uint256 balance2 = IERC20(token1).balanceOf(address(this));
        // Loop through all the startegies
        for (uint256 i = 0; i < allStrategies.length; i++) {
            if (strategies[allStrategies[i]].isSupported == false) {
                continue;
            }
            IStrategy strategy = IStrategy(allStrategies[i]);
            (uint256 currentToken0Bal, uint256 currentToken1Bal) = strategy.checkBalance();
             console.log("VaultCore.checkBalance ", address(strategy));
            console.log("VaultCore.checkBalance %s %s", currentToken0Bal, currentToken1Bal);
            balance1 += currentToken0Bal;
            balance2 += currentToken1Bal;
        }
        // console.log("VaultCore.checkBalance %s %s", balance1, balance2);
        // console.log("VaultCore.checkBalance ", address(strategy));
        return (balance1, balance2);
    }
    /**
     * @notice Check balance equivalent in USD
     * @return uint256, uint256 - Balance equivalent in USD. E8
     **/
    function tvl() public view returns (uint256, uint256) {
        require(oracleRouter != address(0), "Router not set");
        IOracle _oracle = IOracle(oracleRouter);
        (uint256 _b0, uint256 _b1) = checkBalance();
        _b0 = _b0.scaleBy(18, tokenDecimals[0]).mul(_oracle.price(token0)).div(10**18);
        _b1 = _b1.scaleBy(18, tokenDecimals[1]).mul(_oracle.price(token1)).div(10**18);
        return (_b0, _b1);
    }
    function available() public view returns (uint256, uint256) { 
        return  (IERC20(token0).balanceOf(address(this)), IERC20(token1).balanceOf(address(this)));
    }


    function claimReward(address _strategy) public returns (uint256, uint256) {
        IStrategy strategy = IStrategy(_strategy);
        (uint256 _rew0, uint256 _rew1) = strategy.collectRewardTokens();
        return (_rew0, _rew1);
    }

    function claimRewards() public returns (uint256, uint256) {
        uint256 totRew0 = 0;
        uint256 totRew1 = 0;
        // Loop through all the startegies
        for (uint256 i = 0; i < strategyWithWeights.length; i++) {
            (uint256 reward0, uint256 reward1) = claimReward(strategyWithWeights[i].strategy);
            totRew0 += reward0;
            totRew1 += reward1;
        }
        console.log("Total Claim Reward %s %s", totRew0, totRew1);
        return (totRew0, totRew1);
    }

