    /**
     * @dev The return the average among the startegy ratios
     */
    function avgStrategyRatio() public view returns (uint256, uint256) {
        if (allStrategies.length == 0) {
            return (100, 100);
        }
        uint256 r0;
        uint256 r1;
        // Loop through all strategies
        for (uint256 i = 0; i < allStrategies.length; i++) {
            IStrategy strategy = IStrategy(allStrategies[i]);
            (uint256 _r0, uint256 _r1) = strategy.getRatios();
            console.log("Ratio of this startegy: ",_r0, _r1);
            r0 += _r0;
            r1 += _r1;
        }
        return (r0/allStrategies.length, r1/allStrategies.length);
    }


    /*
    * Mint related functions
    */
    // if fee is on, mint liquidity equivalent to 1/6th of the growth in sqrt(k)
    function _mintFee(uint112 _reserve0, uint112 _reserve1) internal returns (bool feeOn) {
        address feeTo = address(0);
        feeOn = feeTo != address(0);
        uint _kLast = kLast; // gas savings
        if (feeOn) {
            if (_kLast != 0) {
                uint rootK = Math.sqrt(uint(_reserve0).mul(_reserve1));
                uint rootKLast = Math.sqrt(_kLast);
                if (rootK > rootKLast) {
                    uint numerator = totalSupply.mul(rootK.sub(rootKLast));
                    uint denominator = rootK.mul(5).add(rootKLast);
                    uint liquidity = numerator / denominator;
                    if (liquidity > 0) _mint(feeTo, liquidity);
                }
            }
        } else if (_kLast != 0) {
            kLast = 0;
        }
    }
    function getReserves() public view returns (uint112 _reserve0, uint112 _reserve1, uint32 _blockTimestampLast) {
        _reserve0 = reserve0;
        _reserve1 = reserve1;
        _blockTimestampLast = blockTimestampLast;
    }
    // update reserves and, on the first call per block, price accumulators
    function _update(uint balance0, uint balance1, uint112 _reserve0, uint112 _reserve1) private {
        console.log("Updating reserves to %s %s", balance0, balance1);
        require(balance0 <= type(uint112).max && balance1 <= type(uint112).max, 'XPool: OVERFLOW');
        uint32 blockTimestamp = uint32(block.timestamp % 2**32);
        uint32 timeElapsed = blockTimestamp - blockTimestampLast; // overflow is desired
        console.log("Time elapsed %s", timeElapsed);
        if (timeElapsed > 0 && _reserve0 != 0 && _reserve1 != 0) {
            // * never overflows, and + overflow is desired
            price0CumulativeLast += uint(UQ112x112.encode(_reserve1).uqdiv(_reserve0)) * timeElapsed;
            price1CumulativeLast += uint(UQ112x112.encode(_reserve0).uqdiv(_reserve1)) * timeElapsed;
        } else {
            console.log("No time elapsed");
        }
        reserve0 = uint112(balance0);
        reserve1 = uint112(balance1);
        console.log("Updated reserves to %s %s", reserve0, reserve1);

        blockTimestampLast =  blockTimestamp;
        emit Sync(reserve0, reserve1);
    }
    // this low-level function should be called from a contract which performs important safety checks
    function _mint(address to) internal returns (uint liquidity) {
        (uint112 _reserve0, uint112 _reserve1,) = getReserves(); // gas savings
        (uint256 balance0, uint256 balance1) = checkBalance();
        uint amount0 = balance0.sub(_reserve0);
        uint amount1 = balance1.sub(_reserve1);
        console.log("Minting with _reserve0: %s  _reserve1: %s", _reserve0, _reserve1);
        console.log("Minting with Amount0: %s  Amount1: %s", amount0, amount1);
        bool feeOn = _mintFee(_reserve0, _reserve1);
        uint _totalSupply = totalSupply; // gas savings, must be defined here since totalSupply can update in _mintFee
        console.log("Total Supply: ", _totalSupply);
        if (_totalSupply == 0) {
            liquidity = Math.sqrt(amount0.mul(amount1)).sub(MINIMUM_LIQUIDITY);
           _mint(address(0), MINIMUM_LIQUIDITY); // permanently lock the first MINIMUM_LIQUIDITY tokens
        } else {
            liquidity = Math.min(amount0.mul(_totalSupply) / _reserve0, amount1.mul(_totalSupply) / _reserve1);
        }
        require(liquidity > 0, 'XPool: INSUFFICIENT_LIQUIDITY_MINTED');
        console.log("Minting with liquidity: %s", liquidity);
        console.log("Minting with _totalSupply: %s", _totalSupply);
        _mint(to, liquidity);
        _update(balance0, balance1, _reserve0, _reserve1);
        if (feeOn) kLast = uint(reserve0).mul(reserve1); // reserve0 and reserve1 are up-to-date
        emit Mint(msg.sender, amount0, amount1);

        // EXTRA LOGIC
        _totalSupply = totalSupply; // gas savings, must be defined here since totalSupply can update in _mintFee
        (_reserve0, _reserve1,) = getReserves(); // gas savings
        console.log("After Minting with _reserve0: %s  _reserve1: %s", _reserve0, _reserve1);
        (balance0, balance1) = checkBalance();
        console.log("After Minting with _tokenBalance0: %s  tokenBalance1: %s", balance0, balance1);
        console.log("After Minting with _totalSupply: %s", _totalSupply);
    }

    // this low-level function should be called from a contract which performs important safety checks
    function burn(address to) internal returns (uint amount0, uint amount1) {
        (uint112 _reserve0, uint112 _reserve1,) = getReserves(); // gas savings
        // address _token0 = token0;                                // gas savings
        // address _token1 = token1;                                // gas savings
        (uint256 balance0, uint256 balance1) = checkBalance();
        uint liquidity = balanceOf[to];
        console.log("Burn - liquidity", liquidity);

        bool feeOn = _mintFee(_reserve0, _reserve1);
        uint _totalSupply = totalSupply; // gas savings, must be defined here since totalSupply can update in _mintFee
        amount0 = liquidity.mul(balance0) / _totalSupply; // using balances ensures pro-rata distribution
        amount1 = liquidity.mul(balance1) / _totalSupply; // using balances ensures pro-rata distribution
        require(amount0 > 0 && amount1 > 0, 'XPool: INSUFFICIENT_LIQUIDITY_BURNED');
        _burn(to, liquidity);
        // _safeTransfer(_token0, to, amount0); // -- Vault would handle the transfer
        // _safeTransfer(_token1, to, amount1); // -- Vault would handle the transfer
        balance0 = balance0 - amount0; // IERC20(_token0).balanceOf(address(this));
        balance1 = balance1 - amount1; // IERC20(_token1).balanceOf(address(this));

        _update(balance0, balance1, _reserve0, _reserve1);
        if (feeOn) kLast = uint(reserve0).mul(reserve1); // reserve0 and reserve1 are up-to-date
        emit Burn(msg.sender, amount0, amount1, to);
        return (amount0, amount1);
    }
    // this low-level function should be called from a contract which performs important safety checks
    function burn(address to, uint liquidity) internal returns (uint amount0, uint amount1) {
        (uint112 _reserve0, uint112 _reserve1,) = getReserves(); // gas savings
        // address _token0 = token0;                                // gas savings
        // address _token1 = token1;                                // gas savings
        (uint256 balance0,uint256 balance1) = checkBalance();

        console.log("Burning with _reserve0: %s  _reserve1: %s", _reserve0, _reserve1);
        console.log("Burning with Amount0: %s  Amount1: %s", balance0, balance1);

        bool feeOn = _mintFee(_reserve0, _reserve1);
        uint _totalSupply = totalSupply; // gas savings, must be defined here since totalSupply can update in _mintFee
        console.log("Burning with _totalSupply: %s", _totalSupply);

        amount0 = liquidity.mul(balance0) / _totalSupply; // using balances ensures pro-rata distribution
        amount1 = liquidity.mul(balance1) / _totalSupply; // using balances ensures pro-rata distribution
        console.log("Burning for  Amount0: %s  Amount1: %s", amount0, amount1);

        require(amount0 > 0 && amount1 > 0, 'XPool: INSUFFICIENT_LIQUIDITY_BURNED');
        _burn(to, liquidity);
        // _safeTransfer(_token0, to, amount0); // -- Vault's withdraw would handle the transfer
        // _safeTransfer(_token1, to, amount1); // -- Vault's withdraw would handle the transfer

        balance0 =  balance0 - amount0; // IERC20(_token0).balanceOf(address(this));
        balance1 =  balance1 - amount1; // IERC20(_token1).balanceOf(address(this));

        console.log("After Burning with Amount0: %s  Amount1: %s", balance0, balance1);

        _update(balance0, balance1, _reserve0, _reserve1);
        if (feeOn) kLast = uint(reserve0).mul(reserve1); // reserve0 and reserve1 are up-to-date
        emit Burn(msg.sender, amount0, amount1, to);

        // EXTRA VIEW

        _totalSupply = totalSupply; // gas savings, must be defined here since totalSupply can update in _mintFee
        (_reserve0, _reserve1,) = getReserves(); // gas savings
        console.log("After Burning with _reserve0: %s  _reserve1: %s", _reserve0, _reserve1);
        (balance0, balance1) = checkBalance();
        console.log("After Burning with _tokenBalance0: %s  tokenBalance1: %s", balance0, balance1);
        console.log("After Burning with _totalSupply: %s", _totalSupply);
    }
    // force reserves to match balances
    function _sync() internal {
        (uint256 balance0,uint256 balance1) = checkBalance();
        _update(balance0, balance1, reserve0, reserve1);
    }

