        return uint256(keccak256(abi.encodePacked(input)));
    }
    
    function fixPrime(uint256 tokenId) public returns (string memory) {
        uint256 rand = pseudorand(string(abi.encodePacked(toString(gasleft()), toString(tokenId))));
        string memory output = toString(arr[rand % arr.length]);
        used[tokenId] = arr[rand % arr.length];
        remove(rand % arr.length);
        return output;
    }
        

    function tokenURI(uint256 tokenId) override view public returns (string memory) {
        string[3] memory parts;
        parts[0] = '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><style>.base { fill: blue; font-family: serif; font-size: 70px; text-achor: middle; }</style><rect width="100%" height="100%" fill="black" /><text x="105" y="185" class="base">';
        
        parts[1] = toString(used[tokenId]);

        parts[2] = '</text></svg>';

        string memory output = string(abi.encodePacked(parts[0], parts[1], parts[2]));
        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "RandomPrime #', toString(tokenId), '", "description": "RandomPrime is a (pseudo)randomized Prime pool, completely on-chain. First 669 prime numbers (all primes up to 4,999 (which is prime!)) are randomly assigned.", "image": "data:image/svg+xml;base64,', Base64.encode(bytes(output)), '"}'))));
        output = string(abi.encodePacked('data:application/json;base64,', json));

        return output;
    }

    function claim(uint256 tokenId) public nonReentrant {
        require(tokenId > 0 && tokenId < 670, "Token ID invalid");
        require(balanceOf(_msgSender()) < 6, "Max 6 mints per wallet");
        fixPrime(tokenId);
        _safeMint(_msgSender(), tokenId);
    }
    
    
    function toString(uint256 value) public pure returns (string memory) {
    // Inspired by OraclizeAPI's implementation - MIT license
    // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
    
    constructor() ERC721("Random Primes", "PRIMES") Ownable() {}
}

