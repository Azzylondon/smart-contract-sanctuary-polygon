pragma solidity ^0.8.9;

import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

contract UltimateOm is Context {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;

    address public constant OM = 0xC3Ec80343D2bae2F8E680FDADDe7C17E71E114ea;
    address public constant USDC = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    address public constant WETH = 0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619;
    uint256 public constant USDC_DECIMALS = 6;
    uint256 public constant DECIMAL_PRECISION = 4;

    IUniswapV2Router02 public constant ROUTER = IUniswapV2Router02(0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff);
    IUniswapV2Pair public constant PAIR = IUniswapV2Pair(0xFF2BBcB399aD50bbD06DebAdd47D290933ae1038);

    uint256 public immutable START_FROM;
    uint256 public immutable BET_COST;
    uint256 public immutable INTERVAL;

    event BetPlaced(
        uint256 indexed roundId,
        uint256 indexed value,
        uint256 cost,
        address indexed sender
    );
    event RoundFinished(
        uint256 indexed roundId,
        address indexed winner,
        uint256 currentPrice,
        uint256 winningPrice,
        uint256 priceDifference,
        uint256 prizePool,
        address indexed sender
    );

    struct Bet {
        uint256 cost;
        address owner;
        uint64 timestamp;
    }

    struct Round {
        bool completed;
        uint256 prizePool;
        uint256[] bets;
    }

    mapping(uint256 => Round) _rounds;
    mapping(uint256 => mapping(uint256 => Bet)) _bets;
    mapping(uint256 => mapping(address => uint256)) _totalUserBets;

    constructor(uint256 startFrom, uint256 interval, uint256 betCost) {
        START_FROM = startFrom;
        BET_COST = betCost;
        INTERVAL = interval;
    }

    function round(uint256 roundId) external view returns (Round memory) {
        return _rounds[roundId];
    }

    function bet(uint256 roundId, uint256 price) external view returns (Bet memory) {
        return _bets[roundId][formatPrice(price)];
    }

    function formatPrice(uint256 price) public pure returns (uint256) {
        uint256 ROUNDING = 10 ** (USDC_DECIMALS - DECIMAL_PRECISION);
        return (price / ROUNDING) * ROUNDING;
    }

    function totalUserBets(address account, uint256 roundId) external view returns (uint256) {
        return _totalUserBets[roundId][account];
    }

    function currentBetCost(address account, uint256 roundId) external view returns (uint256) {
        return (_totalUserBets[roundId][account] + 1) * BET_COST;
    }

    function prizePool(uint256 roundId) external view returns (uint256) {
        return _rounds[roundId].prizePool;
    }

    function roundRemainingDuration() public view returns (uint256) {
        return roundEndDate() - block.timestamp;
    }

    function roundEndDate() public view returns (uint256) {
        return (currentRoundId() + 1) * INTERVAL + START_FROM;
    }

    function currentRoundId() public view returns (uint256) {
        return (block.timestamp - START_FROM) / INTERVAL;
    }

    function currentPrice() public view returns (uint256) {
        (uint112 reserve0, uint112 reserve1,) = PAIR.getReserves();
        (uint256 omReserves, uint256 wethReserves) = PAIR.token0() == OM ? (reserve0, reserve1) : (reserve1, reserve0);

        address[] memory path = new address[](2);

        path[0] = WETH;
        path[1] = USDC;

        uint256[] memory amounts = ROUTER.getAmountsOut(1 ether, path);
        uint256 wethPrice = amounts[1];
        uint256 usdcAmount = (wethPrice * wethReserves) / 1 ether;

        return (usdcAmount * 1 ether) / omReserves; // price per OM
    }

    function finishRound(uint256 roundId) public {
        Round memory _round = _rounds[roundId];

        require(currentRoundId() > roundId, 'Round has not finished');
        require(!_round.completed, 'Round already completed');

        _rounds[roundId].completed = true;

        uint256 _totalBets = _round.bets.length;
        uint256 _currentPrice = currentPrice();

        address _winner;
        uint256 _winningPrice;
        uint256 _priceDifference;
        for (uint i = 0; i < _totalBets; ++i) {
            uint256 amount = _round.bets[i];
            uint256 difference = amount > _currentPrice ? amount - _currentPrice : _currentPrice - amount;
            if (_winner == address(0) || difference < _priceDifference) {
                _winner = _bets[roundId][amount].owner;
                _winningPrice = amount;
                _priceDifference = difference;
            }
        }

        if (_winner != address(0) && _round.prizePool > 0)
            IERC20(OM).safeTransfer(_winner, _round.prizePool);

        emit RoundFinished(
            roundId,
            _winner,
            _currentPrice,
            _winningPrice,
            _priceDifference,
            _round.prizePool,
            _msgSender()
        );
    }

    function placeBet(uint256 price) external {
        address _sender = _msgSender();
        uint256 _currentRoundId = currentRoundId();
        uint256 _roundedPrice = formatPrice(price);
        uint256 totalUserBets_ = _totalUserBets[_currentRoundId][_sender]++;

        require(_bets[_currentRoundId][_roundedPrice].owner == address(0), 'Bet already exists');

        Bet memory _bet = Bet({
            cost: BET_COST * 2 ** totalUserBets_,
            owner: _sender,
            timestamp: block.timestamp.toUint64()
        });

        require(roundRemainingDuration() >= INTERVAL / 2, 'Betting has closed for this round');

        IERC20(OM).safeTransferFrom(_sender, address(this), _bet.cost);

        _bets[_currentRoundId][_roundedPrice] = _bet;
        _rounds[_currentRoundId].prizePool += _bet.cost;
        _rounds[_currentRoundId].bets.push(_roundedPrice);

        emit BetPlaced(_currentRoundId, _roundedPrice, _bet.cost, _sender);
    }
}

