contract CompassRentalV1 is Ownable {
    using Counters for Counters.Counter;

    Counters.Counter private _rentalIdCounter;    
    
    address payable public vault;
    IERC721 public compassNFT;
    IERC20 public gold;
    IERC20 public wise;   

    mapping(uint256 => bool) public availableRent;
    mapping(uint256 => bool) public isRentingOut;
    mapping(uint256 => Rental) public rentals;
    
    mapping(uint256 => address) public renter;
    mapping(uint256 => address) public owner;

    struct Rental {
        uint256 compassId;
        address cRenter;
        address cOwner;
        bool ended;
        uint256 rentEnd;
    }

    constructor(address _vault, address _compass, address _gold, address _wise) {
        vault = payable(_vault);
        compassNFT = IERC721(_compass);
        gold = IERC20(_gold);
        wise = IERC20(_wise);
    }

    function rentOut(uint256 tokenId, uint256 rentTimeout) public {
        address caller = msg.sender;
        address operator = address(this);

        require(compassNFT.isApprovedForAll(caller, operator), "");
        require(compassNFT.ownerOf(tokenId) == caller, "");
        require(rentTimeout > block.timestamp + 86400, "Duration is at least 1d");

        uint256 duration = rentTimeout - block.timestamp;
        uint256 goldFee = duration * 1e15;

        require(gold.allowance(caller, operator) > goldFee, "Insufficient allowance");
        require(gold.balanceOf(caller) > goldFee, "Insufficient balance");
        
        gold.transferFrom(caller, address(0), goldFee); // Burn GOLD.
        compassNFT.transferFrom(caller, operator, tokenId);
        
        uint256 rentalId = _rentalIdCounter.current();
        _rentalIdCounter.increment();      

        Rental storage rental = rentals[rentalId]; 
        rental.compassId = tokenId;   
        rental.cOwner = caller;
        rental.rentEnd = rentTimeout;

        owner[tokenId] = caller;
    }


    function toRent(uint256 rentalId) public {
        address caller = msg.sender;
        Rental storage rental = rentals[rentalId]; 

        rental.cRenter = caller; 
        
        renter[rental.compassId] = caller;
    }

    function endRental(uint256 rentalId) public {
        address caller = msg.sender;
        address operator = address(this);

        Rental storage rental = rentals[rentalId];  
        uint256 tokenId = rental.compassId;

        require(block.timestamp >= rental.rentEnd, "Can only end rental after timeout");

        compassNFT.transferFrom(operator, caller, tokenId); 
        
        renter[tokenId] = address(0);
        owner[tokenId] = address(0);
    } 

    function forceEndRental(uint256 rentalId) public {
        address caller = msg.sender;
        address operator = address(this);

        Rental storage rental = rentals[rentalId];  
        uint256 tokenId = rental.compassId;

        uint256 goldFee = 100 * 1e18;

        require(gold.allowance(caller, operator) > goldFee, "Insufficient allowance");
        require(gold.balanceOf(caller) > goldFee, "Insufficient balance");    

        gold.transferFrom(caller, address(0), goldFee); // Burn 100 GOLD.    

        compassNFT.transferFrom(operator, caller, tokenId); 
        
        renter[tokenId] = address(0);
        owner[tokenId] = address(0);
    }     

}


