contract CompassNFT is ERC721, Ownable {
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;

    mapping(uint256=> uint256) public attribute01;
    mapping(uint256=> uint256) public attribute02;
    mapping(uint256=> uint256) public attribute03;
    mapping(uint256=> uint256) public attribute04;

    mapping(uint256=> uint256) public cSocket01;
    mapping(uint256=> uint256) public cSocket02;
    mapping(uint256=> uint256) public cSocket03;
    mapping(uint256=> uint256) public cSocket04;    

    mapping(uint256=> uint256) public cQuality;
    mapping(uint256=> uint256) public cLevel;
    mapping(uint256=> uint256) public cType;
    mapping(uint256=> uint256) public cMergedCount;
    mapping(uint256=> uint256) public cMergedTimestamp;
    mapping(uint256=> uint256) public cUpgradeTimestamp;

    IERC20 public gold;
    IERC20 public wise;

    event CompassUpgraded();
    event CompassMerged();

    constructor(address _gold, address _wise) ERC721("Compass NFT", "COMPASS") {
        gold = IERC20(_gold);
        wise = IERC20(_wise);
    }

    function createCompass(address _to, uint256 _cQuality, uint256 _cType) public onlyOwner {
        uint256 tokenId = _createOneCompass(_to);
        cQuality[tokenId] = _cQuality;
        cType[tokenId] = _cType;
        _setCompassAttribute(tokenId);    
    }    

    function batchCreate(address _to) public onlyOwner {
        uint tokenCountMax;
        for (uint tokenType=0; tokenType < 4;tokenType++) {
            for (uint tokenQuality=0; tokenQuality < 6;tokenQuality++) {
                if (tokenQuality == 0) {
                    tokenCountMax = 7000;
                } else if (tokenQuality == 1) {
                    tokenCountMax = 2000;
                } else if (tokenQuality == 2) {
                    tokenCountMax = 900;
                } else if (tokenQuality == 3) {
                    tokenCountMax = 90;
                } else if (tokenQuality == 4) {
                    tokenCountMax = 9;
                } else if (tokenQuality == 5) {
                    tokenCountMax = 1;
                } else {
                    tokenCountMax = 0;
                }

                for (uint tokenCount=0; tokenCount < tokenCountMax;tokenCount++) {
                    uint256 tokenId = _createOneCompass(_to);
                    cQuality[tokenId] = tokenQuality;
                    cType[tokenId] = tokenType;
                    _setCompassAttribute(tokenId);                    
                }                
            }
        }            
    }

    function _createOneCompass(address _minter) internal returns (uint256 tokenId) {
        tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(_minter, tokenId);
    }

    function _setCompassAttribute(uint256 tokenId) internal {

        cLevel[tokenId] += 1;
        cUpgradeTimestamp[tokenId] = block.timestamp;

        uint256 maxRange = 5; uint256 cQualityMultiplier = 1;
        uint256 attr01; uint256 attr02; uint256 attr03; uint256 attr04;

        if (cQuality[tokenId] > 0) {
            uint256 multip = cQuality[tokenId] + 1;
            cQualityMultiplier = multip ** multip;
        } else if (cQuality[tokenId] == 1) {
            cQualityMultiplier = 1;
        }

        attr01 = _getPRandom(maxRange) * cQualityMultiplier;
        attr02 = _getPRandom(maxRange) * cQualityMultiplier;
        attr03 = _getPRandom(maxRange) * cQualityMultiplier;
        attr04 = _getPRandom(maxRange) * cQualityMultiplier;

        if (cType[tokenId] == 0) {
            attr01 = attr01 * 2;
        } else if (cType[tokenId] == 1) {
            attr02 = attr02 * 2;
        } else if (cType[tokenId] == 2) {
            attr03 = attr03 * 2;
        } else if (cType[tokenId] == 3) {
            attr04 = attr04 * 2;
        }

        attribute01[tokenId] += attr01;
        attribute02[tokenId] += attr02;
        attribute03[tokenId] += attr03;
        attribute04[tokenId] += attr04;  
    }

    function _getPRandom(uint _maxRange) internal view returns (uint) {        
        uint randomHash = uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp, _maxRange)));
        uint num_ = randomHash % _maxRange;
        if (num_ == 0) {
            num_ = 1;
        }
        return num_;
    }

    function upgradeCompass(uint256 tokenId) public {

        address caller = msg.sender;
        uint256 goldCost; uint256 wiseCost;

        if (cLevel[tokenId] > 30) {
            wiseCost = 1e18;
        }

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        require(wise.allowance(caller, address(this)) >= wiseCost, "Not enough WISE");
        require(wise.balanceOf(caller) >= wiseCost, "Not enough WISE");      

        require(cUpgradeTimestamp[tokenId] < block.timestamp + 86400, "More than 1d"); 

        gold.transferFrom(caller, address(0), goldCost); // BURN GOLD
        wise.transferFrom(caller, address(0), wiseCost); // BURN WISE

        _setCompassAttribute(tokenId);

        cUpgradeTimestamp[tokenId] = block.timestamp;
   
    }

    function mergeCompass(uint256 fatherId, uint256 motherId) public {

        address caller = msg.sender; 
        uint256 cQualityMultiplier;       

        if (cQuality[fatherId] > 0) {
            uint256 multip = cQuality[fatherId] + 1;
            cQualityMultiplier = multip ** multip;
        } else if (cQuality[fatherId] == 1) {
            cQualityMultiplier = 1;
        }    

        uint256 goldCost = 400e18 * cQualityMultiplier; uint256 wiseCost = 1e18;    

        require(cMergedCount[fatherId] < 5, "Father merged >5");
        require(cMergedCount[motherId] < 5, "Mother merged >5");

        require(cMergedTimestamp[fatherId] > block.timestamp, "Merge father ts > now");
        require(cMergedTimestamp[motherId] > block.timestamp, "Merge mother ts > now");

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        require(wise.allowance(caller, address(this)) >= wiseCost, "Not enough WISE");
        require(wise.balanceOf(caller) >= wiseCost, "Not enough WISE");   

        gold.transferFrom(caller, address(0), goldCost); // BURN GOLD
        wise.transferFrom(caller, address(0), wiseCost); // BURN WISE  

        cMergedCount[fatherId] += 1;      
        cMergedCount[motherId] += 1;

        cMergedTimestamp[fatherId] = block.timestamp + (10 * cMergedCount[fatherId]);
        cMergedTimestamp[motherId] = block.timestamp + (10 * cMergedCount[motherId]);

        uint256 tokenId = _createOneCompass(caller);    
        cQuality[tokenId] = cQuality[fatherId];
        cType[tokenId] = cType[motherId];           
        _setCompassAttribute(tokenId);
    }

    function viewCompass(uint256 tokenId) public view returns (
        uint256 attribute01_,uint256 attribute02_,uint256 attribute03_,uint256 attribute04_,
        uint256 cQuality_,uint256 cLevel_,uint256 cType_,uint256 cMergedCount_,
        uint256 cMergedTimestamp_,uint256 cUpgradeTimestamp_) {


        attribute01_ = attribute01[tokenId];
        attribute02_ = attribute02[tokenId];
        attribute03_ = attribute03[tokenId];
        attribute04_ = attribute04[tokenId];
        cQuality_ = cQuality[tokenId];
        cLevel_ = cLevel[tokenId];
        cType_ = cType[tokenId];
        cMergedCount_ = cMergedCount[tokenId];
        cMergedTimestamp_ = cMergedTimestamp[tokenId];
        cUpgradeTimestamp_ = cUpgradeTimestamp[tokenId];
    }

    function viewCompassAttributes(uint256 tokenId) public view returns (
        uint256 attribute01_,uint256 attribute02_,uint256 attribute03_,uint256 attribute04_
    ) {
        attribute01_ = attribute01[tokenId];
        attribute02_ = attribute02[tokenId];
        attribute03_ = attribute03[tokenId];
        attribute04_ = attribute04[tokenId];        
    }  

    function viewCompassBasic(uint256 tokenId) public view returns (
        uint256 cQuality_,uint256 cLevel_,uint256 cType_
    ) {
        cQuality_ = cQuality[tokenId];
        cLevel_ = cLevel[tokenId];
        cType_ = cType[tokenId];       
    }    
 

}


