contract SoloExcursionV1 is Ownable {

    using Counters for Counters.Counter;

    Counters.Counter private _excursionIdCounter; 
    
    ICompassNFT public compassNFT;
    ICompassRentalV1 public compassRental;

    IERC20 public gold;
    IERC20 public wise;  

    uint256 public spanner;
    uint256 public intelligence;
    
    mapping(uint256 => bool) compassInitiated;
    mapping(uint256 => uint256) compassPower;
    mapping(uint256 => uint256) compassBattery;
    mapping(uint256 => uint256) compassBatteryCasingLevel;

    mapping(uint256 => bool) excursionCompleted;
    mapping(uint256 => uint256) lastExcursion;
    mapping(uint256 => uint256) excursionRestRequired;

    event CompassInitiated(uint256 indexed tokenId);
    event ExcursionStarted(uint256 indexed tokenId, uint256 indexed excursionId, address indexed caller);
    event ExcursionEnded(uint256 indexed tokenId, address indexed caller, uint256 goldReward, uint256 wiseReward);
    event BatteryPowerAdded(uint256 indexed tokenId, uint256 powerDrained);
    event BatteryCasingUpgraded(uint256 indexed tokenId, uint256 compassBatteryCasingLevel, uint256 excursionRestRequired);

    constructor(address _compass, address _gold, address _wise) {
        compassNFT = ICompassNFT(_compass);
        gold = IERC20(_gold);
        wise = IERC20(_wise);
        spanner = 1e9;
        intelligence = 1e7;
    }

    function initiateCompass(uint256 tokenId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == false, "Compass must not initiated prior");

        ( , , , uint256 battery) = compassNFT.viewCompassAttributes(tokenId);

        compassInitiated[tokenId] = true;
        compassPower[tokenId] = 10000;
        compassBattery[tokenId] = battery;
        excursionRestRequired[tokenId] = 21600;
        compassBatteryCasingLevel[tokenId] = 0;

        emit CompassInitiated(tokenId);
    }


    function startExcursion(uint256 tokenId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");
        require(block.timestamp > excursionRestRequired[tokenId] + lastExcursion[tokenId], "Not enough rest");

        uint256 excursionId = _excursionIdCounter.current();
        _excursionIdCounter.increment();      

        emit ExcursionStarted(tokenId, excursionId, caller);  

    }

    function endExcursion(uint256 tokenId, uint256 excursionId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(excursionCompleted[excursionId] == false, "Must not be able to edit excursion");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");

        (uint256 accuracy, , uint256 connectivity,) = compassNFT.viewCompassAttributes(tokenId);
        
        uint256 goldReward = spanner / 1e9 * accuracy * compassPower[tokenId] / 10000;
        uint256 wiseReward = intelligence / 1e9 * connectivity * compassPower[tokenId] / 10000; 

        (uint256 cQuality,uint256 cLevel,uint256 cType) = compassNFT.viewCompassBasic(tokenId);

        uint256 multip = cQuality + 1;
        uint256 cQualityMultiplier = multip ** multip;        

        if (cType == 3) {
            goldReward = goldReward * cQualityMultiplier / 100;
            wiseReward = wiseReward * cQualityMultiplier * (100 + cLevel) / 100;
        } else {
            goldReward = goldReward * cQualityMultiplier * (100 + cLevel) / 100;
            wiseReward = wiseReward * cQualityMultiplier / 100;
        }

        gold.transfer(caller, goldReward);
        wise.transfer(caller, wiseReward);

        excursionCompleted[excursionId] = true;
        lastExcursion[tokenId] = block.timestamp;

        emit ExcursionEnded(tokenId, caller, goldReward, wiseReward);
    }

    function addBatteryPower(uint256 tokenId) public {
        address caller = msg.sender;

        uint256 powerDrained = 10000 - compassPower[tokenId];

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");
        require(compassPower[tokenId] < 10000, "Compass Power >10000");
        
        (uint256 accuracy, , , uint256 battery) = compassNFT.viewCompassAttributes(tokenId);
        uint256 goldCost = (accuracy - battery) * powerDrained / 10000;

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        gold.transferFrom(caller, address(0), goldCost); // BURN GOLD        

        compassPower[tokenId] = 10000;

        emit BatteryPowerAdded(tokenId, powerDrained);
    }

    function updateBatteryCasing(uint256 tokenId) public {
        address caller = msg.sender;

        require(compassNFT.ownerOf(tokenId) == caller, "Must be caller");
        require(compassInitiated[tokenId] == true, "Compass must be initiated prior");

        ( , , , uint256 battery) = compassNFT.viewCompassAttributes(tokenId);
        require(battery > compassBattery[tokenId], "Battery stats didn't change");
        require((battery - compassBattery[tokenId]) > 100, "Battery stats need to change > 100");
        
        uint256 goldCost = 100e18;

        require(gold.allowance(caller, address(this)) >= goldCost, "Not enough GOLD");
        require(gold.balanceOf(caller) >= goldCost, "Not enough GOLD");

        gold.transferFrom(caller, address(0), goldCost); // BURN GOLD   

        uint256 newCompassBatteryCasingLevel = compassBatteryCasingLevel[tokenId] + 1;
        uint256 newExcursionRestRequired = 21600 - (900 * newCompassBatteryCasingLevel);

        require(newCompassBatteryCasingLevel < 24, "Maximum battery level is 23");

        compassPower[tokenId] = 10000;
        compassBattery[tokenId] = battery;
        compassBatteryCasingLevel[tokenId] = newCompassBatteryCasingLevel;
        excursionRestRequired[tokenId] = newExcursionRestRequired;

        emit BatteryCasingUpgraded(tokenId, newCompassBatteryCasingLevel, newExcursionRestRequired);
    }    

    function viewCompass(uint256 tokenId) public view returns (bool compassInitiated_, uint256 compassPower_, 
        uint256 compassBattery_, uint256 compassBatteryCasingLevel_) {
            compassInitiated_ = compassInitiated[tokenId];
            compassPower_ = compassPower[tokenId];
            compassBattery_ = compassBattery[tokenId];
            compassBatteryCasingLevel_ = compassBatteryCasingLevel[tokenId];
    }

    function viewTokenBalances() public view returns (uint256 goldBalance, uint256 wiseBalance) {
        address operator = address(this);

        goldBalance = gold.balanceOf(operator);
        wiseBalance = wise.balanceOf(operator);
    }

    function withdrawTokens(uint256 goldAmount, uint256 wiseAmount) public onlyOwner {
        address operator = address(this);
        address caller = msg.sender;
        
        uint256 goldBalance = gold.balanceOf(operator);
        uint256 wiseBalance = wise.balanceOf(operator);        

        require(goldBalance >= goldAmount, "insuffiencient GOLD");
        require(wiseBalance >= wiseAmount, "insuffiencient WISE");

        gold.transfer(caller, goldAmount);
        wise.transfer(caller, wiseAmount);
    }

    function updateValue(uint256 _spanner, uint256 _intelligence) public onlyOwner {
        spanner = _spanner;
        intelligence = _intelligence;
    }
}