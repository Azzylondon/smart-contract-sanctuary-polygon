        _addrToAKXID[_to] = akxIDHash;
        _AKXIDToAddr[akxIDHash] = _to;
        _idToAKXID[_id] = akxIDHash;
    
        emit MintedID(_to, akxIDHash, cIndex);
    }



   
    function mint(
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual returns(uint256) {
        require(hasRole(AKX_OPERATOR_ROLE, _msgSender()), "not allowed");
