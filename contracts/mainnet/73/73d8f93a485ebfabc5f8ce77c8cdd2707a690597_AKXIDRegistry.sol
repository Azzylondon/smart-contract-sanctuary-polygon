    mapping(address => string) internal _keyId;
    mapping(string => bool) internal _names;
    mapping(address => string) internal _addrToName;
    mapping(string => address) internal _nameToAddr;
    mapping(address => bytes32) internal _addrToAKXID;
    mapping(bytes32 => address) internal _AKXIDToAddr;
    mapping(uint256 => bytes32) internal _idToAKXID;
    mapping(bytes32 => uint256) internal _ids;

