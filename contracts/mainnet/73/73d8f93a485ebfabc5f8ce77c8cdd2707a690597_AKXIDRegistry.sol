import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

abstract contract AKXRoles is AccessControlEnumerable {

    function initRoles() internal {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(SYSADMIN_ROLE, _msgSender());
        _grantRole(AKX_OPERATOR_ROLE, _msgSender());
    }

   
    bytes32 internal constant AKX_OPERATOR_ROLE = keccak256("AKX_OPERATOR_ROLE");
    bytes32 internal constant AKX_SALE_CONTRACT = keccak256("AKX_SALE_CONTRACT");
    bytes32 internal constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 internal constant AKX_HOLDER_ROLE = keccak256("AKX_HOLDER_ROLE");
    bytes32 internal constant DAO_ROLE = keccak256("DAO_ROLE");
     bytes32 internal constant SYSADMIN_ROLE = keccak256("SYSADMIN_ROLE");
      bytes32 internal constant ADMIN_HELPER_ROLE = keccak256("ADMIN_HELPER_ROLE");
        bytes32 internal constant AKX_FOUNDER_ROLE = keccak256("AKX_FOUNDER_ROLE");

      modifier onlyOwner() {
        require(hasRole(AKX_OPERATOR_ROLE, msg.sender), "not allowed");
        _;
      }
