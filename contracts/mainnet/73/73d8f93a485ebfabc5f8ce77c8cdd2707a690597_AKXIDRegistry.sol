pragma solidity 0.8.17;


import "@openzeppelin/contracts/access/Ownable.sol";
import "./BytesSplitter.sol";

interface KyberPublic {
    function verify(bytes calldata data) external returns(bool);
   
}

interface DilithiumPublic {
     function verify(bytes calldata data, bytes32 signature) external returns(bool);
}


abstract contract Key is Ownable, BytesSplitter {

   slice internal _slice;
   bytes32 internal keyHash;

    function storeKey(string memory _key) internal onlyOwner {
       slice memory _s = toSlice(_key);
       _slice = _s;
    }

    function hashKey() internal returns(bytes32) {
        
        keyHash = keccak(_slice);
        return keyHash;
    }

    function validKey(string memory _key) public view returns(bool) {
        slice memory ks = toSlice(_key);
        if (compare(_slice, ks) != 0) {
            revert('invalid key');
        }
        return true;
    }

    function useKey(bytes32 _keyHash) internal view returns(string memory key) {
        if(_keyHash != keyHash) {
            revert('key not found');
        }
        slice memory _s = _slice;
        return toString(_s);
    }

}


abstract contract AKXQuantumEncryptionKeyOperator {


}

contract AKXQuantumSignatureKey is Key {
   
    bool private isSet;
    bytes private _signature;
    bytes32 private approvedSignHash;

    constructor(string memory key) {
        setKey(key);
      
    }

   function setKey(string memory _keyString) public onlyOwner {
    require(isSet != true, "key already set");
    storeKey(_keyString);
    hashKey();
    isSet = true;
   }

   function getKey(bytes32 _keyHash) public onlyOwner view returns(string memory key) {
    return useKey(_keyHash);
   }

   function signKey(bytes calldata data) public onlyOwner returns(bytes32 _sigHash) {
    bytes memory signature = abi.encodeWithSignature('signKey(bytes32, bytes)', abi.encodePacked(keccak(_slice), bytes1(0x19), bytes1(0x20), address(this), data));
    _signature = signature;
    approvedSignHash = keccak256(_signature);
    _sigHash = approvedSignHash;
   }

   function getSignedKey(bytes32 _sigHash) public view onlyOwner returns(bytes memory) {
    if(_sigHash != approvedSignHash) {
        revert('not authorized');
    }
    return _signature;
   }

}

error InvalidAKXID();

contract AKXQuantumSecurityOperator is Ownable {

    mapping(bytes32 => mapping(bytes32 => bool)) private _authkeyhashes;

    mapping(bytes32 => AKXQuantumSignatureKey) private _sigKeys;

    mapping(bytes32 => bytes32) private _keyhashes;

    mapping(bytes32 => bool) private _authorized;

    event Authorized(bytes32 akxid);


    function authorize(bytes32 akxid, bytes32 pubKeyHash) public  {
        if(_authkeyhashes[akxid][pubKeyHash] != true) {
            revert InvalidAKXID();
        }
        _authorized[akxid] = true;
        emit Authorized(akxid);
    }


    function isAuthorized(bytes32 akxid) public view returns(bool) {
        return _authorized[akxid] == true;
    }

    function retrieveFullKey(bytes32 akxid) public view onlyOwner returns(string memory __key) {
        if(!isAuthorized(akxid)) {
            revert('not authorized');
        }
        bytes memory _key = _sigKeys[akxid].getSignedKey(_keyhashes[akxid]);
        __key = abi.decode(_key, (string));

    }

    function setKeyHash(bytes32 akxid, bytes calldata data) public onlyOwner {
        string memory key = string(data);
        AKXQuantumSignatureKey _key = new AKXQuantumSignatureKey(key);
        bytes32 h = _key.signKey(data);
        _authkeyhashes[akxid][h] = true;
        _keyhashes[akxid] = h;
        _sigKeys[akxid] = _key;
    }

   




}

