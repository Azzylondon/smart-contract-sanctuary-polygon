pragma solidity 0.8.17;

import "@openzeppelin/contracts/utils/Address.sol";

library SafeAddress {
    using Address for address;

    struct Routes {
        mapping(bytes32 => bool) _routeExist;
        mapping(bytes32 => address) _route;
        mapping(bytes32 => mapping(string => bool)) _allowedToCall;
        mapping(bytes32 => bool) _publicRoute;
        mapping(bytes32 => mapping(string => bytes32)) _requiredRole;
    }

    event LogResponse(
        address indexed caller,
        address indexed callee,
        string fnName,
        bytes response
    );

    function safeCall(
        address routeAddress,
        string memory fnName,
        bytes32 route,
        bytes calldata message,
        uint256 gaslimit
    ) public returns (bytes memory response) {
        require(routeAddress.isContract(), "cannot call non contract address");
        Routes storage _routes = getStorage();
        require(_routes._route[route] == routeAddress, "invalid route");
        (bool success, bytes memory data) = _routes._route[route].call{
            gas: gaslimit
        }(abi.encodeWithSignature(fnName, message));
        require(success, "call failed");
        response = data;
    }

    function safeCall(
        address routeAddress,
        string memory fnName,
        bytes32 route,
        bytes calldata message,
        uint256 gaslimit,
        bool emitLogResponse
    ) public {
        require(routeAddress.isContract(), "cannot call non contract address");

        Routes storage _routes = getStorage();
        require(_routes._route[route] == routeAddress, "invalid route");
        (bool success, bytes memory data) = _routes._route[route].call{
            gas: gaslimit
        }(abi.encodeWithSignature(fnName, message));
        require(success, "call failed");
        if (emitLogResponse) {
            emit LogResponse(msg.sender, _routes._route[route], fnName, data);
        }
        return;
    }

    function getStorage() internal pure returns (Routes storage s) {
        bytes32 ROUTE_STORAGE_ID = keccak256(
            abi.encodePacked("akx.system.route.storage.routes")
        );
        unchecked {
            assembly {
                s.slot := ROUTE_STORAGE_ID
            }
        }
    }
}

