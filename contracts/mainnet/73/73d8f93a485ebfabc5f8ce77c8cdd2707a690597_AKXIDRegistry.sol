pragma solidity 0.8.17;

import "../PresaleContract.sol";

contract OfflinePayments is AKXRoles {

    uint public enabled;

    mapping(address => uint256) private credits;
    mapping(address => uint256) private usedCredits;
 
    address public presaleContract;

    constructor(address presale) {
        presaleContract = presale;
         initRoles();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(AKX_OPERATOR_ROLE, _msgSender());
    }

    function addCredits(address _sender, uint256 amount) public onlyRole(AKX_OPERATOR_ROLE) returns(bool) {
        credits[_sender] += amount;
        return true;
    }

    function getAvailableCredits(address _sender) public view returns(uint256) {
        uint used = usedCredits[_sender];
        uint256 _c = credits[_sender];
        return _c - used;
    }

    function getSpentCredits(address _sender) public view returns(uint256) {
         uint used = usedCredits[_sender];
         return used;
    }

    function exchangeCredits(address _sender, uint256 amount) public returns(bool) {
        if(getAvailableCredits(_sender) < amount) {
            revert("not enought credits");
        }
        PresaleContract(payable(presaleContract)).exchangeCreditsForAKX(_sender, getAvailableCredits(_sender));
        usedCredits[_sender] += amount;
        return true;
    }



}

