import "./Treasury.sol";
import {AKXMath} from "./Logic/AKXMath.sol";
import {Address} from "@openzeppelin/contracts/utils/Address.sol";

contract PresaleContract is AKXRoles, AKXMath {

    using Address for address;

/// @notice 300,000,000 AKX available for presale as stated in the White Paper
uint256 constant PRESALE_AVAILABLE_TOKENS = 300000000 ether;
/// @notice 1,200,000 AKX maximum per wallet (or 4% of circulating max) as stated in the White Paper
uint256 constant MAX_TOKENS_PER_WALLET_CIRCULATING_PERCENT = 4;

uint256 constant PRICE_FORMULA_IMMUTABLE_EXPONENT = 0.00000256 ether;
uint256 constant MANTISSA = 1e18;


uint256 public START_PRICE_ETH;
uint256 public START_PRICE_MATIC;
uint256 public presalePrice;

address public presaleToken;
AKX private _akx;

address public treasuryContract;
Treasury private _treasury;

event PresaleStarted(uint256 time);
event PresaleIsDone(uint256 time);
event TransferedToTreasury(uint256 amount);
event Bought(address indexed from, uint256 amount);
event BoughtWithCredits(address indexed from, uint256 amount);
event TransferedForFees(uint256 amount);
event TransferedForFounders(uint256 amount);
event PriceUpdated(uint256 oldPrice, uint256 newPrice);
event PresaleExtended(uint256 newEndDate);
  event Deposit(address indexed from, uint256 _value);
  uint public presaleStartDate;
  uint public presaleEnded;
  uint256 public presaleEndDate;

   constructor(address _akxToken, uint256[] memory prices)  {
        initRoles();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _grantRole(AKX_OPERATOR_ROLE, _msgSender());
        presaleToken = _akxToken;
        _akx = AKX(payable(presaleToken));
        setPrice(true, prices[0]);
        setPrice(false, prices[1]);
        uint256 _startOn = block.timestamp;
        emit PresaleStarted(_startOn);
        presaleStartDate = _startOn;
        uint256 _endDate = _startOn + 90 days;
        presaleEndDate = _endDate;
        presaleEnded = 0;        
    }

    function givePresaleExtension(uint numdays) public onlyRole(AKX_OPERATOR_ROLE) {
        uint256 _endDate = presaleEndDate + numdays * 1 days;
        presaleEndDate = _endDate;
        presaleEnded = 0; 
        emit PresaleExtended(_endDate);      
    }



    function isDone() internal {
        if(presaleEnded == 1) {
            revert("presale is over");
        }
       if(block.timestamp > presaleEndDate) {
        presaleEnded = 1;
        emit PresaleIsDone(block.timestamp);
        revert("presale is over");
       }
        if(_akx.totalSupply() == PRESALE_AVAILABLE_TOKENS) {
            presaleEnded = 1;
              emit PresaleIsDone(block.timestamp);
            revert("presale is over");
        }

    }

    function setTreasury(address _t) public onlyRole(AKX_OPERATOR_ROLE) {
        treasuryContract = _t; 
    }

    function setPrice(bool isETH, uint256 __price) internal {
        if(isETH) {
            START_PRICE_ETH = __price;
        } else {
            START_PRICE_MATIC = __price;
        }
        presalePrice = __price;
     
      
    }

    function updatePrice(uint256 price) public onlyRole(AKX_OPERATOR_ROLE) {
         uint256 _old = presalePrice;
         presalePrice = price;
         emit PriceUpdated(_old, presalePrice);
    }

   

    /// @dev will be used in phase2 only nodes can update this see white paper for the right formula
    /*


     function updatePrice() internal {
        uint256 _old = presalePrice;
       _calculatePrice();
       emit PriceUpdated(_old, presalePrice);
    }
    function _calculatePrice() internal {

        /// @dev see price formula in the white paper
        /// f(vt) = c * ((i * c) * bp)

        /// @dev there seems to be a problem with the formula on chain due to the 0.00002 need to investigate how to fix to give the right result (hence why in phase2 its automated and not in presale anymore)

        uint256 bp = presalePrice;
        uint256 i = PRICE_FORMULA_IMMUTABLE_EXPONENT;
        //uint256 c = _akx.totalSupply();

        uint256 vt = bp + i;
        if(vt <= presalePrice) {
            return;
        }
        presalePrice = vt;

    }
    */

    function buyXToken(uint256 tokenAmount) public payable {
        isDone();
         if (msg.sender.isContract()) {
            revert("cannot buy from contract");
        }
        require(msg.sender != address(0), "no zero address");
        require(msg.sender != address(this), "cannot buy from self");
        uint256 needValue = tokenAmount * presalePrice;
        require(msg.value >= needValue, "not enough value sent");
        require(_akx.totalSupply() + tokenAmount < PRESALE_AVAILABLE_TOKENS, "not enough tokens available");
        require(_akx.balanceOf(msg.sender) < 1200000 ether, "wallet is over limit");
        require(processBuyingRequest(_msgSender(), needValue, tokenAmount), "error processing buying request");
        mintForFees(tokenAmount);
        mintForFounders(tokenAmount);
        emit Bought(_msgSender(), tokenAmount);
         /// @dev will be used in phase2 only nodes can update this updatePrice();



    }

     function buyToken() public payable {
        isDone();
         if (msg.sender.isContract()) {
            revert("cannot buy from contract");
        }
        require(msg.sender != address(0), "no zero address");
        require(msg.sender != address(this), "cannot buy from self");
        uint256 tokenAmount = msg.value * 1e18  / presalePrice;
        
        require(_akx.totalSupply() + tokenAmount < PRESALE_AVAILABLE_TOKENS, "not enough tokens available");
        require(_akx.balanceOf(msg.sender) < 1200000 ether, "wallet is over limit");
        require(processBuyingRequest(_msgSender(), msg.value, tokenAmount), "error processing buying request");
        mintForFees(tokenAmount);
        mintForFounders(tokenAmount);
        emit Bought(_msgSender(), tokenAmount);
         /// @dev will be used in phase2 only nodes can update this updatePrice();



    }

    function tokensLeft() public view returns(uint256) {
        return PRESALE_AVAILABLE_TOKENS - _akx.totalSupply();
    }

    function exchangeCreditsForAKX(address _sender, uint256 tokenAmount) public onlyRole(AKX_OPERATOR_ROLE) returns(bool success) {
          isDone();
         _akx.mint(_sender, tokenAmount);
                success = true;
        mintForFees(tokenAmount);
        mintForFounders(tokenAmount);
        emit BoughtWithCredits(_msgSender(), tokenAmount);
         /// @dev will be used in phase2 only nodes can update this updatePrice();
    }

    function processBuyingRequest(address from, uint256 value, uint256 tokenAmount) internal returns(bool success) {
               payable(treasuryContract).transfer(value);
               emit TransferedToTreasury(value);
                _akx.mint(from, tokenAmount);
                success = true;
    }

    /// @notice small fees are minted for each tx in AKX does not penalize buyer but insure that minting is done right (no premint!) to respect the whitepaper allocations / token distribution
    function mintForFees(uint256 tokenAmount) internal {
        uint256 fee = getTxFee(tokenAmount);
            _akx.mint(treasuryContract, fee);
            emit TransferedForFees(fee);
    }


    /// @notice mints 4% of total supply to founders as stated in the whitepaper insure that minting is done right (no premint!) to respect the whitepaper allocations / token distribution
    function mintForFounders(uint256 tokenAmount) internal {
        uint256 amt = getPercentForFounder(tokenAmount);
        uint256 tamt = 0;
        address[] memory f = _akx.getFounders();
        for(uint i = 0; i < f.length; i++){
            _akx.mint(f[i], amt);
            tamt += amt;
        }
        emit TransferedForFounders(tamt);        
    }

      receive() external payable {
        if(msg.value <= 0) {
            revert("cannot receive zero value");
        }
        emit Deposit(msg.sender, msg.value);
    }

  


}

