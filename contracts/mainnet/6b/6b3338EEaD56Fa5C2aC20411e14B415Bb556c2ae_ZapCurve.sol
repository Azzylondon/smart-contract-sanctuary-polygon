import "./zapInterfaces/IERC20Extended.sol";


contract ZapCurve {
    using LowGasSafeMath for uint256;
    using SafeERC20 for IERC20;
    using SafeERC20 for IBeefyVault;
    address public immutable WETH;
    uint256 public constant minimumAmount = 1000;

    constructor(address _WETH) {
        // Safety checks to ensure WETH token address
        IWETH(_WETH).deposit{value: 0}();
        IWETH(_WETH).withdraw(0);
        WETH = _WETH;
    }

    receive() external payable {
        assert(msg.sender == WETH);
    }

    function beefInETH (address beefyVault, address pool, uint256 tokenAmountOutMin) external payable {
        require(msg.value >= minimumAmount, "Crypthesia: Insignificant input amount");

        IWETH(WETH).deposit{value: msg.value}();

        _stake(beefyVault, pool, tokenAmountOutMin, WETH);
    }

    function beefIn (address beefyVault, address pool, uint256 tokenAmountOutMin, address tokenIn, uint256 tokenInAmount) external {
        require(tokenInAmount >= minimumAmount, "Crypthesia: Insignificant input amount");
        require(IERC20(tokenIn).allowance(msg.sender, address(this)) >= tokenInAmount, "Crypthesia: Input token is not approved");
        IERC20(tokenIn).safeTransferFrom(msg.sender, address(this), tokenInAmount);
        _stake(beefyVault, pool, tokenAmountOutMin, tokenIn);
    }
    
    function _stake(address beefyVault, address pool, uint256 tokenAmountOutMin, address tokenIn) private {
        //check if tokenIn in pool
        IBeefyVault vault = IBeefyVault(beefyVault);
        IERC20 pair = IERC20(vault.want());
        uint256 poolSize = IStrategyCurveLP(vault.strategy()).poolSize();
        uint256 depositIndex;
        bool tokenInPool = false;
        
        for (uint256 i = 0; i < poolSize; i++){
            if (tokenIn == ICurveSwap(pool).coins(i)){
                tokenInPool = true;
                depositIndex = i;
            }
        }

        
        require(tokenInPool == true, "Crypthesia: Input token not present in pool'");
        
        _approveTokenIfNeeded(tokenIn, pool);
        uint256 fullInvestment = IERC20(tokenIn).balanceOf(address(this));
        uint256 amountLiquidityBefore = pair.balanceOf(address(this));
        if (poolSize == 2) {
            uint256[2] memory amounts;
            amounts[depositIndex] = fullInvestment;
            ICurveSwap(pool).add_liquidity(amounts, tokenAmountOutMin);
        } else if (poolSize == 3) {
            uint256[3] memory amounts;
            amounts[depositIndex] = fullInvestment;
            ICurveSwap(pool).add_liquidity(amounts, tokenAmountOutMin);
        } else if (poolSize == 4) {
            uint256[4] memory amounts;
            amounts[depositIndex] = fullInvestment;
            ICurveSwap(pool).add_liquidity(amounts, tokenAmountOutMin);
        } else if (poolSize == 5) {
            uint256[5] memory amounts;
            amounts[depositIndex] = fullInvestment;
            ICurveSwap(pool).add_liquidity(amounts, tokenAmountOutMin);
        }
        uint256 amountLiquidityAfter = pair.balanceOf(address(this));

        _approveTokenIfNeeded(address(pair), address(vault));

        // total lp after add liquidity
        vault.deposit(amountLiquidityAfter - amountLiquidityBefore);
        // transfer vault token to msg.sender
        vault.safeTransfer(msg.sender, vault.balanceOf(address(this)));
        address[] memory tokenReturn = new address[](1);
        tokenReturn[0] = tokenIn;
        _returnAssets(tokenReturn);
    }

    function beefOut(address beefyVault, address pool, uint256 withdrawAmount, address desiredToken, uint256 desiredTokenOutMin) external {
        
        IBeefyVault vault = IBeefyVault(beefyVault);
        IERC20 pair = IERC20(vault.want());
        require(IERC20(beefyVault).allowance(msg.sender, address(this)) >= withdrawAmount, "Crypthesia: Vault token is not approved");

        uint256 poolSize = IStrategyCurveLP(vault.strategy()).poolSize();
        uint256 withdrawIndex;
        bool tokenInPool = false;
        for (uint256 i = 0; i < poolSize; i++){
            if (desiredToken == ICurveSwap(pool).coins(i)){
                tokenInPool = true;
                withdrawIndex = i;
            }
        }
        // Check if token in pool
        require(tokenInPool == true, "Crypthesia: Input token not present in pool'");
        vault.safeTransferFrom(msg.sender, address(this), withdrawAmount);
        vault.withdraw(withdrawAmount);
        ICurveSwap(pool).remove_liquidity_one_coin(pair.balanceOf(address(this)), withdrawIndex, desiredTokenOutMin);
        
        address[] memory tokenReturn = new address[](1);
        tokenReturn[0] = desiredToken;
        _returnAssets(tokenReturn);
    }

    function _returnAssets(address[] memory tokens) private {
        uint256 balance;
        for (uint256 i; i < tokens.length; i++) {
            balance = IERC20(tokens[i]).balanceOf(address(this));
            if (balance > 0) {
                if (tokens[i] == WETH) {
                    IWETH(WETH).withdraw(balance);
                    (bool success,) = msg.sender.call{value: balance}(new bytes(0));
                    require(success, "Beefy: ETH transfer failed");
                } else {
                    IERC20(tokens[i]).safeTransfer(msg.sender, balance);
                }
            }
        }
    }

    function _approveTokenIfNeeded(address token, address spender) private {
        if (IERC20(token).allowance(address(this), spender) == 0) {
            IERC20(token).safeApprove(spender, type(uint256).max);
        }
    }

}

