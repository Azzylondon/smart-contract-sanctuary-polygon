pragma solidity ^0.8.0;

/**
 * @title XVault Admin Contract
 * @author XStabl Protocol Inc
 */

import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { StableMath } from "../utils/StableMath.sol";
import { IOracle } from "../interfaces/IOracle.sol";
import { IVault } from "../interfaces/IVault.sol";
import { IHarvester } from "../interfaces/IHarvester.sol";
import "./VaultStorage.sol";
import "../utils/Sort.sol";
import "../utils/Array.sol";
import { IXPoolPair } from "../interfaces/IXPoolPair.sol";
import "hardhat/console.sol";

contract VaultAdmin is VaultStorage {
    using SafeERC20 for IERC20;
    using StableMath for uint256;

    /**
     * @dev Verifies that the caller is the Vault or Governor
     */
    modifier onlyVaultOrGovernor() {
        require(
            msg.sender == address(this) ||
                isGovernor(),
            "Caller is not the Vault or Governor"
        );
        _;
    }

    /***************************************
                 Oracle Router
    ****************************************/
    function setOracleRouter(address _oracleRouter) external onlyGovernor {
        oracleRouter = _oracleRouter;
    }
    /***************************************
                 Configuration
    ****************************************/
    function setName(string memory _name, string memory _symbol)
        external
        onlyGovernor
    {
        name = _name;
        symbol = _symbol;
    }

    function setPrimaryOfKind(uint8 _kind, address _primaryAsset) external onlyGovernor {
        require(_kind >= 0, "Invalid kind");
        require(_primaryAsset != address(0), "Invalid address");
        primaryOfKinds[_kind] = _primaryAsset;
        emit PrimaryOfKindSet(_kind, _primaryAsset);
    }

    function setTokens(address _token0, address _token1) external onlyGovernor {
        require(_token0 != address(0), "Invalid address");
        require(_token1 != address(0), "Invalid address");
        token0 = _token0;
        token1 = _token1;
        tokenDecimals[0] = uint8(Helpers.getDecimals(_token0));
        tokenDecimals[1] = uint8(Helpers.getDecimals(_token1));
        emit TokensSet(_token0, _token1);
    }
    /**
     * @dev Set the combination of assets which Vault would manage
     * @param _assets Addresses of the assets which are allowed to be managed by the vault
     */
    function addMintableCombination(address[] calldata _assets)
        external
        onlyGovernor
    {
        require(_assets.length > 0, "No assets provided");
        require(_assets.length <= MAX_ASSET_COMBINATION, "Too many assets provided");

        bytes32 hash = keccak256(abi.encodePacked(_assets));
        require(!assetCombinations[hash].isSupported, "Already added");

        assetCombinations[hash] = Combination({assets: _assets, isSupported: true});
        mintableCombinations.push(hash);
        assetCombinationsCount++;
        emit AssetCombinationAdded(_assets);
    }

    function getMintableCombination(bytes32 _hash) external view returns (address[] memory) {
        if (assetCombinations[_hash].isSupported) {
            return assetCombinations[_hash].assets;
        }
    }

  

    /**
     * @dev Add a supported asset to the contract, i.e. one that can be used
     *         to mint CASH.
     * @param _asset Address of asset
     * @param _kind kind of the asset
     */
    function supportAsset(address _asset, uint8 _kind) external onlyGovernor {
        require(!assets[_asset].isSupported, "Asset already supported");
        assets[_asset] = Asset({ isSupported: true, kind: _kind});
        allAssets.push(_asset);
        emit AssetSupported(_asset);
    }

    /**
     * @dev Remove a supported asset
     * @param _asset Address of asset
     */
    function removeAsset(address _asset) external onlyGovernor {
        require(assets[_asset].isSupported, "Asset not even supported");
        assets[_asset] = Asset({ isSupported: false, kind: type(uint8).max});
        Array.remove(allAssets, _asset);
        emit AssetRemoved(_asset);
    }

    /**
     * @dev Add a strategy to the Vault.
     * @param _addr Address of the strategy to add
     */
    function approveStrategy(address _addr) external onlyGovernor {
        require(!strategies[_addr].isSupported, "Strategy already approved");
        strategies[_addr] = Strategy({ isSupported: true });
        allStrategies.push(_addr);
        emit StrategyApproved(_addr);
    }

    /**
     * @dev Remove a strategy from the Vault.
     * @param _addr Address of the strategy to remove
     */

    function removeStrategy(address _addr) external onlyGovernor {
        require(strategies[_addr].isSupported, "Strategy not approved");
        uint256 strategyIndex = Array.remove(allStrategies, _addr);
        if (strategyIndex < allStrategies.length) {
            // Mark the strategy as not supported
            strategies[_addr].isSupported = false;
            // Withdraw all assets
            IStrategy strategy = IStrategy(_addr);
            strategy.withdrawAll();
        }
        Array.remove(quickDepositStrategies, _addr);
        strategyIndex = strategyWithWeights.length;
        for (uint256 i = 0; i < strategyWithWeights.length; i++) {
            if (strategyWithWeights[i].strategy == _addr) {
                strategyIndex = i;
                break;
            }
        }

        if (strategyIndex < strategyWithWeights.length) {
            strategyWithWeights[strategyIndex] = strategyWithWeights[
                strategyWithWeights.length - 1
            ];
            strategyWithWeights.pop();
            delete strategyWithWeightPositions[_addr];
        }
        emit StrategyRemoved(_addr);
    }
    /***************************************
                    Pause
    ****************************************/
    /**
     * @dev Set the deposit paused flag to true to prevent capital movement.
     */
    function pauseCapital() external onlyGovernor {
        capitalPaused = true;
        emit CapitalPaused();
    }

    /**
     * @dev Set the deposit paused flag to false to enable capital movement.
     */
    function unpauseCapital() external onlyGovernor {
        capitalPaused = false;
        emit CapitalUnpaused();
    }


    /***************************************
             Strategies Admin
    ****************************************/

    /**
     * @dev Withdraws all assets from the strategy and sends assets to the Vault.
     * @param _strategyAddr Strategy address.
     */
    function withdrawAllFromStrategy(address _strategyAddr)
        external
        onlyGovernor
    {
        require(
            strategies[_strategyAddr].isSupported,
            "Strategy is not supported"
        );
        IStrategy strategy = IStrategy(_strategyAddr);
        strategy.withdrawAll();
    }

    /**
     * @dev Withdraws all assets from all the strategies and sends assets to the Vault.
     */
    function withdrawAllFromStrategies() external onlyGovernor {
        for (uint256 i = 0; i < allStrategies.length; i++) {
            IStrategy strategy = IStrategy(allStrategies[i]);
            strategy.withdrawAll();
        }
    }

    /*************************************
              Startegies Weights
    *************************************/
    
    function sortWeightsByTarget(StrategyWithWeight[] memory weights) internal pure returns(StrategyWithWeight[] memory) {
        uint[] memory targets = new uint[](weights.length);
        for(uint i = 0; i < weights.length; i++) {
            targets[i] = weights[i].targetWeight;
        }
        uint[] memory indices = new uint[](targets.length);
        for (uint z = 0; z < indices.length; z++) {
            indices[z] = z;
        }
        Sort.quickSort(targets, 0, int(targets.length-1), indices);
        StrategyWithWeight[] memory sorted = new StrategyWithWeight[](targets.length);
        for (uint z = 0; z < indices.length; z++) {
            sorted[z] = weights[indices[z]];
        }
        return sorted;
    }

    function setStrategyWithWeights(StrategyWithWeight[] calldata _strategyWithWeights) external onlyGovernor {
        uint256 totalTarget = 0;
        for (uint8 i = 0; i < _strategyWithWeights.length; i++) {
            StrategyWithWeight memory strategyWithWeight = _strategyWithWeights[i];
            require(strategyWithWeight.strategy != address(0), "weight without strategy");
            require(
                strategyWithWeight.minWeight <= strategyWithWeight.targetWeight,
                "minWeight shouldn't higher than targetWeight"
            );
            require(
                strategyWithWeight.targetWeight <= strategyWithWeight.maxWeight,
                "targetWeight shouldn't higher than maxWeight"
            );
            totalTarget += strategyWithWeight.targetWeight;
        }
        require(totalTarget == TOTAL_WEIGHT, "Total target should equal to TOTAL_WEIGHT");
        StrategyWithWeight[] memory sorted = sortWeightsByTarget(_strategyWithWeights);
        for (uint8 i = 0; i < sorted.length; i++) {
            _addStrategyWithWeightAt(sorted[i], i);
            strategyWithWeightPositions[strategyWithWeights[i].strategy] = i;
        }
        // truncate if need
        if (strategyWithWeights.length > sorted.length) {
            uint256 removeCount = strategyWithWeights.length - sorted.length;
            for (uint8 i = 0; i < removeCount; i++) {
                strategyWithWeights.pop();
            }
        }
    }
    function _addStrategyWithWeightAt(StrategyWithWeight memory strategyWithWeight, uint256 index) internal {
        uint256 currentLength = strategyWithWeights.length;
        // expand if need
        if (currentLength == 0 || currentLength - 1 < index) {
            uint256 additionalCount = index - currentLength + 1;
            for (uint8 i = 0; i < additionalCount; i++) {
                strategyWithWeights.push();
            }
        }
        strategyWithWeights[index] = strategyWithWeight;
    }
    function getStrategyWithWeight(address strategy) public view returns (StrategyWithWeight memory) {
        return strategyWithWeights[strategyWithWeightPositions[strategy]];
    }

    function getAllStrategyWithWeights() public view returns (StrategyWithWeight[] memory) {
        return strategyWithWeights;
    }

    /*********************************
                BRIDGE
    **********************************/
    function setBridge(address _bridge) external onlyGovernor {
        bridge = _bridge;
    }
    /*********************************
                CHILD XVAULTS
    **********************************/
    function setCXVaults(CXVault[] calldata _childXVaults) external onlyGovernor {
        // Pop out everthing from childXVaults

        // TODO: Currently expecting the allocations are sorted. May need to put sorting later
        uint256 totalTarget = 0;
        for (uint8 i = 0; i < _childXVaults.length; i++) {
            CXVault memory childXVault = _childXVaults[i];
            require(childXVault.xvault != address(0) && childXVault.chainId != 0 && childXVault.allocation != 0, "0x0 childXVault");
            totalTarget += childXVault.allocation;
            require(totalTarget >= TOTAL_WEIGHT, ">=TOTAL_WEIGHT");
            cXVaults[childXVault.chainId] = childXVault;
        }
    }

    
    /***********************************
            QuickDepositStrategies
    ************************************/
   /**
    * @dev Set the quick deposit strategies for quickAllocation of funds.
    * @param _quickDepositStrategies Array of pre-appproved startegy addresses
    */
    function setQuickDepositStrategies(address[] calldata _quickDepositStrategies) external onlyGovernor {
        for (uint8 i = 0; i < _quickDepositStrategies.length; i++) {
            require(strategies[_quickDepositStrategies[i]].isSupported, "Strategy should be supported by the Vault");
        }
        quickDepositStrategies = _quickDepositStrategies;
    }
    /**
    * @dev Get the quick deposit strategies for quickAllocation of funds.
    * @return QuickDepositStrategies Array of pre-appproved startegy addresses
    */
    function getQuickDepositStrategies() external onlyGovernor view returns (address[] memory) {
        return quickDepositStrategies;
    }

    /***********************************
                setSwapper
    ************************************/
    function setSwapper(address _swapper) external onlyGovernor {
        swapper = ISwapper(_swapper);
    }
    /***********************************
                Withdraw Lockup
    ************************************/
    function setWithdrawLockup(uint256 _duration) external onlyGovernor {
        withdrawLockup = _duration;
    }

    /***************************
              PAYOUT
    ****************************/
    /**
    * @dev Set nextPayoutTime timestamp. Should be called once at the time initialization
           Can be arbitary as it will auto set by payout()
    * @param _nextPayoutTime timestamp of next Payout
    */
    function setNextPayoutTime(uint256 _nextPayoutTime) external onlyGovernor {
        require(_nextPayoutTime > 0, "Time cannot be 0");
        nextPayoutTime = _nextPayoutTime;
    }

    /**
    * @dev Set _payoutPeriod and _payoutPeriod duration.
|    * @param _payoutPeriod Period for the payout. Ex: 24 * 60 * 60;
|    * @param _payoutTimeRange duration to honor payout time. Ex: 15 * 60;
    */
    function setPayoutIntervals(uint256 _payoutPeriod, uint256 _payoutTimeRange) external onlyGovernor {
        require((_payoutPeriod > 0) && (_payoutTimeRange > 0), "Time cannot be 0");
        payoutPeriod = _payoutPeriod;
        payoutTimeRange = _payoutTimeRange;
    }

    function payout() external {
        _payout();
    }
    function _payout() internal {
        if (block.timestamp + payoutTimeRange < nextPayoutTime) {
            return;
        }
        _balance();
        // update next payout time. Cycle for preventing gaps
        for (; block.timestamp >= nextPayoutTime - payoutTimeRange;) {
            nextPayoutTime = nextPayoutTime + payoutPeriod;
        }
    }

    /***************************
            REBALANCE
    ****************************/
    function balance() external onlyGovernor {
        _balance();
    }
    function _prepareBalance() internal returns (uint256, uint256, uint256)  {
        StrategyWithWeight[] memory strategies = getAllStrategyWithWeights();

        // 1. calc total USDC equivalent
        uint256 totalToken0 = 0;
        uint256 totalToken1 = 0;
        uint256 totalWeight = 0;

        for (uint8 i; i < strategies.length; i++) {
            if (!strategies[i].enabled) {// Skip if strategy is not enabled
                continue;
            }
            // console.log("Target Weight:", strategies[i].targetWeight);
            // UnstakeFull from Strategies with targetWeight == 0
            if(strategies[i].targetWeight == 0){
                IStrategy(strategies[i].strategy).withdrawAll();
            }else {
                (uint256 token0Bal, uint256 token1Bal) = IStrategy(strategies[i].strategy).checkBalance();
                totalToken0 += token0Bal;
                totalToken1 += token1Bal;
                totalWeight += strategies[i].targetWeight;
            }

        }
        // console.log("Prepare REBalance: Total Weight:", totalWeight);
        // console.log("Prepare REBalance: Total Token0:", totalToken0);
        // console.log("Prepare REBalance: Total Token1:", totalToken1);

        return (totalToken0, totalToken1, totalWeight);
    }
    function _balance() internal {
        StrategyWithWeight[] memory strategies = getAllStrategyWithWeights();

        IERC20 _token0 = IERC20(token0);
        IERC20 _token1 = IERC20(token1);
        address[] memory tokens = new address[](2);
        tokens[0] = address(_token0);
        tokens[1] = address(_token1);

        (uint256 totalToken0,uint256 totalToken1, uint256 totalWeight) = _prepareBalance();
        totalToken0 += _token0.balanceOf(address(this));
        totalToken1 += _token1.balanceOf(address(this));
        // console.log("Total token0 : ", totalToken0);
        // console.log("Total token1 : ", totalToken1);

        
        // 3. calc diffs for strategies liquidity
        uint8 stakeOrdersCount = 0;
        uint256 targetWeight = 0;
        Order[] memory stakeOrders = new Order[](strategies.length);
        uint256[] memory _amounts = new uint256[](2);
        uint256[] memory targetLiquidity = new uint256[](2);
        uint256[] memory currentLiquidity = new uint256[](2);
        uint256[] memory ratio = new uint256[](2);
        IStrategy strategyImp;
        for (uint8 i; i < strategies.length; i++) {
            if (!strategies[i].enabled) {// Skip if strategy is not enabled
                continue;
            }
            targetWeight = strategies[i].targetWeight;

            strategyImp = IStrategy(strategies[i].strategy);
            (currentLiquidity[0], currentLiquidity[1]) = strategyImp.checkBalance();
            (ratio[0], ratio[1]) = strategyImp.getRatios();
            // console.log("checkBalance", currentLiquidity[0], currentLiquidity[1]);
            // console.log("ratio", ratio[0], ratio[1]);
            (targetLiquidity[0], targetLiquidity[1]) = _calculateTarget(tokens, totalToken0, totalToken1, ratio, targetWeight, totalWeight);
    
            console.log("targetLiquidity", targetLiquidity[0], targetLiquidity[1]);
            if (targetLiquidity[0] == currentLiquidity[0] && targetLiquidity[1] == currentLiquidity[1]) {
                // skip already at target strategies
                continue;
            }

            if (targetLiquidity[0] < currentLiquidity[0] || targetLiquidity[1] < currentLiquidity[1]) {
                // unstake now
                console.log("REBALANCE Withdraw now amount from", address(strategyImp));
                _amounts[0] = currentLiquidity[0] - targetLiquidity[0];
                _amounts[1] = currentLiquidity[1] - targetLiquidity[1];
                console.log("REBALANCE", _amounts[0], _amounts[1]);
                console.log("REBALANCE", _amounts[0]/10**18, _amounts[1]/10**6);
                strategyImp.withdraw(
                    address(this),
                    tokens,
                    _amounts
                );
            } else {
                console.log("REBALANCE Deposit later amount from", address(strategyImp));
                _amounts[0] = targetLiquidity[0] - currentLiquidity[0];
                _amounts[1] = targetLiquidity[1] - currentLiquidity[1];
                console.log("REBALANCE", _amounts[0], _amounts[1]);
                console.log("REBALANCE", _amounts[0]/10**18, _amounts[1]/10**6);
                // save to stake later
                stakeOrders[stakeOrdersCount] = Order(
                    true,
                    address(strategyImp),
                    _amounts
                );
                stakeOrdersCount++;
            }
        }
        _stakeOrders(stakeOrders, stakeOrdersCount);

    }
    function _calculateTarget(address[] memory tokens, uint256 totalToken0, uint256 totalToken1, uint256[] memory ratio, uint256 targetWeight, uint256 totalWeight)
            internal view returns (uint256, uint256) {
        if (targetWeight > 0) {
            uint256[2] memory decimals = [Helpers.getDecimals(tokens[0]), Helpers.getDecimals(tokens[1])];
            uint256 minTokenValue = (totalToken0.scaleBy(18, decimals[0]) > totalToken1.scaleBy(18, decimals[1])) ? totalToken1.scaleBy(18,  decimals[1]) : totalToken0.scaleBy(18,  decimals[0]);
            minTokenValue = minTokenValue * targetWeight / totalWeight;
            console.log("Min Token Value: ", minTokenValue);
            console.log("Ratio: ", ratio[0], ratio[1]);
            uint256 targetLiquidity0;
            uint256 targetLiquidity1;
            if (ratio[0] < ratio[1]) {
                targetLiquidity0 = (minTokenValue * ratio[0] ) / (100 * 10**18);
                targetLiquidity1 = targetLiquidity0 + (targetLiquidity0 * (ratio[1] - ratio[0]) / (100 * 10**18));
                console.log("R0 < R1 : _calculateTarget", targetLiquidity0.scaleBy(decimals[0], 18), targetLiquidity1.scaleBy(decimals[1], 18));
            } else if (ratio[1] < ratio[0]) {
                targetLiquidity1 = (minTokenValue * ratio[1]  ) / (100 * 10**18);
                targetLiquidity0 = targetLiquidity1 + (targetLiquidity1 * (ratio[0] - ratio[1]) / (100 * 10**18));
                console.log("R0 > R1 : _calculateTarget", targetLiquidity0.scaleBy(decimals[0], 18), targetLiquidity1.scaleBy(decimals[1], 18));

            } else {
                targetLiquidity1 = (minTokenValue * ratio[1]) / (100 * 10**18);
                targetLiquidity0 = targetLiquidity1;
                console.log("R0 = R1 : _calculateTarget", targetLiquidity0.scaleBy(decimals[0], 18), targetLiquidity1.scaleBy(decimals[1], 18));

            }


            

            return (targetLiquidity0.scaleBy(decimals[0], 18), targetLiquidity1.scaleBy(decimals[1], 18));
        }
        return (0,0);
    }
    function _stakeOrders(Order[] memory stakeOrders, uint256 stakeOrdersCount) internal {
        IERC20 _token0 = IERC20(token0);
        IERC20 _token1 = IERC20(token1);
        address[] memory tokens = new address[](2);
        tokens[0] = address(_token0);
        tokens[1] = address(_token1);

        // console.log("Vault Total token0 : ", _token0.balanceOf(address(this)));
        // console.log("Vault Total token1 : ", _token1.balanceOf(address(this)));
        // 4.  make staking
        for (uint8 i; i < stakeOrdersCount; i++) {

            address strategy = stakeOrders[i].strategy;
            // console.log("Processing stake order of", strategy );
            // console.log("-- amounts ", stakeOrders[i].amounts[0], stakeOrders[i].amounts[1]);

            if (_token0.balanceOf(address(this)) < stakeOrders[i].amounts[0]) {
                stakeOrders[i].amounts[0] = _token0.balanceOf(address(this));
            }
            if (_token1.balanceOf(address(this)) < stakeOrders[i].amounts[1]) {
                stakeOrders[i].amounts[1] = _token1.balanceOf(address(this));
            }
            _token0.transfer(strategy, stakeOrders[i].amounts[0]);
            _token1.transfer(strategy, stakeOrders[i].amounts[1]);

            IStrategy(strategy).deposit(
                tokens,
                stakeOrders[i].amounts
            );
        }
    }
}

