pragma solidity ^0.8.9;





// File contracts/test.sol

contract Yazuka is ERC721A, Ownable, ReentrancyGuard {
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    string private baseURI = "";
    uint256 private WLPrice = 6 ether;
    uint256 private PresalePrice = 9 ether;
    uint256 private price = 90 ether;
    uint256 private presaleAmount;
    uint256 private whitelistAmount;
    string private notRevealedUri;
    uint256 private MAX_SUPPLY = 10000;
    string private constant baseExtension = ".json";
    EnumerableMap.AddressToUintMap private holdersMap;
    bool public paused = false;
    bool public revealed = false;
    address[] public whitelist;
    uint256 public WLEndTime;
    uint256 public PresaleEndTime;

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _initBaseURI,
        string memory _initNotRevealedUri,
        uint256 _WLSaleDuration,
        uint256 _presaleDuration,
        uint256 _presaleAmount,
        uint256 WLAmount
    ) ERC721A(_name, _symbol) {
        setBaseURI(_initBaseURI);
        setNotRevealedURI(_initNotRevealedUri);
        WLEndTime = block.timestamp + _WLSaleDuration;
        PresaleEndTime = WLEndTime + _presaleDuration;
        presaleAmount = _presaleAmount;
        whitelistAmount = WLAmount;
    }

    function mint(uint256 _amount) external payable {
        address _caller = msg.sender;
        require(!paused, "Paused");
        require(MAX_SUPPLY >= totalSupply() + _amount, "Exceeds max supply");
        require(_amount > 0, "No 0 mints");
        require(tx.origin == _caller, "No contracts");
        uint256 currPrice = currentPrice();
        uint256 currSupply = totalSupply();
        uint256 currBalance = balanceOf(_caller);
        if (_caller != owner()) {
            if (block.timestamp <= WLEndTime) {
                require(isWhitelisted(_caller), "user is not whitelisted");
                require(
                    currSupply + _amount <= whitelistAmount,
                    "Whitelist is Exhausted"
                );
            }
            if (block.timestamp < PresaleEndTime) {
                require(currSupply + _amount <= presaleAmount + whitelistAmount, "Presale is Exhausted");
            }

            require(_amount * currPrice == msg.value, "Invalid funds provided");
            
        }

        _safeMint(_caller, _amount);
        if (contains(_caller)) {
            uint256 newBalance = currBalance + _amount;
            set(_caller, newBalance);
        } else {
            uint256 newBalance = _amount;
            set(_caller, newBalance);
        }

    }

    function updateHoldersMap() public {
        uint256 currSupply = totalSupply();
        for (uint256 i = 0; i< currSupply;i++) {
            address currWallet = ownerOf(i);
            uint256 currWalletBalance = balanceOf(currWallet);
            if (!contains(currWallet)) {
                set(currWallet, currWalletBalance);
            }
        }
    }



    //view functions

    function isApprovedForAll(address owner, address operator)
        public
        view
        override
        returns (bool)
    {
        return super.isApprovedForAll(owner, operator);
    }

    

    function isWhitelisted(address _user) public view returns (bool) {
        for (uint256 i = 0; i < whitelist.length; i++) {
            if (whitelist[i] == _user) {
                return true;
            }
        }
        return false;
    }

    function getRandomWinners(uint256 _winners)
        public
        view
        onlyOwner
        returns (address[] memory)
    {
        address[] memory winners = new address[](_winners);
        for (uint256 i = 0; i < _winners; i++) {
            (address randomWinner, ) = selectRandomWinner(i);
            uint256 winnerBalance = balanceOf(randomWinner);
            if (winnerBalance >= 6) {
                winners[i] = randomWinner;
            }
        }
        return winners;
    }

    function currentPrice() public view returns (uint256) {
        if (block.timestamp <= WLEndTime) {
            return WLPrice;
        } else if (block.timestamp <= PresaleEndTime) {
            return PresalePrice;
        } else {
            return price;
        }
    }

    function currentStageEndsAt() public view returns(uint256) {
        if (block.timestamp <= WLEndTime) {
            return WLEndTime;
        } else if (block.timestamp <= PresaleEndTime) {
            return PresaleEndTime;
        } else {
            return 0;
        }
    }

    function currentStage() public view returns (uint256) {
         if (block.timestamp <= WLEndTime) {
            return 1;
        } else if (block.timestamp <= PresaleEndTime) {
            return 2;
        } else {
            return 3;
        }
    }

    function getMintableSupply() public view returns (uint256) {
        if (block.timestamp <= WLEndTime) {
            return whitelistAmount;
        } else if (block.timestamp <= PresaleEndTime) {
            uint256 __amount = whitelistAmount + presaleAmount;
            return __amount;
        } else {
            return MAX_SUPPLY;
        }
    }

    function getGoldenWinners() public view returns(address[] memory) {
        uint256 iters = length();
        address[] memory winnerList = new address[](iters);
        uint256 winnersAdded = 0;
        for (uint256 i = 0; i < iters; i++) {
            (address currWallet,) = at(i);
            uint256 ownerBalance = balanceOf(currWallet);
            if (ownerBalance >= 4) {
                winnerList[winnersAdded] = currWallet;
                winnersAdded++;

            }
        }
        return winnerList;
    }

    function getWallet(address _owner) public view returns (uint256[] memory) {
        uint256 ownerBalance = balanceOf(_owner);
        uint256[] memory ownerWallet = new uint256[](ownerBalance);
        uint256 tokensAdded = 0;
        uint256 currSupply = totalSupply();
        for (uint256 i = 0; i < currSupply; i++) {
            address currOwner = ownerOf(i);
            if (currOwner == _owner) {
                ownerWallet[tokensAdded] = i;
                tokensAdded++;
            }
        }
        return ownerWallet;
    }




    //holder Functions
    function contains(address key) public view returns (bool) {
        return holdersMap.contains(key);
    }

    function set(address key, uint256 value) private {
        holdersMap.set(key, value);
    }

    function remove(address key) private {
        holdersMap.remove(key);
    }

    function length() public view returns (uint256) {
        return holdersMap.length();
    }

    function at(uint256 index)
        public
        view
        returns (address key, uint256 value)
    {
        return holdersMap.at(index);
    }

    function tryGet(address key) private view returns (bool, uint256) {
        return holdersMap.tryGet(key);
    }

    function get(address key) public view returns (uint256) {
        return holdersMap.get(key);
    }

    //onlyOwner functions

    function airdrop(address _to, uint256 _amount) public onlyOwner {
        _safeMint(_to,_amount);
        uint256 newBalance = balanceOf(_to);
        if (contains(_to)) {
            
            set(_to, newBalance);
        } else {
            
            set(_to, newBalance);
        }
    }

    function setPresaleAmount(uint256 newAmount) public onlyOwner {
        presaleAmount = newAmount;
    }

    function setWhitelistAmount(uint256 newAmount) public onlyOwner {
        whitelistAmount = newAmount;
    }


    function reveal() public onlyOwner {
        revealed = true;
    }

    function withdraw() external onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function selectRandomWinner(uint256 targetBlock)
        public
        view
        onlyOwner
        returns (address key, uint256 value)
    {
        uint256 totalHolders = length();
        uint256 blockTarget = block.number - targetBlock;
        uint256 randomSelector = ((uint256(blockhash(blockTarget)) +
            block.timestamp) % totalHolders);
        return at(randomSelector);
    }

    function setPrice(uint256 _newCost) public onlyOwner {
        if (block.timestamp <= WLEndTime) {
            WLPrice = _newCost;
        } else if (block.timestamp <= PresaleEndTime) {
            PresalePrice = _newCost;
        } else {
            price = _newCost;
        }
    }

    function extendSalesTime(uint256 _newWLTime,uint256 _newPresaleTime) public {
        WLEndTime = _newWLTime;
        PresaleEndTime = _newPresaleTime;
    }

    function setMaxSupply(uint256 _newSupply) public onlyOwner {
        MAX_SUPPLY = _newSupply;
    }

    

    function whitelistUsers(address[] calldata _users) public onlyOwner {
        delete whitelist;
        whitelist = _users;
        
    }

    function whitelistSingleUser(address _user) public onlyOwner {
        whitelist.push(_user);
    }

    function setupOS() external onlyOwner {
        _safeMint(_msgSender(), 1);
        updateHoldersMap();
    }

    function pause(bool _state) external onlyOwner {
        paused = _state;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    function setNotRevealedURI(string memory _notRevealedURI) public onlyOwner {
        notRevealedUri = _notRevealedURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        if (revealed == false) {
            return notRevealedUri;
        }

        string memory currentBaseURI = baseURI;
        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        Strings.toString(tokenId),
                        baseExtension
                    )
                )
                : "";
    }
}