        require(isPreSalePaused == false, "SALE IS NOT ACTIVE AT THE MOMENT");
        require(totalSupply() + chosenAmount <= ( _TOTALSUPPLY - reservedTokens ) && ( totalSupply() + chosenAmount ) <= preSaleSupply, "QUANTITY MUST BE LESS THEN MAX SUPPLY");
        require(chosenAmount <= preSaleMaxQuantity && chosenAmount > 0, "CHOSEN AMOUNT SHOULD BE GREATER THAN 0 & LESS THAN MAX QUANTITY");
        require(_preSaleAddresses[msg.sender].counter + chosenAmount <= preSaleMaxQuantity, "QUANTITY MUST BE LESS THAN MAX SUPPLY PER WALLET");
        require(preSalePrice.mul(chosenAmount) == msg.value, "SENT ETHER VALUE IS INCORRECT");
        bytes32 leaf = keccak256(abi.encodePacked(msg.sender));
        require(MerkleProof.verify(_merkleProof, PreSaleRootHash, leaf), "INVALID PROOF");
        for (uint i = 0; i < chosenAmount; i++) {
            _safeMint(msg.sender, totalsupply());
            tokenId++;
        }
        _preSaleAddresses[msg.sender].counter += chosenAmount;
    }
    function mintReservedTokens(uint quantity) public payable onlyOwner {
        require(reserveId + quantity <= _TOTALSUPPLY + 1, "The quantity exceeds the reserve.");
        for (uint i = 0; i < quantity; i++) {
            _safeMint(msg.sender, reserveId);
            reserveId++;
        }
    }
    function freeMint(uint quantity) public payable onlyOwner {
        require(totalSupply() + quantity <= (_TOTALSUPPLY - reservedTokens), "QUANTITY MUST BE LESS THEN MAX SUPPLY");
        for (uint i = 0; i < quantity; i++) {
            _safeMint(msg.sender, totalsupply());
            tokenId++;
        }
    }

    // To Get The Token Of Owner
    function tokensOfOwner(address _owner) public view returns (uint256[] memory) {
        uint256 count = balanceOf(_owner);
        uint256[] memory result = new uint256[](count);
        for (uint256 index = 0; index < count; index++) {
            result[index] = tokenOfOwnerByIndex(_owner, index);
        }
        return result;
    }

    // Function To Withdraw Amount From Contract
    function withdraw() public onlyOwner {
        uint totalBalance = address(this).balance;
        payable(msg.sender).transfer(totalBalance);
    }
}