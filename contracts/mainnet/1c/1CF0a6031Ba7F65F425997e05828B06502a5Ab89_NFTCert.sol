pragma solidity ^0.8.7;

import "./libs/NFTreasuryLib.sol";
import "./interfaces/INFTicket.sol";
import "./interfaces/INFTCert.sol";
import "./interfaces/INFTServiceTypes.sol";

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol"; // For initializer modifier
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";


contract NFTicket is Initializable, ERC721URIStorageUpgradeable, IERC721ReceiverUpgradeable, INFTicket {
    using NFTreasuryLib for NFTreasuryLib.Treasury;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.UintToAddressMap;
    
    address private owner;
    EnumerableSetUpgradeable.AddressSet _whitelistSet;

    NFTreasuryLib.Treasury Treasury;
    CountersUpgradeable.Counter private _tokenIds;
    
    mapping(uint256 => string) _tokenURIs;
    mapping(string => DataSchema) _dataSchemata;
    mapping(bytes32 => DataSchema) _schemaHashes;

    mapping(address => bool) _providerIsRegistered;


    // constructor() ERC721("NFTicket BLXM", "NTC") { 
    //     owner == msg.sender;
    //     Treasury.TRANSACTIONFEE = 15;
    //     Treasury.ratioBASE = 1000;
    // }

    function initialize() initializer public {
        __ERC721_init("NFTicket BLXM", "NTC");
        owner == msg.sender;
        Treasury.TRANSACTIONFEE = 15;
        Treasury.ratioBASE = 1000;
    }


    event evtTicketSubmitted(address _contract, uint256 ticketID, uint256 serviceType, uint256 deductedFee );
    event evtSplitRevenue(uint256 indexed newTicketID, uint256 value, uint256 serviceFee, uint256 resellerFee, uint256 transactionFee);
    event evtSchemaRegistered(string name, DataSchema schema);
    event evtConsumedCredits(uint256 indexed _tID, uint256 creditsConsumed, uint256 creditsRemain, DataRecords);
    event evtRegisterServiceProvider(address indexed serviceProvideContract, bytes4 serviceDescriptor, uint16 status);
    event exceptionWrongSender(address sender, address expectedSender, string message);
    event exceptionInsufficientPayment(uint256 value, uint256 credits, uint256 pricePerCredit);

    /*****************************
    * @param _operator The address which called `safeTransferFrom` function
    * @param _from The address which previously owned the token
    * @param _tokenId The NFT identifier which is being transfered
    * @param data Additional data with no specified format
    * @return `bytes4(keccak256("onERC721Received(address,uint256,bytes)"))`
    *******************************/
    function onERC721Received(address operator, address from, uint256 _tokenID, bytes calldata data ) public override returns (bytes4) {
    
    return this.onERC721Received.selector;
    }

    function extractSchemaFromData(DataRecords calldata _calldata) 
        public
        returns(bytes32) 
    {
        DataSchema memory _schema;
        bytes32 schemaHash;

        _schema = _calldata._schema;

        _dataSchemata[_schema.name] = _schema;
        _schemaHashes[schemaHash] = _schema;

        emit evtSchemaRegistered(_schema.name, _schema);
        return(schemaHash);
    }

    function registerServiceProvider(address serviceProvider, bytes4 serviceDescriptor ) 
        external
        override
        returns(uint16 status)
    {
        require(_whitelistSet.contains(msg.sender), "Unauthorized Access");
        status = Treasury.registerServiceProvider(serviceProvider, serviceDescriptor);
        emit evtRegisterServiceProvider(serviceProvider, serviceDescriptor, status);
        return(status);
    }
    function isRegisteredServiceProvider(address serviceProvider)
        external
        override
        view
        returns(bool)
    {
        return Treasury.isRegisteredServiceProvider(serviceProvider);
    }
    function getServiceDescriptor(address serviceProvider)
        external
        view
        returns(bytes4 descriptor)
    {
        bytes4 _descriptor = Treasury.getServiceDescriptor(serviceProvider);
        return(_descriptor);     
    }

    function registerResellerServiceProvider(address serviceProvider, address reseller)
        external
        override
        returns(uint16 status) 
    {
        require(_whitelistSet.contains(msg.sender), "Unauthorized Access");
        if(! Treasury.isRegisteredServiceProvider(serviceProvider)) {
            status = 403;
        }
        else {
            status = Treasury.registerServiceReseller(serviceProvider, reseller);
        }
        return(status);
    }
    function getServiceProviderReseller(address serviceProvider)
        external
        override
        view
        returns(address)
    {
        return Treasury.getServiceProviderReseller(serviceProvider);
    }

    function consumeCredits(address serviceProvider, uint256 _tokenID, uint256 _consumeCredits, DataRecords calldata _energyRequests) 
        external 
        override
        returns(uint256 creditsConsumed, uint256 creditsRemain, DataRecords memory consumed)
    {
        
        require(Treasury._ticketData[_tokenID].credits - _consumeCredits >= 0, "not enough credits left on Ticket");
        
        uint256 _serviceProvider = uint256(uint160(serviceProvider));
        address reseller;
        if (Treasury.serviceProvidersReseller.contains(uint256(_serviceProvider))) {
            reseller = Treasury.serviceProvidersReseller.get(uint256(_serviceProvider));
        }
        if ( reseller != address(0)) {
            if (msg.sender != reseller) {
                emit exceptionWrongSender(msg.sender, reseller, "we need this to come from a valid reseller");
                return(0, creditsRemain, consumed);
            }
        }

        // TODO this needs to be INFTReseller, which NFTCert then implements
        (creditsConsumed, consumed) = INFTCert(serviceProvider).consumeCredits(Treasury._ticketData[_tokenID], _consumeCredits, _energyRequests);
        
        Treasury._ticketData[_tokenID].credits -= creditsConsumed;
        creditsRemain = Treasury._ticketData[_tokenID].credits;
        emit evtConsumedCredits(_tokenID, creditsConsumed, creditsRemain, consumed);
        return(creditsConsumed, creditsRemain, consumed);
    }



    function getTicketData(uint256 _ticketID) public override view returns(Ticket memory)
    {
        // TODO hb220710 verify calling party is whitelisted for this ticket
        return(Treasury._ticketData[_ticketID]);

    }

    function updateTicketData(uint256 _ticketID, Ticket memory _t) public override returns(bool)
    {
        Treasury._ticketData[_ticketID] = _t;
        return(true);
    }

    function tokenURI(uint256 _tokenID) 
        public
        view 
        override
        returns(string memory) {
            return(_tokenURIs[_tokenID]);
    }

    /***************************
    *
    * @author bloXmove silverlynx
    * @notice this is where an NFT is minted, metadata is set and internal Ticket state is initialized
    * @dev this function needs to call NFTicket.mintNFTicket() in the end
    * @param Ticket _ticket a structure containing {issuedTo, credits, pricePerCredit and the three fees: service,reseller and transaction}
    * @param buyNFTicketParams params a structure containing {reseller, serviceProvider, recipient, tokenURI}
    * @return (newTicketID, transactionFee, feeSplitRatio) ticket and info on transactionFee
    *
    ****************************/
    function mintNFTicket(Ticket memory _ticket, buyNFTicketParams memory params)
        external
        override
        payable 
        returns (uint256 newTicketID, uint256 transactionFee, uint256 feeSplitRatio)
    {
        // TODO require() that reseller is either a registered reseller or serviceProvider;
        newTicketID = _tokenIds.current();

        if ( msg.value < (_ticket.credits * _ticket.pricePerCredit)) {
            // TODO 
            // emit exceptionInsufficientPayment(msg.value, _ticket.credits, _ticket.pricePerCredit);
        }
        
        (transactionFee, feeSplitRatio) = Treasury.splitRevenue(_ticket, msg.value, params.reseller, params.serviceProvider, params.recipient);
        Treasury.registerTicket(params.reseller, params.serviceProvider, newTicketID,_ticket);

        emit evtSplitRevenue(newTicketID, msg.value, _ticket.serviceFee, _ticket.resellerFee, transactionFee);
        _mint(params.recipient, newTicketID);
        _setTokenURI(newTicketID, params.tokenURI);
        _tokenURIs[newTicketID] = params.tokenURI;
        _tokenIds.increment();

        return (newTicketID, transactionFee, feeSplitRatio);
    }

    function addWhitelist(address _address)
        external
        override 
    {
        _whitelistSet.add(_address);
    }

    function removeWhitelist(address _address)
        external
        override
    {
        require(msg.sender == owner, "only owner can de-whitelist");
        _whitelistSet.remove(_address);
    }

    function inWhitelist(address _address)
        external
        view
        override
        returns (bool)
    {
        return _whitelistSet.contains(_address);
    }

}

