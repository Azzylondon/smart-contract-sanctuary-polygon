pragma solidity ^0.8.7;

import "../interfaces/INFTicket.sol";

import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";

library NFTreasuryLib {

    using SafeMathUpgradeable for uint;
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.UintToAddressMap;

    event exceptionFeesInsufficientValue(uint256 value,uint256 serviceFee, uint256 resellerFee, uint256 transactionFee);

    /**********************************
    *
    * manage funds:
      _bank: overall balance
      _tokenBalance: funds available per token
      _contractBalance: total balance for each of registered reseller or service provider
    *
    ***********************************/

    struct Treasury {
        uint256 TRANSACTIONFEE; // promil, i.e. 1.5%
        uint256 ratioBASE;
        mapping(uint256 => uint256) _tokenBalance;
        mapping(address => uint256) _contractBalance;
        mapping(address => uint256) bank;
        mapping(uint256 => Ticket)  _ticketData; // look up the specific Ticket data

        mapping(address => bytes4)  _providers;
        mapping(string => address)   _providerAddress;
        mapping(string => address)  _resellerAddress;

        /*
        * TODO
        * only have 1:1 mapping between reseller and provider for now. 
        * Need to extends this to m:n or at least 
        * 1:n 1 reseller having several service provider
        */
        EnumerableMapUpgradeable.UintToAddressMap resellersServiceProvider;
        EnumerableMapUpgradeable.UintToAddressMap serviceProvidersReseller;

        mapping(address => RevenuePool) serviceProviderAccount;
    }

    struct RevenuePool {
        address userAccount; // user's (owner of the NFT) account
        address resellerAccount; // reseller's wallet
        address serviceProviderAccount; // service provider's wallet
        uint256 userPool; // user wallet
        uint256 resellerPool; // acc revenue pool for reseller
        uint256 serviceProviderPool; // acc revenue pool for service provider
    }

    // provider by name by address


    function isRegisteredServiceProvider(Treasury storage _treasury, address serviceProvider)
        internal
        view
        returns(bool)
    {
        uint256 _serviceProvider = uint256(uint160(serviceProvider));
        return(_treasury.serviceProvidersReseller.contains(_serviceProvider));
    }
    function registerServiceProvider(Treasury storage _treasury, address serviceProvider, bytes4 providerDescriptor) 
        internal
        returns(uint16 status)
    {
        uint256 _serviceProvider = uint256(uint160(serviceProvider));
        if(_treasury.serviceProvidersReseller.contains(_serviceProvider)) {
            // this provider is already registered
            // TODO CRUD?
            return(403);
        }
        else {
        // register self as counterparty both as reseller as well as serviceProvider
        // this means this is a serviceProvider without a reseller
                _treasury._providers[serviceProvider] = providerDescriptor;
                _treasury.serviceProvidersReseller.set(_serviceProvider,serviceProvider);
                _treasury.resellersServiceProvider.set(_serviceProvider,serviceProvider);
        }
        return(200);
    }
    function getServiceProviderReseller(Treasury storage _treasury, address _serviceProvider)
        internal
        view
        returns(address)
    {
        uint256 serviceProvider = uint256(uint160(_serviceProvider));
        return _treasury.serviceProvidersReseller.get(serviceProvider);
    }

    function getServiceDescriptor(Treasury storage _treasury, address serviceProvider)
        internal
        view
        returns(bytes4 descriptor)
    {
        return _treasury._providers[serviceProvider];
        
    }
    
    function registerServiceReseller(Treasury storage _treasury, address _serviceProvider, address _reseller)
        internal
        returns(uint16 status)
    {
        uint256 serviceProvider = uint256(uint160(_serviceProvider));
        uint256 reseller = uint256(uint160(_reseller));
        _treasury.resellersServiceProvider.set(reseller,_serviceProvider);
        _treasury.serviceProvidersReseller.set(serviceProvider,_reseller);
        return(200);
    }

    function registerTicket(Treasury storage _treasury, address reseller, address serviceProvider, uint256 newTicketID, Ticket memory _ticket ) 
        internal
    {
            _treasury._ticketData[newTicketID] = _ticket;

    }

    /*****************************************
    *
    * TODO: this should be called once to determine the main values
    * then just read the values and split the revenue
    * strong case to be made to use ERC155 here, because all tokens with the same ID can share same formula
    * but we are free to create different token classes identified by their tokenIDs
    *
    ******************************************/
    function splitRevenue(Treasury storage _treasury, Ticket memory _ticket, uint256 value, address reseller, address serviceProvider, address owner)
        internal
        returns(uint256 transactionFee, uint256 r)
    {
       
        uint256 f = _ticket.resellerFee.add(_ticket.serviceFee);

        // r is the ratio of reseller vs. total in per thousand, e.g 100 in case of 90 service and 10 reseller fee
        r = _ticket.resellerFee.mul(_treasury.ratioBASE);
        r = r.div(f);
        
        // total transaction fee = _treasury.TRANSACTIONFEE * (f = resellerFee + serviceFee)
        transactionFee = f.mul(_treasury.TRANSACTIONFEE);
        transactionFee = transactionFee.div(_treasury.ratioBASE);
        _treasury.bank[address(this)] += transactionFee;

        // transactionFee is split among reseller and service provider along
        // reseller: r * transactionFee
        // service provider: (1-r) * transactionFee
        uint256 resellerTransaction = transactionFee.mul(r);
        resellerTransaction = resellerTransaction.div(_treasury.ratioBASE);
        _ticket.resellerFee = _ticket.resellerFee.sub(resellerTransaction);
        uint256 serviceTransaction = transactionFee.sub(resellerTransaction);
        _ticket.serviceFee = _ticket.serviceFee.sub(serviceTransaction);

        if(value < (_ticket.resellerFee + _ticket.serviceFee + transactionFee)) {
            emit exceptionFeesInsufficientValue(value,_ticket.serviceFee, _ticket.resellerFee, transactionFee);
        }


        RevenuePool memory rp;

        rp.userAccount = owner;
        rp.resellerAccount = reseller;
        rp.serviceProviderAccount = serviceProvider;
        rp.resellerPool += _ticket.resellerFee;
        rp.userPool += _ticket.serviceFee;

        _treasury.serviceProviderAccount[serviceProvider] = rp;

        return(transactionFee,r);
    }

    // function storeCash(Treasury storage _t, address payable _owner,  uint256 value) public {
    //     _t.bank[_owner] += value;
    // }
  
}

