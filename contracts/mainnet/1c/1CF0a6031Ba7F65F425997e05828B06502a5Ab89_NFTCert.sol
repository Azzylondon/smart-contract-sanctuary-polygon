import "@openzeppelin/contracts/access/Ownable.sol";
import "./libs/SafeMath.sol";
import "./libs/TransferHelper.sol";
import "./interfaces/INFTicket.sol";
import "./interfaces/INFTServiceTypes.sol";


/*****************************************
* 
* @title NFTservice
* @author bloXmove silverlynx
* @notice Abstract class for services built upon the bloXmove NFTicket protocol
* @param _nfticket: address of the Master Smart Contract for the NFTicket protocol
*
*******************************************/


abstract contract NFTService is Ownable {
    using SafeMath for uint;
    address public nfticket;

    event TicketBought(uint256 value, uint256 feeRatio, uint256 indexed newTicketId, Ticket _t, address buyer, address recipient);

    // TODO this should probably be passed during an NFTicket-approved onboarding
    bytes4 _serviceDescriptor;
    constructor(address _nfticket)
    {
        nfticket = _nfticket;
        _serviceDescriptor = REC & NRVERSE;
    }

    /****************
    *
    * _shareRevenue()
    * issueNFTicketWithPayload()
    * _preMint()
    * _postMint()
    * jointly define the abstract functionality of NFTService
    *
    ******************/

    /***************************
    *
    * @author bloXmove silverlynx
    * @notice the master function to coordinate revenue sharing between 
        1) Reseller (can be same as service provider)
        2) Service Provider
        3) Platfrm (TX)
    * @dev override this in the service provider smart contract to set how revenue should be split
    * @param Ticket _t to hold .serviceFee , .resellerFee and .transactionFee
    * @param uint256 payment - total received funds in Wei
    * @return (uint256 serviceFee, uint256 resellerFee, uint256 transactionFee)
    *
    ****************************/
    function _shareRevenue(Ticket memory _t, uint256 payment) 
        internal 
        virtual 
        returns (uint256 serviceFee, uint256 resellerFee) 
        {}
    
    /***************************
    *
    * @author bloXmove silverlynx
    * @notice this function issues an NFT, metadata is set and service-specific payload is passed
    * @dev this function needs to call NFTicket.mintNFTicket() in the end
    * @param address recipient - wallet to receive the minted NFT
    * @param string @tokenURI - the Metadata URI
    * @param DataRecords payload - service-specific payload serialized into DataRecords scructure
    *
    ****************************/
    function issueNFTicketWithPayload(address recipient, string calldata tokenURI, DataRecords calldata payload, uint256 credits) 
        external 
        virtual 
        payable
        {}
        
    function _preMint(address sender, uint256 value, address recipient, string memory tokenURI, uint256 credits) 
        internal 
        virtual 
        returns(Ticket memory) 
        { }

    function _postMint(address recipient, string memory tokenURI, uint256 credits, uint256 newTicketId) 
        internal 
        virtual 
        returns(uint8) 
        { }


    
   
    function buyNFTicket(buyNFTicketParams memory params)
        public
        payable
        returns (uint256 newTicketId, Ticket memory _t, uint256 transactionFee, uint256 feeRatioInPerThousand)
    {
        // TODO pass reseller and serviceProvider in here as well?
        _t = _preMint(params.issuer, params._value, params.recipient, params.tokenURI, params.credits);

        (uint256 serviceFee, uint256 resellerFee)
            = _shareRevenue(_t, params._value);

        (newTicketId, transactionFee, feeRatioInPerThousand) = INFTicket(nfticket).mintNFTicket{value: params._value}
         (_t, params);

        _t.transactionFee = transactionFee; // it's NFTicket who sets the transactionFee
        uint256 resellerTransaction = transactionFee.div(1000).mul(feeRatioInPerThousand);
        uint256 serviceTransaction = transactionFee.sub(resellerTransaction);
        _t.resellerFee -= resellerTransaction;
        _t.serviceFee -= serviceTransaction;
        _postMint(params.recipient, params.tokenURI, params.credits, newTicketId);

        emit TicketBought(msg.value, feeRatioInPerThousand, newTicketId, _t, msg.sender, params.recipient);

        return (newTicketId, _t, transactionFee, feeRatioInPerThousand);
    }

}

