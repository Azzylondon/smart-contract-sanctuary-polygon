 @author bloXmove.harry
 @notice the NRVerse reseller contract
 @dev resells energy_requests for NFTCert as service provider
*/

contract NRService is NFTService, INRService {
     using StringLib for string;
     using SafeMath for uint256;
     using EnumerableSet for EnumerableSet.AddressSet;

    event ticketPaid(uint256 ticketID, uint256 serviceType, uint256 serviceFee, uint256 resellerFee, uint256 transactionFee);
    event ticketIssued(uint ticketID, Ticket _t, DataRecords _consumedEnergy, uint256 value);


    EnumerableSet.AddressSet private _whitelistSet;

    
    address public serviceProvider;
    string constant serviceProviderName = "NFTCert";

        
    // Price per credit for a ticket in Wei
    uint256 public override _pricePerCredit;

    // Keep track of allocated energy for a ticket
    mapping(uint256 => ConsumedRecord[]) private _allocations;


    constructor (address _nfticket, address _nftCert, uint256 _pPCredit) NFTService(_nfticket)
    {
        nfticket = _nfticket;
        serviceProvider = _nftCert; 

        // price per "credit" unit in "base token" (TODO -> ERC20 -> BLXM)
        _pricePerCredit = _pPCredit;
        
        // whitelist the creator of the smart contract
        _whitelistSet.add(msg.sender);

    }

    /****************************************
    * @author bloXmove silverlynx
    * @notice issueNFTicketWithPayload is the NRVerse specific ticket issuing function
    * @param receipient - address which will receive the NFT issued
    * @param tokenURI - metadata of the NFTicket. This will be displayed on e.g. OpenSea
    * @param DataRecords calldata energyRequests coming in in our marshaling format "DataRecords"
    * @param credits - initial credits available for this NFTicket
    *****************************************/
    function issueNFTicketWithPayload(address recipient, string calldata tokenURI, DataRecords calldata _energyRequests, uint256 credits)
        external 
        override
        payable 
    {
        console.log("issueNFTicketWithPayload: having received %s", msg.value);
        // in case of GUI-based web3 this is the end customer

        /*
        struct buyNFTicketParams {
        address reseller;
        address serviceProvider;
        address issuer;
        uint256 _value;
        address recipient;
        string tokenURI;
        uint256 credits;
        uint256 pricePerCredit;
        }
        */
        buyNFTicketParams memory params = buyNFTicketParams({reseller: address(this), serviceProvider: serviceProvider, 
            issuer: msg.sender, _value: msg.value, credits: credits, recipient: recipient, tokenURI: tokenURI});
        (uint256 _ticketID, Ticket memory _t, uint256 transactionFee, uint256 feeRatio)  = buyNFTicket(params); 
        
        (uint256 consumedCredits, uint256 remainCredits, DataRecords memory _retConsumedRecords) = INFTicket(nfticket).consumeCredits(serviceProvider, _ticketID, credits, _energyRequests);
        
        (ConsumedRecord[] memory consumedRecords, uint _length) = deserializeConsumedRecords(_retConsumedRecords._schema,_retConsumedRecords._data);
        for (uint256 i = 0; i < _length; i++) {
            _allocations[_ticketID].push(consumedRecords[i]);
            
        }
        emit ticketIssued(_ticketID, _t, _retConsumedRecords, msg.value);

    }

    function registerAsReseller(address _serviceProvider) 
        public
        returns(uint16 status)
    {
        status = INFTicket(nfticket).registerResellerServiceProvider(address(this), _serviceProvider);
        return(status);
    }

    /****************************************
    * @author bloXmove silverlynx
    * @notice deserializeConsumedRecords creates an array of ConsumedRecords[] to be used for allocations
    * @param DataSchema - the schema for the array of array in _data
    * @param _data - an array of strings containing the "flattened" data structures

        struct ConsumedRecord {
            uint32 certId;
            string energyType;
            string location;
            uint32 amount;
        }
    *
    *****************************************/
    function deserializeConsumedRecords(DataSchema memory schema, string[] memory _data) 
    internal
    returns (ConsumedRecord[] memory consumedRecords, uint _length) {
        _length = _data.length / schema.size;
        consumedRecords = new ConsumedRecord[](_length);
        for (uint i = 0; i < _length; i++) {
            uint j = i * schema.size;
            consumedRecords[i] = ConsumedRecord(
              {certId: _data[j].stringToUint(), energyType: _data[j + 1], location: _data[j + 2], amount: _data[j + 3].stringToUint()}
             );

        } 

    }
    /****************************************
    *
    * @author bloXmove silverlynx
    * @notice _shareRevenue determines how the received _value is shared among
        1) Service Provider: serviceFee
            and
        2) Reseller
        NFTicket will determine how much the platform is to receive:
        3) transactionFee
    * @param Ticket - Ticket containing fees, credits and price per credit
    * @param value - total value to share
    * @return (serviceFee, resellerFee) - percent for each of reseller and service total of 100%. Note that transactionFee will be aplied to this total in the ratio between them
    *
    *****************************************/
    function _shareRevenue(Ticket memory _t, uint256 _value) 
    internal
    override
    returns(uint256 serviceFee, uint256 resellerFee) {
        serviceFee = _value.mul(9).div(10);
        resellerFee = _value.div(10);
        _t.serviceFee = serviceFee;
        _t.resellerFee = resellerFee;
    }


    /****************************************
    *
    * @author bloXmove silverlynx
    * @notice _preMint is the hook for each service provider to provide ticketing logic
    * @param sender - who is triggering the transaction
    * @param value - total amount paid
    * @param tokenURI - token URI for the metadata of the NFT
    * @param credits - initial credits available for this NFTicket
    * @return Ticket - initial pre-mint ticket data
    *
    *****************************************/
    function _preMint(address sender, uint256 value, address recipient, string memory tokenURI, uint256 _credits) 
        internal
        override
        returns(Ticket memory)
    {
        require(_whitelistSet.contains(msg.sender), "Unauthorized Access");
        uint256 totalPrice = _pricePerCredit * _credits;
        console.log(totalPrice);
        require(totalPrice <= value, "Incorrect payment"); 
        uint32 zero = 0;
       
        Ticket memory _ticket = Ticket({ tokenID: zero, providerContract: serviceProvider, issuedTo: recipient, 
            serviceDescriptor: _serviceDescriptor, credits: _credits, pricePerCredit: _pricePerCredit, serviceFee: zero, transactionFee: zero, resellerFee: zero});

        return(_ticket);
     
    }

    function _postMint(address recipient, string memory tokenURI, uint256 credits, uint256 newTicketId)
        internal
        override
        returns(uint8 STATUS)
    {
        STATUS = 200;
    }

    function updatePricePerCredit(uint256 _pPCredit)
        external
        override
        onlyOwner
    {
        _pricePerCredit = _pPCredit;
    }

    function viewAllocations(uint256 ticketId)
        external
        override
        view
        returns (ConsumedRecord[] memory)
    {
        return _allocations[ticketId];
    }

    function addWhitelist(address _address)
    external
    override
    onlyOwner
    {
        _whitelistSet.add(_address);
    }

  function removeWhitelist(address _address)
    external
    override
    onlyOwner
    {
        _whitelistSet.remove(_address);
    }

  function inWhitelist(address _address)
    external
    view
    override
    returns (bool)
  {
    return _whitelistSet.contains(_address);
  }
}

