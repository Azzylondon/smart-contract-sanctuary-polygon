pragma solidity ^0.8.2;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "./ICommonGameData.sol";

import "hardhat/console.sol";

contract CommonGameDataUpgradeable is
    ICommonGameData,
    Initializable,
    AccessControlUpgradeable
{
    uint32 public season;
    uint8 public lootBoxTiers;

    // keeps track of probabilities of NFT tier for each loot box tier
    mapping(uint8 => uint24[]) public NFTTierChances;
    // keeps track of probabilities of NFT type
    mapping(uint8 => uint64[]) public NFTTypeChances;
    // thresholds and costs for each loot box tier
    mapping(uint8 => LootBoxTierInfo) public lootBoxTierInfo;

    struct LootBoxTierInfo {
        uint256 threshold;
        uint256 cost;
    }

    function initialize() public initializer {
        __AccessControl_init();

        season = 1;

        // NFT type probabilities
        initTypeChances();

        // init NFT tier probabilities for each loot box
        initTierChances();

        // setting loot box tier thresholds & costs
        lootBoxTierInfo[1] = LootBoxTierInfo(50, 49000000); // common loot box threshold
        lootBoxTierInfo[2] = LootBoxTierInfo(30, 99000000); // rare loot box threshold
        lootBoxTierInfo[3] = LootBoxTierInfo(20, 199000000); // epic loot box threshold

        // set deployer as default admin
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function initTypeChances() internal {
        // NFT types
        uint48[3] memory types = [
            uint48(1), // hero
            uint48(2), // champion
            uint48(3) // item
        ];

        // NFT type probailities for each loot box
        uint16[3][3] memory typeProbabilities = [
            /**
             * @dev NFT type probabilities
             * rare:      5%     25%     70%
             * epic:      7.5%   32.5%   60%
             * legendary: 10%    45%     45%
             */
            [500, 3000, 10000], // rare loot box
            [750, 4000, 10000], // epic loot box
            [1000, 5500, 10000] // legendary loot box
        ];

        // create the loot box tiers and set their probabilities
        uint64[3] memory typeProb;
        for (uint8 i = 0; i < 3; i++) {
            for (uint8 j = 0; j < 3; j++) {
                typeProb[j] =
                    (uint64(types[j]) << 16) +
                    uint64(typeProbabilities[i][j]);
            }
            // adding 1 to the index because loot box tiers start from 1
            NFTTypeChances[i + 1] = typeProb;
        }
    }

    function initTierChances() internal {
        // NFT tiers
        uint8[5] memory tiers = [
            uint8(1), // bronze
            uint8(2), // silver
            uint8(3), // gold
            uint8(4), // platinum
            uint8(5) // legendary
        ];
        // NFT tier probailities for each loot box
        /**
         * @dev NFT tier probabilities
         * rare:         58.50%     33.50%     5.00%     2.50%    0.50%
         * epic:         39.00%     45.00%     10.00%    5.00%    1.00%
         * legendary:    22.50%     50.00%     15.00%    10.00%   2.50%
         */
        uint16[5][3] memory tierProbabilities = [
            [5850, 9200, 9700, 9950, 10000], // rare loot box
            [3900, 8400, 9400, 9900, 10000], // epic loot box
            [2250, 7250, 8750, 9750, 10000] // legendary loot box
        ];

        // create the loot box tiers and set their probabilities
        uint24[5] memory tierProb;
        for (uint8 i = 0; i < 3; i++) {
            for (uint8 j = 0; j < 5; j++) {
                tierProb[j] =
                    (uint24(tiers[j]) << 16) +
                    uint24(tierProbabilities[i][j]);
            }
            // adding 1 to the index because loot box tiers start from 1
            NFTTierChances[i + 1] = tierProb;
            lootBoxTiers++;
        }
    }

    /**
     * @dev sets the new season of the game
     */
    function setNewSeason(uint256[] memory _thresholds)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        setThresholds(_thresholds);
        season++;
    }

    function setTierThresholds(uint256[] memory _thresholds)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        setThresholds(_thresholds);
    }

    function setThresholds(uint256[] memory _thresholds) internal {
        require(
            _thresholds.length == lootBoxTiers,
            "Incorrect number of thresholds provided"
        );
        for (uint8 i = 0; i < lootBoxTiers; i++) {
            lootBoxTierInfo[i + 1].threshold = _thresholds[i];
        }
    }

    function setCosts(uint256[] memory _costs)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(
            _costs.length == lootBoxTiers,
            "Incorrect number of costs provided"
        );
        for (uint8 i = 0; i < lootBoxTiers; i++) {
            lootBoxTierInfo[i + 1].cost = _costs[i];
        }
    }

    /**
     * @dev creates a new type in the game
     */
    function createNewNFTType() public onlyRole(DEFAULT_ADMIN_ROLE) {
        // number of current types
        uint256 numberOfTypes = NFTTypeChances[1].length;
        require(
            numberOfTypes + 1 <= ((~uint48(0) << 1) >> 1),
            "NFT types limit reached"
        );

        // append a 0 probability for new NFT type in each loot box tier
        for (uint8 i = 1; i <= lootBoxTiers; i++) {
            NFTTypeChances[i].push(uint64((numberOfTypes + 1) << 16));
        }
    }

    /**
     * @dev creates a new NFT tier in the game and adds it to loot box tiers with 0% probability
     */
    function createNewNFTTier() public onlyRole(DEFAULT_ADMIN_ROLE) {
        // number of current tiers
        uint256 numberOfTiers = NFTTierChances[1].length;
        require(numberOfTiers + 1 <= ~uint8(0), "NFT tier limit reached");

        // append a 0 probability for new NFT tier in each loot box tier
        for (uint8 i = 1; i <= lootBoxTiers; i++) {
            NFTTierChances[i].push(uint24((numberOfTiers + 1) << 16));
        }
    }

    /**
     * @dev sets NFT tier chances for a loot box tier
     * @param _tier tier of loot box for which the chances are being set
     * @param _nftTypeChances probabilities of the NFT tiers being set for the loot box tier
     */
    function setLootBoxTypeChances(uint8 _tier, uint16[] memory _nftTypeChances)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_tier > 0 && _tier <= lootBoxTiers, "Incorrect tier provided");
        // number of current tiers
        uint256 numberOfTypes = NFTTypeChances[1].length;
        require(
            numberOfTypes == _nftTypeChances.length,
            "Incorrect number of probailities provided"
        );

        isChanceCorrect(_nftTypeChances);

        for (uint48 i = 0; i < numberOfTypes; i++) {
            NFTTypeChances[_tier][i] =
                (uint64((i + 1)) << 16) +
                uint64(_nftTypeChances[i]);
        }
    }

    /**
     * @dev sets NFT type chances for a loot box tier
     * @param _tier tier of loot box for which the chances are being set
     * @param _nftTierChances probabilities of the NFT tiers being set for the loot box tier
     */
    function setLootBoxTierChances(uint8 _tier, uint16[] memory _nftTierChances)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_tier > 0 && _tier <= lootBoxTiers, "Incorrect tier provided");
        // number of current tiers
        uint256 numberOfTiers = NFTTierChances[1].length;
        require(
            numberOfTiers == _nftTierChances.length,
            "Incorrect number of probailities provided"
        );

        isChanceCorrect(_nftTierChances);

        for (uint8 i = 0; i < numberOfTiers; i++) {
            NFTTierChances[_tier][i] =
                (uint24((i + 1)) << 16) +
                uint24(_nftTierChances[i]);
        }
    }

    /**
     * @dev creates a new loot box tier in the game and sets the probabilities for newly created tier
     * @param nftTierChances NFT tier chances for the new loot box tier being created
     */
    function createNewLootBoxTier(
        uint16[] memory nftTierChances,
        uint256 _threshold,
        uint256 _cost
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            nftTierChances.length == NFTTierChances[1].length,
            "Incorrect number of probabilities provided"
        );
        // check if probs add up to 100% => 100,00
        isChanceCorrect(nftTierChances);

        lootBoxTiers++;
        uint24 nftTier;
        for (uint256 i = 0; i < nftTierChances.length; i++) {
            // insert tier and make space for tier probability (16 bits)
            nftTier = uint24(i + 1) << 16;
            NFTTierChances[lootBoxTiers].push(nftTier + nftTierChances[i]);
        }

        lootBoxTierInfo[lootBoxTiers].threshold = _threshold;
        lootBoxTierInfo[lootBoxTiers].cost = _cost;
    }

    function getSeason() public view returns (uint32) {
        return season;
    }

    function getNFTInfo()
        public
        view
        returns (
            uint32 _season,
            uint8 _nftTiers,
            uint48 _nftTypes
        )
    {
        _season = getSeason();
        _nftTiers = getNFTTiers();
        _nftTypes = getTypes();
    }

    function getLootBoxTierInfo(uint8 _tier)
        public
        view
        returns (uint256, uint256)
    {
        require(_tier > 0 && _tier <= lootBoxTiers, "Incorrect Loot box tier");
        return (lootBoxTierInfo[_tier].threshold, lootBoxTierInfo[_tier].cost);
    }

    function getLootBoxInfo(uint8 _tier)
        public
        view
        returns (
            uint256,
            uint256,
            uint8,
            uint32
        )
    {
        (uint256 _threshold, uint256 _cost) = getLootBoxTierInfo(_tier);
        return (_threshold, _cost, lootBoxTiers, season);
    }

    /**
     * @dev returns number of NFT types (CHAMPION, HERO, ITEM..) for game
     */
    function getTypes() public view returns (uint48) {
        return uint48(NFTTypeChances[1].length);
    }

    /**
     * @dev returns number of NFT tiers (bronze, silver, gold, platinum, legendary)
     */
    function getNFTTiers() public view returns (uint8) {
        return uint8(NFTTierChances[1].length);
    }

    /**
     * @dev gets NFT tier probabilities for a loot box tier
     * @param _tier loot box tier
     */
    function getNFTTierProbabilities(uint8 _tier)
        public
        view
        returns (uint24[] memory)
    {
        require(_tier > 0 && _tier <= lootBoxTiers, "Incorrect Loot box tier");
        return NFTTierChances[_tier];
    }

    /**
     * @dev returns probabilities of NFT types (champions, items, heroes...)
     * @param _tier loot box tier
     */
    function getTypeProbabilities(uint8 _tier)
        public
        view
        returns (uint64[] memory)
    {
        require(_tier > 0 && _tier <= lootBoxTiers, "Incorrect Loot box tier");

        return NFTTypeChances[_tier];
    }

    /**
     * @dev returns the number of loot box tiers created
     */
    function getLootBoxTiers() public view returns (uint8) {
        return lootBoxTiers;
    }

    /**
     * @dev verifies that the probabilities all add up to 100%
     */
    function isChanceCorrect(uint16[] memory _probabilities) public pure {
        uint256 probSum = _probabilities[_probabilities.length - 1];
        for (uint256 i = 0; i < _probabilities.length - 1; i++) {
            probSum -= _probabilities[i];
        }
        require(probSum == 0, "Incorrect type probabilities provided");
    }
}

