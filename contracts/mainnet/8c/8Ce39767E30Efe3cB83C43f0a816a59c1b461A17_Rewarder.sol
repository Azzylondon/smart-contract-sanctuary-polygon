import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "../../utils/AccessProtectedUpgradeable.sol";
import "../../utils/BaseRelayRecipient.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

/*
Note on IERC721Issuer/IERC1155Issuer contracts, can be implemented either as:
1) can be normal ERC721/ERC1155 nft contracts, where issueToken mints new NFTs
2) or can be a contract which implements ERC721Receiver/ERC1155Receiver, where
   issueToken only transfers ownership of an NFT from the Issuer contract to
   the user
*/

interface IERC721Issuer {
    function issueToken(
        address recipient
    ) external returns (uint256);
}

interface IERC1155Issuer {
    function issueToken(
        address recipient,
        uint256 tokenId,
        uint256 amount
    ) external;

    function issueTokenBatch(
        address recipient,
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) external;
}

contract Rewarder is    Initializable,
                        AccessProtectedUpgradeable,
                        BaseRelayRecipient,
                        ReentrancyGuardUpgradeable {

    function initialize() public virtual initializer {
        __Rewarder_init();
    }

    // reward events

    event RewardERC20(
        address indexed tokenAddress,
        address indexed recipient,
        uint256 indexed amount
    );

    event RewardERC721(
        address indexed tokenAddress,
        address indexed recipient,
        uint256 indexed tokenId
    );

    event RewardERC1155(
        address indexed tokenAddress,
        address indexed recipient,
        uint256 indexed tokenId,
        uint256 amount
    );

    event RewardERC1155Batch(
        address indexed tokenAddress,
        address indexed recipient,
        uint256[] indexed tokenIds,
        uint256[] amounts
    );

    function __Rewarder_init() internal initializer {
        __Ownable_init_unchained();
        __AccessProtected_init_unchained();

        __ReentrancyGuard_init_unchained();

        __Rewarder_init_unchained();
    }

    function __Rewarder_init_unchained() internal initializer {
    }

    // reward functions

    function rewardERC20(
        address tokenAddress,
        address recipient,
        uint256 amount
    ) public virtual onlyAdmin {
        require(tokenAddress != address(0), "Rewarder: tokenAddress cannot be 0 address");
        require(recipient != address(0), "Rewarder: recipient cannot be 0 address");
        require(amount > 0, "Rewarder: amount cannot be <= 0");

        IERC20(tokenAddress).transfer(recipient, amount);

        emit RewardERC20(tokenAddress, recipient, amount);
    }

    // same as above without reward event
    function withdrawERC20(
        address tokenAddress,
        uint256 amount
    ) public virtual onlyOwner {
        require(tokenAddress != address(0), "Rewarder: tokenAddress cannot be 0 address");
        require(amount > 0, "Rewarder: amount cannot be <= 0");

        IERC20(tokenAddress).transfer(_msgSender(), amount);
    }

    function rewardERC721(
        address tokenAddress,
        address recipient
    ) public virtual onlyAdmin {
        require(tokenAddress != address(0), "Rewarder: tokenAddress cannot be 0 address");
        require(recipient != address(0), "Rewarder: recipient cannot be 0 address");

        uint256 tokenId = IERC721Issuer(tokenAddress).issueToken(recipient);

        emit RewardERC721(tokenAddress, recipient, tokenId);
    }

    function rewardERC1155(
        address tokenAddress,
        address recipient,
        uint256 tokenId,
        uint256 amount
    ) public virtual onlyAdmin {
        require(tokenAddress != address(0), "Rewarder: tokenAddress cannot be 0 address");
        require(recipient != address(0), "Rewarder: recipient cannot be 0 address");
        require(amount > 0, "Rewarder: amount cannot be <= 0");

        IERC1155Issuer(tokenAddress).issueToken(recipient, tokenId, amount);

        emit RewardERC1155(tokenAddress, recipient, tokenId, amount);
    }

    function rewardERC1155Batch(
        address tokenAddress,
        address recipient,
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) public virtual onlyAdmin {
        require(tokenAddress != address(0), "Rewarder: tokenAddress cannot be 0 address");
        require(recipient != address(0), "Rewarder: recipient cannot be 0 address");
        require(tokenIds.length > 0, "Rewarder: number of tokenIds must be greater than 0");
        require(tokenIds.length == amounts.length, "Rewarder: tokenIds and amounts length mismatch");

        IERC1155Issuer(tokenAddress).issueTokenBatch(recipient, tokenIds, amounts);

        emit RewardERC1155Batch(tokenAddress, recipient, tokenIds, amounts);
    }

    /**
     * Set Trusted Forwarder
     *
     * @param _trustedForwarder - Trusted Forwarder address
     */
    function setTrustedForwarder(address _trustedForwarder) external onlyAdmin {
        require(_trustedForwarder != address(0), "Rewarder: Biconomy trusted forwarder cannot be 0 address");

        trustedForwarder = _trustedForwarder;
    }

    /**
     * returns the message sender
     */
    function _msgSender() internal view override(ContextUpgradeable, BaseRelayRecipient) returns (address) {
        return BaseRelayRecipient._msgSender();
    }
}

