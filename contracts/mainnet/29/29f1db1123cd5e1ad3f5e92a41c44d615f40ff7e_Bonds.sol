pragma solidity ^0.8.13;




abstract contract Base is NFT {
  using SafeMath for uint;
  using SafeERC20 for IERC20;
  uint256 public minimumAmount = 100 * (10**6);
  uint256 public maximumContractAmount;
  address internal managementFeeAccount;
  address internal holdingAccount;
  address internal withdrawTokenAdress = address(0xc2132D05D31c914a87C6611C10748AEb04B58e8F);
  mapping(uint256 => _Order) internal orders;
  uint256 public orderCount;
  mapping(address => _Token) public allowedTokens;
  mapping(uint256 => _LockingPeriod) public allowedTimePeriods;
  event Order(_Order order);
  event Claim(_Order order);
  struct _Order {
    uint256 id;
    address user;
    _LockingPeriod lockingPeriod;
    uint256 amountContracts;
    address token;
    uint256 amount;
    uint256 initialFeeAmount;
    uint256 performanceFeeAmount;
    uint256 claimableAmount;
    bool isClaimable;
    uint256 NFTId;
    uint256 timestampOrder;
    uint256 timestampExpire;
    uint256 timestampClaim;
  }
  struct _Token {
    string name;
    address contractAddress;
    address priceAddress; // https://docs.chain.link/docs/matic-addresses/
    bool needsConversion;
    uint8 decimals;
    uint8 priceDecimals;
    bool active;
  }
  struct _LockingPeriod {
    uint256 lockingDays;
    uint256 initialPercentage;
    uint256 performancePercentage;
    uint256 fixedProfit;
    string profitType;
    uint256 maxCap;
    bool active;
  }
  constructor () {
    addAllowedToken('USDT', address(0xc2132D05D31c914a87C6611C10748AEb04B58e8F), address(0x0A6513e40db6EB1b165753AD52E80663aeA50545), true, 6, 8, true);
    addAllowedToken('USDC', address(0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174), address(0xfE4A8cc5b5B2366C1B58Bea3858e81843581b2F7), true, 6, 8, true);
    addAllowedToken('ETH', address(0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619), address(0xF9680D99D6C9589e2a93a78A04A279e509205945), true, 18, 8, true);
    addAllowedToken('MATIC', address(0x0000000000000000000000000000000000001010), address(0xAB594600376Ec9fD91F8e885dADF0CE036862dE0), true, 18, 8, true);
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(BURNER_ROLE, msg.sender);
    _setupRole(KEEPER_MANAGER_ROLE, msg.sender);
  }
  function setMinimumAmount(uint256 _minimumAmount) external virtual onlyOwner {minimumAmount = _minimumAmount;}
  function setManagementFeeAccount(address _managementFeeAccount) external virtual onlyOwner {managementFeeAccount = _managementFeeAccount;}
  function setHoldingAccount(address _holdingAccount) external virtual onlyOwner {holdingAccount = _holdingAccount;}
  function setWithdrawTokenAdress(address _withdrawTokenAdress) external virtual onlyOwner {withdrawTokenAdress = _withdrawTokenAdress;}
  function addAllowedToken(string memory name, address token, address priceAddress, bool needsConversion, uint8 decimals, uint8 priceDecimals, bool active) public virtual onlyOwner {
      allowedTokens[token] = _Token(name, token, priceAddress, needsConversion, decimals, priceDecimals, active);
  }
  function removeAllowedToken(address token) external virtual onlyOwner {allowedTokens[token].active = false;}
  function addAllowedLockingPeriod(uint256 lockingDays, uint256 initialFee, uint256 performanceFee, uint256 fixedProfit, string memory profitType, uint256 maxCap) public virtual onlyOwner {
      allowedTimePeriods[lockingDays] = _LockingPeriod(lockingDays, initialFee, performanceFee, fixedProfit, profitType, maxCap, true);
  }
  function removeAllowedLockingPeriod(uint256 lockingDays) external virtual onlyOwner {allowedTimePeriods[lockingDays].active = false;}
  function _isLockingPeriodAllowed(uint256 lockingDays) internal virtual view returns (bool) {
    return allowedTimePeriods[lockingDays].lockingDays > 0 && allowedTimePeriods[lockingDays].active;
  }
  function _calculateContracts(address token, uint256 amount) internal virtual view returns (uint256) {
    if (allowedTokens[token].needsConversion){
      AggregatorV3Interface priceFeed = AggregatorV3Interface(allowedTokens[token].priceAddress);
      (/*uint80 roundID*/, int256 price, /*uint startedAt*/, /*uint timeStamp*/, /*uint80 answeredInRound*/) = priceFeed.latestRoundData();
      return scalePrice(((uint256(price) * amount) / (10**allowedTokens[token].priceDecimals)), allowedTokens[token].decimals, 6);
    }
    return scalePrice(amount, allowedTokens[token].decimals, 6);
  }
  function scalePrice(uint256 _price, uint8 _priceDecimals, uint8 _decimals) internal pure returns (uint256) {
      if (_priceDecimals < _decimals) {
          return _price * uint256(10 ** uint256(_decimals - _priceDecimals));
      } else if (_priceDecimals > _decimals) {
          return _price / uint256(10 ** uint256(_priceDecimals - _decimals));
      }
      return _price;
  }
  function _depositToken(address _token, uint256 amount) public payable virtual {
    if (isChainToken(_token)) {
      require((msg.value >= amount), "Amont specified is higher than sent amount. Please send exact amount of MATIC tokens.");
    } else {
      IERC20 token = IERC20(_token);
      require(token.allowance(msg.sender, address(this)) >= amount, "Token allowance is too low for the needed amount.");
      token.safeTransferFrom(msg.sender, address(this), amount);
    }
  }
  function maximumAmountReached(uint256 amount, uint256 _lockingDays) public virtual view returns (bool) {
    if (maximumContractAmount > 0 || allowedTimePeriods[_lockingDays].maxCap > 0) {
      uint256 contractAmount = amount;
      uint256 contractAmountPeriod = amount;
      for (uint i = 0; i < orderCount; i++) {
        contractAmount += orders[i + 1].amountContracts;
        if (orders[i + 1].lockingPeriod.lockingDays == _lockingDays) {
          contractAmountPeriod += orders[i + 1].amountContracts;
        }
      }
      if (allowedTimePeriods[_lockingDays].maxCap > 0 && allowedTimePeriods[_lockingDays].maxCap < contractAmountPeriod) {
        return false;
      }
      return maximumContractAmount == 0 || maximumContractAmount >= contractAmount;
    }
    return true;
  }
  function isChainToken(address token) internal view returns (bool) {
    return keccak256(bytes(allowedTokens[token].name)) == keccak256(bytes("MATIC"));
  }
  function makeOrder(address token, uint256 amount, uint256 lockPeriod, bytes32[] calldata proof, bool whitelist) public payable virtual{
    _LockingPeriod storage lockingPeriod = allowedTimePeriods[lockPeriod];
    require(allowedTokens[token].active, "Token is currently not supported.");
    require(_isLockingPeriodAllowed(lockPeriod), "NFT Locking Period is not allowed."); 
    isChainToken(token) ? require((msg.value >= amount), "Amont specified is higher than sent amount. Please send exact amount of MATIC tokens.") : require(IERC20(token).balanceOf(msg.sender) >= amount, "Your balance is too low.");
    if (whitelist) {
      require(isWhitelistOpen(), "Fund is currently not open.");
      require(verifyWhitelist(proof, _merkleRoot), "Not whitelisted.");
      require(maxMintsPerAddressWhitelist == 0 || mintedTokensWhitelistByAddress[msg.sender] + 1 <= maxMintsPerAddressWhitelist, "Already minted max amount of NFTs");
    } else {
      require(isFundOpen(), "Fund is currently not open.");
    }
    uint256 amountContracts = _calculateContracts(token, amount);
    require(minimumAmount <= amountContracts, "Number of contracts is too low.");
    uint256 feeAmount = amount.mul(lockingPeriod.initialPercentage).div(10000);
    uint256 amountAfterFee = amount.sub(feeAmount);
    uint256 feeAmountContracts = amountContracts.mul(lockingPeriod.initialPercentage).div(10000);
    amountContracts = amountContracts.sub(feeAmountContracts);
    require(maximumAmountReached(amountContracts, lockPeriod), "Maximum amount for this contract is reached");
    uint256 NFTId;
    uint256 timestampExpire;
    uint256 currentTime = block.timestamp;
    _depositToken(token, amount);
    timestampExpire = currentTime + (60 * 60 * 24 * lockPeriod);
    NFTId = whitelist ? mintWhitelist(proof) : mint();
    sendFunds(token, feeAmount, amountAfterFee);
    orderCount = orderCount.add(1);
    orders[orderCount] = _Order(orderCount, msg.sender, lockingPeriod, amountContracts, token, amountAfterFee, feeAmount, 0, 0, false, NFTId, currentTime, timestampExpire, 0);
    emit Order(orders[orderCount]);
  }
  function sendFunds(address token, uint256 feeAmount, uint256 amountAfterFee) internal virtual {
    if (isChainToken(token)) {
      address payable receiverManagement = payable(managementFeeAccount);
      if (feeAmount > 0) {
        (bool successManagement,) = receiverManagement.call{value: feeAmount}("");
        require(successManagement, "Sending management fee funds failed.");
      }
      address payable receiverHolding = payable(holdingAccount);
      (bool success,) = receiverHolding.call{value: amountAfterFee}("");
      require(success, "Sending funds to Holding address failed.");
    }
    else {
      IERC20 tokenInstance = IERC20(token);
      if (feeAmount > 0) {
        tokenInstance.safeTransfer(managementFeeAccount, feeAmount);
      }
      tokenInstance.safeTransfer(holdingAccount, amountAfterFee);
    }
  }
  function claimOrder(uint256 _id) public virtual {
    require(_id > 0 && _id <= orderCount, "The order does not exist.");
    require(orders[_id].timestampClaim == 0, "This order has already been claimed.");
    require(orders[_id].user == msg.sender, "This order was not created by your wallet.");
    require(block.timestamp >= orders[_id].timestampExpire, "Locking period on this order has not yet expired.");
    require(orders[_id].isClaimable, "This order is not yet marked as claimable.");
    IERC20 token = IERC20(withdrawTokenAdress);
    uint256 currentContractBalance = token.balanceOf(address(this));
    require(currentContractBalance >= orders[_id].claimableAmount, "Contract funds are not yet received from the Holding wallet.");
    orders[_id].timestampClaim = block.timestamp;
    token.safeTransfer(orders[_id].user, orders[_id].claimableAmount);
    emit Claim(orders[_id]);
  }
  function fetchItemsCreated() public view returns (_Order[] memory) {
      uint totalItemCount = orderCount;
      uint itemCount = 0;
      uint currentIndex = 0;
      for (uint i = 0; i < totalItemCount; i++) {
        if (orders[i + 1].user == msg.sender) {
            itemCount += 1;
        }
      }
      _Order[] memory items = new _Order[](itemCount);
      for (uint i = 0; i < totalItemCount; i++) {
        if (orders[i + 1].user == msg.sender) {
            uint currentId = i + 1;
            _Order storage currentItem = orders[currentId];
            items[currentIndex] = currentItem;
            currentIndex += 1;
        }
      }
      return items;
  }
  function getOrder(uint256 id) external view onlyRole(KEEPER_MANAGER_ROLE) returns (_Order memory){return orders[id];}
  function setOrderClaimableAmount(uint256 id, uint256 amount) external onlyRole(KEEPER_MANAGER_ROLE) virtual{orders[id].claimableAmount = amount;}
  function setOrderIsClaimable(uint256 id, bool isClaimable) external onlyRole(KEEPER_MANAGER_ROLE) virtual{orders[id].isClaimable = isClaimable;}
  function preventFrozenTokens(address token) external onlyOwner{
      if (isChainToken(token)) {
        uint256 erc20balance = address(this).balance;
        require(erc20balance > 0, "There are no funds to withdraw.");
        address payable receiverHolding = payable(holdingAccount);
        (bool success,) = receiverHolding.call{value: erc20balance}("");
        require(success, "Sending funds failed.");
      }
      else {
        IERC20 tokenInstance = IERC20(token);
        uint256 erc20balance = tokenInstance.balanceOf(address(this));
        require(erc20balance > 0, "There are no funds to withdraw.");
        tokenInstance.safeTransfer(holdingAccount, erc20balance);
      }
  }
  receive() external payable {}
}

