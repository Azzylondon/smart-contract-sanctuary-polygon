     * @param from address representing the previous owner of the given token ID
     * @param to target address that will receive the tokens
     * @param tokenId uint256 ID of the token to be transferred
     * @param data bytes optional data to send along with the call
     * @return bool whether the call correctly returned the expected magic value
     */
    function _checkOnERC721Received(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) private returns (bool) {
        if (to.isContract()) {
            try IERC721Receiver(to).onERC721Received(_msgSender(), from, tokenId, data) returns (bytes4 retval) {
                return retval == IERC721Receiver.onERC721Received.selector;
            } catch (bytes memory reason) {
                if (reason.length == 0) {
                    revert("ERC721: transfer to non ERC721Receiver implementer");
                } else {
                    /// @solidity memory-safe-assembly
                    assembly {
                        revert(add(32, reason), mload(reason))
                    }
                }
            }
        } else {
            return true;
        }
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256, /* firstTokenId */
        uint256 batchSize
    ) internal virtual {
        if (batchSize > 1) {
            if (from != address(0)) {
                _balances[from] -= batchSize;
            }
            if (to != address(0)) {
                _balances[to] += batchSize;
            }
        }
    }

    /**
     * @dev Hook that is called after any token transfer. This includes minting and burning. If {ERC721Consecutive} is
     * used, the hook may be called as part of a consecutive (batch) mint, as indicated by `batchSize` greater than 1.
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, ``from``'s tokens were transferred to `to`.
     * - When `from` is zero, the tokens were minted for `to`.
     * - When `to` is zero, ``from``'s tokens were burned.
     * - `from` and `to` are never both zero.
     * - `batchSize` is non-zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 firstTokenId,
        uint256 batchSize
    ) internal virtual {}
}






pragma solidity ^0.8.7;

contract SVG is ERC721, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;
    
    string private _description = "Token 0"; 
    string private _name = "Token 0";
    string private svgInput = "abcdefg";

    constructor() ERC721("SVG", "SVG") {}

//v Set NFT name and description at mint v

    struct Attr {
        string _name;
        string _description;
    }
    mapping(uint256 => Attr) internal attributes;

    function _setAttributes(uint256 tokenId, string memory _name, string memory _description) internal {
        attributes[tokenId] = Attr(_name, _description);
        _name = _name;
        _description = _description;
    }

//v recall NFT name and description for metadata v

    function getAttributes(uint256 tokenId)
        internal
        view
        returns (string memory _name, string memory _description)
        {
           Attr memory attribute = attributes[tokenId];
            return (attribute._name, attribute._description);
        }


//v Set SVG Image data at mint, Base64 encode v 

    struct SVG {
        string svgInput;
    }

    mapping(uint256 => SVG) internal svgImages;

    function _setSvg(uint256 tokenId, string memory svgInput_) private{
        svgInput = Base64.encode(bytes(svgInput_));
        svgImages[tokenId] = SVG(svgInput);
       
    }

//v recall SVG Image data for metadata v  

    function getSvg(uint tokenId) internal view returns (string memory svgInput) {
        SVG memory Image = svgImages[tokenId];
        return string(abi.encodePacked("data:image/svg+xml;base64,",(Image.svgInput)));
    }

    function safeMint (
        string calldata svgInput,
        string calldata _name,
        string calldata _description
        
    ) public onlyOwner() {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(msg.sender, tokenId);
        _setSvg(tokenId, svgInput);
        _setAttributes(tokenId, _name, _description);
    }

// v concat metadata info & Base64 encode   v

    function tokenURI(uint256 tokenId)
        public
        view
        returns (string memory)
    {

         string memory json = Base64.encode(
            abi.encodePacked(
                '{"name": "',
                attributes[tokenId]._name,
                '", "description": "',attributes[tokenId]._description,'", "image": "',
                getSvg(tokenId),
                '"}'
            )
        );
         string memory output = string(
            abi.encodePacked("data:application/json;base64,", json)
          );
           require(_exists(tokenId), "Token doesn't exist.");
           return output;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}