        address _config,
        uint256 _priceEurCent,
        address _presale,
        address _cs4
    ) CS4BaseShop(_config, _priceEurCent) {
        presale = _presale;
        cs4 = IERC721Enumerable(_cs4);
        vault = new CS4Vault(_cs4);
        presaleVault = new CS4Vault(_cs4);
    }

    function handleRedeem(
        uint256 quantity,
        bytes memory data
    ) internal requireOpen {
        require(msg.sender == presale, "only presale tokens can be redeemed");
        (address destination, bool _acceptTermsCryptoPostAt, string memory _acceptanceText) = abi.decode(data, (address, bool, string));
        require(_acceptTermsCryptoPostAt, "AGBs not accepted");
        presaleVault.transferMany(destination, quantity);
    }

    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes memory data
    ) public virtual override returns (bytes4) {
        handleRedeem(value, data);
        return super.onERC1155Received(operator, from, id, value, data);
    }

    function onERC1155BatchReceived(
        address operator,
        address from,
        uint256[] memory ids,
        uint256[] memory values,
        bytes memory data
    ) public virtual override returns (bytes4) {
        require(ids.length == 1 && ids[0] == 0 && values.length == 1);
        handleRedeem(values[0], data);
        return super.onERC1155BatchReceived(operator, from, ids, values, data);
    }

    function acceptAndBuyFor(
        address payable _recipient,
        uint256 quantity,
        bool weth,
        uint80[2] calldata forceRounds,
        bool _acceptTermsCryptoPostAt,
        string calldata /* _acceptanceText */
    ) public payable requireOpen {
        require(_acceptTermsCryptoPostAt, "You need to accept the terms.");
        handlePaymentEUR(weth, quantity, forceRounds);
        vault.transferMany(_recipient, quantity);
        emit Sold(msg.sender, _recipient, quantity);
    }

    function quantityLeft() public view returns (uint256) {
        return cs4.balanceOf(address(vault));
    }

    function presaleQuantityLeft() public view returns (uint256) {
        return cs4.balanceOf(address(presaleVault));
    }
