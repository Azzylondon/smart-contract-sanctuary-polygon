pragma solidity ^0.8.12;

import {Counters} from "@openzeppelin/contracts/utils/Counters.sol";

import {Ownable} from "@solidstate/contracts/access/ownable/Ownable.sol";
import {ERC165} from "@solidstate/contracts/introspection/ERC165.sol";
import {Pausable} from "@solidstate/contracts/security/Pausable.sol";
import {ERC1155Base} from "@solidstate/contracts/token/ERC1155/base/ERC1155Base.sol";
import {ERC1155Metadata} from "@solidstate/contracts/token/ERC1155/metadata/ERC1155Metadata.sol";
import {ERC1155MetadataStorage} from "@solidstate/contracts/token/ERC1155/metadata/ERC1155MetadataStorage.sol";
import {IERC1155Receiver} from "@solidstate/contracts/token/ERC1155/IERC1155Receiver.sol";

import {LibDiamond} from "../../vendor/diamond-2-hardhat/contracts/libraries/LibDiamond.sol";

import {Cred} from "../../token/Cred.sol";

import {CredRegistryStorage} from "./CredRegistryStorage.sol";
import {CredRegistryTypes} from "./CredRegistryTypes.sol";
import {ICredRewardCalculator} from "./ICredRewardCalculator.sol";

import "hardhat/console.sol";

contract CredRegistryFacet is ERC1155Base, ERC1155Metadata, IERC1155Receiver, ERC165, Ownable, Pausable {
    using Counters for Counters.Counter;

    uint256 internal constant TYPE_SHIFT = 192;

    /**
     * @dev token type for a project (NFT)
     */
    uint256 public constant PROJECT = uint256(uint64(bytes8("PROJECT"))) << TYPE_SHIFT;

    /**
     * @dev token type for a project vintage (NFT)
     */
    uint256 public constant VINTAGE = uint256(uint64(bytes8("VINTAGE"))) << TYPE_SHIFT;

    /**
     * @dev token type for a batch of N kilos from a specific vintage (NFT)
     */
    uint256 public constant BATCH = uint256(uint64(bytes8("BATCH"))) << TYPE_SHIFT;

    /**
     * @dev token type for fractionalized carbon from a specific vintage (partially fungible, at kilo granularity)
     */
    uint256 public constant CARB = uint256(uint64(bytes8("CARB"))) << TYPE_SHIFT;

    /**
     * @dev token type for an offset of N kilos (NFT)
     */
    uint256 public constant OFFSET = uint256(uint64(bytes8("OFFSET"))) << TYPE_SHIFT;

    // string public name = "CRED Registry Prototype";
    // string public symbol = "CRED-Registry";

    event BatchConfirmed(
        address indexed operator,
        uint256 indexed batchID,
        uint256 vintageID,
        uint256 kilos,
        string serialNumber
    );

    event BatchRejected(address indexed operator, uint256 indexed batchID, string reason);

    event BatchStatusUpdated(address indexed operator, uint256 indexed batchID, CredRegistryTypes.BatchStatus status);

    event BatchUpdated(address indexed operator, uint256 indexed batchID, uint256 kilos, string serialNumber);

    function init() external {
        // one time only
        if (_owner() == address(0)) {
            _transferOwnership(LibDiamond.contractOwner());
        }

        console.log("CredRegistryFacet.init ran -- address, owner:", address(this), _owner());
    }

    function numProjects() public view returns (uint256) {
        return CredRegistryStorage.layout().projectCounter.current();
    }

    function numVintages() public view returns (uint256) {
        return CredRegistryStorage.layout().vintageCounter.current();
    }

    function numBatches() public view returns (uint256) {
        return CredRegistryStorage.layout().batchCounter.current();
    }

    function numOffsets() public view returns (uint256) {
        return CredRegistryStorage.layout().offsetCounter.current();
    }

    function getProject(uint256 id) public view returns (CredRegistryTypes.Project memory) {
        require(CredRegistryTypes.isValidTokenID(id, PROJECT), "!VALID_ID");
        uint256 simpleID = CredRegistryTypes.simpleID(id);
        require(simpleID >= 1 && simpleID <= numProjects(), "!TOKEN_EXISTS");
        return CredRegistryStorage.layout().projects[id];
    }

    function getVintage(uint256 id) public view returns (CredRegistryTypes.Vintage memory) {
        require(CredRegistryTypes.isValidTokenID(id, VINTAGE), "!VALID_ID");
        uint256 simpleID = CredRegistryTypes.simpleID(id);
        require(simpleID >= 1 && simpleID <= numVintages(), "!TOKEN_EXISTS");
        return CredRegistryStorage.layout().vintages[id];
    }

    function getBatch(uint256 id) public view returns (CredRegistryTypes.Batch memory) {
        require(CredRegistryTypes.isValidTokenID(id, BATCH), "!VALID_ID");
        uint256 simpleID = CredRegistryTypes.simpleID(id);
        require(simpleID >= 1 && simpleID <= numBatches(), "!TOKEN_EXISTS");
        return CredRegistryStorage.layout().batches[id];
    }

    function getOffset(uint256 id) public view returns (CredRegistryTypes.OffsetInfo memory) {
        require(CredRegistryTypes.isValidTokenID(id, OFFSET), "!VALID_ID");
        uint256 simpleID = CredRegistryTypes.simpleID(id);
        require(simpleID >= 1 && simpleID <= numOffsets(), "!TOKEN_EXISTS");

        CredRegistryStorage.Layout storage l = CredRegistryStorage.layout();
        CredRegistryTypes.Offset storage offset = l.offsets[id];
        uint256 projectID = l.vintages[offset.vintageID].projectID;
        return
            CredRegistryTypes.OffsetInfo(
                projectID,
                offset.vintageID,
                offset.kilos,
                offset.timestamp,
                offset.credReward,
                offset.reason,
                uri(id)
            );
    }

    /**
     * @dev Returns the token ID for the CARB token corresponding to the specified VINTAGE.
     */
    function vintageCarbID(uint256 vintageID) public pure returns (uint256) {
        return CARB + CredRegistryTypes.simpleID(vintageID);
    }

    /**
     * @dev Returns the VINTAGE ID corresponding to the specified CARB token.
     */
    function carbVintageID(uint256 carbID) public pure returns (uint256) {
        return VINTAGE + CredRegistryTypes.simpleID(carbID);
    }

    /**
     * @dev Registers (mints) a new PROJECT with the given IRL registry ID.
     */
    function registerProject(string memory registryID) public onlyOwner whenNotPaused {
        require(bytes(registryID).length > 0, "!VALID_REGISTRY_ID");

        CredRegistryStorage.Layout storage l = CredRegistryStorage.layout();
        _incrementIdCounter(l.projectCounter);
        uint256 id = PROJECT + l.projectCounter.current();
        l.projects[id] = CredRegistryTypes.Project(registryID);
        _mint(_msgSender(), id, 1, "");
    }

    /**
     * @dev Registers (mints) a new VINTAGE of the PROJECT with token ID `projectID`, with the given vintage name.
     */
    function registerVintage(uint256 projectID, string memory vintageName) public onlyOwner whenNotPaused {
        require(CredRegistryTypes.isValidTokenID(projectID, PROJECT), "!VALID_ID");
        require(bytes(vintageName).length > 0, "!VALID_VINTAGE_NAME");
        uint256 simpleID = CredRegistryTypes.simpleID(projectID);
        require(simpleID >= 1 && simpleID <= numProjects(), "!TOKEN_EXISTS");

        CredRegistryStorage.Layout storage l = CredRegistryStorage.layout();
        _incrementIdCounter(l.vintageCounter);
        uint256 id = VINTAGE + l.vintageCounter.current();
        l.vintages[id] = CredRegistryTypes.Vintage(projectID, vintageName);
        _mint(_msgSender(), id, 1, "");
    }

    /**
     * @dev Creates (mints) a new batch in PENDING state.
     */
    function createBatch() public whenNotPaused {
        CredRegistryStorage.Layout storage l = CredRegistryStorage.layout();
        _incrementIdCounter(l.batchCounter);
        uint256 id = BATCH + l.batchCounter.current();
        l.batches[id] = CredRegistryTypes.Batch(0, 0, "", CredRegistryTypes.BatchStatus.PENDING);
        _mint(_msgSender(), id, 1, "");
    }

    function updateBatch(
        uint256 batchID,
        uint256 kilos,
        string memory serialNumber
    ) public whenNotPaused {
        require(CredRegistryTypes.isValidTokenID(batchID, BATCH), "!VALID_ID");
        require(kilos > 0, "!VALID_KILOS");
        require(bytes(serialNumber).length > 0, "!VALID_SERIAL_NUMBER");
        address operator = _msgSender();
        require(balanceOf(operator, batchID) == 1, "!OWNED");

        CredRegistryTypes.Batch storage batch = CredRegistryStorage.layout().batches[batchID];
        require(batch.status != CredRegistryTypes.BatchStatus.CONFIRMED, "!VALID_STATUS");

        batch.kilos = kilos;
        batch.serialNumber = serialNumber;
        _setBatchStatus(batchID, CredRegistryTypes.BatchStatus.PENDING);

        emit BatchUpdated(operator, batchID, kilos, serialNumber);
    }

    function confirmBatch(uint256 batchID, uint256 vintageID) public onlyOwner whenNotPaused {
        require(CredRegistryTypes.isValidTokenID(batchID, BATCH), "!VALID_ID");
        require(CredRegistryTypes.isValidTokenID(vintageID, VINTAGE), "!VALID_ID");

        CredRegistryTypes.Batch storage batch = CredRegistryStorage.layout().batches[batchID];
        require(batch.status == CredRegistryTypes.BatchStatus.PENDING, "!PENDING");
        require(batch.kilos > 0, "KILOS_!SET");
        require(bytes(batch.serialNumber).length > 0, "SERIAL_NUMBER_!SET");

        batch.vintageID = vintageID;
        _setBatchStatus(batchID, CredRegistryTypes.BatchStatus.CONFIRMED);

        emit BatchConfirmed(_msgSender(), batchID, vintageID, batch.kilos, batch.serialNumber);
    }

    function rejectBatch(uint256 batchID, string memory reason) public onlyOwner whenNotPaused {
        require(CredRegistryTypes.isValidTokenID(batchID, BATCH), "!VALID_ID");

        CredRegistryTypes.Batch storage batch = CredRegistryStorage.layout().batches[batchID];
        require(batch.status == CredRegistryTypes.BatchStatus.PENDING, "!PENDING");

        _setBatchStatus(batchID, CredRegistryTypes.BatchStatus.REJECTED);

        emit BatchRejected(_msgSender(), batchID, reason);
    }

    /**
     * @dev Fractionalizes a BATCH of N kilos. Mints N of the corresponding VINTAGE's CARB token,
     * with `to` as their owner. The sender must own the BATCH token, which is then locked in this contract.
     */
    function fractionalizeBatch(address to, uint256 batchID) public whenNotPaused {
        require(CredRegistryTypes.isValidTokenID(batchID, BATCH), "!VALID_ID");
        require(balanceOf(_msgSender(), batchID) == 1, "!OWNED");

        CredRegistryTypes.Batch storage batch = CredRegistryStorage.layout().batches[batchID];
        require(batch.status == CredRegistryTypes.BatchStatus.CONFIRMED, "!VALID_STATUS");

        uint256 carbID = vintageCarbID(batch.vintageID);
        safeTransferFrom(_msgSender(), address(this), batchID, 1, "");
        _mint(to, carbID, batch.kilos, "");
    }

    /**
     * @dev Creates a new offset for a given vintage. Retires `kilos` of the VINTAGE's CARB token
     * and mints an OFFSET to the `to` address, along with the CRED reward.
     * The sender must own the specified CARBs, which are burned.
     */
    function createOffset(
        address to,
        uint256 carbID,
        uint256 kilos,
        string memory reason
    ) public {
        createOffsetFrom(_msgSender(), to, carbID, kilos, reason);
    }

    /**
     * @dev Creates a new offset for a given vintage. Retires `kilos` of the VINTAGE's CARB token
     * and mints an OFFSET to the `to` address, along with the CRED reward.
     * The `from` account must own the specified CARBs, which are burned.
     */
    function createOffsetFrom(
        address from,
        address to,
        uint256 carbID,
        uint256 kilos,
        string memory reason
    ) public whenNotPaused {
        // checks

        require(CredRegistryTypes.isValidTokenID(carbID, CARB), "!VALID_ID");
        require(kilos > 0, "!VALID_KILOS");
        require(balanceOf(from, carbID) >= kilos, "!SUFFICIENT_CARB_HELD");

        // effects

        CredRegistryStorage.Layout storage l = CredRegistryStorage.layout();

        uint256 vintageID = carbVintageID(carbID);
        CredRegistryTypes.Vintage storage vintage = l.vintages[vintageID];

        uint256 credReward;
        ICredRewardCalculator rewardCalculator = l.rewardCalculator;
        if (address(rewardCalculator) != address(0)) {
            credReward = rewardCalculator.calculateReward(vintage.projectID, vintageID, kilos);
        }

        _incrementIdCounter(l.offsetCounter);
        uint256 id = OFFSET + l.offsetCounter.current();
        l.offsets[id] = CredRegistryTypes.Offset(carbVintageID(carbID), kilos, block.timestamp, credReward, reason);

        // interactions

        // burn the CARB
        burn(from, carbID, kilos);

        // mint the OFFSET
        _mint(to, id, 1, "");

        // console.log("minted offset:", id, vintageID, kilos);
        // console.log("  reason:", reason);

        // mint the CRED reward
        if (credReward != 0) {
            l.credToken.mint(to, credReward);
        }
    }

    /**
     * @dev Sets a new URI for all token types.
     */
    function setURI(string memory uri) external onlyOwner {
        _setBaseURI(uri);
    }

    function burn(
        address account,
        uint256 id,
        uint256 amount
    ) public whenNotPaused {
        _burn(account, id, amount);
    }

    function burnBatch(
        address account,
        uint256[] memory ids,
        uint256[] memory amounts
    ) public whenNotPaused {
        _burnBatch(account, ids, amounts);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function transferOwnership(address account) public override onlyOwner {
        super.transferOwnership(account);
        LibDiamond.setContractOwner(account); // this emits a second OwnershipTransferred event
    }

    // ==== CRED token ====

    function CRED() public view returns (Cred) {
        return CredRegistryStorage.layout().credToken;
    }

    // ==== Internal functions ===

    function _incrementIdCounter(Counters.Counter storage counter) internal {
        require(counter.current() < CredRegistryTypes.MAX_ID, "MAX_ID_EXCEEDED"); // highly unlikely, but we'll still check
        counter.increment();
    }

    function _msgSender() internal view returns (address) {
        return msg.sender;
    }

    function _setBatchStatus(uint256 batchID, CredRegistryTypes.BatchStatus status) internal {
        CredRegistryTypes.Batch storage batch = CredRegistryStorage.layout().batches[batchID];
        if (batch.status != status) {
            batch.status = status;
            emit BatchStatusUpdated(_msgSender(), batchID, status);
        }
    }
}

