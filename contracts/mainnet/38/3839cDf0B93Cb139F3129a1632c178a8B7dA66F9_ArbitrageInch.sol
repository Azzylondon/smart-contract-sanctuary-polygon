pragma solidity ^0.8.0;

import "./LogExpMath.sol";
import "./Math.sol";
import "./SafeMath.sol";
import "./FullMath.sol";
import "./interfaces/IERC20.sol";
import "./interfaces/IExchange.sol";
import "./interfaces/IUsdPlusToken.sol";
import "./interfaces/inch/IInchRouter.sol";
import "./interfaces/inch/IAggregationExecutor.sol";

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";

import "hardhat/console.sol";


contract ArbitrageInch is Initializable, AccessControlUpgradeable, UUPSUpgradeable, PausableUpgradeable {
    using LogExpMath for uint256;
    using SafeMath for uint160;
    using SafeMath for uint256;

    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    address constant usdcAddress = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174;
    address constant usdpAddress = 0x236eeC6359fb44CCe8f97E99387aa7F8cd5cdE1f;
    address constant inchRouterAddress = 0x1111111254fb6c44bAC0beD2854e76F90643097d;
    address constant botAddress = 0x6B5aCa17Dfc8fD86EF7f0ed9050f29fF448B8B22;

    address owner1;
    address balancesReceiver;
    
    function initialize() public initializer {
        __AccessControl_init();
        __Pausable_init();
        __UUPSUpgradeable_init();
        _grantRole(UPGRADER_ROLE, msg.sender);

        owner1 = msg.sender;
        balancesReceiver = owner1;
        IERC20(usdcAddress).approve(inchRouterAddress, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        IERC20(usdpAddress).approve(inchRouterAddress, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
    }

    function _authorizeUpgrade(address newImplementation) internal onlyRole(UPGRADER_ROLE) override {}

    function lol() public payable returns (uint256) { return 0;}

    function sendAmountToOwner(address tokenAddress, uint256 amount) public {
        require(msg.sender == owner1, "not owner");
        IERC20(tokenAddress).transfer(owner1, amount);
    }

    function allow() public {
        IERC20(usdcAddress).approve(inchRouterAddress, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        IERC20(usdpAddress).approve(inchRouterAddress, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
    }

    function fix(IAggregationExecutor caller, IInchRouter.SwapDescription memory desc, bytes calldata data) external returns (uint256) {
        require(msg.sender == owner1, "not owner");
        desc.dstReceiver = payable(botAddress);
        if (address(desc.srcToken) == usdcAddress) {
            
            IInchRouter router = IInchRouter(inchRouterAddress);
            //IERC20(usdcAddress).approve(inchRouterAddress, desc.amount);
            router.swap(caller, desc, data);
            
            uint256 usdpBalance = IERC20(usdpAddress).balanceOf(address(this)) - desc.amount;
            address exchange = IUsdPlusToken(usdpAddress).exchange();
            IERC20(usdpAddress).approve(exchange, usdpBalance);
            uint256 result = IExchange(exchange).redeem(usdcAddress, usdpBalance);
        } else {
            address exchange = IUsdPlusToken(usdpAddress).exchange();
            IERC20(usdcAddress).approve(exchange, desc.amount);
            uint256 result = IExchange(exchange).buy(usdcAddress, desc.amount);

            uint256 usdpBalance = IERC20(usdpAddress).balanceOf(address(this)) - desc.amount;
            IInchRouter router = IInchRouter(inchRouterAddress);
            //IERC20(usdpAddress).approve(inchRouterAddress, usdpBalance);
            router.swap(caller, desc, data);
        }
        return 0;
    }

    function wrap(uint256 amountIn) internal returns (uint256) {
        address exchange = IUsdPlusToken(usdpAddress).exchange();
        IERC20(usdcAddress).approve(exchange, amountIn);
        uint256 result = IExchange(exchange).buy(usdcAddress, amountIn);
        return result;
    }

    function unwrap(uint256 amountIn) internal returns (uint256) {
        address exchange = IUsdPlusToken(usdpAddress).exchange();
        IERC20(usdpAddress).approve(exchange, amountIn);
        uint256 result = IExchange(exchange).redeem(usdcAddress, amountIn);
        return result;
    }

}

