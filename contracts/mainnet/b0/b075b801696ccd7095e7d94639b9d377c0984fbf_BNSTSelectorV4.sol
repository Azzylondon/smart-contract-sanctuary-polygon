    bytes memory json = abi.encodePacked(
      '{"name": "Bansho-Shinra Datsui Token (BSDT)", "description": "Bansho-Shinra Datsui Token. This token certifies that the owner has ',
      balance.toString(),
      ' BSDT.\\n\\nhttps://banshou-shinra.com/", "image": "',
      url,
      '", "decimals": "',
      decimals.toString(),
      '"}'
    );
    return
      string(
        abi.encodePacked("data:application/json;base64,", Base64.encode(json))
      );
  }

  uint256[45] private __gap;
}

