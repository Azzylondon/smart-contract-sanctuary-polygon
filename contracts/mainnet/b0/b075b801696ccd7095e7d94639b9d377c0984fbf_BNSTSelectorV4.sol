contract BNSTSelector is Initializable, AccessControlEnumerableUpgradeable {
  using AddressUpgradeable for address;
  using StringsUpgradeable for uint256;
  using StringsUpgradeable for uint8;
  using SafeMathUpgradeable for uint256;

  IBNSTMetadata public _bsdt;

  string _baseUri;

  function initialize() public initializer {
    __BNSTSelector_init();
  }

  function __BNSTSelector_init() internal onlyInitializing {
    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
  }

  function setBaseUri(string memory uri_) external {
    require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "only ADMIN");
    _baseUri = uri_;
  }

  function setBNST(address addressBSDT_) external {
    require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "only ADMIN");
    _bsdt = IBNSTMetadata(addressBSDT_);
  }

  function uri(address owner_) public view virtual returns (string memory) {
    uint8 decimals = _bsdt.decimals();
    uint256 balance = _bsdt.balanceOf(owner_);
    uint8 number;
    bytes memory url;

    for (uint8 i; i < decimals; i++) {
      balance = balance.div(10);
