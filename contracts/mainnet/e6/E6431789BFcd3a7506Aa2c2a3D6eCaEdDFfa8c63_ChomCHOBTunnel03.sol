pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";

import "./interfaces/IChomCHOBTunnel03.sol";
import "./interfaces/IUniswapV2Router02.sol";
import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/IUniswapV2Pair.sol";

// import "hardhat/console.sol";

// ChomCHOBTunnel V0.3
// reimplement but keep storage layout
contract ChomCHOBTunnel03 is IChomCHOBTunnel03, AccessControlUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // storage layout from V0.1
    uint256 public MIN_DESTINATION_TOKEN;
    uint256 public MAX_DESTINATION_TOKEN;
    address public DESTINATION_TOKEN;
    address public destinationWallet;
    bytes32 public constant CONFIG_ROLE = keccak256("CONFIG_ROLE");
    bytes32 public constant FUND_OWNER_ROLE = keccak256("FUND_OWNER_ROLE");
    mapping(address => TokenInfo) public tokensInfo;

    // new storage layout
    address public WETH;
    address public middleWallet;
    bool public isPause;

    modifier notPause() {
        require(!isPause, "TUNNEL: METHOD_WAS_PAUSE");
        _;
    }

    function _safeTransferETH(address payable to, uint256 amount) internal {
        (bool sent, ) = to.call{value: amount}("");
        require(sent, "TUNNEL: SEND_ETHER_FAIL");
    }

    function _safeTransferETHWithData(
        address payable to,
        uint256 amount,
        bytes memory data
    ) internal {
        (bool sent, ) = to.call{value: amount}(data);
        require(sent, "TUNNEL: SEND_ETHER_FAIL");
    }

    fallback() external payable {}

    receive() external payable {}

    function initialize(
        address _destinationWallet,
        address _destinationToken,
        uint256 _minDestinationToken,
        uint256 _maxDestinationToken,
        address _configer,
        address _fundOwner
    ) external initializer {
        __AccessControl_init();
        destinationWallet = _destinationWallet;
        DESTINATION_TOKEN = _destinationToken;
        MIN_DESTINATION_TOKEN = _minDestinationToken;
        MAX_DESTINATION_TOKEN = _maxDestinationToken;

        // setup access role
        _setupRole(DEFAULT_ADMIN_ROLE, _fundOwner);
        _setupRole(FUND_OWNER_ROLE, _fundOwner);
        _setupRole(CONFIG_ROLE, _configer);
    }

    function getAmountIn(address token, uint256 amountOut)
        external
        view
        virtual
        override
        returns (uint256 amountIn, uint256 reserveIn)
    {
        TokenInfo memory _info = tokensInfo[token];
        require(_info.router != address(0), "TUNNEL: NOT_SUPPORT_TOKEN");

        uint256[] memory amountIns = IUniswapV2Router02(_info.router)
            .getAmountsIn(amountOut, _info.paths);
        amountIn = amountIns[0];

        (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(_info.firstPair)
            .getReserves();
        reserveIn = IUniswapV2Pair(_info.firstPair).token0() == token
            ? reserve0
            : reserve1;
    }

    function getAmountOut(address token, uint256 amountIn)
        external
        view
        virtual
        override
        returns (uint256 amountOut, uint256 reserveIn)
    {
        TokenInfo memory _info = tokensInfo[token];
        require(_info.router != address(0), "TUNNEL: NOT_SUPPORT_TOKEN");

        uint256[] memory amountOuts = IUniswapV2Router02(_info.router)
            .getAmountsOut(amountIn, _info.paths);
        amountOut = amountOuts[amountOuts.length - 1];

        (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(_info.firstPair)
            .getReserves();
        reserveIn = IUniswapV2Pair(_info.firstPair).token0() == token
            ? reserve0
            : reserve1;
    }

    function convert(
        address _token,
        uint256 amountIn,
        uint256 amountOutMin,
        uint256 deadline,
        string memory uref
    ) external virtual override notPause returns (uint256 amountOut) {
        require(deadline >= block.timestamp, "TUNNEL: OUT_OF_DATE");
        TokenInfo memory _info = tokensInfo[_token];
        require(_info.router != address(0), "TUNNEL: NOT_SUPPORT_TOKEN");

        IERC20Upgradeable(_token).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );
        uint256[] memory amountOuts = IUniswapV2Router02(_info.router)
            .swapExactTokensForTokens(
                amountIn,
                amountOutMin,
                _info.paths,
                destinationWallet,
                deadline
            );
        amountOut = amountOuts[amountOuts.length - 1];
        require(
            amountOut >= MIN_DESTINATION_TOKEN &&
                amountOut <= MAX_DESTINATION_TOKEN,
            "TUNNEL: TOKEN_OUT_OF_RANGE"
        );

        emit Convert(msg.sender, _token, amountIn, amountOut, uref);
    }

    function convertSupportingFeeOnTransfer(
        address _token,
        uint256 amountIn,
        uint256 amountOutMin,
        uint256 deadline,
        string memory uref
    ) external virtual override notPause returns (uint256 amountOut) {
        require(deadline >= block.timestamp, "TUNNEL: OUT_OF_DATE");
        TokenInfo memory _info = tokensInfo[_token];
        require(_info.router != address(0), "TUNNEL: NOT_SUPPORT_TOKEN");

        IERC20Upgradeable(_token).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );
        uint256 balanceBefore = IERC20Upgradeable(DESTINATION_TOKEN).balanceOf(
            address(this)
        );
        IUniswapV2Router02(_info.router)
            .swapExactTokensForTokensSupportingFeeOnTransferTokens(
                amountIn,
                amountOutMin,
                _info.paths,
                address(this),
                deadline
            );

        amountOut =
            IERC20Upgradeable(DESTINATION_TOKEN).balanceOf(address(this)) -
            balanceBefore;
        require(
            amountOut >= MIN_DESTINATION_TOKEN &&
                amountOut <= MAX_DESTINATION_TOKEN,
            "TUNNEL: TOKEN_OUT_OF_RANGE"
        );

        IERC20Upgradeable(DESTINATION_TOKEN).safeTransfer(
            destinationWallet,
            amountOut
        );

        emit Convert(msg.sender, _token, amountIn, amountOut, uref);
    }

    function convertWithTransfer(
        address token,
        uint256 amountIn,
        uint256 deadline,
        string memory uref
    ) external virtual override notPause {
        require(deadline >= block.timestamp, "TUNNEL: OUT_OF_DATE");
        require(
            // set router address(1) for allow use in only this method
            tokensInfo[token].router == address(1),
            "TUNNEL: NOT_SUPPORT_TOKEN"
        );

        IERC20Upgradeable(token).safeTransferFrom(
            msg.sender,
            destinationWallet,
            amountIn
        );

        emit ConvertWithTransfer(msg.sender, token, amountIn, uref);
    }

    function convertWithTransferMiddleETH(uint256 deadline, string memory uref)
        external
        payable
        virtual
        override
        notPause
    {
        require(deadline >= block.timestamp, "TUNNEL: OUT_OF_DATE");
        uint256 amountIn = msg.value;

        _safeTransferETHWithData(payable(middleWallet), amountIn, bytes(uref));

        emit ConvertWithTransferETH(msg.sender, amountIn, uref);
    }

    function convertForETH(
        address _token,
        uint256 amountIn,
        uint256 amountOutMin,
        uint256 deadline,
        string memory uref
    ) external virtual override notPause returns (uint256 amountOut) {
        require(deadline >= block.timestamp, "TUNNEL: OUT_OF_DATE");
        TokenInfo memory _info = tokensInfo[_token];
        require(_info.router != address(0), "TUNNEL: NOT_SUPPORT_TOKEN");

        IERC20Upgradeable(_token).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );
        uint256[] memory amountOuts = IUniswapV2Router02(_info.router)
            .swapExactTokensForETH(
                amountIn,
                amountOutMin,
                _info.paths,
                middleWallet,
                deadline
            );
        amountOut = amountOuts[amountOuts.length - 1];

        emit ConvertForETH(msg.sender, _token, amountIn, amountOut, uref);
    }

    function convertForETHSupportingFeeOnTransfer(
        address _token,
        uint256 amountIn,
        uint256 amountOutMin,
        uint256 deadline,
        string memory uref
    ) external virtual override notPause returns (uint256 amountOut) {
        // check
        require(deadline >= block.timestamp, "TUNNEL: OUT_OF_DATE");
        TokenInfo memory _info = tokensInfo[_token];
        require(_info.router != address(0), "TUNNEL: NOT_SUPPORT_TOKEN");

        // effect

        // interact
        IERC20Upgradeable(_token).safeTransferFrom(
            msg.sender,
            address(this),
            amountIn
        );

        uint256 balanceBefore = address(this).balance;
        IUniswapV2Router02(_info.router)
            .swapExactTokensForETHSupportingFeeOnTransferTokens(
                amountIn,
                amountOutMin,
                _info.paths,
                address(this),
                deadline
            );

        amountOut = address(this).balance - balanceBefore;

        // transfer
        _safeTransferETH(payable(middleWallet), amountOut);
        emit ConvertForETH(msg.sender, _token, amountIn, amountOut, uref);
    }

    function setupTokenInfo(
        address _token,
        address _router,
        address _firstPair,
        address[] calldata _paths
    ) external virtual override onlyRole(CONFIG_ROLE) {
        require(_paths.length >= 2, "TUNNEL: AT_LEAST_TWO_TOKEN");
        require(
            _paths[0] == _token &&
                (_paths[_paths.length - 1] == DESTINATION_TOKEN ||
                    _paths[_paths.length - 1] == WETH),
            "TUNNEL: INVALID_PATH"
        );
        // allow _router = address(0) for disable token

        tokensInfo[_token] = TokenInfo(_token, _router, _firstPair, _paths);

        if (_router != address(0)) {
            IERC20Upgradeable(_token).approve(_router, type(uint256).max);
        }

        emit SetupTokenInfo(_token, _router, _paths.length);
    }

    function setDestinationWallet(address _destinationWallet)
        external
        virtual
        override
        onlyRole(FUND_OWNER_ROLE)
    {
        require(_destinationWallet != address(0), "TUNNEL: INVALID_ADDRESS");

        destinationWallet = _destinationWallet;
        emit SetDestinationWallet(_destinationWallet);
    }

    function setMiddleWallet(address _middleWallet)
        external
        virtual
        override
        onlyRole(FUND_OWNER_ROLE)
    {
        require(_middleWallet != address(0), "TUNNEL: INVALID_ADDRESS");

        middleWallet = _middleWallet;
        emit SetDestinationWallet(_middleWallet);
    }

    function setMinMaxDestinationToken(uint256 _minDest, uint256 _maxDest)
        external
        virtual
        override
        onlyRole(CONFIG_ROLE)
    {
        require(_minDest < _maxDest, "TUNNEL: INVALID_VALUE");

        MIN_DESTINATION_TOKEN = _minDest;
        MAX_DESTINATION_TOKEN = _maxDest;

        emit SetMinMaxDestinationToken(_minDest, _maxDest);
    }

    // WETH should setup in initialize but WETH come in v0.3, so we cannot use initialize in production contract anymore
    function setWETH(address _weth) external onlyRole(CONFIG_ROLE) {
        require(_weth != address(0), "TUNNEL: INVALID_ADDRESS");

        WETH = _weth;
    }
}

