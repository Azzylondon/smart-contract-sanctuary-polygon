pragma solidity 0.8.4;

import { Initializable } from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { EnumerableSet } from "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import { EnumerableMapUpgradeableExtra } from "../../external/EnumerableMapUpgradeableExtra.sol";
import { SafeERC20 } from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import { FixedPoint } from "../../core/FixedPoint.sol";

contract ProductVault is Initializable {
	using EnumerableMapUpgradeableExtra for EnumerableMapUpgradeableExtra.UintToUintMap;
	using EnumerableSet for EnumerableSet.UintSet;
	using SafeERC20 for IERC20;

	/// @dev feeId => percent
	EnumerableMapUpgradeableExtra.UintToUintMap private feesPercent;
	uint256 private totalFeesPercent;
	struct AccrualInfo {
		uint256 amount;
		uint256 updatedAt;
	}
	mapping(IERC20 => AccrualInfo) private feesAccrued;

	/// @dev fees are accrued once per this period
	uint256 private constant PERIOD_PRECISION = 1 hours;

	function _depositToken(IERC20 _token, uint256 _amount) internal {
		_accrueFees(_token);
		_token.safeTransferFrom(msg.sender, address(this), _amount);
	}

	function _withdrawToken(IERC20 _token, address _to) internal {
		_accrueFees(_token);
		_token.safeTransfer(_to, _pendingBalance(_token));
	}

	function _approveTokenWithdrawal(IERC20 _token, address _to)
		internal
		returns (uint256 amount)
	{
		_accrueFees(_token);
		amount = _pendingBalance(_token);
		_token.safeApprove(_to, amount);
	}

	/**
	 * @notice Returns amount of tokens available to withdraw using `_withdrawToken`.
	 * @param _token Token balance to check
	 */
	function _pendingBalance(IERC20 _token) internal view returns (uint256) {
		return _token.balanceOf(address(this)) - _pendingFees(_token);
	}

	/**
	 * @notice Accrue and withdraw all fees since last withdrawal.
	 * @param _to Recipient address
	 * @param _tokens Tokens to withdraw
	 */
	function _withdrawFees(address _to, IERC20[] memory _tokens) internal {
		uint256 tokensLen = _tokens.length;
		for (uint256 i = 0; i < tokensLen; i++) {
			_accrueFees(_tokens[i]);
		}

		for (uint256 i = 0; i < tokensLen; i++) {
			IERC20 token = _tokens[i];
			uint256 amount = feesAccrued[token].amount;
			feesAccrued[token].amount = 0;
			token.safeTransfer(_to, amount);
		}
	}

	/**
	 * @notice Set new fee percent.
	 * Accrues this fee with previous percent before setting a new fee.
	 * @param _feeId ID of the fee to set
	 * @param _newFee A new fee percent
	 * @param _tokens The fee will be accrued for this tokens
	 */
	function _setFeePerHour(
		uint256 _feeId,
		uint256 _newFee,
		IERC20[] memory _tokens
	) internal {
		uint256 len = _tokens.length;
		for (uint256 i = 0; i < len; i++) {
			_accrueFees(_tokens[i]);
		}
		if (_newFee == 0) {
			feesPercent.remove(_feeId);
		} else {
			feesPercent.set(_feeId, _newFee);
		}
		uint256 _totalFeesPercent = 0;
		uint256 feesLen = feesPercent.length();
		for (uint256 i = 0; i < feesLen; i++) {
			(, uint256 feePerPeriod) = feesPercent.at(i);
			_totalFeesPercent += feePerPeriod;
		}
		totalFeesPercent = _totalFeesPercent;
	}

	function _feePerHour(uint256 _feeId) internal view returns (uint256) {
		(bool success, uint256 fee) = feesPercent.tryGet(_feeId);
		if (!success) {
			return 0;
		}
		return fee;
	}

	function _pendingFees(IERC20 _token) internal view returns (uint256) {
		(uint256 accruedAmount, ) = _calcNewlyAccruedFees(_token);
		return accruedAmount;
	}

	/**
	 * @notice Accrue fees for a token since last accrual.
	 * @param _token Token to accrue
	 */
	function _accrueFees(IERC20 _token) private {
		(uint256 accruedAmount, uint256 currentPeriod) = _calcNewlyAccruedFees(
			_token
		);
		AccrualInfo storage accrual = feesAccrued[_token];
		accrual.amount = accruedAmount;
		accrual.updatedAt = currentPeriod;
	}

	function _calcNewlyAccruedFees(IERC20 _token)
		private
		view
		returns (uint256 accruedAmount, uint256 currentPeriod)
	{
		AccrualInfo storage accrual = feesAccrued[_token];
		currentPeriod = _getCurrentPeriod();
		uint256 lastPeriod = accrual.updatedAt;
		if (lastPeriod == 0) {
			lastPeriod = currentPeriod;
		}
		if (lastPeriod == currentPeriod) {
			return (accrual.amount, currentPeriod);
		}

		uint256 bal = _token.balanceOf(address(this)) - accrual.amount;
		uint256 newlyAccrued = bal -
			FixedPoint.mulFixedPoint(
				bal,
				FixedPoint.pow(
					FixedPoint.ONE - totalFeesPercent,
					currentPeriod - lastPeriod
				)
			);
		return (accrual.amount + newlyAccrued, currentPeriod);
	}

	function _getCurrentPeriod() private view returns (uint256 period) {
		return block.timestamp / PERIOD_PRECISION;
	}
}

