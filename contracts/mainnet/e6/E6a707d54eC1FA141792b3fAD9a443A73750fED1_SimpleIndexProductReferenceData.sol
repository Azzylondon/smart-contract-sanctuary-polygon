import { ERC721Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import { OwnableUpgradeable } from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import { IERC20 } from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import { Address } from "@openzeppelin/contracts/utils/Address.sol";
import { FixedPoint } from "../../core/FixedPoint.sol";
import { ProductVault } from "./ProductVault.sol";
import { IUniswapRouter } from "../../external/IUniswapRouter.sol";

contract SimpleIndexProductReferenceData is OwnableUpgradeable {
	address internal _manager;
	struct TokenInfo {
		IERC20 token;
		uint256 percentage;
	}
	TokenInfo[] internal _tokenInfos;
	uint256 internal _managementFeePerHour;

	modifier onlyManager() {
		require(msg.sender == _manager, "caller is not the manager");
		_;
	}

	modifier onlyManagerOrOwner() {
		require(
			msg.sender == _manager || msg.sender == owner(),
			"caller is not the manager or the owner"
		);
		_;
	}

	function initialize(address manager_) external initializer {
		__Ownable_init();
		_manager = manager_;
	}

	function getReferenceData()
		external
		view
		returns (
			address manager,
			TokenInfo[] memory tokenInfos,
			uint256 managementFeePerHour
		)
	{
		return (_manager, _tokenInfos, _managementFeePerHour);
	}

	function setManager(address manager_) external onlyManagerOrOwner {
		_manager = manager_;
	}

	function setTokenReferenceData(TokenInfo[] calldata tokenInfos_)
		external
		onlyManager
	{
		delete _tokenInfos;
		uint256 sum = 0;
		for (uint256 i = 0; i < tokenInfos_.length; i++) {
			TokenInfo calldata info = tokenInfos_[i];
			sum += info.percentage;
			_tokenInfos.push(info);
		}
		require(sum == FixedPoint.ONE, "invalid percentages");
	}

	function setManagementFeePerHour(uint256 managementFeePerHour_)
		external
		onlyManager
	{
		_managementFeePerHour = managementFeePerHour_;
	}
}

contract SimpleIndexProduct is ProductVault, ERC721Upgradeable {
	address public feeReceiver;
	IERC20[] public tokens;

	uint256 public constant BENEFICIARY_TOKEN_ID = 1;
	uint256 public constant MANAGER_TOKEN_ID = 2;

	uint256 public constant MANAGEMENT_FEE_ID = 1;

	modifier onlyBeneficiary() {
		require(msg.sender == beneficiary(), "caller is not the beneficiary");
		_;
	}

	modifier onlyManager() {
		require(msg.sender == manager(), "caller is not the manager");
		_;
	}

	function initialize(
		address _beneficiary,
		address _manager,
		address _feeReceiver,
		uint256 _managementFeePerHour,
		IERC20[] calldata _tokens,
		uint256[] calldata _amounts
	) external initializer {
		__ERC721_init("SimpleIndexProduct", "SIP");
		_mint(_beneficiary, BENEFICIARY_TOKEN_ID);
		_mint(_manager, MANAGER_TOKEN_ID);

		feeReceiver = _feeReceiver;
		_setFeePerHour(MANAGEMENT_FEE_ID, _managementFeePerHour, _tokens);

		require(_tokens.length == _amounts.length, "lengths differ");
		uint256 len = _tokens.length;
		for (uint256 i = 0; i < len; ++i) {
			_depositToken(_tokens[i], _amounts[i]);
		}
		tokens = _tokens;
	}

	function balanceOfBeneficiary(IERC20 _token)
		external
		view
		returns (uint256)
	{
		return _pendingBalance(_token);
	}

	function depositMore(IERC20[] calldata _tokens, uint256[] calldata _amounts)
		external
		onlyBeneficiary
	{
		uint256 len = _tokens.length;
		require(len == tokens.length, "invalid tokens");
		require(len == _amounts.length, "lengths differ");
		for (uint256 i = 0; i < len; ++i) {
			IERC20 token = _tokens[i];
			require(token == tokens[i], "invalid tokens");
			_depositToken(token, _amounts[i]);
		}
	}

	function dissolve() external onlyBeneficiary {
		(address benef, IERC20[] memory _tokens) = _dissolveBase();
		uint256 len = _tokens.length;
		for (uint256 i = 0; i < len; ++i) {
			_withdrawToken(_tokens[i], benef);
		}
	}

	struct SwapExactInInfo {
		address[] path;
		uint256 amountOutMin;
	}

	function dissolveAndSell(
		IUniswapRouter _router,
		SwapExactInInfo[] calldata _swaps,
		uint256 _deadline
	) external onlyBeneficiary {
		(address benef, IERC20[] memory _tokens) = _dissolveBase();

		uint256 len = _tokens.length;
		require(_swaps.length == len, "invalid swaps length");
		for (uint256 i = 0; i < len; ++i) {
			SwapExactInInfo calldata swap = _swaps[i];
			IERC20 token = _tokens[i];
			require(swap.path[0] == address(token), "invalid swap path[0]");
			uint256 amountIn = _approveTokenWithdrawal(token, address(_router));
			_router.swapExactTokensForTokens(
				amountIn,
				swap.amountOutMin,
				swap.path,
				benef,
				_deadline
			);
			require(
				token.balanceOf(address(this)) == 0,
				"did not sell all tokens"
			);
		}
	}

	function dissolveAndSellForETH(
		IUniswapRouter _router,
		bytes4 _swapExactTokensForETHSelector,
		SwapExactInInfo[] calldata _swaps,
		uint256 _deadline
	) external onlyBeneficiary {
		(address benef, IERC20[] memory _tokens) = _dissolveBase();

		uint256 len = _tokens.length;
		require(_swaps.length == len, "invalid swaps length");
		for (uint256 i = 0; i < len; ++i) {
			SwapExactInInfo calldata swap = _swaps[i];
			IERC20 token = _tokens[i];
			require(swap.path[0] == address(token), "invalid swap path[0]");
			uint256 amountIn = _approveTokenWithdrawal(token, address(_router));
			Address.functionCall(
				address(_router),
				abi.encodeWithSelector(
					_swapExactTokensForETHSelector,
					amountIn,
					swap.amountOutMin,
					swap.path,
					benef,
					_deadline
				)
			);
			require(
				token.balanceOf(address(this)) == 0,
				"did not sell all tokens"
			);
		}
	}

	// TODO: sell all tokens to one token and trasnfer to the beneficiary
	function _dissolveBase() private returns (address, IERC20[] memory) {
		address benef = beneficiary();
		IERC20[] memory _tokens = tokens;
		_burn(BENEFICIARY_TOKEN_ID);
		_burn(MANAGER_TOKEN_ID);
		_withdrawFees(feeReceiver, _tokens);
		return (benef, _tokens);
	}

	function beneficiary() public view returns (address) {
		return ownerOf(BENEFICIARY_TOKEN_ID);
	}

	function manager() public view returns (address) {
		return ownerOf(MANAGER_TOKEN_ID);
	}

	function setManagementFeePerHour(uint256 _newFee) external onlyManager {
		_setFeePerHour(MANAGEMENT_FEE_ID, _newFee, tokens);
	}

	function managementFeePerHour() external view returns (uint256) {
		return _feePerHour(MANAGEMENT_FEE_ID);
	}

	function setTokens(IERC20[] calldata _tokens) external onlyBeneficiary {
		tokens = _tokens;
	}

	function getTokens() external view returns (IERC20[] memory) {
		return tokens;
	}

	function setFeeReceiver(address _feeReceiver) external onlyManager {
		feeReceiver = _feeReceiver;
	}

	function withdrawFees() external onlyManager {
		_withdrawFees(feeReceiver, tokens);
	}

	function pendingFees(IERC20 _token) external view returns (uint256) {
		return _pendingFees(_token);
	}
}

