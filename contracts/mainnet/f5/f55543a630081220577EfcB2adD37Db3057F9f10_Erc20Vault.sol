pragma solidity 0.8.2;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./../interface/IErc20StrategiesManager.sol";
import "./../interface/IErc20RewardManager.sol";

contract Erc20Vault is ERC20, Ownable {
    using SafeERC20 for IERC20;

    // @notice The address of the ERC20 token to deposit.
    address public token;
    // @notice In charge of the Curve deposits/withdrawals.
    address public strategyManager;
    // @notice Helper to swap tokens.
    address public rewardManager;

    // @notice Variables used to calculate a fair IOU value.
    mapping(uint8 => uint256) strategiesLps;
    mapping(uint8 => uint256) strategiesPreviousLps;
    mapping(uint8 => uint256) strategiesLpsVirtualPrice;
    uint256 ious;
    uint256 strategiesLpsSum;
    uint256 addedStrategiesLpsSum;

    // @notice Boolean to block deposits/withdrawals.
    bool public isClosed;

    // @notice Secondary role with less privileges than owner.
    address public relayer;
    // @notice Temporary mapping to whitelist users.
    mapping(address => bool) private team;

    // @notice An event triggered when a deposit is completed.
    // @param _value The amount of token deposited.
    event Fund(uint256 _value);

    // @notice An event triggered when a withdrawal is completed.
    // @param _to The destination address.
    // @param _value The amount in token withdrawn.
    event Withdrawal(address indexed _to, uint256 _value);

    // @notice An event triggered when rebalancing orders are set.
    // @param instruction The instruction code signaling the function to call.
    // @param strategy The address of the strategy where the instruction is executed.
    // @param amount The token amount getting rebalanced.
    event Rebalance(uint256 instruction, address strategy, uint256 amount);

    modifier onlyTeam() {
        require(team[msg.sender] == true, "only team");
        _;
    }

    modifier onlyOwnerAndRelayer() {
        require(msg.sender == owner() || msg.sender == relayer);
        _;
    }

    /**
     * @notice Creates a new Vault which accepts a token.
     * @param _name Name of the vault token.
     * @param _symbol Symbol of the vault token.
     * @param _token The address of the token that can be deposited or withdrawn.
     * @param _rewardManager Address of the reward manager.
     * @param _relayer Address of the relayer.
     */
    constructor(
        string memory _name,
        string memory _symbol,
        address _token,
        address _rewardManager,
        address _relayer
    ) ERC20(_name, _symbol) {
        isClosed = false;
        token = _token;
        rewardManager = _rewardManager;
        relayer = _relayer;

        team[0xAfCb545E3F2fA80f1AF9F29262b0bD823CD660D5] = true;
        team[0x0C0BB3535E96b47C0E7A65bEFd1A11B7e13BCBeb] = true;
        team[0xEA29fa603cd0DEdDb183a5Db54FA182b564E8412] = true;
        team[0x90Cc775BB8f21eF9cbA7868c6Aa7e91a13A8a7B8] = true;
        team[0x5fBBa8D0D0C77DB5EF559b2D02BC42460aefd452] = true;
        team[0xD0114334C97b446d6360e6994F742F3F9AB63462] = true;
        team[0x2Dea1237679c5df256DbFddA0fD82b274cD59Bdd] = true;
        team[0x6Cf9AA65EBaD7028536E353393630e2340ca6049] = true;
    }

    /**
     * @notice External-facing method to deposit an amount in the vault.
     * @param _amount The ERC20 token amount to deposit.
     */
    function deposit(uint256 _amount) public onlyTeam {

        IERC20(token).safeTransferFrom(msg.sender, address(this), _amount);
        (uint256 totalStrategies, uint256 totalValueAdded, uint256 slipInSum) = _deposit(_amount);
        uint256 cygIous = calculateIous(totalStrategies, totalValueAdded, slipInSum);

        emit Fund(_amount);

        _mint(msg.sender, cygIous);
    }

    /**
     *
     * @notice Handles the logic of the deposit.
     * @param _amount The ERC20 token amount to deposit.
     * @return The total amount of strategies, the total value added and the slippage of the deposit.
     */
    function _deposit(uint256 _amount) internal returns (uint256, uint256, uint256) {
        require(isClosed == false, "Vault closed");
        require(IErc20StrategiesManager(strategyManager).ratioTotal() == 100 * 1e18);

        uint256 totalStrategies = IErc20StrategiesManager(strategyManager).totalStrategies();
        uint256 depositRatio = 0;

        // Reset variables
        uint256[] memory valueAdded = new uint256[](totalStrategies);
        uint256 totalValueAdded = 0;
        uint256 slipInSum = 0;
        strategiesLpsSum = 0;
        addedStrategiesLpsSum = 0;
        ious = totalSupply();

        for (uint8 i = 0; i < totalStrategies; i++) {
            uint256 strategyRatio = IErc20StrategiesManager(strategyManager).getRatio(i);
            if (strategyRatio > 0) {
                uint256 strategyAmount = _amount * strategyRatio / 1e18 / 100;
                uint256 existingLps = IErc20StrategiesManager(strategyManager).getConvexLpBalance(i);
                uint256 lps = IErc20StrategiesManager(strategyManager).deposit(i, token, strategyAmount);
                uint256 virtualBalance = IErc20StrategiesManager(strategyManager).getVirtualPrice(i);
                addedStrategiesLpsSum += lps;
                strategiesPreviousLps[i] = existingLps;
                strategiesLps[i] = existingLps + lps;
                strategiesLpsSum += existingLps;
                strategiesLpsVirtualPrice[i] = virtualBalance;
                slipInSum += (lps * virtualBalance) / strategyAmount * strategyRatio / 1e18 / 100;
                valueAdded[i] = lps * virtualBalance / 1e18;
                totalValueAdded += lps * virtualBalance / 1e18;
            } else {
                valueAdded[i] = 0;
            }
        }

        // Update ratios
        for (uint8 i = 0; i < totalStrategies; i++) {
            if (i < totalStrategies - 1) {
                valueAdded[i] = strategiesLps[i] * 1e18 / dotStrategiesVirtualPrice(false, totalStrategies) * 100;
                depositRatio += valueAdded[i];
            } else {
                valueAdded[i] = (100 * 1e18) - depositRatio;
                depositRatio = 100 * 1e18;
            }
        }
        IErc20StrategiesManager(strategyManager).setRatios(valueAdded);

        return (totalStrategies, totalValueAdded, slipInSum);
    }

    /**
     * @notice Withdraws an amount to the token holder.
     * @param _shares The amount of vault shares to withdraw.
     * @return The ERC20 token amount sent to the user.
     */
    function withdraw(uint256 _shares) public onlyTeam returns (uint256) {
        require(_shares <= balanceOf(msg.sender), "withdraw amount > shares owned");

        ious = totalSupply();

        _burn(msg.sender, _shares);

        uint256 tokenAmount = 0;

        if (isClosed) {
            uint256 tokenBalance = IERC20(token).balanceOf(address(this));
            tokenAmount = (tokenBalance * _shares) / (totalSupply() + _shares);
        } else {
            require(IErc20StrategiesManager(strategyManager).ratioTotal() == 100 * 1e18, "Funds not allocated");
            uint256 totalStrategies = IErc20StrategiesManager(strategyManager).totalStrategies();
            tokenAmount = removeLiquidity(_shares, totalStrategies);
        }

        require(tokenAmount > 0);

        IERC20(token).safeTransfer(msg.sender, tokenAmount);
        emit Withdrawal(msg.sender, tokenAmount);

        return tokenAmount;
    }

    /*
     * @notice This method is called by the optimizer to pass orders (deposit
     * /withdraw) that the vault should execute to rebalance the strategies.
     * @param stratIds: The ids of active strategies.
     * @param sigs: The function signatures to be called on the strategies.
     * @param payloads: input arguments to the sigs. This is a byte array, where
     * each element is an array of bytes containing all input arguments. The optimiser
     * should pass in individual arguments padded to 32 bytes (as normally done in eth transaction input field)
     */
    function rebalance(uint8[] memory stratIds, bytes4[] memory sigs, bytes[] memory payloads) external onlyOwnerAndRelayer {
        require(isClosed == false);

        for (uint256 i = 0; i < stratIds.length; i++) {
            address _strategyManager = strategyManager;
            require(stratIds[i] < IErc20StrategiesManager(strategyManager).totalStrategies());

            bytes4 sig = sigs[i];
            // Only call deposit or withdraw using call
            require((sig == bytes4(0x00838172)) || (sig == bytes4(0x70563d6b)), "wrong sig");
            bytes memory payload = payloads[i];

            uint256 ret;
            // solhint-disable-next-line no-inline-assembly
            assembly {
                let x := mload(0x40)
                mstore(x, sig)
                mstore(add(x, 0x04), mload(add(payload, 0x20)))
                mstore(add(x, 0x24), mload(add(payload, 0x40)))
                mstore(add(x, 0x44), mload(add(payload, 0x60)))

                ret := call(
                    gas(), // send all remaining gas
                    _strategyManager, // To stratAddress
                    0, // No eth value to send
                    x, // Inputs are stored at location x
                    0x64, // 4 + 32 + 32 + 32 = 100 byte input, 64 in hex.
                    x, // Store output over input location (saves space)
                    0x00 // No output, so zero size
                )

                let size := returndatasize()

                let ptr := mload(0x40)
                returndatacopy(ptr, 0, size)

                // if the call returned error data, throw it here
                switch ret
                case 0 {
                    revert(ptr, size)
                }
            }
        }
    }

    /**
     * @notice Will harvest all active strategies for CRV, CVX and reward tokens.
     */
    function harvestAll() external {
        _harvest(true);
    }


    /**
     * @notice Will harvest all active strategies for CRV and CVX only.
     */
    function harvestCrvCvx() external {
        _harvest(false);
    }

    /**
     * @notice Handles the logic to perform a harvesting on strategies.
     * @param rewardTokens Boolean to indicate if it should also harvest reward tokens.
     */
    function _harvest(bool rewardTokens) internal {
        require(rewardManager != address(0));

        // Swap all CRV/CVX, eventually rewards tokens and return funds
        IErc20RewardManager(rewardManager).collectCrvCvx(token);

        if (rewardTokens == true) {
            uint256 totalStrategies = IErc20StrategiesManager(strategyManager).totalStrategies();
            for (uint8 i = 0; i < totalStrategies; i++) {
                IErc20StrategiesManager(strategyManager).harvest(i);
                IErc20RewardManager(rewardManager).collectRewardTokens(token, IErc20StrategiesManager(strategyManager).getRewardTokens(i), IErc20StrategiesManager(strategyManager).getSwapRouter(i));
            }
        }

        IErc20RewardManager(rewardManager).returnRewards(token);
    }

    /**
     * @notice It calculates the fair number of IOUs to mint for the deposit amount taking into account slippage in and out.
     * @param totalStrategies The number of strategies.
     * @param totalValueAdded The value added after the deposit in Curve and Convex based on LP quantity and virtual price.
     * @param slipInSum The sum of the slippages taken on the deposit.
     * @return The number of IOUs to mint for the deposit.
     */
    function calculateIous(uint256 totalStrategies, uint256 totalValueAdded, uint256 slipInSum) internal returns (uint256) {

        uint256 iousToMint;

        if (ious > 0 && strategiesLpsSum > addedStrategiesLpsSum) {

            uint256 iousSimulate = ious * totalValueAdded / dotStrategiesVirtualPrice(true, totalStrategies);
            uint256 slipOut = simulateSlippageOut(ious, iousSimulate, totalStrategies);
            uint256 slipOutOld = slipOut;
            bool converged = false;

            for (uint8 i = 0; i < 5; i++) {
                iousSimulate = ious * totalValueAdded / slipOut * 1e36 / slipInSum / dotStrategiesVirtualPrice(true, totalStrategies);
                slipOut = simulateSlippageOut(ious, iousSimulate * 1e12, totalStrategies);
                if (slipOutOld > slipOut) {
                    if (slipOutOld - slipOut < 1e12) {
                        converged = true;
                        break;
                    }
                } else {
                    if (slipOut - slipOutOld < 1e12) {
                        converged = true;
                        break;
                    }
                }
                slipOutOld = slipOut;
            }

            require(converged, "Can't compute IOU amount");

            iousToMint = ious * totalValueAdded / slipOut * 1e18 / slipInSum * 1e18 / dotStrategiesVirtualPrice(true, totalStrategies) * 1e12;
        } else {
            iousToMint = totalValueAdded;
        }

        ious += iousToMint;

        return iousToMint;
    }

    /**
     * @notice Simulates how much slippage a withdraw from a Curve pool will take.
     * @param cygIous The current number of vault IOUs.
     * @param iousAmount The estimated amount of IOUs to mint for the current deposit.
     * @param totalStrategies The number of strategies.
     * @return The simulated amount of slippage out for the deposit amount.
     */
    function simulateSlippageOut(uint256 cygIous, uint256 iousAmount, uint256 totalStrategies) private returns (uint256) {

        uint256 withdrawalShare = (iousAmount * 1e18) / (cygIous + iousAmount);
        uint256 withdrawalValue = withdrawalShare * dotStrategiesVirtualPrice(false, totalStrategies) / 1e18;

        uint256 tokensReturned = 0;

        for (uint8 i = 0; i < totalStrategies; i++) {
            if (strategiesLps[i] > 0) {
                uint256 amountWithdrawal = (strategiesLps[i] * withdrawalShare) / 1e18;
                uint256 strategyTokensReturned = IErc20StrategiesManager(strategyManager).simulateWithdraw(i, token, amountWithdrawal);
                tokensReturned += strategyTokensReturned;
            }
        }

        return (tokensReturned * 1e18 * 1e12) / withdrawalValue;
    }

    /**
     * @notice Removes liquidity from the pool according to the currency allocation.
     * @param iousAmount The total amount of shares to withdraw.
     * @param totalStrategies The number of strategies.
     * @return The amount of ERC20 tokens returned.
     */
    function removeLiquidity(uint256 iousAmount, uint256 totalStrategies) internal returns (uint256) {
        uint256 withdrawalShare = iousAmount * 1e18 / ious;
        uint256 tokensReturned = 0;

        for (uint8 i = 0; i < totalStrategies; i++) {
            uint256 existingLps = IErc20StrategiesManager(strategyManager).getConvexLpBalance(i);
            uint256 amountWithdrawal = existingLps * withdrawalShare / 1e18;
            uint256 strategyTokensReturned = IErc20StrategiesManager(strategyManager).withdraw(i, token, amountWithdrawal);
            tokensReturned += strategyTokensReturned;
        }

        return tokensReturned;
    }

    /**
     * @notice The dot product of the quantity of LPs owned by the vault with their virtual price.
     * @param old Calculation with previous or current LPs amounts.
     * @param length The number of active strategies.
     * @return Result of the dot product.
    */
    function dotStrategiesVirtualPrice(bool old, uint256 length) private view returns (uint256) {
        uint256 total = 0;

        for (uint8 i = 0; i < length; i++) {
            if (old) {
                total += strategiesPreviousLps[i] * strategiesLpsVirtualPrice[i];
            } else {
                total += strategiesLps[i] * strategiesLpsVirtualPrice[i];
            }
        }

        return total / 1e18;
    }

    /**
     * @notice Closes vault, brings back funds from strategies.
     */
    function closeVault() external onlyOwner {
        isClosed = true;
        IErc20StrategiesManager(strategyManager).closePositions();
    }

    /**
     * @notice Opens vault and allocates funds to strategies.
     */
    function openVault() external onlyOwner {
        require(IErc20StrategiesManager(strategyManager).ratioTotal() == 100 * 1e18);
        isClosed = false;

        _deposit(IERC20(token).balanceOf(address(this)));
    }

    /**
     * @dev Sets a new reward manager on the vault and the strategies manager.
     * @param _newRewardManager The address of the new reward manager.
     */
    function setRewardManager(address _newRewardManager) external onlyOwner {
        require(strategyManager != address(0));
        rewardManager = _newRewardManager;
        IErc20StrategiesManager(strategyManager).setRewardManager(_newRewardManager);
    }

    /**
     * @dev Sets a new relayer.
     * @param _newRelayer The address of the new relayer.
     */
    function setRelayer(address _newRelayer) external onlyOwner {
        relayer = _newRelayer;
    }

    /**
     * @dev Set a new strategy manager and approves {token} for eventual rebalances.
     * @param _newStrategyManager The address of the new strategy manager.
     */
    function setStrategyManager(address _newStrategyManager) external onlyOwner {
        strategyManager = _newStrategyManager;
        IERC20(token).safeApprove(strategyManager, type(uint256).max);
    }

    /**
     * @dev Temporary method to set a team member
     * @param memberAddress The address of the new team member.
     */
    function setTeamMember(address memberAddress) public onlyOwner {
        team[memberAddress] = true;
    }
}

