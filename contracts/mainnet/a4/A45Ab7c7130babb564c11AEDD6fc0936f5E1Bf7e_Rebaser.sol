pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/interfaces/IERC1155.sol"; // for BloodShop
import "./utils/EnumDeclarations.sol";
import "hardhat/console.sol";

interface IBloodShop is IERC1155 {
    function isActiveBoon(address account, uint16 tokenId) external pure returns (bool);
}

interface ICultBinder {
    function bindingsCount() external view returns (uint16);
    function getBoundGawds() external view returns (uint16[] memory);
    function bindersCount() external view returns (uint16);
    function getBinders() external view returns (address[] memory);
    function bind(uint16 gawdId, Cult cult) external;
    function unbind(uint16 gawdId) external;
    function isBound(uint16 gawdId) external view returns (bool);
    function boundAt(uint16 gawdId) external view returns (uint256);
    function cultForGawd(uint16 gawdId) external view returns (Cult);
    function cultForOwner(address owner) external view returns (Cult);
    function bindersCountForCult(Cult cult) external view returns (uint16);
}

interface IGawdOwners {
    function getOwners() external view returns (address[] memory);
    function ownersCount() external view returns (uint16);
    function gawdsForOwner(address owner) external view returns (uint16[] memory);
    function gawdsCountForOwner(address owner) external view returns (uint256);
    function ownerForGawd(uint16 gawdId) external view returns (address);
}

interface IGawdsOnchain {
    function getGawdPowers(uint16 gawdId) external view returns (Power[] memory);
    function getDominantPower(uint16 gawdId) external view returns (string memory);
    function getDominantPowerId(uint16 gawdId) external view returns (Power);
}

interface IGawchi {
    function getHealth(uint16 gawd) external view returns (uint256);
    function getAlive(uint16 gawd) external view returns (bool);
}

interface IBlood {
    function mint(address to, uint256 amount) external;
    function decimals() external view returns (uint8);
    function transfer(address to, uint256 amount) external returns (bool);
    function burn(address account, uint256 amount) external;
}

contract Rebaser is OwnableUpgradeable {

    mapping(uint16 => address) internal gawdOwners;

    address public cultContract;
    address public gawchiContract;
    address public bloodContract;
    address public gawdsContract;
    address public ownersContract;
    address public bloodShopContract;

    uint256 public precision;
    uint256 public currentYield;

    uint256 public bloodPerGawd;

    Power public alignedPower;
    uint256 public alignedPowerBoost;

    uint256 public cultBoost;

    // polygon block time target is 2s
    uint256 public blocksPerEpoch;
    uint256 public lastRebaseBlock;

    uint16 public secondsPerBlock;

    event Rebase(uint256 indexed blockNum, uint16 totalHappyGawds, uint256 totalBlood);
    event YieldChanged(uint256 oldYield, uint256 newYield);

    // ** UPGRADE-FRIENDLY INIT ** //
    function initialize() public initializer {
        __Ownable_init();

        precision = 1000;
        currentYield = precision; // 100%
        bloodPerGawd = 1;
        alignedPowerBoost = 0; // disabled by default
        cultBoost = 0; // disabled by default
        secondsPerBlock = 2;
        blocksPerEpoch = 3600 * 4 / secondsPerBlock; // every 4 hours
    }

    // ** CONFIGURATION ** //

    // ** LINKED CONTRACTS ** //
    function setCultContractAddress(address addr) public onlyOwner {
        cultContract = addr;
    }

    function setGawchiContractAddress(address addr) public onlyOwner {
        gawchiContract = addr;
    }

    function setBloodContractAddress(address addr) public onlyOwner {
        bloodContract = addr;
    }

    function setGawdsContractAddress(address addr) public onlyOwner {
        gawdsContract = addr;
    }

    function setOwnersContractAddress(address addr) public onlyOwner {
        ownersContract = addr;
    }

    function setBloodShopContractAddress(address addr) public onlyOwner {
        bloodShopContract = addr;
    }

    // ** GAME CONFIG ** //
    function setBlocksPerEpoch(uint256 newBlocks) public onlyOwner {
        blocksPerEpoch = newBlocks;
    }

    function setSecondsPerBlock(uint16 blocks) public onlyOwner {
        setBlocksPerEpoch((blocksPerEpoch * secondsPerBlock) / blocks);
        secondsPerBlock = blocks;
    }

    function setBloodPerGawd(uint256 blood) public onlyOwner {
        bloodPerGawd = blood;
    }

    function setCurrentYield(uint256 newYield) public onlyOwner {
        uint256 oldYield = currentYield;
        currentYield = newYield;
        emit YieldChanged(oldYield, newYield);
    }

    // ** TIMING ** //
    function updateLastRebaseBlock() public onlyOwner {
        lastRebaseBlock = block.number;
    }

    function nextRebaseBlock() public view returns (uint256) {
        if(lastRebaseBlock == 0) {
            return 0;
        }
        return lastRebaseBlock + blocksPerEpoch;
    }

    function blocksTilNextRebase() public view returns (uint256) {
        if(lastRebaseBlock == 0) {
            return 0;
        }
        return lastRebaseBlock + blocksPerEpoch - block.number;
    }

    function rebaseAvailable() public view returns (bool) {
        return blocksTilNextRebase() <= 0;
    }

    // ** REWARD CALCULATIONS ** //
    function computeReward(uint256 overallHealth, uint16 aliveGawds, uint16 alignedGawds, uint256 cultBoostPercent) public view returns (uint256) {
        if (overallHealth <= 0) {
            return 0;
        }

        // console.log("...overallHealth %", overallHealth);
        // console.log("...alignedGawds", alignedGawds);
        // console.log("...aliveGawds", aliveGawds);
        // console.log("...cultBoostPercent %", cultBoostPercent);   // 1800 = 1.8 = 180%
        // console.log("...alignedPowerBoost %", alignedPowerBoost); // 2000 = 2.0 = 200% = double blood
        // console.log("...earlyAccessBoost %", currentYield);       // 1000 = 1.0 = 100%

        uint256 quantity1 = overallHealth * bloodPerGawd * aliveGawds;
        // console.log('.....Regular Gawd Earnings:', quantity1);

        uint256 quantity2 = overallHealth * bloodPerGawd * alignedGawds * alignedPowerBoost / precision;
        // console.log('.....Power Aligned Gawd Earnings:', quantity2);

        uint256 totalBoost = (cultBoostPercent + currentYield);
        // console.log('.....totalBoost %', totalBoost);

        uint256 quantity3 = ((quantity1 + quantity2) * totalBoost) / precision;
        // console.log('.....Cult + Early Access Boost Earnings:', quantity3);

        uint256 total = quantity1 + quantity2 + quantity3;
        // console.log('=> grand total:', total);

        return total;
    }

    function mintForOwner(address account, uint256 quantity) public onlyOwner {
        require(bloodContract != address(0), "missing blood contract address");
        uint256 preciseQuantity = quantity * (10**18) / precision;
        IBlood(bloodContract).mint(account, preciseQuantity);
    }

    function getGawdsCount(address account) public view returns (uint256) {
        require(ownersContract != address(0), "missing gawdsOnchain contract address");
        return IGawdOwners(ownersContract).gawdsCountForOwner(account);
    }

    // legacy, DEPRECATEME
    function getHappyGawdsCount(address account) public view returns (uint16) {
        require(gawchiContract != address(0), "missing gawchi contract address");
        require(ownersContract != address(0), "missing gawdsOnchain contract address");

        uint16[] memory gawds = IGawdOwners(ownersContract).gawdsForOwner(account);
        uint16 happyGawds = 0;

        for(uint256 j = 0; j < gawds.length; j++) {
            uint16 id = gawds[j];

            bool alive = IGawchi(gawchiContract).getAlive(id);
            if(alive) {
                happyGawds++;
            }
        }

        return happyGawds;
    }

    function getGawdsCountForCults() public view returns (uint256[4] memory) {
        uint256[4] memory totals;
        totals[0] = 0;
        totals[1] = 0;
        totals[2] = 0;
        totals[3] = 0;

        address[] memory binders = ICultBinder(cultContract).getBinders();
        for(uint16 i = 0; i < binders.length; i++) {
            address account = binders[i];
            Cult accountCult = ICultBinder(cultContract).cultForOwner(account);
            uint256 gawdsCount = IGawdOwners(ownersContract).gawdsCountForOwner(account);
            totals[uint(accountCult)] += gawdsCount;
        }

        return totals;
    }

    function getOverallHealthAndAlignedPowersCount(address account, Power checkPower) public view returns (uint256, uint16, uint16) {
        require(gawchiContract != address(0), "missing gawchi contract address");
        require(ownersContract != address(0), "missing gawdsOnchain contract address");

        uint16[] memory gawds = IGawdOwners(ownersContract).gawdsForOwner(account);

        if (gawds.length <= 0) {
            return (0, 0 ,0);
        }

        uint256 overallHealth = 0;
        uint16 aliveGawds = 0;
        uint16 alignedGawds = 0;

        for(uint16 i = 0; i < gawds.length; i++) {
            uint16 id = gawds[i];

            uint256 health = IGawchi(gawchiContract).getHealth(id);
            overallHealth += health;

            if (health > 0) {
                aliveGawds += 1;

                // only check dominantPower
                Power power = IGawdsOnchain(gawdsContract).getDominantPowerId(id);
                if (power == checkPower) {
                    alignedGawds += 1;
                }
            }
        }

        return (overallHealth / gawds.length, aliveGawds, alignedGawds);
    }

    // ** POWER ALIGNMENT BOOSTS ** //
    function setAlignedPower(Power power) public onlyOwner {
        alignedPower = power;
    }

    // alignedPower() = getAlignedPower()

    function setAlignedPowersBoost(uint256 boost) public onlyOwner {
        alignedPowerBoost = boost;
    }

    // alignedPowerBoost() = getAlignedPowerBoost()

    function getAlignedPowersCount(uint16 gawdId) public view returns (uint16) {
        Power[] memory powers = IGawdsOnchain(gawdsContract).getGawdPowers(gawdId);
        uint16 alignedPowers = 0; // TODO
        for(uint16 i = 0; i < powers.length; i++) {
            if(powers[i] == alignedPower) {
                alignedPowers++;
            }
        }
        return alignedPowers;
    }

    function getAlignedPowersBoost(uint16 gawdId) public view returns (uint256) {
        uint16 alignedPowers = getAlignedPowersCount(gawdId);
        return alignedPowers * alignedPowerBoost;
    }

    // ** CULT POPULATION BOOSTS ** //
    function calculateCultPopulationBoost(uint16 population, uint16 highestPopulation, uint16 total) private view returns (uint256) {
        if(total == 0 || highestPopulation == 0) {
            return 0;
        }

        uint256 highestPercent = highestPopulation * precision / total;
        uint256 percent = population * precision / total;
        uint256 amount = (highestPercent - percent) ** 3 * 10 / precision / precision;

        if (amount > 5000) {
            amount = 5000;
        }
        return amount;
    }

    function calculateCultPopulationBoosts(uint16[4] memory populations, uint16 highestValue) private view returns (uint16[4] memory, uint256[4] memory) {
        // arguably should be calculating highestValue in this function, but it is more efficient
        // to only loop through all the binders once in getAllCultPopulationsAndBoosts()
        // it's already bad enough we are iterating through all the binders at all!
        uint16 total = ICultBinder(cultContract).bindersCount();

        uint256[4] memory amounts;

        for(uint8 i = 1; i <= 3; i++) {
            uint16 population = populations[i];
            amounts[i] = calculateCultPopulationBoost(population, highestValue, total);
        }

        return (populations, amounts);
    }

    function getAllCultPopulationsAndBoosts() public view returns (uint16[4] memory, uint256[4] memory) {
        // using an array rather than a mapping so we can iterate and select highest value
        // inserting 0 for index 0 so our cult indexes align
        uint16[4] memory populations;
        populations[0] = 0;

        uint16 highestValue = 0;

        for(uint8 i = 1; i <= 3; i++) {
            populations[i] = ICultBinder(cultContract).bindersCountForCult(Cult(i));
            if(populations[i] > highestValue) {
                highestValue = populations[i];
            }
        }

        return calculateCultPopulationBoosts(populations, highestValue);
    }

    // ** MAIN BEAST ** //
    // FIXME this no longer fits within a reasonable amount of gas
    // RIP on-chain rebase()
    function rebase() public onlyOwner {
        require(cultContract != address(0), "missing cult contract address");
        require(gawchiContract != address(0), "missing gawchi contract address");
        require(bloodContract != address(0), "missing blood contract address");
        require(ownersContract != address(0), "missing gawdsOnchain contract address");

        updateLastRebaseBlock();

        address[] memory binders = ICultBinder(cultContract).getBinders();

        uint256 totalBlood = 0;
        uint16 totalHappyGawds = 0;

        // cache all cultBoosts. ignore populations array, we're not using it
        (, uint256[4] memory cultBoosts)  = getAllCultPopulationsAndBoosts();

        // iterate all binders and issue their rewards
        for(uint256 i = 0; i < binders.length; i++) {
            address account = binders[i];

            // skip empty slots caused by unbinding
            if(account == address(0)) {
              continue;
            }

            // fetch this person's cult and that cult's boost amount
            Cult cult = ICultBinder(cultContract).cultForOwner(account);
            uint256 myCultBoost = cultBoosts[uint(cult)];

            // fetch this person's # of happyGawds, alignedGawds, and alignedParts
            (uint256 overallHealth, uint16 aliveGawds, uint16 alignedGawds) = getOverallHealthAndAlignedPowersCount(account, alignedPower);
            uint256 quantity = computeReward(overallHealth, aliveGawds, alignedGawds, myCultBoost);

            if(aliveGawds > 0) {
                mintForOwner(account, quantity);
                totalHappyGawds += aliveGawds;
                totalBlood += quantity;
            }
        }

        emit Rebase(lastRebaseBlock, totalHappyGawds, totalBlood);
    }


    // ** BOON/ITEM CHECKS ** //
    function autofarmerBoonIds() public pure returns (uint16[4] memory) {
        return [uint16(2), uint16(3), uint16(4), uint16(5)];
    }

    function hasBoon(address account, uint16 tokenId) public view returns (bool) {
        require(bloodShopContract != address(0), "bloodShop contract not set");
        bool result =
            IBloodShop(bloodShopContract).balanceOf(account, tokenId) > 0 ||
            IBloodShop(bloodShopContract).isActiveBoon(account, tokenId);
        return result;
    }

    // a simple delegate to BloodShop.isActiveBoon(), so that Gawchi only needs to connect here
    function isActiveBoon(address account, uint16 tokenId) public view returns (bool) {
        require(bloodShopContract != address(0), "bloodShop contract not set");
        return IBloodShop(bloodShopContract).isActiveBoon(account, tokenId);
    }

    function accountHasActiveBoon(address account, uint16 tokenId) public view returns (bool) {
        return hasBoon(account, tokenId) && isActiveBoon(account, tokenId);
    }

    function gawdHasActiveBoon(uint16 gawdId, uint16 tokenId) public view returns (bool) {
        require(ownersContract != address(0), "no ownersContract set");

        address owner = IGawdOwners(ownersContract).ownerForGawd(gawdId);
        if(owner == address(0)) {
            return false;
        }

        return accountHasActiveBoon(owner, tokenId);
    }
}

