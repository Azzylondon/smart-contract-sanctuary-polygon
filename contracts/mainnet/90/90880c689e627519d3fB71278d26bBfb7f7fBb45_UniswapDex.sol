pragma solidity 0.6.12;

import { IERC20 } from "../erc20/IERC20.sol";
import { FlashLoanReceiverBase } from "@aave/protocol-v2/contracts/flashloan/base/FlashLoanReceiverBase.sol";
import { ILendingPool } from "@aave/protocol-v2/contracts/interfaces/ILendingPool.sol";
import { AccessControl } from "../accessControl/AccessControl.sol";
import { IDexSwap } from "../dex/IDexSwap.sol";

contract Arbitrage is AccessControl { 

    ILendingPool public LENDING_POOL;

    bytes32 public constant EXECUTOR_ROLE = keccak256("EXECUTOR_ROLE");
    bytes32 public constant BENEFICIARY_ROLE = keccak256("BENEFICIARY_ROLE");
    bytes32 public constant LENDING_POOL_ROLE = keccak256("LENDING_POOL_ROLE");

    address beneficiary;

    struct FlashLoanParams {
        address dexA;
        address dexB;
        address tokenFrom;
        address tokenTo;
        uint256 amount;
        uint256 minAmount;
        uint24 fee;
    }

    constructor(ILendingPool _lendingPool) public {
        LENDING_POOL = _lendingPool;
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(LENDING_POOL_ROLE, address(_lendingPool));
    }

    event FlashLoanExecuted();

    function executeOperation(
        address[] calldata assets,
        uint256[] calldata amounts,
        uint256[] calldata premiums,
        address initiator,
        bytes calldata params
    ) onlyRole(LENDING_POOL_ROLE)
        external
        returns (bool)
    {

        FlashLoanParams memory params = decodeFlashLoanParams(params);

        _setAllowance(IERC20(params.tokenFrom), params.amount, address(params.dexA));
        IDexSwap(params.dexA).safeSwap(address(this), params.tokenFrom, params.tokenTo, params.amount, params.minAmount, abi.encode(params.fee));
        
        _setAllowance(IERC20(params.tokenTo), IERC20(params.tokenTo).balanceOf(address(this)), address(params.dexB));
        IDexSwap(params.dexB).safeSwapAll(address(this), params.tokenTo, params.tokenFrom, abi.encode(params.fee));
        

        for (uint i = 0; i < assets.length; i++) {
            uint amountOwing = amounts[i] + (premiums[i]);
            // Move the profits to beneficiary
            IERC20 asset = IERC20(assets[i]);
            uint256 qtyToTransferToBeneficiary = asset.balanceOf(address(this)) - amountOwing;
            asset.transfer( beneficiary, qtyToTransferToBeneficiary);

            //Approve lending amount with fees to lending pool
            IERC20(assets[i]).approve(address(LENDING_POOL), amountOwing);
        }
        
        return true;
    }
    
    function executeFlashLoan(address _dexA, address _dexB, address _tokenFrom, address _tokenTo, uint256 _amount, uint256 _minAmount, uint24 _fee) public onlyRole(EXECUTOR_ROLE) {
        address receiverAddress = address(this);

        address[] memory assets = new address[](1);
        assets[0] = address(_tokenFrom);

        uint256[] memory amounts = new uint256[](1);
        amounts[0] = _amount;

        // Mode 0 reverts if contract has
        uint256[] memory modes = new uint256[](1);
        modes[0] = 0;

    
        uint16 referralCode = 0;

        FlashLoanParams memory flashLoanParams = FlashLoanParams(_dexA, _dexB, _tokenFrom, _tokenTo, _amount, _minAmount, _fee);

        bytes memory params = encodeFlashLoanParams(flashLoanParams);

        LENDING_POOL.flashLoan(
            receiverAddress,
            assets,
            amounts,
            modes,
            address(this),
            params,
            referralCode
        );

        emit FlashLoanExecuted();
    }

    function setLendingPool(address _lendingPool) external onlyRole(DEFAULT_ADMIN_ROLE) {
        LENDING_POOL = ILendingPool(_lendingPool);
    }

    function setBeneficiary(address _beneficiary) external onlyRole(DEFAULT_ADMIN_ROLE) {
        beneficiary = _beneficiary;
    }

    function encodeFlashLoanParams(FlashLoanParams memory _params) public view returns (bytes memory) {
        return abi.encode(_params.dexA, _params.dexB, _params.tokenFrom, _params.tokenTo, _params.amount, _params.minAmount, _params.fee);
    }

    function decodeFlashLoanParams(bytes memory _encodedParams) public view returns (FlashLoanParams memory) {
        (address dexA,address dexB,address tokenFrom,address tokenTo,uint256 amount,uint256 minAmount, uint24 fee) = abi.decode(_encodedParams, (address,address,address,address,uint256,uint256,uint24));
        return FlashLoanParams(dexA, dexB, tokenFrom, tokenTo, amount, minAmount, fee);
    }


    function _setAllowance(IERC20 _token, uint256 _amount, address _spender) internal {
        if(_token.allowance(address(this), address(_spender)) >= _amount) {
            return;
        }

        _token.approve(address(_spender),  2**256 - 1);
    }
}

