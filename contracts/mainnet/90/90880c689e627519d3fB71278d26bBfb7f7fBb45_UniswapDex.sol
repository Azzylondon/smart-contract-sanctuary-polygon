pragma solidity 0.6.12;

import "./IDexSwap.sol";
import { UniswapV2Router02 } from "./dependencies/SushiDependencies.sol";
import { IERC20 } from "../erc20/IERC20.sol";

contract SushiSwapDex is IDexSwap {

    UniswapV2Router02 public sushiSwap;
    address nativeTokenAddress;

    event SwapEthForToken();
    event SwapTokenForEth();
    event SwapTokenForToken();

    constructor(UniswapV2Router02 _sushiSwap, address _nativeTokenAddress) public {
        sushiSwap = _sushiSwap;
        nativeTokenAddress = _nativeTokenAddress;
    }

    // Amount max is calculated in sushi swap frontend.
    function safeSwap(address beneficiary, address _tokenFrom, address _tokenTo, uint256 _amount, uint256 _minAmountToReceive, bytes memory _data) public override {
        _setAllowance(IERC20(_tokenFrom), _amount);
        _moveFundsFromBeneficiary(IERC20(_tokenFrom), beneficiary, _amount);
        
        address[] memory path = new address[](2);
        path[0] = _tokenFrom;
        path[1] = _tokenTo;

        uint256 deadline = block.timestamp + (10 * 60);
        if(_tokenFrom == nativeTokenAddress) {
            sushiSwap.swapExactETHForTokens.value(_amount)(_minAmountToReceive, path, address(this), deadline);
            emit SwapEthForToken();
            _moveAllFunds(IERC20(_tokenTo), beneficiary);
            return;
        }

        if (_tokenTo == nativeTokenAddress) {
            sushiSwap.swapExactTokensForETH(_amount, _minAmountToReceive, path, address(this), deadline);
            emit SwapTokenForEth();
            _moveAllFunds(IERC20(_tokenTo), beneficiary);
            return;
        }
        
        sushiSwap.swapExactTokensForTokens(_amount, _minAmountToReceive, path, address(this), deadline);
        emit SwapTokenForToken();
        _moveAllFunds(IERC20(_tokenTo), beneficiary);
    }


    function safeSwapAll(address beneficiary, address _tokenFrom, address _tokenTo, bytes memory _data) public override {
        uint256 amount = IERC20(_tokenFrom).balanceOf(beneficiary);
        safeSwap(beneficiary, _tokenFrom, _tokenTo, amount, 0, _data);
    }


    function _setAllowance(IERC20 _token, uint256 _amount) internal {
        if (address(_token) == nativeTokenAddress) {
            return;
        }

        if(_token.allowance(address(this), address(sushiSwap)) >= _amount) {
            return;
        }

        _token.approve(address(sushiSwap),  2**256 - 1);
    }

    function _moveAllFunds(IERC20 _token, address _beneficiary) internal {
        uint256 totalAmount = _token.balanceOf(address(this));
        _token.transfer(_beneficiary, totalAmount);
    }

    function _moveFundsFromBeneficiary(IERC20 _token, address _beneficiary, uint256 _amount) internal {
        _token.transferFrom(_beneficiary, address(this), _amount);
    }
}