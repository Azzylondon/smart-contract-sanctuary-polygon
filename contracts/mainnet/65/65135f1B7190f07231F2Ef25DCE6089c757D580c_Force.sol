import "./S3.sol";

abstract contract S6 is S3 {
    function buy(address _bayer, uint256 lvl) public isRegistred {
        require(
            activate[msg.sender][lvl] == false,
            "This level is already activated"
        );
        require(lvl < 12, "lvl is max");
        // ОТправляем деньги на контракт

        emit simpleBuy(_bayer, lvl);
        uint256 _price = prices[lvl];
        token.transferFrom(msg.sender, address(this), _price);
        // (bool success, ) = address(SFC).call(
        //     abi.encodeWithSignature(
        //         "buyForce(address, uint)",
        //         msg.sender,
        //         _price
        //     )
        // );
        // require(success, "Call failed");

        for (uint256 i; i < lvl; i++) {
            require(
                activate[_bayer][i] == true,
                string(abi.encodePacked("Following lvl is not activated :", i))
            );
        }

        // Activate new lvl
        activate[_bayer][lvl] = true;

        if (products[lvl] == Product.s3) {
            updateS3(_bayer, lvl, false);
        } else {
            console.log("bayer ", _bayer);
            updateS6(_bayer, lvl, false);
        }
        // not sure but _fromPreviouslvl is really first buy, so we dont

        token.transfer(
            payerToReceipy[_bayer].to,
            payerToReceipy[_bayer].amount
        );
        payerToReceipy[_bayer].amount = 0;
        // ПЕРПВОД БАБОК (КЭШ, МАНЕЙ). Проверить на запуск функции единожды
    }

    function buyInternal(address _bayer, uint256 lvl) internal isRegistred {
        require(
            activate[msg.sender][lvl] == false,
            "This level is already activated"
        );
        require(lvl < 12, "lvl is max");
        // for (uint256 i; i < lvl; i++) {
        //     require(
        //         activate[_bayer][i] == true,
        //         string(abi.encodePacked("Following lvl is not activated :", i))
        //     );
        // }
        activate[_bayer][lvl] = true;
        if (products[lvl] == Product.s3) {
            updateS3(_bayer, lvl, true);
        } else {
            updateS6(_bayer, lvl, true);
        }
    }

    function updateS3(
        address _child,
        uint256 _lvl,
        bool _fromPreviousLvl
    ) internal isRegistred {
        address _parent = getActivateParent(_child, _lvl);

        structS3 storage _parentStruct = matrixS3[_parent][_lvl];
        uint256 _lastChild = _parentStruct.lastChild;
        _addNavigation(_parent, _lvl, _lastChild, false);
        _parentStruct.lastChild++;
        _lastChild = _lastChild % 3;

        uint256 _price = prices[_lvl];
        payerToReceipy[msg.sender].to = _parent;

        if (_lastChild == 0) {
            if (!_fromPreviousLvl) {
                if (users[_parent].autoUpgrade[_lvl]) {
                    uint256 amoutSC = (_price * 25) / 100;
                    innerFoundation = innerFoundation + amoutSC;
                    payerToReceipy[msg.sender].amount += (_price - amoutSC);
                } else {
                    _parentStruct.frozenMoneyS3 =
                        _parentStruct.frozenMoneyS3 +
                        _price;
                }
            }
        }

        if (_lastChild == 1) {
            if (users[_parent].autoUpgrade[_lvl]) {
                uint256 amoutSC = (_price * 25) / 100;
                innerFoundation = innerFoundation + amoutSC;
                payerToReceipy[msg.sender].amount += (_price - amoutSC);
            } else {
                // THINK
                if (_lvl < 11) {
                    require(
                        // Добавить frozen money, проверить Frozen Money
                        _parentStruct.frozenMoneyS3 >= _price,
                        "Not enough frozen money"
                    );
                    emit upgrade(msg.sender, _lvl + 1);
                    if (!activate[_parent][_lvl + 1]) {
                        buyInternal(_parent, _lvl + 1);
                    } else {
                        payerToReceipy[msg.sender].amount += _price * 2;
                    }
                } else {
                    payerToReceipy[msg.sender].amount += _price * 2;
                }

                _parentStruct.frozenMoneyS3 =
                    _parentStruct.frozenMoneyS3 -
                    _price;
            }
        }

        if (_lastChild == 2) {
            if (users[_parent].autoReCycle[_lvl]) {
                uint256 amoutSC = (_price * 25) / 100;
                innerFoundation = innerFoundation + amoutSC;
                payerToReceipy[msg.sender].amount += (_price - amoutSC);
                activate[msg.sender][_lvl] = false;
            } else {
                if (_parent != owner()) {
                    emit newSlot(msg.sender, _lvl);
                    // i think here should be payerToReceipy[msg.sender].amount += _price and
                    updateS3(_parent, _lvl, true); // update parents product
                }
            }
            _parentStruct.slot++;
        }

        childsS3[_parent][_lvl].push(_child);
    }

    function updateS6(
        address _child,
        uint256 lvl,
        bool _fromPreviousLvl
    ) internal isRegistred returns (address) {
        // Get active parent
        address _parent = getActivateParent(_child, lvl);
        console.log("PARENT ", _parent);
        structS6 storage _parentStruct = matrixS6[_parent][lvl];
        uint256 _lastChild1 = _parentStruct.lastChild1;
        uint256 _row = 1;


        if (_lastChild1 == 0) {
            // If lc==0, then row == 1
            _setNull(_parent, lvl);
        } else {
            console.log("PARENT SLOT", _parentStruct.slot);
            uint256 _emptySpot = _findEmptySpotOnStruct(
                _parentStruct.slot,
                _parent,
                lvl
            ); // Looking for empty spot on parent struct
            console.log("EMPTY SPOT IN PAR", _emptySpot);
            if (_emptySpot > 1) _row = 2; // Find Row
        }
        _addNavigation(_parent, lvl, _lastChild1, _row == 1 ? false : true);
        uint256 _price = prices[lvl];


        //Check row
        if (_row == 1) {
            // Get GrandPa
            address _grandpa = marketingParents[_parent][lvl];
            uint256 _grandpaSlot = matrixS6[_grandpa][lvl].slot;


            marketingParents[_child][lvl] = _parent; // set marketing parent
            console.log("SLOT", _grandpaSlot);
            console.log("GRANDPA", _grandpa);

            // Check admin
            if (_parent != _grandpa) {
                // Get grandpa leg
                uint256 _grandpaLeg;
                if (
                    childsS6Lvl1[_grandpa][lvl][_grandpaSlot * 2] == _parent &&
                    childsS6Lvl1[_grandpa][lvl][_grandpaSlot * 2 + 1] != _parent
                ) {
                    _grandpaLeg = 0;
                } else {
                    _grandpaLeg = 1;
                }

                uint256 _grandpaPosition;

                // set info on grandparent
                if (
                    childsS6Lvl1[_parent][lvl][_parentStruct.slot * 2] !=
                    address(0)
                ) {
                    if (_grandpaLeg == 0) {
                        _grandpaPosition = 1;
                    } else {
                        _grandpaPosition = 3;
                    }
                    _changePosition(
                        _grandpa,
                        _price,
                        _grandpaPosition,
                        lvl,
                        _fromPreviousLvl
                    );
                } else {
                    if (_grandpaLeg == 0) {
                        _grandpaPosition = 0;
                    } else {
                        _grandpaPosition = 2;
                    }

                    _changePosition(
                        _grandpa,
                        _price,
                        _grandpaPosition,
                        lvl,
                        _fromPreviousLvl
                    );
                }
            } else {
                payerToReceipy[msg.sender].to = _parent;
            }
            
            // Set first row
            if (
                childsS6Lvl1[_parent][lvl][_parentStruct.slot * 2] == address(0)
            ) {
                require(
                    childsS6Lvl1[_parent][lvl][_parentStruct.slot * 2] ==
                        address(0),
                    "This position is set"
                );
                childsS6Lvl1[_parent][lvl][_parentStruct.slot * 2] = _child;
            } else {
                require(
                    childsS6Lvl1[_parent][lvl][_parentStruct.slot * 2 + 1] ==
                        address(0),
                    "This position is set"
                );
                childsS6Lvl1[_parent][lvl][_parentStruct.slot * 2 + 1] = _child;
            }

            _parentStruct.lastChild1++;
        } else {


            uint256 _position = _findEmptySpot(_parent, lvl);
            console.log("FIX2", _position);
            // Set second row
            _changePosition(_parent, _price, _position, lvl, _fromPreviousLvl);

            address __child;
            if (_position < 2) {
                __child = childsS6Lvl1[_parent][lvl][
                    _parentStruct.lastChild1 - 2
                ];
            } else {
                __child = childsS6Lvl1[_parent][lvl][
                    _parentStruct.lastChild1 - 1
                ];
            }

            structS6 storage _childStruct = matrixS6[__child][lvl];

            if (_childStruct.lastChild1 == 0) {
                _setNull(__child, lvl);
            }
            //updates6 in other way
            if (
                childsS6Lvl1[__child][lvl][_childStruct.slot * 2] == address(0)
            ) {
                require(
                    childsS6Lvl1[__child][lvl][_childStruct.slot * 2] ==
                        address(0),
                    "This position is set"
                );
                childsS6Lvl1[__child][lvl][_childStruct.slot * 2] = msg.sender;
            } else {
                require(
                    childsS6Lvl1[__child][lvl][_childStruct.slot * 2 + 1] ==
                        address(0),
                    "This position is set"
                );
                childsS6Lvl1[__child][lvl][_childStruct.slot * 2 + 1] = msg
                    .sender;
            }
                marketingParents[msg.sender][lvl] = __child;
            _childStruct.lastChild1++;
        }

        return _parent;
    }

    function _findEmptySpotOnStruct(
        uint256 _slot,
        address _parent,
        uint256 _lvl
    ) internal view returns (uint256 _position) {
        uint256 _index;
        for (uint256 i = 0; i < 2; i++) {
            _index = _slot * 2 + i;
            if (childsS6Lvl1[_parent][_lvl][_index] == address(0)) return i;
        }

        _index = _findEmptySpot(_parent, _lvl);
        return _index + 2;
    }

    function _findEmptySpot(address _parent, uint256 _lvl)
        internal
        view
        returns (uint256 _position)
    {
        structS6 memory _parentStruct = matrixS6[_parent][_lvl];
        uint256 _index;
        for (uint256 i = 0; i < 4; i++) {
            _index = (_parentStruct.slot * 4 + i) % 4;

            if (
                _index == 0 &&
                childsS6Lvl2[_parent][_lvl][_parentStruct.slot * 4 + 0] ==
                address(0)
            ) {
                return 0;
            }

            if (
                _index == 1 &&
                childsS6Lvl2[_parent][_lvl][_parentStruct.slot * 4 + 2] ==
                address(0)
            ) {
                return 2;
            }

            if (
                _index == 2 &&
                childsS6Lvl2[_parent][_lvl][_parentStruct.slot * 4 + 1] ==
                address(0)
            ) {
                console.log("BIG", _parent);
                return 1;
            }

            if (
                _index == 3 &&
                childsS6Lvl2[_parent][_lvl][_parentStruct.slot * 4 + 3] ==
                address(0)
            ) {
                return 3;
            }
        }
    }

    function _getPositionLvl2(structS6 memory _parent)
        internal
        pure
        returns (uint256 position)
    {
        position = _parent.lastChild2 % 4;
    }

    function _changePosition(
        address _parent,
        uint256 _price,
        uint256 _position,
        uint256 _lvl,
        bool _fromPreviousLvl
    ) internal {
        structS6 storage _parentStruct = matrixS6[_parent][_lvl];
        uint256 _slot = _parentStruct.slot;

        uint256 _spot;
        uint256 _index;
        for (uint256 i; i < 4; i++) {
            _index = _slot * 4 + i;
            if (childsS6Lvl2[_parent][_lvl][_index] != address(0)) _spot++;
        }
        console.log("FIX", _spot);

        require(
            childsS6Lvl2[_parent][_lvl][(_slot * 4) + _position] == address(0),
            "This position is set"
        );
        childsS6Lvl2[_parent][_lvl][(_slot * 4) + _position] = msg.sender;

        _addNavigation(_parent, _lvl, _slot * 4 + _position, true);

        payerToReceipy[msg.sender].to = _parent;

        if (_spot == 0) {
            if (!_fromPreviousLvl) {
                payerToReceipy[msg.sender].amount =
                    payerToReceipy[msg.sender].amount +
                    _price;
            }
        }

        if (_spot == 1) {
            if (users[_parent].autoUpgrade[_lvl]) {
                uint256 amoutSC = (_price * 25) / 100;
                innerFoundation = innerFoundation + amoutSC;
                payerToReceipy[msg.sender].amount += (_price - amoutSC);
            } else {
                // Записать в замороженные деньги
                _parentStruct.frozenMoneyS6 =
                    _parentStruct.frozenMoneyS6 +
                    _price;
            }
        }

        if (_spot == 2) {
            if (users[_parent].autoUpgrade[_lvl]) {
                uint256 amoutSC = (_price * 25) / 100;
                innerFoundation = innerFoundation + amoutSC;
                payerToReceipy[msg.sender].amount += (_price - amoutSC);
            } else {
                // THINK
                if (_lvl < 11) {
                    require(
                        // Добавить frozen money, проверить Frozen Money
                        _parentStruct.frozenMoneyS6 >= _price,
                        "Not enough frozen money"
                    );
                    emit upgrade(msg.sender, _lvl + 1);

                    if (!activate[_parent][_lvl + 1]) {
                        buyInternal(_parent, _lvl + 1);
                    } else {
                        payerToReceipy[msg.sender].amount += _price * 2;
                    }
                } else {
                    payerToReceipy[msg.sender].amount += _price * 2;
                }

                _parentStruct.frozenMoneyS6 =
                    _parentStruct.frozenMoneyS6 -
                    _price;
            }
        }

        if (_spot == 3) {
            if (users[_parent].autoReCycle[_lvl]) {
                uint256 amoutSC = (_price * 25) / 100;
                innerFoundation = innerFoundation + amoutSC;
                payerToReceipy[msg.sender].amount += (_price - amoutSC);
                activate[msg.sender][_lvl] = false;
            } else {
                if (_parent != owner()) {
                    emit newSlot(msg.sender, _lvl);
                    updateS6(_parent, _lvl, true);
                }
            }
            _parentStruct.slot++;

            _setNull(_parent, _lvl);
        }

        _parentStruct.lastChild2++;
    }

    function _setNull(address _parent, uint256 lvl) internal {
        for (uint256 i = 0; i < 2; i++) {
            childsS6Lvl1[_parent][lvl].push(address(0));
        }

        for (uint256 i = 0; i < 4; i++) {
            childsS6Lvl2[_parent][lvl].push(address(0));
        }
    }

    function _addNavigation(
        address _structOwner,
        uint256 _lvl,
        uint256 _slot,
        bool _row
    ) internal {
        navigator[msg.sender].lvl.push(_lvl);
        navigator[msg.sender].ownerOfStruct.push(_structOwner);
        navigator[msg.sender].slot.push(_slot);
        navigator[msg.sender].row.push(_row);
    }
}

