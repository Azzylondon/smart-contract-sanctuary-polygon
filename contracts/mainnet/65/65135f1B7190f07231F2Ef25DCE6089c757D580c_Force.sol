pragma solidity >=0.7.0 <0.9.0;

import "hardhat/console.sol";
import "./programs.sol";

abstract contract S3 is Programs {
    struct structS3 {
        uint128 slot;
        uint128 lastChild;
        uint256 frozenMoneyS3;
    }

    struct structS6 {
        uint128 slot;
        uint64 lastChild1;
        uint64 lastChild2;
        uint256 frozenMoneyS6;
    }

    struct presenceOf {
        uint256[] lvl;
        uint256[] slot;
        address[] ownerOfStruct;
        bool[] row;
    }

    struct longTransferStorage {
        address to;
        uint256 amount;
    }

    event simpleBuy(address buyer, uint256 lvl);
    event upgrade(address buyer, uint256 newLvl);
    event newSlot(address buyer, uint256 lvl);

    mapping(address => presenceOf) internal navigator; // user -> to all of his presences

    mapping(address => mapping(uint256 => structS3)) public matrixS3; // user -> lvl -> structS3

    mapping(address => mapping(uint256 => address[])) public childsS3; // user -> lvl -> row1

    mapping(address => mapping(uint256 => structS6)) public matrixS6; // user -> lvl -> structS6

    mapping(address => mapping(uint256 => address[])) public childsS6Lvl1; // user -> lvl -> child
    mapping(address => mapping(uint256 => address[])) public childsS6Lvl2; // user -> lvl -> child

    mapping(address => longTransferStorage) internal payerToReceipy;

    uint256 innerFoundation;

    function addFrozenMoneyS3(uint256 _lvl, uint256 _amount) external {
        require(_lvl < 12, "Wrong level");
        structS3 storage _struct = matrixS3[msg.sender][_lvl];
        _struct.frozenMoneyS3 = _struct.frozenMoneyS3 + _amount;
        token.transferFrom(msg.sender, address(this), _amount);
    }

    function addFrozenMoneyS6(uint256 _lvl, uint256 _amount) external {
        require(_lvl < 12, "Wrong level");
        structS6 storage _struct = matrixS6[msg.sender][_lvl];
        _struct.frozenMoneyS6 = _struct.frozenMoneyS6 + _amount;
        console.log(msg.sender);
        console.log(_amount);
        token.transferFrom(msg.sender, address(this), _amount);
    }

    function changeAddress(address _addr) external {
        for (
            uint256 index = 0;
            index < navigator[msg.sender].lvl.length;
            index++
        ) {
            uint256 lvl = navigator[msg.sender].lvl[index];
            address strucOwner = navigator[msg.sender].ownerOfStruct[index];
            uint256 slot = navigator[msg.sender].slot[index];
            bool row = navigator[msg.sender].row[index];

            if (lvl == 3 || lvl == 6 || lvl == 9) {
                matrixS3[_addr][lvl] = matrixS3[msg.sender][lvl];

                childsS3[strucOwner][lvl][slot] = _addr;
            } else {
                matrixS6[_addr][lvl] = matrixS6[msg.sender][lvl];
                if (!row) {
                    childsS6Lvl1[strucOwner][lvl][slot] = _addr;
                } else {
                    childsS6Lvl2[strucOwner][lvl][slot] = _addr;
                }
            }
        }
        //delete matrixS6[msg.sender];
        //delete matrixS3[msg.sender];
    }
}

