import "./S6.sol";

contract Force is S6 {

  constructor(address _token) Ownable() {
    token = IERC20(_token);
  }

  function setTokens(address _SFC) external onlyOwner {
    SFC = _SFC;
  }

  function changeAutoReCycle(bool flag, uint _lvl) external {
    User storage cUser = users[msg.sender];
    cUser.autoReCycle[_lvl] = flag;
  }

  function changeAutoUpgrade(bool flag, uint _lvl) external {
    structS3 storage _structure = matrixS3[msg.sender][_lvl];
    if (_structure.frozenMoneyS3 != 0) {
      _structure.frozenMoneyS3 = 0;
      _sendDevisionMoney(msg.sender, _structure.frozenMoneyS3, 25);
    }

    User storage cUser = users[msg.sender];
    cUser.autoUpgrade[_lvl] = flag;
  }

}

