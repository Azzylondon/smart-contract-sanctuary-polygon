/**
 * @title ERC721 Non-Fungible Token Standard basic implementation
 * @dev see https://eips.ethereum.org/EIPS/eip-721
 */
contract MyCode is  BaseRelayRecipient {
    using SafeMath for uint256;
    using Address for address;
	
	
	// Mapping from address, token ID to owner address
    mapping (address => mapping(uint256 => address)) public tokenOwner;
    mapping (address => mapping(uint256 => address)) public approvedParty;

	
    // Events

    /// @notice event emitted when a user has TransferFromd a token
    event TransferFrom1(address owner, address whitelistedNFT, uint256 tokenId);



    // This is to support Native meta transactions
    // never use msg.sender directly, use _msgSender() instead
    function _msgSender() internal view returns (address payable sender) {
        return BaseRelayRecipient.msgSender();
    }


    /// @notice TransferFrom NFT and earn reward tokens.
    function TransferFrom(address _NFTaddress, address to_user,uint256 _tokenId) external {
        // require();
        _TransferFrom(_msgSender(),to_user, _NFTaddress, _tokenId);
    }
    /// @notice TransferFrom multiple NFTs and earn reward tokens.
    function TransferFromBatch(address to_user,address[] memory _NFTaddress,uint256[] memory _tokenIds) external {
        require(_NFTaddress.length == _tokenIds.length, "GuildWhitelisedNFTStaking.TransferFromBatch: Arrays must be equivalent lengths");
        for (uint i = 0; i < _tokenIds.length; i++) {
            _TransferFrom(_msgSender(),to_user, _NFTaddress[i], _tokenIds[i]);
        }
    }

    /**
     * @dev All the staking goes through this function
     * @dev Rewards to be given out is calculated
     * @dev Balance of TransferFromrs are updated as they TransferFrom the nfts based on ether price
    */
    function _TransferFrom(address _user, address to_user,address _NFTaddress, uint256 _tokenId) internal {

        IERC721(_NFTaddress).safeTransferFrom(
            _user,
            to_user,
            _tokenId
        );


        emit TransferFrom1(_user, _NFTaddress, _tokenId);
	}
}