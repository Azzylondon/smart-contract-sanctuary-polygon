// File: contracts/EventReusable.sol



pragma solidity ^0.8.0;










contract MortEvent is ERC1155Receiver, Ownable {
    using SafeCast for uint256;
    using SafeMath for uint256;
    using SignedSafeMath for int256;
    using mortlib for *;

    mapping(uint256 => address[]) public participants;
    mapping(address => mapping(uint256 => uint256)) public playerCont;
    uint256[] public totalRec;
    uint256[][] public unweightedRec;
    uint256[] public totalGoal;
    uint256[][] public multi;
    uint256[][] public thresholds;
    string[][] public thresholdTexts;
    MERC20[] public rewardsTokenContract;
    MERC1155 public immutable whitelistedNftContract;
    bool public enabled = true;
    bool public timed = false;
    uint256[] public activePools;
    mapping(uint256 => string) public poolColors;
    mapping(uint256 => string) public poolTitles;
    string public url;
    string public desc;
    string public title;
    uint256 public startTime;
    string public imageUrl;
    uint256 public poolcount;
    /**
     * Constructor.
     * @dev Reverts if the period length value is zero.
     * @dev Reverts if the cycle length value is zero.
     * @dev Warning: cycles and periods need to be calibrated carefully. Small values will increase computation load while estimating and claiming rewards. Big values will increase the time to wait before a new period becomes claimable.
     * @param whitelistedNftContract_ The ERC1155-compliant (optional ERC721-compliance) contract from which staking is accepted.
     * @param rewardsTokenContract_ The ERC20-based token used as staking rewards.
     */
    constructor(
        MERC1155 whitelistedNftContract_,
        MERC20[] memory rewardsTokenContract_
    )  {
        whitelistedNftContract = whitelistedNftContract_;
        rewardsTokenContract = rewardsTokenContract_;   
    }
    function definePool(uint256 index, string memory color_, string memory title_) public onlyOwner{
        poolColors[index] = color_;
        poolTitles[index] = title_;
    }
    function renew (uint256[] memory goal,
        uint256[][] memory multiplier,
        uint256[][] memory unweightedRec_,
        bool timed_,
        string memory desc_,
        string memory url_,
        string memory imageUrl_,
        string memory title_
        ) public onlyOwner {
        unweightedRec = unweightedRec_;
        totalGoal = goal;
        multi = multiplier;
        timed = timed_;
        desc = desc_;
        url = url_;
        imageUrl = imageUrl_;
        title = title_;
        enabled = true;
    }
    function renewTwo(uint256[] memory pools_,
        uint256[] memory totalRec_,
        uint256 poolcount_
  ) public onlyOwner {
        activePools = pools_;
        totalRec = totalRec_;
        poolcount = poolcount_;
    }
    function renewThree(
        uint256[][] memory thresholds_,
        string[][] memory thresholdTexts_) public onlyOwner {
        thresholds = thresholds_;
        thresholdTexts = thresholdTexts_;
    }
    function moveTheGoalPosts(uint256[] memory goals_) public onlyOwner {
        totalGoal = goals_;
    }
    function manualTimestamp(uint256 time) public onlyOwner {
        startTime = time;
    }
    function updateTokenContracts (MERC20[] memory rewardsTokenContract_)public onlyOwner{
        rewardsTokenContract = rewardsTokenContract_;
    }
    modifier isEnabled() {
        require(getEnabled(), "NftStaking: contract is not enabled");
        _;
    }
    function playerCount(uint pool) external view returns(uint count){
        count = participants[pool].length;
    }
    function overView () external view returns (string memory ret){
        for(uint i = 0; i < totalRec.length; i++){
            ret = string(abi.encodePacked(ret, "*"));
            ret = string(abi.encodePacked(ret, mortlib.uint2str(totalGoal[i])));
            ret = string(abi.encodePacked(ret, "|"));
            ret = string(abi.encodePacked(ret, mortlib.uint2str(totalRec[i] / (10 ** 18))));
            ret = string(abi.encodePacked(ret, "|"));
            for(uint j = 0; j < unweightedRec[i].length; j++){
                ret = string(abi.encodePacked(ret, mortlib.uint2str(unweightedRec[i][j] / (10 ** 18))));
                ret = string(abi.encodePacked(ret, "."));
            }
            ret = string(abi.encodePacked(ret, "|"));
            for(uint j = 0; j < multi[i].length; j++){
                ret = string(abi.encodePacked(ret, mortlib.uint2str(multi[i][j])));
                ret = string(abi.encodePacked(ret, "."));
            }
            ret = string(abi.encodePacked(ret, "|"));
            for(uint j = 0; j < thresholds[i].length; j++){
                ret = string(abi.encodePacked(ret, mortlib.uint2str(thresholds[i][j])));
                ret = string(abi.encodePacked(ret, "."));
            }
            ret = string(abi.encodePacked(ret, "|"));
            for(uint j = 0; j < thresholdTexts[i].length; j++){
                ret = string(abi.encodePacked(ret, thresholdTexts[i][j]));
                ret = string(abi.encodePacked(ret, "^"));
            }
            ret = string(abi.encodePacked(ret, "|"));
            ret = string(abi.encodePacked(ret, mortlib.uint2str(activePools[i])));
            ret = string(abi.encodePacked(ret, "|"));
            ret = string(abi.encodePacked(ret, poolColors[activePools[i]]));
            ret = string(abi.encodePacked(ret, "|"));
            ret = string(abi.encodePacked(ret, poolTitles[activePools[i]]));
        }
    }
    /**
     * ERC1155Receiver hook for single transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC1155Received(
        address, /*operator*/
        address,
        uint256,
        uint256, /*value*/
        bytes calldata /*data*/
    ) public virtual override returns (bytes4) {
        require(address(whitelistedNftContract) == _msgSender(), "Contract not whitelisted");
        return this.onERC1155Received.selector;
    }
    function metareport(uint pool) external view returns (string memory report){
        for(uint i = 0; i < activePools.length; i++){
            report = string(abi.encodePacked(report, "x", mortlib.uint2str(totalRec[i] / (10 ** 18))));
            for(uint j = 0; j < unweightedRec[i].length; j++){
                report = string(abi.encodePacked(report, "y", mortlib.uint2str(unweightedRec[i][j] / (10 ** 18))));
            }
        } 
    }
    function contReport(uint pool, uint size, uint startIndex) external view returns (string memory report){
        uint index = 0;
        for(uint i = startIndex; i < size + startIndex; i++){
           if(i >= participants[pool].length)
                break;
           report = string(abi.encodePacked(report, toAsciiString(participants[pool][i]), "|", mortlib.uint2str(playerCont[participants[pool][i]][pool] / (10 ** 18)), "*"));
        }
    }
    function deposit(uint256[] memory donate, uint pool) public isEnabled {
        if(timed)
            require(block.timestamp < startTime + totalGoal[pool], "Event is over RIP");
        for(uint i = 0; i < donate.length; i++){
            if(donate[i] == 0)
                continue;
            rewardsTokenContract[i].transferFrom(_msgSender(), address(0x000000000000000000000000000000000000dEaD), donate[i] * 10 ** 18);
            unweightedRec[pool][i] += donate[i] * 10 ** 18;
            playerCont[_msgSender()][activePools[pool]] += (donate[i] * (10 ** 18) * multi[i][pool] / 1000);
            totalRec[pool] += (donate[i] * (10 ** 18) * multi[i][pool] / 1000);
        }
        bool exists = false;
        for(uint i = 0; i < participants[activePools[pool]].length; i++){
            exists = exists || _msgSender() == participants[activePools[pool]][i];
            if(exists)
                break;
        }
        if(!exists){
            participants[activePools[pool]].push(_msgSender());
        }
    }

    function toAsciiString(address x) internal pure returns (string memory) {
    bytes memory s = new bytes(40);
    for (uint i = 0; i < 20; i++) {
        bytes1 b = bytes1(uint8(uint(uint160(x)) / (2**(8*(19 - i)))));
        bytes1 hi = bytes1(uint8(b) / 16);
        bytes1 lo = bytes1(uint8(b) - 16 * uint8(hi));
        s[2*i] = char(hi);
        s[2*i+1] = char(lo);            
    }
    return string(s);
}

function char(bytes1 b) internal pure returns (bytes1 c) {
    if (uint8(b) < 10) return bytes1(uint8(b) + 0x30);
    else return bytes1(uint8(b) + 0x57);
}

    /**
     * ERC1155Receiver hook for batch transfer.
     * @dev Reverts if the caller is not the whitelisted NFT contract.
     */
    function onERC1155BatchReceived(
        address, /*operator*/
        address,
        uint256[] calldata,
        uint256[] calldata, /*values*/
        bytes calldata /*data*/
    ) public virtual override returns (bytes4) {
        require(false, "Stop it. Get some help.");
        return this.onERC1155BatchReceived.selector;
    }

    function disable() internal {
        enabled = false;
    }

    function adminSwitch() public onlyOwner {
        enabled = !enabled;
    }

    function getEnabled() public view returns (bool) {
        if(timed)
            return startTime != 0 && (startTime + totalGoal[0]) > block.timestamp;
        else
            return enabled;
    }
}