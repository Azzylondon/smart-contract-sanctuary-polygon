/** @title Basket Contract. */
contract IndexInvest is Ownable {
    IPolylasticTokenV3 public polx;
    IUniswapPair public pairpolx;
    address public immutable WMATIC;
    uint256[] public multiplier;
    uint256[] public multiplier_paths;
    IUniswapPair[] public pairs;
    address[][] public paths;
    address router;
    uint256 constant totalPercentage = 900000;

    /** @notice Contract Events */
    event TokenBought();
    event Invested(address stakingAddress, address owner);
    event Unstaked(Staking stakingAddress, address owner);
    event PairsChanged(IUniswapPair[] pairs, uint256[] multiplier);
    event PathsChanged(address[][] paths, uint256[] multiplier_paths);

    /** @notice Contract Constructor.
     * @param _polx POLX token deployed address.
     * @param _pairpolx POLX / WMATIC Pair.
     * @param _WMATIC WMATIC token deployed address.
     * @param _multiplier Array of basket tokens percentages.
     * @param _multiplier_paths Array of tokens paths percentages.
     * @param _pairs Array of basket tokens pairs.
     * @param _paths Array of basket tokens paths used when pairs don't exist.
     * @param _router Array of basket tokens pairs.
     */
    constructor(
        IPolylasticTokenV3 _polx, // POLX deployed address:
        IUniswapPair _pairpolx, // Pair WMATIC / POLX deployed address:
        address _WMATIC, // WMATIC deployed address:
        uint256[] memory _multiplier, // Tokens Pairs Percentages array.
        uint256[] memory _multiplier_paths, // Tokens Pairs Paths Percentages array.
        IUniswapPair[] memory _pairs, // Token Pairs deployed addresses array.
        address[][] memory _paths, // Path of tokens needed for swaping non-existent pairs.
        address _router // Router address
    ) {
        polx = IPolylasticTokenV3(_polx);
        pairpolx = IUniswapPair(_pairpolx);
        WMATIC = _WMATIC;
        multiplier = _multiplier;
        multiplier_paths = _multiplier_paths;
        pairs = _pairs;
        paths = _paths;
        router = _router;

        require(
            pairs.length == multiplier.length,
            "Transaction failed: Pairs and Multipliers with different lengths."
        );
        require(
            paths.length == multiplier_paths.length,
            "Transaction failed: Paths and Path's Multipliers with different lengths."
        );
    }

    /** @notice Receive a MATIC amount from user and exchange it for basket tokens. */
    receive() external payable {
        uint256 amount;
        uint256 amountOfMATIC;
        uint256 amountOfWMATIC;
        require(msg.value > 0, "Transaction failed: Incorrect receive value.");
        uint256 matic = msg.value;

        // Iteration for calculates how much WMATIC it's needed.
        for (uint256 index = 0; index < pairs.length; index++) {
            // Verify that pair has matic or is zero address (pair doesnÂ´t exist):
            if (checkPairMATIC(pairs[index])) {
                amountOfMATIC += (multiplier[index] * matic) / 1_000_000;
            }
        }
        for (uint256 index = 0; index < paths.length; index++) {
            if (checkPathMATIC(paths[index])) {
                amountOfMATIC += (multiplier_paths[index] * matic) / 1_000_000;
            }
        }
        // Calculates amount of WMATIC needed:
        amountOfWMATIC = address(this).balance - amountOfMATIC;

        if (amountOfWMATIC > 0) {
            IWMATIC(WMATIC).deposit{value: amountOfWMATIC}(); // Deposit MATIC to WMATIC contract.
        }

        // Iteration for buying tokens in exchange of MATIC.
        for (uint256 index = 0; index < multiplier.length; index++) {
            amount = (multiplier[index] * matic) / 1_000_000;
            buyToken(amount, pairs[index], _msgSender());
        }
        for (uint256 index = 0; index < multiplier_paths.length; index++) {
            amount = (multiplier_paths[index] * matic) / 1_000_000;
            assert(IWMATIC(WMATIC).approve(router, amount));
            if (address(paths[index][0]) == address(0)) {
                address[] memory newPath = new address[](paths[index].length);
                for (uint256 i = 0; i < paths[index].length; i++) {
                    newPath[i] = paths[index][i] == address(0) ? WMATIC : paths[index][i];
                }
                IUniswapV2Router02(router).swapExactETHForTokens{value: amount}(1, newPath, _msgSender(), block.timestamp + 2000);
            } else {
                IUniswapV2Router02(router).swapExactTokensForTokens(amount, 1, paths[index], _msgSender(), block.timestamp + 2000);
            }
        }

        // Buying POLX with the remaining amount of MATIC.
        amount = IWMATIC(WMATIC).balanceOf(address(this));
        address staking = getStakingAddress(_msgSender(), block.timestamp);
        buyToken(amount, pairpolx, staking);
        emit Invested(staking, _msgSender());
    }

    /** @notice Exchange an amount of MATIC or WMATIC to a particular basket token or POLX.
     * @param amount Amount of MATIC or WMATIC used to buy the specific token or POLX.
     * @param pair Pair of tokens involved.
     */
    function buyToken(
        uint256 amount,
        IUniswapPair pair,
        address recipient
    ) private {
        if(address(pair) == address(0)) {
            assert(IWMATIC(WMATIC).transfer(recipient, amount));
        } else {
            require(
                checkPairMATIC(pair) || checkPairWMATIC(pair),
                "Buy Token failed: pair: pair not made up of matic or wmatic."
            );
            uint256 amountOutput = getAmountOut(pair, amount);
            if (checkPairWMATIC(pair)) {
                assert(IWMATIC(WMATIC).transfer(address(pair), amount));
                if (pair.token0() == WMATIC)
                    pair.swap(uint256(0), amountOutput, recipient, new bytes(0));
                else pair.swap(amountOutput, uint256(0), recipient, new bytes(0));
            } else {
                payable(address(pair)).transfer(amount);
                if (pair.token0() == address(0))
                    pair.swap(uint256(0), amountOutput, recipient, new bytes(0));
                else pair.swap(amountOutput, uint256(0), recipient, new bytes(0));
            }
        }
    }

    /** @notice Verify if pair contains MATIC token on its configuration
     * @param pair Pair of tokens.
     * @return bool
     */
    function checkPairMATIC(IUniswapPair pair) private view returns (bool) {
        if (address(pair) == address(0)) { // If address zero involved, it'll return WMATIC to user.
            return false;
        } else {
            if (pair.token0() == address(0)) return true;   
            if (pair.token1() == address(0)) return true;
        }
        return false;
    }

    /** @notice Verify if pair contains WMATIC token on its configuration
     * @param pair Pair of tokens.
     * @return bool
     */
    function checkPairWMATIC(IUniswapPair pair) private view returns (bool) {
        if (address(pair) == address(0)) {
            return false;
        } else {
            if (pair.token0() == WMATIC) return true;
            if (pair.token1() == WMATIC) return true;
        }
        return false;
    }

    /** @notice Verify if path contains MATIC token on its configuration
     * @param path Path of tokens.
     * @return bool
     */
    function checkPathMATIC(address[] memory path) private pure returns (bool) {
        if (path[0] == address(0)) return true;
        return false;
    }

    /** @notice Calculates Amount Out. Used in buyToken & buyPolx.
        @param amount Amount of transaction.
        @param pair Pair involved.
        @return amountOutput.
    */
    function getAmountOut(IUniswapPair pair, uint256 amount)
        private
        view
        returns (uint256)
    {
        (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();
        (uint256 reserveInput, uint256 reserveOutput) = pair.token0() == WMATIC
            ? (reserve0, reserve1)
            : (reserve1, reserve0);
        uint256 amountInWithFee = amount * 9970;
        uint256 numerator = amountInWithFee * reserveOutput;
        uint256 denominator = reserveInput * 10000 + amountInWithFee;
        uint256 amountOutput = numerator / denominator;
        return amountOutput;
    }

    /** @notice Calculate bytecode of unstake contract who will be deployed on the moment where 
    user decides to remove its POLX.
      * @param _owner User address.
      * @param _timestamp Timestamp created when user invested on POLX.
      * @param _polx Amount of POLX invested.
    */
    function getBytecode(
        address _owner,
        uint256 _timestamp,
        IPolylasticTokenV3 _polx
    ) private pure returns (bytes memory) {
        bytes memory bytecode = type(Staking).creationCode;
        return
            abi.encodePacked(bytecode, abi.encode(_polx, _owner, _timestamp));
    }

    /** @notice Calculate how much the user can claim from the POLX token and how much 
    it will burn based on the time it was stacked
      * @param _balance POLX user balance.
      * @param _timestamp Timestamp created when user invested on POLX.
    */
    function getStakingVal(uint256 _balance, uint256 _timestamp)
        public
        view
        returns (uint256 toClaim, uint256 toBurn)
    {
        // Here it goes the staking logic, example: 25% after 1 year etc.
        if (_timestamp < block.timestamp) {
            if (block.timestamp - _timestamp >= 360 days) {
                return (_balance, 0);
            } else if (block.timestamp - _timestamp >= 270 days) {
                return (
                    _balance - (_balance * 10) / 100,
                    (_balance * 10) / 100
                );
            } else if (block.timestamp - _timestamp >= 180 days) {
                return (
                    _balance - (_balance * 20) / 100,
                    (_balance * 20) / 100
                );
            } else {
                return (
                    _balance - (_balance * 30) / 100,
                    (_balance * 30) / 100
                );
            }
        }
    }

    /** @notice Calculate the address of unstake contract who will be deployed on the moment where 
    user decides to remove its POLX.
      * @param _owner User address.
      * @param _timestamp Timestamp created when user invested on POLX.
    */
    function getStakingAddress(address _owner, uint256 _timestamp)
        public
        view
        returns (address)
    {
        bytes memory bytecode = getBytecode(_owner, _timestamp, polx);
        bytes32 salt = keccak256(abi.encodePacked(_owner, _timestamp));
        bytes32 hash = keccak256(
            abi.encodePacked(
                bytes1(0xff),
                address(this),
                salt,
                keccak256(bytecode)
            )
        );

        // NOTE: cast last 20 bytes of hash to address
        return address(uint160(uint256(hash)));
    }

    /** @notice Deploy the unstake contract address which means user wants to remove its POLX.
     * @param timestamp Actual timestamp when user decides to unstake.
     */
    function unstake(uint256 timestamp) public {
        bytes32 salt = keccak256(abi.encodePacked(_msgSender(), timestamp));
        Staking stakingAddress = new Staking{salt: salt}(
            polx,
            payable(_msgSender()),
            timestamp
        );

        emit Unstaked(stakingAddress, _msgSender());
    }

    /** @notice OnlyOwner change pairs of basket and their percentages.
     * @param _pairs Array of pairs received from the owner.
     * @param _multiplier Array of percentages received from the owner.
     */
    function changePairs(
        IUniswapPair[] memory _pairs,
        uint256[] memory _multiplier
    ) public onlyOwner {
        require(
            _pairs.length == _multiplier.length,
            "Reverted: pairs and multipliers arrays lengths are not the same."
        );

        uint256 percentage;
        for (uint256 index = 0; index < _multiplier.length; index++) {
            percentage += _multiplier[index];
        }
        for (uint256 index = 0; index < multiplier_paths.length; index++) {
            percentage += multiplier_paths[index];
        }
        require(
            totalPercentage == percentage,
            "Reverted: total percentage must be 90%."
        );

        pairs = _pairs;
        multiplier = _multiplier;
        emit PairsChanged(pairs, multiplier);
    }

    /** @notice OnlyOwner change paths of basket and their percentages.
     * @param _paths Array of paths received from the owner.
     * @param _multiplier_paths Array of percentages received from the owner.
     */
    function changePaths(
        address[][] memory _paths,
        uint256[] memory _multiplier_paths
    ) public onlyOwner {
        require(
            _paths.length == _multiplier_paths.length,
            "Reverted: paths and their multipliers arrays lengths are not the same."
        );

        uint256 percentage;
        for (uint256 index = 0; index < _multiplier_paths.length; index++) {
            percentage += _multiplier_paths[index];
        }
        for (uint256 index = 0; index < multiplier.length; index++) {
            percentage += multiplier[index];
        }
        require(
            totalPercentage == percentage,
            "Reverted: total percentage must be 90%."
        );

        paths = _paths;
        multiplier_paths = _multiplier_paths;
        emit PathsChanged(paths, multiplier_paths);
    }

    function getPairs() public view returns (IUniswapPair[] memory) {
        return pairs;
    }

    function getPaths() public view returns (address[][] memory) {
        return paths;
    }

    function getMultipliers() public view returns (uint256[] memory) {
        return multiplier;
    }

    function getMultipliersPaths() public view returns (uint256[] memory) {
        return multiplier_paths;
    }
}

/** @title Staking Contract. */
contract Staking {
    address public constant deadWallet =
        0x000000000000000000000000000000000000dEaD;

    constructor(
        IPolylasticTokenV3 polx,
        address payable owner,
        uint256 timestamp
    ) {
        uint256 balance = polx.balanceOf(address(this));
        (uint256 toClaim, uint256 toBurn) = IndexInvest(payable(msg.sender))
            .getStakingVal(balance, timestamp);
        polx.transfer(owner, toClaim);

        /* After POLX "toClaim" transfer, the burn amount chenged because
        reflection. The new amount to burn is the balance of POLX tight now.*/
        toBurn = polx.balanceOf(address(this));
        if (toBurn > 0) polx.transfer(deadWallet, toBurn);

        // No actual MATIC should be held by this contract.
        selfdestruct(owner);
    }
}

