/**
 * @title TheOwnersClubPack
 *
 * TheOwnersClubPack - a tradeable pack of collectibles which can be unpacked
 */
contract TheOwnersClubPack is ERC721, ERC721URIStorage, ERC721Enumerable, ERC721Burnable, ContextMixin, NativeMetaTransaction, Ownable {
    using PackRandomizer for PackRandomizer.PackRandomizerState;
    using SafeMath for uint256;

    address ownerAddress = msg.sender;
    address proxyRegistryAddress;
    address factoryAddress;
    uint256 numCollectiblesInPack = 5;

    struct OwnersClubPack {
        string name;
        string season;
        uint256 serial;
    }

    OwnersClubPack[] public packs;
    PackRandomizer.PackRandomizerState state;

    event Minted(uint256 indexed tokenId, address indexed _to);

    constructor(address _proxyRegistryAddress) ERC721("TheOwnersClubPack", "TOCPACK") {
        proxyRegistryAddress = _proxyRegistryAddress;
        _initializeEIP712("TheOwnersClubPack");
    }

    function setState(uint256 _numClasses, address _factoryAddress, uint256 _seed) public onlyOwner {
        factoryAddress = _factoryAddress;
        PackRandomizer.initState(state, _factoryAddress, _numClasses, _seed);
    }

    /**
     *  Mint the pack token
     */
    function safeMint(
        address _to,
        string memory name,
        uint256 serial,
        string memory season,
        string memory _tokenURI
    ) public onlyOwner returns (uint256){
        uint256 tokenId = packs.length;

        // Enumerate in contract
        packs.push(
            OwnersClubPack(
                name,
                season,
                serial
            )
        );

        // Mint
        _safeMint(_to, tokenId);

        // Assign tokenURI
        _setTokenURI(tokenId, _tokenURI);

        // Emit minted event
        emit Minted(tokenId, _to);

        return tokenId;
    }

    /**
    * For chargebacks we will need the ability to transfer the token back to the owner
    */
    function chargebackTransfer(uint256 tokenId) public onlyOwner {
        address owner = ownerOf(tokenId);
        _safeTransfer(owner, ownerAddress, tokenId, ""); // always transfers to pack contract owner
    }

    /**
    * Add transferFrom
    */
    function transferFrom(address from, address to, uint256 tokenId) public override onlyOwner {
        _safeTransfer(from, to, tokenId, "");
    }

    /**
    * Distribute pack contents based w/ off-chain randomization
    */
    function unpackWithTokenIds(uint256 _tokenId, address _toAddress, address _ownerAddress, uint256[5] memory tokenIds) external onlyOwner {
        require(ownerOf(_tokenId) == _toAddress, "TheOwnersPack: Pack can only be unpacked to the packs owner");
        // Distribute nfts
        PackRandomizer.distribute(factoryAddress, _tokenId, _toAddress, _ownerAddress, tokenIds);
        // Burn the pack
        _burn(_tokenId);
    }

    /**
     *  Set items per pack
     */
    function setItemsPerPack(uint256 _amount) public onlyOwner {
        numCollectiblesInPack = _amount;
    }

    /**
     *  Return items per pack
     */
    function itemsPerPack() public view returns (uint256){
        return numCollectiblesInPack;
    }

    /**
   * Return the metadata for a tokenId
   */
    function getMetadata(uint256 tokenId) public view returns (
        string memory,
        uint256,
        string memory
    ){
        return (
        packs[tokenId].name,
        packs[tokenId].serial,
        packs[tokenId].season
        );
    }

    /**
    * Return the contract uri
    */
    function contractURI() public pure returns (string memory) {
        return "https://toc.otmnft.com/api/contract/theownersclubpack";
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory){
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool){
        return super.supportsInterface(interfaceId);
    }

    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-less listings.
     */
    function isApprovedForAll(address owner, address operator) override public view returns (bool){
        // Whitelist OpenSea proxy contract for easy trading.
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);

        if (address(proxyRegistry.proxies(owner)) == operator) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender() internal override view returns (address sender){
        return ContextMixin.msgSender();
    }
}


