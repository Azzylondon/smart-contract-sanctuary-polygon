contract TheOwnersClubFactory is Ownable {
    using Strings for string;
    using SafeMath for uint256;

    event Transfer(
        address indexed from,
        address indexed to,
        uint256 indexed tokenId
    );

    address public proxyRegistryAddress;
    address public nftAddress;
    address public packNftAddress;

    uint256 private supplyCap = 50000;

    constructor(address _proxyRegistryAddress, address _nftAddress, address _packNftAddress) {
        proxyRegistryAddress = _proxyRegistryAddress;
        nftAddress = _nftAddress;
        packNftAddress = _packNftAddress;
        fireTransferEvents(address(0), owner());
    }

    function name() external pure returns (string memory) {
        return "The Owners Club Factory";
    }

    function symbol() external pure returns (string memory) {
        return "TOCF";
    }

    function totalSupplyCap() external view returns (uint256) {
        return supplyCap;
    }

    function setSupplyCap(uint256 newSupplyCap) public onlyOwner returns (bool) {
        supplyCap = newSupplyCap;
        return true;
    }

    function transferOwnership(address newOwner) override public onlyOwner {
        address _prevOwner = owner();
        super.transferOwnership(newOwner);
        fireTransferEvents(_prevOwner, newOwner);
    }

    function fireTransferEvents(address _from, address _to) private {
        for (uint256 i = 0; i < 2; i++) {
            emit Transfer(_from, _to, i);
        }
    }

    function mintPack(
        address _toAddress,
        string memory _packName,
        uint256 _serial,
        string memory _season,
        string memory _tokenURI
    ) public onlyOwner {
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        uint256 numItemsAllocated = tocPack.itemsPerPack();
        require(canMint(numItemsAllocated), "Supply requirement not met");
        tocPack.safeMint(_toAddress, _packName, _serial, _season, _tokenURI);
    }

    function setupPackDistribution(uint256 _seed) public onlyOwner {
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        tocPack.setState(5, address(this), _seed);
    }

    function unpackWithTokenIds(
        uint256 _packTokenId,
        address _toAddress,
        address _ownerAddress,
        uint256[5] memory tokenIds
    ) public onlyOwner {
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        tocPack.unpackWithTokenIds(_packTokenId, _toAddress, _ownerAddress, tokenIds);
    }

    function transferPack(address owner, address toAddress, uint256 tokenId) public {
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        tocPack.transferFrom(owner, toAddress, tokenId);
    }

    function transferPacksBatch(address owner, address toAddress, uint256[] memory tokenIds) public {
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            tocPack.transferFrom(owner, toAddress, tokenId);
        }
    }

    function mintCard(
        address _toAddress,
        string memory _city,
        string memory _position,
        string memory _division,
        uint256 _serial,
        string memory _season,
        string memory _tokenURI
    ) public onlyOwner {
        TheOwnersClub toc = TheOwnersClub(nftAddress);
        require(canMint(1), "Supply requirement not met");
        toc.safeMint(_toAddress, _city, _position, _division, _serial, _season, _tokenURI);
    }

    function transferFrom(address owner, address toAddress, uint256 tokenId) public {
        TheOwnersClub toc = TheOwnersClub(nftAddress);
        toc.transferFrom(owner, toAddress, tokenId);
    }

    function transferCardsBatch(address owner, address toAddress, uint256[] memory tokenIds) public {
        TheOwnersClub toc = TheOwnersClub(nftAddress);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            toc.transferFrom(owner, toAddress, tokenId);
        }
    }

    /**
    * Chargeback card transfer
    */
    function chargebackTransferCard(uint256 tokenId) public {
        TheOwnersClub toc = TheOwnersClub(nftAddress);
        toc.chargebackTransfer(tokenId);
    }

    /**
    * Chargeback pack transfer
    */
    function chargebackTransferPack(uint256 tokenId) public {
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        tocPack.chargebackTransfer(tokenId);
    }

    function canMint(uint256 numItemsAllocated) public view returns (bool) {
        TheOwnersClub toc = TheOwnersClub(nftAddress);
        uint256 tocSupply = toc.totalSupply();
        return tocSupply < (supplyCap - numItemsAllocated);
    }

    function getCardTotalSupply() public view returns (uint256){
        TheOwnersClub toc = TheOwnersClub(nftAddress);
        return toc.totalSupply();
    }

    function getPackTotalSupply() public view returns (uint256){
        TheOwnersClubPack tocPack = TheOwnersClubPack(packNftAddress);
        return tocPack.totalSupply();
    }

    function isApprovedForAll(address _owner, address _operator) public view returns (bool){
        if (owner() == _owner && _owner == _operator) {
            return true;
        }

        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);

        if (
            owner() == _owner &&
            address(proxyRegistry.proxies(_owner)) == _operator
        ) {
            return true;
        }

        return false;
    }

    function ownerOf(uint256 _tokenId) public view returns (address _owner) {
        return owner();
    }
}


// File contracts/TheOwnersClubMarketplace.sol

pragma solidity ^0.8.4;








contract TheOwnersClubMarketplace is ReentrancyGuard, IERC721Receiver {
  using Counters for Counters.Counter;
  using SafeMath for uint256;
  using Strings for string;

  Counters.Counter private _itemIds;
  Counters.Counter private _itemsSold;

  IERC20 token;

  address payable owner;

  constructor(address _token) {
    owner = payable(msg.sender);
    token = IERC20(_token);
  }

  struct MarketItem {
    uint itemId;
    address nftContract;
    uint256 tokenId;
    string itemType;
    address payable seller;
    address payable owner;
    uint256 price;
    bool sold;
  }

  mapping(uint256 => MarketItem) private idToMarketItem;

  uint256 private _basisPoints = 100;
  uint256 private _feeBasis = 5;

  event MarketItemCreated (
    uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    string itemType,
    address seller,
    address owner,
    uint256 price,
    bool sold
  );

  event MarketItemSold (
    uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    string itemType,
    address seller,
    address buyer,
    uint256 price,
    bool sold
  );

  event MarketItemRemoved (
    uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    string itemType,
    address seller,
    address remover
  );

  event MarketItemPriceChanged (
    uint indexed itemId,
    address indexed nftContract,
    uint256 indexed tokenId,
    string itemType,
    address seller,
    uint256 price
  );

  function getFee() public view returns (uint256) {
    return _feeBasis;
  }

  function setFee(uint256 _newBasis) public returns (bool) {
    require(msg.sender == owner, "Can only be set by owner");
    _feeBasis = _newBasis;
    return true;
  }

  function getItemsSold() public view returns (uint256) {
    return _itemsSold.current();
  }

  function compareStrings(string memory a, string memory b) private view returns (bool) {
    return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
  }

  function createMarketItem(
    address nftContract,
    uint256 tokenId,
    uint256 price,
    string memory itemType
  ) public nonReentrant {
    require(price > 0, "Price must be at least 1 wei");
    require(compareStrings(itemType,"card") || compareStrings(itemType,"pack"), "Item for sale must be a card or a pack");

    _itemIds.increment();
    uint256 itemId = _itemIds.current();

    idToMarketItem[itemId] = MarketItem(
      itemId,
      nftContract,
      tokenId,
      itemType,
      payable(msg.sender),
      payable(address(this)),
      price,
      false
    );

     TheOwnersClubFactory factory = TheOwnersClubFactory(nftContract);

     if(compareStrings(itemType,"card")){
        factory.transferFrom(msg.sender, address(this), tokenId);
    } else {
        factory.transferPack(msg.sender, address(this), tokenId);
    }

    emit MarketItemCreated(
      itemId,
      nftContract,
      tokenId,
      itemType,
      msg.sender,
      address(this),
      price,
      false
    );
  }

  // Accept weth tokens (non-payable as its not matic native)
  function createMarketSale(
    address nftContract,
    uint256 itemId,
    uint256 tokenAmount
    ) public nonReentrant {
    uint price = idToMarketItem[itemId].price;
    uint tokenId = idToMarketItem[itemId].tokenId;
    require(tokenAmount == price, "Please submit the asking price in order to complete the purchase");

    uint256 sellerBasis = SafeMath.sub(_basisPoints, _feeBasis);
    uint256 sellerPayment = tokenAmount.mul(sellerBasis).div(_basisPoints);
    uint256 ownerPayment = tokenAmount.mul(_feeBasis).div(_basisPoints);

    idToMarketItem[itemId].owner = payable(msg.sender);
    idToMarketItem[itemId].sold = true;

    _itemsSold.increment();

    TheOwnersClubFactory factory = TheOwnersClubFactory(nftContract);

    if(compareStrings(idToMarketItem[itemId].itemType,"card")){
        factory.transferFrom(address(this), msg.sender, tokenId);
    } else {
        factory.transferPack(address(this), msg.sender, tokenId);
    }

    // Transfer funds to seller and owner
    require(token.transferFrom(msg.sender, idToMarketItem[itemId].seller, sellerPayment));
    require(token.transferFrom(msg.sender, payable(owner), ownerPayment));

    emit MarketItemSold(
      itemId,
      nftContract,
      tokenId,
      idToMarketItem[itemId].itemType,
      idToMarketItem[itemId].seller,
      msg.sender,
      price,
      true
    );
  }

  function unlist(
      address nftContract,
      uint256 itemId
      ) public payable nonReentrant {
      address payable seller = payable(idToMarketItem[itemId].seller);
      bool sold = idToMarketItem[itemId].sold;
      require(!sold, "Item was already sold");
      require(msg.sender == seller || msg.sender == owner, "Only seller can unlist");

      TheOwnersClubFactory factory = TheOwnersClubFactory(nftContract);

      uint tokenId = idToMarketItem[itemId].tokenId;

      idToMarketItem[itemId].owner = seller;
      idToMarketItem[itemId].sold = true;

      if(compareStrings(idToMarketItem[itemId].itemType,"card")){
          factory.transferFrom(address(this), seller, tokenId);
      } else {
          factory.transferPack(address(this), seller, tokenId);
      }

      // Treated the same as s  old as it removes the available items
      _itemsSold.increment();

      emit MarketItemRemoved(
        itemId,
        nftContract,
        tokenId,
        idToMarketItem[itemId].itemType,
        idToMarketItem[itemId].seller,
        msg.sender
      );
   }

   function changeListingPrice(
      address nftContract,
      uint256 itemId,
      uint256 newPrice
      ) public payable nonReentrant {
      require(newPrice > 0, "New price must be at least 1 wei");

      address payable seller = payable(idToMarketItem[itemId].seller);
      bool sold = idToMarketItem[itemId].sold;
      require(!sold, "Item was already sold");
      require(msg.sender == seller || msg.sender == owner, "Only seller can unlist");

      uint tokenId = idToMarketItem[itemId].tokenId;

      idToMarketItem[itemId].price = newPrice;

      emit MarketItemPriceChanged(
        itemId,
        nftContract,
        tokenId,
        idToMarketItem[itemId].itemType,
        idToMarketItem[itemId].seller,
        newPrice
      );
   }

  function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
    return this.onERC721Received.selector;
  }

  function fetchMarketItems() public view returns (MarketItem[] memory) {
    uint itemCount = _itemIds.current();
    uint unsoldItemCount = _itemIds.current() - _itemsSold.current();
    uint currentIndex = 0;

    MarketItem[] memory items = new MarketItem[](unsoldItemCount);

    if(unsoldItemCount == 0){
       return items;
    }

    for (uint i = 0; i < itemCount; i++) {
      if (idToMarketItem[i + 1].owner == address(this)) {
        uint currentId = i + 1;
        MarketItem storage currentItem = idToMarketItem[currentId];
        items[currentIndex] = currentItem;
        currentIndex += 1;
      }
    }

    return items;
  }

  function ownerItems() public view returns (MarketItem[] memory) {
    uint totalItemCount = _itemIds.current();
    uint itemCount = 0;
    uint currentIndex = 0;

    for (uint i = 0; i < totalItemCount; i++) {
      if (idToMarketItem[i + 1].owner == msg.sender) {
        itemCount += 1;
      }
    }

    MarketItem[] memory items = new MarketItem[](itemCount);
    for (uint i = 0; i < totalItemCount; i++) {
      if (idToMarketItem[i + 1].owner == msg.sender) {
        uint currentId = i + 1;
        MarketItem storage currentItem = idToMarketItem[currentId];
        items[currentIndex] = currentItem;
        currentIndex += 1;
      }
    }
    return items;
  }

  function sellerItems() public view returns (MarketItem[] memory) {
    uint totalItemCount = _itemIds.current();
    uint itemCount = 0;
    uint currentIndex = 0;

    for (uint i = 0; i < totalItemCount; i++) {
      if (idToMarketItem[i + 1].seller == msg.sender) {
        itemCount += 1;
      }
    }

    MarketItem[] memory items = new MarketItem[](itemCount);
    for (uint i = 0; i < totalItemCount; i++) {
      if (idToMarketItem[i + 1].seller == msg.sender) {
        uint currentId = i + 1;
        MarketItem storage currentItem = idToMarketItem[currentId];
        items[currentIndex] = currentItem;
        currentIndex += 1;
      }
    }
    return items;
  }
}