pragma solidity =0.8.11;

import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {IUniswapV2Pair} from "./IUniswapV2Pair.sol";

/// @author Philippe Dumonet
contract Dumper is Ownable {
    constructor() Ownable() {}

    function arbTx(address _target, bytes calldata _data) external onlyOwner {
        (bool success, bytes memory result) = _target.call(_data);
        require(success, string(result));
    }

    function dumpAllV2(IUniswapV2Pair _pool, address _token) external onlyOwner {
        address token0 = _pool.token0();
        address token1 = _pool.token1();
        bool isToken0 = token0 == _token;
        require(isToken0 || token1 == _token, "Dumper: Token not in pool");
        uint256 amountIn = IERC20(_token).balanceOf(msg.sender);
        IERC20(_token).transferFrom(msg.sender, address(_pool), amountIn);
        (uint256 reserve0, uint256 reserve1, ) = _pool.getReserves();
        (uint256 reserveIn, uint256 reserveOut) = isToken0
            ? (reserve0, reserve1)
            : (reserve1, reserve0);
        uint256 amountOut = reserveOut -
            (reserveOut * reserveIn * 1000) /
            (reserveIn * 1000 + amountIn * 997) -
            1;
        (uint256 amountOut0, uint256 amountOut1) = isToken0
            ? (uint256(0), amountOut)
            : (amountOut, uint256(0));
        _pool.swap(amountOut0, amountOut1, msg.sender, "");
    }
}

