        uint256 _rewards = _spendSmartAssetsCreditFunction(2, 1);
        arianeeEvent.create(_eventId, _tokenId, _imprint, _uri, _rewards, msg.sender);
        _dispatchRewardsAtHydrate(_providerBrand, _rewards);
