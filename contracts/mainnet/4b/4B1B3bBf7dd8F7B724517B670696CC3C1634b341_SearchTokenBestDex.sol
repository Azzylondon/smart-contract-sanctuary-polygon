interface IFactory {
    function getPair(address tokenA, address tokenB) external view returns (address pair);
}

interface IPair {
    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
}

contract SearchTokenBestDex {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint;
    
    address public governor;
    
    // Factory Set
    EnumerableSet.AddressSet factorySet;
    
    constructor() public {
        governor = msg.sender;
    }
    
    modifier onlyGov() {
        require(msg.sender == governor, 'SearchTokenBestDex: NOT_THE_GOVERNOR');
        _;
    }
    
    /* Gov Op */
    
    function setGov(address governor_) external onlyGov {
        require(governor_ != address(0), 'SearchTokenBestDex: ZERO_ADDRESS');
        governor = governor_;
    }
    
    /* Factory Op */
    
    function addFactory(address factory_) external onlyGov {
        require(factory_ != address(0), "SearchTokenBestDex: ZERO_ADDRESS");
        factorySet.add(factory_);
    }
    
    function removeFactory(address factory_) external onlyGov {
        require(factory_ != address(0), "SearchTokenBestDex: ZERO_ADDRESS");
        factorySet.remove(factory_);
    }
    
    function inFactory(address factory_) public view returns(bool) {
        require(factory_ != address(0), "SearchTokenBestDex: ZERO_ADDRESS");
        return factorySet.contains(factory_);
    }
    
    function getFactoryLength() public view returns(uint256) {
        uint256 len = factorySet.length();
        return len;
    }
    
    function getFactory(uint256 i) public view returns(address) {
        address _factory = factorySet.at(i);
        return _factory;
    }
    
    function getAllFactory() external view returns(address[] memory) {
        uint256 len = factorySet.length();
        address[] memory factoryAddrs = new address[](len);
        for(uint256 i; i < len; i++) {
            address _factory = factorySet.at(i);
            factoryAddrs[i] = _factory;
        }
        return factoryAddrs;
    } 
    
    /* Search Token Liquidity */
    
    function sortTokens(address tokenA, address tokenB) public pure returns (address token0, address token1) {
        require(tokenA != tokenB, 'SearchTokenBestDex: IDENTICAL_ADDRESSES');
        (token0, token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), 'SearchTokenBestDex: ZERO_ADDRESS');
    }
    
    function getReserves(address pair, address tokenA, address tokenB) public view returns (uint, uint) {
        uint reserveA;
        uint reserveB;
        (address token0,) = sortTokens(tokenA, tokenB);
        if (pair == address(0)) {
            return (reserveA, reserveB);
        } else {
            (uint reserve0, uint reserve1,) = IPair(pair).getReserves();
            (reserveA, reserveB) = tokenA == token0 ? (reserve0, reserve1) : (reserve1, reserve0);   
            return (reserveA, reserveB);
        }
    }
    
    function getPair(address factory, address tokenA, address tokenB) public view returns(address) {
        return IFactory(factory).getPair(tokenA, tokenB);
    }
    
    function searchTokenLiquidity(address token, address coin, bool isCoinStandard) external view returns(uint256[] memory) {
        uint256 len = factorySet.length();
        uint256[] memory tokenLiquidity = new uint256[](len);
        for(uint256 i; i < len; i++) {
            IFactory factory = IFactory(getFactory(i));
            address pair = factory.getPair(token, coin);
            if(pair == address(0)) {
                tokenLiquidity[i] = 0;
            } else {
                (uint256 tokenReserve, uint256 coinReserve) = getReserves(pair, token, coin);
                if (isCoinStandard) {
                    tokenLiquidity[i] = coinReserve.mul(2);
                } else {
                    tokenLiquidity[i] = tokenReserve.mul(2);
                }
            }
        }
        return tokenLiquidity;
    }
    
}